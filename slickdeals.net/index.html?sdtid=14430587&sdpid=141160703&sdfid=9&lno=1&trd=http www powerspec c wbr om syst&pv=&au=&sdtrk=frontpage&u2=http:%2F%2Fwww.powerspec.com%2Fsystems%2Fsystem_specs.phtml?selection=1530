<script type="text/javascript">
// function to get a value from a GET string given a parameter name
function fillin()
{
	var name = "selection";
	name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
	var regexS = "[\\?&]"+name+"=([^&#]*)";
	var regex = new RegExp( regexS );
	var results = regex.exec( window.location.href );
	if ( results == null ) {
		// no keyword in URL
	} else {
		var keyword_string = results[1].replace("+", " ");
		document.getElementById('modellink').href = "/support/support_archive.phtml?selection="+keyword_string;
		keyword_string = results[1].replace("%20", " ");
		document.getElementById('modelnumber').innerHTML = keyword_string;
	}
}
</script>
