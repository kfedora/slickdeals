                <div class="ajaxcart__totals_value">{{{totalPrice}}}</div>
            </div>
            <div class="cart-actions">
                <a class="cart-btn cta-1" href="/cart" role="button"><span>Continue To Cart</span></a>
                
                
            </div>
        </div>
      
    </form>

</script>
  
