    display: none;
  }

  /* Sticky Nav - text */
  .header-subbottom-mobile p {
    margin: 0;
    color: #fff;
    font-style: normal;
    text-transform: uppercase;
    font-size: 13px;
    line-height: 14px;
    padding: 0.4em 0;
  }

  /* 'Satisfaction Guaranteed' link */
  .sticky-subnav-text a {
    color: rgb(231, 117, 111);
    text-decoration: underline;
  }

  @media only screen and (max-width: 575px), only screen and (max-device-width: 575px) {
    /* show subnav/sticky nav on mobile */
    .header-subbottom-mobile {
      display: block;
    }

    /* remove padding below existing sticky nav */
    .responsive #header.fixed {
      padding-bottom: 0 !important;
    }

    /* push the page content down to make room for the sticky nav */
    .responsive.with-bottom-nav #main {
      padding-top: 75px !important;
    }
  }
</style>
