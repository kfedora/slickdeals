   postStr += "clientId=samsHelp&"
if(getCookie("firstNameCookie")) {
   postStr += "userName=" + getCookie("firstNameCookie") +"&";
}
else {
  postStr += "userName=" + $('.samsUsername').html() + "&";
}
if(getCookie("pilotusercookie")){
    postStr += "membershipId=" + (decodeURIComponent(getCookie("pilotusercookie")).split(":")[1]).split("|")[0] + "&";
}
if(getCookie("samsHubbleSession")){
      postStr += "jSessionId=" + getCookie("samsHubbleSession") + "&";
}
isChatbotLoad = true;
$('#iframer').attr('src',postStr);
 $('.display-initial').fadeIn(500);
 $("#TRM_ChatBot").fadeIn(500);
 $('.display-frame').show();
 $('.feedbackCont_chatBot').hide();
 $('.samsChatIconCont_article').addClass('chat_relative');
 if($(".samsChatIconCont_article").hasClass("myClass")){
        $("html,body").animate({ scrollTop: 0 }, "slow");
     }
  if(samsIsMobile()){
      $('body').css('overflow','hidden')
     $('.storeHeader, .rctWrapper, .footer').hide();
     $('#leaderboardBottomAd').hide();
  }
  else
  {
    $('#oo_tab.oo_tab_right').animate({right: "-75px"});
  }
 try{
    ajaxSiteCatalyst("help-center","chatbot","open");
   }catch(e)
   {}
}
function handleChatbotposition(){
   if(!samsIsMobile()){
   $('.samsChatIconCont_article').css('top', initialChatBoxTop - $(window).scrollTop());
   $('.samsChatIconCont_article').css('left',$('.samsRightCont').offset().left + 20);   
   $(window).scroll(function () {      
      var foot_Height = $('.footer').outerHeight();
      var win_height = ($(window).height() - 200);
      var top = $('.samsRightCont').height() + $('.samsRightCont').offset().top + ($('.samsChatIconCont_article').height()/2);
      if ($(window).scrollTop() > top / 2) {
         $('.samsChatIconCont_article').css('top', $(window).innerHeight() / 2);
      }else {
         $('.samsChatIconCont_article').css('top', top);
      }           
    });
   }
}
var isFeedbackTaken; 
function onMessageReceived(event) {
   //console.log("In onMessageReceived");
   var data = event.data;
   if(data.from === 'Chatbot') {
      // Define a global variable which can be used later to check if feedback is already taken.
      isFeedbackTaken = data.isFeedbackTaken;
      console.log('isFeedbackTaken', isFeedbackTaken);
   }   
}
window.addEventListener('message',onMessageReceived, false);
function hideDisplay() {
   var ratingVal;
   if(isFeedbackTaken) {
      $('.samsChatIconCont_article').removeClass('chat_relative');
      $('.display-initial').fadeOut(500);
      $("#TRM_ChatBot").fadeOut(500);
      handleChatbotposition();
   }else{
         $('.display-frame').hide();
         $('.feedbackCont_chatBot').show();
   }
  //$('.feedbackCont_chatBot').show();
  if(samsIsMobile()){
    $('.FeedbacksmileyImg').click(function(e){
        smileyFn(this);
        e.stopPropagation();
        e.stopImmediatePropagation();
    });
    $('.FeedbacksmileyImgHide').click(function(e){
    ratingVal = $(this).parent().attr('rate');
    $(this).addClass('face');
    setTimeout(function(){
    $('.feedBackMsgCont').show();
    $('.feedbackCont_chatBot,.display-frame').hide();
    $('.feedbackMsg').click(function(e){
        e.stopPropagation();
        e.stopImmediatePropagation();
    });
    $('.feedbackSendMsgBtn').click(function(e){
        var data = {ccid : '-1',
                    comment_card : '1',
                    referer : "http://chatbot.samsclub.com/",
                    overall : ratingVal,
                    comments : $('.feedbackMsg').val(),
                    customVars : ''
                  };
                $.ajax({
                  url: "https://secure.opinionlab.com/rate40.asp",
                type: "POST",
                data: data,
                dataType:'jsonp',
                crossDomain: true,
                  success: function(data)
                  {
                    console.log(data);
                    $('.feedBackMsgCont').hide();
                    $('.FeedbackSentCont').show();
                  },
                  error: function(data){
                    console.log(data);
                    $('.feedBackMsgCont').hide();
                    $('.FeedbackSentCont').show();
                  }
                });
        e.stopPropagation();
        e.stopImmediatePropagation();
    });
  },1500);
    e.stopPropagation();
    e.stopImmediatePropagation();
  });
  }
else
{
  $('.FeedbacksmileyImg').hover(function(){
      smileyFn(this);
  });
  $('.feedbackSmileyCont').mouseleave(function(){
    $('.FeedbacksmileyImgHide,.smileyTxt').hide();
    $('.FeedbacksmileyImg').show();
  });
  $('.FeedbacksmileyImgHide').click(function(e){
    ratingVal = $(this).parent().attr('rate');
    $(this).addClass('face');
    setTimeout(function(){
    $('.feedBackMsgCont').show();
    $('.feedbackCont_chatBot,.display-frame').hide();
    $('.feedbackMsg').click(function(e){
        e.stopPropagation();
        e.stopImmediatePropagation();
    });
    $('.feedbackSendMsgBtn').click(function(e){
        var data = {ccid : '-1',
                    comment_card : '1',
                    referer : "http://chatbot.samsclub.com/",
                    overall : ratingVal,
                    comments : $('.feedbackMsg').val(),
                    customVars : ''
                  };
                $.ajax({
                  url: "https://secure.opinionlab.com/rate40.asp",
                type: "POST",
                data: data,
                dataType:'jsonp',
                crossDomain: true,
                  success: function(data)
                  {
                    console.log(data);
                    $('.feedBackMsgCont').hide();
                    $('.FeedbackSentCont').show();
                  },
                  error: function(data){
                    console.log(data);
                    $('.feedBackMsgCont').hide();
                    $('.FeedbackSentCont').show();
                  }
                });
        e.stopPropagation();
        e.stopImmediatePropagation();
    });
  },1500);
    e.stopPropagation();
    e.stopImmediatePropagation();
  });
}
  $('.samsChatCloseIcon').unbind('click').bind('click', function(e){
   $('.samsChatIconImg').addClass('chatIconAnimate');
    $('.samsChatIconCont_article').removeClass('chat_relative');
     $('.display-initial').fadeOut(500);
      $("#TRM_ChatBot").fadeOut(500);
      $('.display-frame').attr('src', '');
      if(samsIsMobile()){
        $('body').css('overflow','auto')
        $('.storeHeader, .rctWrapper, .footer').show();
        $('#leaderboardBottomAd').show();
    }
    else
    {
      $('#oo_tab.oo_tab_right').animate({right: "-47px"});
    }
      try{
      ajaxSiteCatalyst("help-center","chatbot","close");
     }catch(e)
     {
     }
      handleChatbotposition();
     e.stopPropagation();
    e.stopImmediatePropagation();
   });
}
