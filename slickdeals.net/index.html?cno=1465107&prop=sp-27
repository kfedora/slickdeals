    if( typeof(marketingDigitalData) == "undefined" ) {
        var marketingDigitalData = {"page":{"pageInfo":{"pageID":"/content/t-mobile/consumer/cell-phone-plans/military-discount-plans","templateName":"marketing-page-template","pageName":"military-discount-plans","pageTitle":"Unlimited Phone Plans w/ Military & Veteran Family Discounts | T-Mobile","version":"","pageURL":"","language":"en_US","variant":"","currencyCode":"USD","TFN":{}},"category":{"subCategory":""},"modules":{"moduleCount":0,"moduleList":[],"moduleListAll":[]}}};
    }
    marketingDigitalData.page.pageInfo.pageURL = window.location.href;
    marketingDigitalData.invoca = marketingDigitalData.invoca || {};
    marketingDigitalData.invoca.fireTag = "T";
    marketingDigitalData.invoca.numberSelector = ".tmo_tfn_number";
    
