			var mobileMenu = false;
			
			function ShowMobileMenuHover(show){
				mobileMenu = !show;
				ShowMobileMenu ();
			}
			
			function ShowMobileMenu () {
				mobileMenu = !mobileMenu;
				
				if(mobileMenu){
					document.getElementById("mobile-menu-wrapper").querySelectorAll("#cssmenu")[0].setAttribute("style", "width: 300px");
				}else{
					document.getElementById("mobile-menu-wrapper").querySelectorAll("#cssmenu")[0].setAttribute("style", "width: 0px");
				}
			}
