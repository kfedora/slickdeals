			<!-- Custom JS -->
			<script type="text/javascript">
				//var showMenus = false;
				var showEmbedLinks = false;
				var showReportMenu = false;

				function ShowEmbedLinks() {
					//showMenus = false;
					
					showReportMenu = false;
					document.getElementById("report-screenshot").style.display = "none";
					
					showEmbedLinks = !showEmbedLinks;
					if(showEmbedLinks){
						document.getElementById("screenshot-embed-links").style.display = "block";
					}else{
						document.getElementById("screenshot-embed-links").style.display = "none";
					}

					//document.getElementById("menus").style.display = "none";
				}
				
				function ShowReportForm() {
					//showMenus = false;
					//showEmbedLinks = false;
					//document.getElementById("screenshot-embed-links").style.display = "none";
					
					showReportMenu = !showReportMenu;
					
					if(showReportMenu){
						document.getElementById("report-screenshot").style.display = "block";
					}else{
						document.getElementById("report-screenshot").style.display = "none";
					}
					
					//document.getElementById("menus").style.display = "none";
				}
				
				//Prevent form from submitting, call jQuery function instead
				$("#report-form-main").submit(function( event ) {
					//alert( "Handler for .submit() called." );
					event.preventDefault();
					
					var screenshotReference = $("#ref").val();
					var reason =  $("input[name='reason']:checked", "#report-form-main").val(); 
					//var email = $("#email").val();
					var captcha = $("#g-recaptcha-response").val();
					ReportScreenshot(screenshotReference, reason/*, email*/, captcha);
				});
				
				//Focus on embed link we click
				$("#screenshot-embed-links input").click(function() {
					$(this).select();
				});
			</script>
				
		
		
