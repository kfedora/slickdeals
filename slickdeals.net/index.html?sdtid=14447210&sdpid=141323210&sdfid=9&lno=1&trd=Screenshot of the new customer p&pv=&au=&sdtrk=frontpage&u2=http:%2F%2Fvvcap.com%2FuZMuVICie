					<script type="text/javascript">
						var zoomed = false;
						
						$("#main-image").click(function(){
							zoomed = !zoomed;
							
							if(zoomed){
								$("#main-image").removeAttr('style');
								
								$("#img-parent-wrapper").css({
								   "max-width" : "100%",
								   "max-height" : "none"
								});
								
								$("#main-image").css("cursor", "-webkit-zoom-out");
								$("#main-image").css("cursor", "-moz-zoom-out");
								$("#main-image").css({
								   "max-width" : "none",
								   "max-height" : "none"
								});
							}else{
								$("#img-parent-wrapper").removeAttr('style');
								$("#main-image").removeAttr('style');
								$("#main-image").css("max-height", "725px");
							}
						});
							
					</script>
