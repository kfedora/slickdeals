<script type="text/javascript">
$(document).ready(function() {
	if (App.Components && App.Components.Marquee) {
	App.Components.Marquee.init = function(a) {
                var b = a.children().not("style,link,script"),
                    c = this;
                b.eq(c.current).addClass("active"), b.length < 2 || (clearInterval(c.interval), c.interval = setInterval(function() {
                    c.current = (c.current + 1) % b.length, b.filter(".active").removeClass("active").addClass("inactive"), b.eq(c.current).addClass("active"), setTimeout(function() {
                        b.removeClass("inactive")
                    }, 500)
                }, 4000), a.off("focus.pause-marquee").on("focus.pause-marquee", "a, button, [tabindex=0]", function(d) {
                    a.scrollTop(0);
                    var e = $(this),
                        f = $(this).closest(b);
                    f.length > 0 ? setTimeout(function() {
                        e.hasClass("focus") && (clearInterval(c.interval), c.interval = null, b.filter(".active").removeClass("active").addClass("inactive"), f.addClass("active"), b.each(function(a, b) {
                            return b === f[0] ? (c.current = a, !1) : void 0
                        }), setTimeout(function() {
                            b.removeClass("inactive")
                        }, 500))
                    }) : c.init(a)
                }).off("blur.unpause-marquee").on("blur.unpause-marquee", "a, button, [tabindex=0]", function() {
                    setTimeout(function() {
                        0 == $(":focus").closest(a).length && c.init(a)
                    })
                }))
            };

	App.Components.Marquee.init($('[data-component="Marquee"]'));
	}
});
</script>

