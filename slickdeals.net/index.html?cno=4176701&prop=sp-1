        <div class="pd-features">
            <h2 class="hidden-xs pd-section-title">Features</h2>
                    
                    
<div class="pd-feature-wrap ImageTop">

        <div class="pd-feature-item">
            
            <div class="pd-content-wrap">
                
<h2 class="pd-bottom-title">Operating System</h2>

<div class="pd-item-desc"><strong>Available with Windows 10 Pro</strong> — for a smooth, versatile PC experience.</div>

            </div>
        </div>

</div>

                    
                    
<div class="pd-feature-wrap ImageTop">

        <div class="pd-feature-item">
            
<div class="pd-media-container">
    
    <div>
        
<dell-media-scene7-video player-type='Gallery' player='{&quot;instructionalMessage&quot;:&quot;Click and drag product for a 360 tour&quot;,&quot;assetUrl&quot;:&quot;DellComputer/laptops-xps_centenario-white-black-Carousel2-html5-thumb-image-set&quot;,&quot;playertype&quot;:&quot;Gallery&quot;,&quot;carouseltype&quot;:&quot;Swatches&quot;,&quot;initialHorizontalAix&quot;:&quot;1&quot;,&quot;autospinEnabled&quot;:&quot;0&quot;,&quot;autospinDuration&quot;:&quot;&quot;,&quot;autospinDirection&quot;:&quot;0&quot;,&quot;autospinRotations&quot;:&quot;&quot;,&quot;maxLoadRadiusPreloadframes&quot;:&quot;&quot;,&quot;maxLoadRadiusResolution&quot;:&quot;0&quot;}' id='video-542c8ad5bd45401889dcb85f7363a6cb' locale='en'>
