  $(document).ready(function(){

    var refreshId = setInterval(function() {
          var properID = $("#ui-id-2").length;
          if (properID > 0) {
            $("#ui-id-2").mouseover(function() {
                $('.nav-panel').attr('aria-expanded','true');
                $('.nav-panel').css('height','156px');
                $('.nav-panel').find('.nav-panel__content--search').addClass('visible');
                $(this).css('display','block');
            });

            $("#ui-id-2").mouseout(function() {
                $('.nav-panel').attr('aria-expanded','false');
                $('.nav-panel').css('height','0px');
                $('.nav-panel').find('.nav-panel__content--search').removeClass('visible');
                $(this).css('display','none');
            });
            clearInterval(refreshId);
          }
    }, 10);

  });


  setInterval(function() {
    $('input.product-option__input.a11y').removeAttr("disabled"); // variant disable to enable

  }, 10);







