			<script>
		var scrollTimer=null;
		function nextPageScrollHandler() {
			if (scrollTimer!=null)
				return;

			scrollTimer=setTimeout(function() {
				var pos = $(window).scrollTop() + window.innerHeight;
				var targetPos = $('.site-footer').offset().top;

				$('#keep-scrolling').toggleClass('show', pos>=targetPos);

				if (($('#keep-scrolling-buffer').hasClass('show')) && (!$('#keep-scrolling').hasClass('loading'))) {
					var height=Math.max(
						Math.max(document.body.scrollHeight, document.documentElement.scrollHeight),
						Math.max(document.body.offsetHeight, document.documentElement.offsetHeight),
						Math.max(document.body.clientHeight, document.documentElement.clientHeight)
					);
					if (pos>=height) {
						$('#keep-scrolling').addClass('loading');
						setTimeout(function() { window.location.href='/review/abkoncore-a900/2.html'; }, 100);

												var lastTick=Date.now();
						setTimeout(function keepScrollingTick() {
							if (Date.now()-lastTick<1000) {
								lastTick=Date.now();
								setTimeout(keepScrollingTick, 500);
								return;
							}

							$('#keep-scrolling').removeClass('loading');
						}, 500);
					}
				}

				scrollTimer=null;
			}, 50);
		}

		loadjs.ready(['jquery'], function() {
			if (!('ontouchstart' in window))
				return;

			$(window).on('beforeunload', function(e) {
				$('#keep-scrolling-buffer').removeClass('show');
				$('#keep-scrolling').removeClass('loading').removeClass('show');
			});

			$(document).on('touchstart touchend', function(evt) {
				if ((evt.touches.length>0) && (document.getElementById('keep-scrolling')==null)) {
					$('<div id="keep-scrolling"></div><div id="keep-scrolling-buffer"></div>').appendTo('body');
					$(window).scroll(nextPageScrollHandler);
				}

				if (evt.touches.length==0)
					$('#keep-scrolling-buffer').removeClass('show');
				else {
					if ($('#keep-scrolling').hasClass('show'))
						$('#keep-scrolling-buffer').addClass('show');
				}
			});
		});
	</script>
