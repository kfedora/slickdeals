<script defer type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.css" />
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css"/>

<script>


function runSlick() {
   setTimeout(function() {
     $(document).ready(function () { 
     
    var changeTabIndex = function () {
       $(".slick-slide.slick-active").removeAttr("tabindex");
     }
     $(".responsive-caro").slick({
       accessibility:true,
       dots: false,
       infinite: true,
        speed: 300,
        slidesToShow: 4,     
        slidesToScroll: 4,   
        prevArrow:"<button class='slick-arrow-left-container' aria-label='previous'><div style='position:relative; padding: 1.75em .75em;'><svg class='svg-carousel-arrow-cta caroPrevBtn' viewBox='0 0 35.96 8.18'><use xlink:href='#svg-carousel-chevron-left'></use></svg></div></button>", 
        nextArrow:"<button class='slick-arrow-right-container' aria-label='next'><div style='position:relative; padding: 1.75em .75em;'><svg class='svg-carousel-arrow-cta caroNxtBtn' viewBox='0 0 35.96 8.18'><use xlink:href='#svg-carousel-chevron-right'></use></svg></div></button>"
      });
      $('.responsive-caro').on('afterChange', function(event, slick, currentSlide){
        setTimeout(function(){ 
          changeTabIndex(); 
        }, 100);
      }); 
      changeTabIndex();
    });
  }, 2000);
}

function jQueryIsLoaded() {
  if (window.jQuery) {
    console.log("jQuery is loaded");
    
    runSlick();
  } else {
    setTimeout(jQueryIsLoaded, 50);
  }
}

jQueryIsLoaded();
</script>










































































    
































