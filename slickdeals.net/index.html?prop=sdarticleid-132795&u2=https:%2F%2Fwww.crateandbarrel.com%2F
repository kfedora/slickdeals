    <script type="text/javascript">
        document.addEventListener('DOMContentLoaded', function () {
            var counter = 0;
            var timer = setInterval(function() {
                counter++;
                if (Crate.International && Crate.International.checkGeoLocationPopup(false)) {
                    if (Crate.React && Crate.React.Actions && Crate.React.Actions.PopupActions) {
                        clearInterval(timer);

                      Crate.React.Actions.PopupActions.fetchContent('/SharedLayer/PopupV2?popupName=InCA_USAEntry_USASitePopup');
                    }
                } else if (Crate.International && !Crate.International.checkGeoLocationPopup(false)) {
                    clearInterval(timer);
                }

                if (counter === 10) {
                    clearInterval(timer);
                }
                },
                1000);
        });
    </script>


