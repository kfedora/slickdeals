    var ua = window.navigator.userAgent;
    if (ua.indexOf("MSIE ") !== -1 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) {
        document.write('<script src="/scripts/common/polyfills/custom-event-polyfill.min.js"><\/script>');
    }

