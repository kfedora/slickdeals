    <script>
        //lazy load images
        var isMobile = "0";
        var lazyLoadRootMarginDesktop = "499px 499px";
        var lazyLoadRootMarginMobile = "200px 200px";

        var lazyLoadRootMargin = +isMobile ? lazyLoadRootMarginMobile : lazyLoadRootMarginDesktop;
        var lazyLoadClass = document.querySelectorAll("img.lazyload");
        var lazyLoadAttribute = document.querySelectorAll("img[data-lazy-load]");
        if (lazyLoadClass) {
            Crate.Services.LazyLoadService.watch(lazyLoadClass, lazyLoadRootMargin);
        }
        if (lazyLoadAttribute) {
            Crate.Services.LazyLoadService.watch(lazyLoadAttribute, lazyLoadRootMargin);
        }
    </script>


