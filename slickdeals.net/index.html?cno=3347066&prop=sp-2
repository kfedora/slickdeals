    document.addEventListener( 'DOMContentLoaded', function () {
        
        var $carousels = $('.carousel.carousel-1');
        $carousels.each(function() {
            if ($(this).hasClass("slick-initialized")) {
                $(this).find("video").each(function() {
                    this.play();
                });
            } else {
                $(this).on('init', function(ev, el) {
                    $(this).find("video").each(function() {
                        this.play();
                    });
                });
            }
        });
        
        if (!$("body").hasClass("responsive-site")) {            
            function changeImages() {
                $("img[data-interchange]").each(function() {
                    // Get the data-interchange attribute that contains the URLs
                    var images = $(this).attr("data-interchange");
                    var imageArray = images.split("], ");
                    // Parse out each URL
                    var mobileUrl = imageArray[0].substring(1,imageArray[0].indexOf(","));
                    var tabletUrl = imageArray[1].substring(1,imageArray[1].indexOf(","));
                    var desktopUrl = imageArray[2].substring(1,imageArray[2].indexOf(","));
                    // Set the URL based on browser width
                    if ($(window).width() > 1024) {
                        $(this).attr("src", desktopUrl)
                    } else if ($(window).width() > 640) {
                        $(this).attr("src", tabletUrl)
                    } else {
                        $(this).attr("src", mobileUrl)
                    }
                });
            }
            
            // Resize Throttler
            var imageChangeTimeout;
            function imageChangeThrottler() {
                if ( !imageChangeTimeout ) {
                    imageChangeTimeout = setTimeout(function() {
                        imageChangeTimeout = null;
                        changeImages();
                    }, 1000);
                }
            }
            
            // Initialize resize event
            $(window).off("resize", imageChangeThrottler).on('resize', imageChangeThrottler);
            // Call the resize event on page load
            changeImages();
        }
    }, false );
</script><style type="text/css">#ImageGridContainer_1 .image-grid-promo-image-cta{ display:none; }
