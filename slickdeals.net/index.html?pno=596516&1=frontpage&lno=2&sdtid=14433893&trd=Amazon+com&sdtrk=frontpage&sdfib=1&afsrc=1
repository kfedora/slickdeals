<script type="text/javascript">(function(f) {f(window.P._namespace("GoldboxUDPAssets"));}(function(P) {
//this function to be removed after the AUI migration is successful
function isComponentRegistered(componentName) {
var isRegistered = false;
P.now(componentName).execute(function (module) {
isRegistered = !!module;
});
return isRegistered;
}

var dealOfferQuantityDropdown = 1;

P.when('A', 'jQuery', 'udp_ld_widget_js').execute(function(A, $, widget) {
Backbone.setDomLibrary($);
gbResources.registerCustomerData ({
"marketplaceId" : "ATVPDKIKX0DER",
"customerId" : "",
"sessionId" : "000-0000000-0000000",
"csrfToken" : "gHmDbHaNBQPqRZZ7e0Yrt5Z7dcnBTVoMjaingwgAAAAMAAAAAF+GdW9yYXcAAAAA",
"realm" : "USAmazon",
"isPrimeMember" : false,
"hasEarlyAccessBenefit" : false,
"hasExclusiveAccessBenefit" : false,
"isApp" : false,
"isMobile" : false,
"suppressMultipleStrikeThroughPrices" : false
});
gbResources.registerStrings({
'gbd_ends_in_label' : 'Ends in',
'gbd_prime_starts_in_label' : 'Starts for you in ',
'gbd_hour_label' : 'h',
'gbd_min_label' : 'm',
'gbd_sec_label' : 's',
'gb_starts_in'  : 'Starts in'
});

if (typeof gbResources.registerApiConfigs == 'function') {
gbResources.registerApiConfigs({"GDS":{"baseRetryInterval":60000,"maxRetries":0},"GD":{"baseRetryInterval":4000,"maxRetries":1},"CD":{"baseRetryInterval":4000,"maxRetries":1}});
}





if (!window.Deal.udpController) {
Deal.udpController = new UDPDealController(Deal.configManager.getControllerConfig(), 'dealContentService');
}
var widgetName = "udpDealLDWidget";
var udp_deal_widget = Deal.udpController.getWidget(widgetName);
if (udp_deal_widget === undefined || udp_deal_widget === null) {
udp_deal_widget = new UDPDealWidget(widgetName);
Deal.Widgets.push(udp_deal_widget);
}
udp_deal_widget.setDealId({
"dealId" :"4f3df6bc",
"asin" : "B075W1LFWP",
"dealType" : "LIGHTNING_DEAL",
"primeAccessType" : "PRIME_ONLY_DOTD",
"primeAccessDurationInMs" : 0,
"dealStatus": {
"4f3df6bc": {
"claimedCouponCount": 0,
"dealState": "AVAILABLE",
"isValid": true,
"lastUpdated": 1602647407471,
"msCacheTtl": 600000,
"msToEnd": 68992531,
"msToStart": -17407469,
"percentClaimed": 0,
"totalCouponCount": 0,
"dealItemStatus": {
"B075W1LFWP": {
"claimedCouponCount": 0,
"customerState": "NONE",
"itemState": "AVAILABLE",
"lastUpdated": 1602647407471,
"msCacheTtl": 0,
"msToCustomerStateExpiry": 0,
"percentClaimed": 0,
"totalCouponCount": 0,
"waitlistChance": "",
"waitlistPosition": 0
}
}
}
}
});


P.when('mash', 'udp_ld_widget_js').execute(function(mash) {
var deal = Deal.udpController.getDeal('4f3df6bc');
if (mash.privateAPI) {
mash.privateAPI.getPushNotificationInfo({
successCallback: function(pushNotificationInfo) {
udp_deal_widget.setPushNotificationDetails(pushNotificationInfo);
deal.trigger('change', deal);
},
failCallback: function (error) {

ue.count("goldboxDetailDeviceTokenUnavailable", (ue.count("goldboxDetailDeviceTokenUnavailable") || 0) + 1);
deal.trigger('change', deal);
}
});
}
});

udp_deal_widget.registerEvents();
Deal.udpController.setRedemptionCallback(udp_deal_widget.widgetID, redeemCallback);
function redeemDeal(dealInfoMap) {
if (Deal.udpController.buying[dealInfoMap.data.dealId]) {
Deal.log("Redeem Deal is already in progress.");
return;
}

if (dealInfoMap.data.purchaseRefTag.search("jwl") >= 0) {
var pushNotificationInfo = Deal.udpController.getWidget("udpDealLDWidget").pushNotificationDetails;
if (typeof(pushNotificationInfo) === 'object' && Object.getOwnPropertyNames(pushNotificationInfo).length > 0) {
Deal.udpController.registerForNotifications(pushNotificationInfo);
}
}



$('#gb_atc_4f3df6bc').addClass("a-hidden");
$('#gb_join_waitlist_4f3df6bc').addClass("a-hidden");
$("#deal_processing_4f3df6bc").removeClass("a-hidden");
Deal.udpController.claimDeal(udp_deal_widget.widgetID, dealInfoMap.data.dealId, dealInfoMap.data.asin, dealInfoMap.data.purchaseRefTag, dealOfferQuantityDropdown);
}
function redeemCallback(dealAsin) {
$("#deal_processing_4f3df6bc").addClass("a-hidden");
}
P.when('a-button').execute(function (getButtonCollection) {
// We keep the ATC button inactive until the JS functions are bound
var atcButtonSelector = '.gb-btn-atc';
var atcButtonCollection = getButtonCollection(atcButtonSelector);
atcButtonCollection.enable();
$(atcButtonSelector).bind('click', { dealId: '4f3df6bc', asin: 'B075W1LFWP', purchaseRefTag: 'gbdp_atc_web_4f3df6bc'}, redeemDeal);
if (typeof uet == 'function') {
uex('ld', "goldboxBuyBox", { wb: 1 });
}
});

$('#gb_join_waitlist_4f3df6bc').bind('click', { dealId: '4f3df6bc', asin: 'B075W1LFWP', purchaseRefTag: 'gbdp_jwl_4f3df6bc'}, redeemDeal);
$('#deal_waitlisted_old_app_4f3df6bc').bind('click', (function (){
P.when("mash").execute(function(mash) {
mash.canLaunchIntentURL({
url: "",
successCallback : function(canLaunch) {
if (canLaunch) {
mash.launchIntentURL({
url: ""
});
}
}
});
});
}));


return udp_deal_widget.widgetID;
});
}));</script>
