	<SCRIPT CPK="8844822381628">$(document).ready(function () {
    try {
        var prodIDforCart = $("meta[name='productid']").attr("content");
        var partModelsOnPageStrings = $("meta[name='productname']").attr("content");
        var partModelsOnPage = partModelsOnPageStrings.split(",");
        var heroPrice = $('.hero-column .cta-price dd[itemprop="price"]');
        var thinkSmart500Teams = '11SP1TSH500'
        var thinkSmart500Zoom = '11SP1TSH50010V5'
        var zoomModels = ["10V50006US"];
        var teamModels = ["10V50000US"];
        var modelSelector, modelPriceSelector;

            if (prodIDforCart == thinkSmart500Teams) {
                partModelsOnPage.forEach((pageModel, indexPage, arrayPage) => {
                    zoomModels.forEach((model, indexZoom, arrayZoom) => {
              
                        if (model == pageModel) {
                            
                            modelSelector =$('button#addToCartButtonTop').filter('[data-productcode=' + model + ']').parents('.tabbedBrowse-productListing-container');
                            modelSelector.hide();
                   
                        }



                        
                   
                    })

                    teamModels.forEach((model, indexZoom, arrayZoom) => {

                        modelPriceSelector =$('button#addToCartButtonTop').filter('[data-productcode=' + model + ']').parents('.tabbedBrowse-productListing-footer').siblings('.tabbedBrowse-productListing-body').find('.pricingSummary-details-final-price').html();
                        $(heroPrice).text(modelPriceSelector)
                       
                       
                    })
                })
                
            } else if (prodIDforCart == thinkSmart500Zoom) {
                partModelsOnPage.forEach((pageModel, indexPage, arrayPage) => {
                    teamModels.forEach((model, indexTeam, arrayTeam) => {
                        if (model == pageModel) {
                            
                            modelSelector =$('button#addToCartButtonTop').filter('[data-productcode=' + model + ']').parents('.tabbedBrowse-productListing-container');
                            modelSelector.hide();
                        }
                   
                    })

