                        modelPriceSelector =$('button#addToCartButtonTop').filter('[data-productcode=' + model + ']').parents('.tabbedBrowse-productListing-footer').siblings('.tabbedBrowse-productListing-body').find('.pricingSummary-details-final-price').html();
                        $(heroPrice).text(modelPriceSelector)
                        
                       
                    })
                })
            }
            $(".tabbedBrowse-productListing-container .tabbedBrowse-productListing-featureList:visible:first dt").css("visibility", "visible");
    } catch (ex) {
        console.log(ex.stack)
    }
