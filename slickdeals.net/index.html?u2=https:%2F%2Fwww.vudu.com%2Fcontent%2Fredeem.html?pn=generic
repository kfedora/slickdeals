<!-- Google Tag Manager -->
<script>
    if (VUDU.configs.gTagMgrId) {
        if( !(_vuduGetCookieTemporary && _vuduGetCookieTemporary("sod"))){
            function retrieveCampaignParams() {
                var campaignId = _vuduGetCookie("myvudu.campaignId") || null;
                /**
                 * campaignId is structured like this: cid-scid-kwid-adid
                 * adid is not needed
                 */ 
                var res = [];
                if (campaignId != null) {
                    var keys = ["cid", "scid", "kwid", "adid"];
                    var values = campaignId.split("-");
                    for (var i = 0; i < keys.length; ++i) {
                        if (values[i] != "undefined") {
                            var curr =  keys[i] + "=" + values[i];
                            res.push(curr);
                        }
                    }
                }
                return res.join("-");
            }
            // default structure
            var gtmParams = {
                'gtm.start': new Date().getTime(), 
                event: 'gtm.js'
            };
            // Only add it if it has data
            var campaignData = retrieveCampaignParams();
            if (campaignData != null && campaignData.length) {
                gtmParams["campaignParams"] = campaignData;
            }

            setTimeout(function(){
                (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push(gtmParams);var f=d.getElementsByTagName(s)[0],
                    j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
                    'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
                    })(window,document,'script','gTMgrData',VUDU.configs.gTagMgrId);
                }, 3000
            );
        }
    }
</script>
<!-- End Google Tag Manager -->


