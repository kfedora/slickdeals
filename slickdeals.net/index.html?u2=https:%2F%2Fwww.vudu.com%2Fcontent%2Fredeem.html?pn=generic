        <script type="text/javascript"> 
    var _vuduGetCookie = function(key)	{
        key = encodeURIComponent(btoa(unescape(encodeURIComponent(key))));
        if (document.cookie && document.cookie.length > 0 ) {
            var start = document.cookie.indexOf(key + '=');
            if (start == -1) { return null;  }
            start += key.length + 1;
            var end = document.cookie.indexOf(";", start);
            if (end == -1) { end = document.cookie.length; }
            if (typeof key === "undefined") return "";
            var value = document.cookie.substr(start, end - start);
            return decodeURIComponent(escape(atob(decodeURIComponent(value))));
         }
        return null;
    };
</script>
