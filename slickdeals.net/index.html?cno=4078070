    function IsJSEnabled() {
        document.getElementById("jsEnabled").style.visibility = 'visible';
        document.getElementById("jsDisabled").style.visibility = 'hidden';
    }
    setTimeout(function () { IsJSEnabled(); }, 1000);

    var ImagesUrl = '/K600-011/' + 'images/';
    var lpNumber = '6000637';
    var Year = '2020';
    var isMobile = function ()
    {
        var mobileFinder = {
            Android: function () { return navigator.userAgent.match(/Android/i); },
            BlackBerry: function () { return navigator.userAgent.match(/BlackBerry/i); },
            iOS: function () { return navigator.userAgent.match(/iPhone|iPad|iPod/i); },
            Opera: function () { return navigator.userAgent.match(/Opera Mini/i); },
            Windows: function () { return navigator.userAgent.match(/IEMobile/i) || navigator.userAgent.match(/WPDesktop/i); },
            any: function () { return mobileFinder.Android() || mobileFinder.BlackBerry() || mobileFinder.iOS() || mobileFinder.Opera() || mobileFinder.Windows();}
        };
        return mobileFinder.any() ? true : false;
    };
</script>
