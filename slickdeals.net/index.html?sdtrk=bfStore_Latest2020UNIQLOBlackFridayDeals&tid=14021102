function gaTagger() {
    dataLayer.push({
        'event': 'eventTracking',
        'eventCategory': 'product page',
        'eventAction': 'clicked marketing banner',
        'eventLabel': 'l4_airism_featured',
        'eventCallback': function() {
            if (event.href) {
                location.href = event.href;
            }
        },
        eventTimeout: 1000
    })
}
</script>
