<script>
function setCk(value, days){
    let date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 999));
    let expires = "expires="+date.toUTCString();
   document.cookie = 'UQZ2VZGV=' + value + '; ' + expires
}
function getCk() {
    let ck = document.cookie.split(';').filter(e => e.includes('UQZ2VZGV'))
    if(ck.length) {
       return Number(ck[0].slice(-1))
    }
    else {
      return false
    }
}
$(document).ready(() => {
    $('a, .name-link').on('click', function() {
    if(this.href) {
         if(this.href.includes('/men')) {
         setCk(1, 1000)
    }
    else if (this.href.includes('/women')) {
         setCk(0, 1000)
    }
    else if (this.href.includes('/girls') || this.href.includes('/boys')) {
         setCk(2, 1000)
    }
    else if (this.href.includes('/baby')) {
         setCk(3, 1000)
    }
    else {
      return
    }
  }
})
})
</script>
