<script type="text/javascript">
function certonaRecommendations(res) {
console.log(res);
try {
var scheme;
for (var i in res.resonance.schemes) {
scheme = res.resonance.schemes[i];
console.log(scheme);
if (scheme.display == 'yes') {
productHitTiles(scheme.items, scheme.scheme);
}
}
} catch (e) {
console.log('Certona recommendations failed: '+ e.message);
}
}
function productHitTiles(productArray, scheme) {
console.log(arguments);
var items = productArray,
$scheme = $("#" + scheme);
//break if no schemes
if ($scheme.length == 0)
return;
for (var j = 0; j < items.length; j++) {
var item = items[j];
var query = '?pid=' + item.id;
item.dw_url = "/on/demandware.store/Sites-UniqloUS-Site/default/Product-HitTile" + query;
var processed = 0;
//Closures SON, pass that loopcount to the ajax asynx callback, SON
(function(j) {
$.ajax({
url: item.dw_url,
success: function(data) {
var listHTML = [];
//check data length to make sure we dont get an empty product (happens if SB catalog not updated)
if (data.length === 0) {
return;
}
listHTML.push(data);
$scheme.append(listHTML.join(''));
},
error: function() {},
complete: function() {
processed++;
}
});
})(j);
}
}
</script>


