      <script type="text/javascript">
          $("#privacyAccept").click(function ()
          {
              createCookie("MicrocenterPrivacy", "Accepted", 365);
              $("#privacyAlertDiv").hide();
          }
        );

          $("#privacyClose").click(function () {
              $("#privacyAlertDiv").hide();
          }
