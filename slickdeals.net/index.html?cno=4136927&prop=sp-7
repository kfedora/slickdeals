@media screen and (max-width: 991px) {
#komen h2 {
font-size: 1.2vw;
margin-bottom: .3vw
}


@media screen and (max-width: 780px) {
#komen h2 {
font-size: 1.2vw;
margin-bottom: 2vw;
}
#komen p {
display: none;
}
}

@media screen and (max-width: 575px) {
#komen h2 {
font-size: 3.4vw;
margin-bottom: 2vw;
width: 60%;
}
#komen p {
font-size: 2.6vw;
display: block;
}
}
</style>
        
    
      
      
        
      
        
    
      
      
        
          
          
