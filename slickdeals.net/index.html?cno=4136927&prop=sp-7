  document.addEventListener('DOMContentLoaded', function() {
    var USMenu = document.querySelectorAll('[data-store="USD"]');
    if ((window.bounteous.customer_tags == null || window.bounteous.customer_tags == 'employee') && USMenu.length > 0) {
      USMenu.forEach(function(menu) {
        menu.setAttribute('data-cust', 'direct')
      });
    }
  });
</script>

