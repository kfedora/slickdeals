<script>
    function pageLoad(actionable, newTitle) {
        var analyticsInterval = window.setInterval(function () {
            if (newTitle) {
                document.querySelector('meta[name="title"]').setAttribute("content", newTitle);
                window._analytics.page.pageInfo.pageTitle = newTitle;
            }
            if (window.triggerEvent && window._PDL) {
                if (!window._analytics.page.pageInfo.icidList) {
                    window._analytics.page.pageInfo.icidList = [];
                }
                $.each(window._analytics.page.modules.moduleListAll, function (module) {
                    document.querySelector('[data-analytics-id="' + module.ID + '"]');
                    $.each(module.actionableElements, function (element) {
                        if (typeof (element.icid) != "undefined") {
                            window._analytics.page.pageInfo.icidList.push(element.icid);
                        }
                    });
                });
                var icidsList = document.querySelectorAll('[data-analytics-id]');
                $.each(icidsList, function (element) {
                    if (typeof (document.querySelectorAll('[data-analytics-id]')[element].dataset.icid) != "undefined") {
                        window._analytics.page.pageInfo.icidList.push(document.querySelectorAll('[data-analytics-id]')[element].dataset.icid);
                    }
                });
                var modals = document.querySelectorAll('[data-analytics-config]');
                var modalIcids = [];

                $.each(modals, function (modalSection) {
                    // var modalAnalytics = modalSection.querySelector('[data-analytics-config]');
                    var modalData = JSON.parse(document.querySelectorAll('[data-analytics-config]')[modalSection].dataset.analyticsConfig);

                    $.each(modalData.actionableElements, function (actionableElements) {
                        var icid = actionableElements.icid;

                        modalIcids.push(icid);
                    });
                });

                var uniqModalIcids = $.unique(modalIcids);
                var allPageIcids = $.unique(window._analytics.page.pageInfo.icidList);
                var pageLoadIcids = [];// $.difference(allPageIcids, uniqModalIcids);

                $.grep(uniqModalIcids, function (icids) {
                    if ($.inArray(icids, allPageIcids) == -1) difference.push(pageLoadIcids);
                });

                var evt = {};
                evt.attributes = {};
                evt.eventInfo = {};
                evt.attributes = {};
                evt.eventDescription = {};
                evt.eventInfo.eventAction = "load";
                evt.eventInfo.eventType = "page";
                evt.eventInfo.timeStamp = new Date().toString();

                window._analytics.page.pageInfo.icidList = $.unique(window._analytics.page.pageInfo.icidList);
                window._analytics.page.pageInfo.timeStamp = new Date().toString();

                if (window._analytics.page.pageInfo.icidList.length) {
                    evt.attributes = {};
                    evt.attributes.icidList = $.unique(allPageIcids);
                }
                evt.eventDescription.type = "default page";
                if (typeof(window._PDL) == "undefined") {
                    window._PDL = [];
                }
                //$.merge(window._PDL, window._analytics);
                $.extend(window._PDL, window._analytics);

                if (typeof (window._PDL.eventData) == "undefined") {
                    window._PDL.eventData = [];
                }
                /* Commented below line code to  remove the page load event. */
                /*window._PDL.eventData = evt;
                $(window).trigger("load");*/
                window.clearInterval(analyticsInterval);
            }
        }, 100);
    }
</script>
