embedded_svc.settings.disabledMinimizedText = ' '; //(Defaults to Agent Offline)
  
  

//embedded_svc.settings.loadingText = ''; //(Defaults to Loading)
//embedded_svc.settings.storageDomain = 'yourdomain.com'; //(Sets the domain for your deployment so that visitors can navigate subdomains during a chat session)

// Settings for Live Agent
//embedded_svc.settings.directToButtonRouting = function(prechatFormData) {
// Dynamically changes the button ID based on what the visitor enters in the pre-chat form.
// Returns a valid button ID.
//};
//embedded_svc.settings.prepopulatedPrechatFields = {}; //Sets the auto-population of pre-chat form fields
//embedded_svc.settings.fallbackRouting = []; //An array of button IDs, user IDs, or userId_buttonId
//embedded_svc.settings.offlineSupportMinimizedText = '...'; //(Defaults to Contact Us)

embedded_svc.settings.enabledFeatures = ['LiveAgent'];
embedded_svc.settings.entryFeature = 'LiveAgent';

  

embedded_svc.init(
  'https://allswellhome.my.salesforce.com', 
  'https://allswellhome.secure.force.com', 
  gslbBaseURL, 
  '00D1N000001haxW', 
  'Omni_Snap_In_Chat', 
  { 
    baseLiveAgentContentURL: 'https://c.la1-c1-iad.salesforceliveagent.com/content', 
    deploymentId: '5721N0000001Uwu', 
    buttonId: '5731N0000001SJN', 
    baseLiveAgentURL: 'https://d.la1-c1-iad.salesforceliveagent.com/chat', 
    eswLiveAgentDevName: 'EmbeddedServiceLiveAgent_Parent04I1N000000XZDfUAO_165ed1aff88', 
    isOfflineSupportEnabled: true
  }
); 
  };
if (!window.embedded_svc) { var s = document.createElement('script'); s.setAttribute('src', 'https://allswellhome.my.salesforce.com/embeddedservice/5.0/esw.min.js'); s.onload = function() { initESW(null); }; document.body.appendChild(s); } else { initESW('https://service.force.com'); }

  setInterval(function(){
    let mainColor = '#3f5962';
    let offlineUI = $('.offlineSupportUI');
    if(offlineUI.size() > 0){
      $('.embeddedServiceSidebar .sidebarHeader').css({
        backgroundColor:'#3f5962'
      })
    }
    
    charCount(offlineUI.find('.How_Can_We_Help__c'));
    
  }, 2000);
  
  function charCount(input){
  	input.on("input", function(e){
      e.stopImmediatePropagation();
      var maxlength = $(this).attr("maxlength");
      var currentLength = $(this).val().length;
      
      if(input.next('.charcount').size() <= 0){
      	input.parent().append('<p class="charcount"><small>characters left <span>'+maxlength+'</span></small></p>');
      }

      input.parent().find('.charcount span').text(maxlength - currentLength);
    });
  }
  
  
</script>


<footer role="contentinfo"><script>
  $(function(){
    $('#footer .links > li').click(function(){
      if($(window).width()<=900) {
        $(this).find('ul').slideToggle();
        $(this).find('div:first').toggleClass('minus').toggleClass('plus');

        if($(this).find('div:first').hasClass('plus')) {
          $(this).find('div:first').html('+');
        } else {
          $(this).find('div:first').html('-');
        }
      }
    });
    
    $('#footer .email-signup form').submit(function(e){
      e.preventDefault();
      e.stopImmediatePropagation();

      $(this).find('.error').removeClass('error');

      var error = false;
      $(this).find('.required').each(function(){
        if($(this).val()=="") {
          error = true;
          $(this).addClass('error');
        }
      });
      if(!error) {
        if(!validateEmail($(this).find('.email').val())) {
          error = true;
          $(this).find('.email').addClass('error');
        }
      }

      if(!error) {
        var form = $(this);
        $.ajax({
          url: form.attr('action'),
          data: form.serialize(),
          dataType: 'json',
          type: 'POST'
        }).always(function(){
          form.hide();
          form.siblings('.success').show();
        });
      }
    });
    
  });
</script>

