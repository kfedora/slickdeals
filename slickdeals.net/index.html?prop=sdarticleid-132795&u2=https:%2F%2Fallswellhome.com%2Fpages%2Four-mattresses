<script type="text/javascript">
  $(function(){
  	$('.legal-wrapper .bottom-descriptions .title').click(function(){
      	if($(window).width()>900) {
        	var desktopElement = $('.legal-wrapper .bottom-descriptions .desktop-text');
			$('.legal-wrapper .bottom-descriptions .grid__item').removeClass('active');
            if(desktopElement.data('section')==$(this).text()) {
                desktopElement.slideUp();
                desktopElement.data('section', false);
            } else {
                desktopElement.html($(this).parent().find('.text').html());
              	$(this).parent().parent().parent().addClass('active');
                if(!desktopElement.is(':visible')) {
                    desktopElement.slideDown();
                  	
                    if(desktopElement.find('.affirm-site-modal').size() > 0){
                      desktopElement.find('.affirm-site-modal').click(function(){
                      	$('.legal-wrapper .bottom-descriptions .grid__item .text .affirm-site-modal').click()
                      })	
                    }
                  
                }
                desktopElement.data('section', $(this).text());
            }
        } else {
        	$(this).parent().find('.text').slideToggle();
          	$(this).parent().parent().parent().toggleClass('active');
        }
      	
    });
  })
</script>
