<script>
  $(function(){

    function showComparePopup() {
      var maxHeight = 0;
      // console.log(maxHeight)
      modal = new tingle.modal({
        closeMethods: ['overlay', 'escape', 'button'],
        closeLabel: "Close",
        cssClass: ['size-chart'],
        beforeClose: function() {
          $('.tingle-modal .grid__item .prod_inside').css('height','0');
          return true;
        }
      });
      modal.setContent($('#compare_popup').html());
      modal.open();
      setTimeout(function() { 
        $('.compare_popup .selected .prod').find('.prod_inside').each(function(){
          if ($(this).height() > maxHeight) { 
            maxHeight = $(this).height(); 
          }
        });
        // console.log(maxHeight)
        $('.tingle-modal .grid__item .prod_inside').css('height',maxHeight);
        $('.tingle-modal-box__content .prod a').removeClass('hide');
      }, 1000);
    }

    var prevClass;

    

    function Compare(width_size) {
      if (prevClass !== undefined) {
        $('#compare_popup').find('.grid__item').removeClass(prevClass);
      }
      var items = $('#compare_popup').find('.selected');
      var numItems = items.length;
      if ( numItems > 0 ) {
        var message = '';
        if ( numItems === 1) {
          var message = '<p>1 selected. choose another mattress to compare</p>';
          $('.compare_footer').find('.compare-selected').addClass('disabled');
        } else {
          var message = '<p>' + numItems + ' selected</p>';
        };
        if ( numItems !== 1) {
          $('.compare_footer').find('.compare-selected').removeClass('disabled');
        };
        $('.compare_footer').addClass('active');
        $('.compare_footer').find('.copy').html(message);
        if ( numItems === 2 ){
          var width_size = 'one-half'
          //if ($(window).width() < 600) {
          //  $('.compare_popup .container').css('width','140vw');
          //  $('.tingle-modal .inner .prod').css('max-width','75%');
          //}
        } ;
        if ( numItems === 3 ){
          var width_size = 'one-third'
          //if ($(window).width() < 600) {
          //  $('.compare_popup .container').css('width','220vw');
          //  $('.tingle-modal .inner .prod').css('max-width','50%');
          //}
        } ;
        if ( numItems === 4 ){
          var width_size = 'one-quarter'
          //if ($(window).width() < 600) {
          //  $('.compare_popup .container').css('width','310vw');
          //  $('.tingle-modal .inner .prod').css('max-width','40%');
          //}
        } 
        $('#compare_popup').find('.grid__item').addClass(width_size);
        prevClass = width_size;
      } else {
        $('.compare_footer').removeClass('active');
      }
    }

    function ClearCompare() {
      $('#mattress_prod').find('.selected').removeClass('selected');
      $('#compare_popup').find('.selected').toggleClass('hide selected');
      $('.compare_footer').find('.compare-selected').addClass('disabled');
      $('.compare_footer').removeClass('active');
    }

    $(document).on('click', '.compare_popup .close', function(){
      modal.close();
      $('.compare_footer').addClass('active');
    });

    $('.btn.compare').click(function(){
      $(this).toggleClass('selected');
      var selector = $(this).attr('data-block-selector');
      var item = $('div[data-block^="' + selector + '"]'); 
       
      // var firmness_val = item.attr('data-firmness');
      // console.log('firmness '+firmness_val);
      // if ( firmness_val === '5' ){
      //   item.find(".firmness .firmness_bar .step.4").addClass('active fifth');
      // } else {
      //   item.find(".firmness .firmness_bar .step."+firmness_val).addClass('active');
      // }



      $(this).closest('div[data-block="' + selector + '"]').toggleClass('selected');
      $('#compare_popup').find('div[data-block^="' + selector + '"]').toggleClass('hide selected');
      Compare();
    });


    $('.compare-selected').click(function(){
      var items = $('#compare_popup').find('.selected');
      var numItems = items.length;
      if ( numItems === 1) {
        $('.compare_footer .compare-selected').on('click', function(){
          return false;
        })
      } else {
        $('.compare_footer').removeClass('active');
        showComparePopup();
        return false;
      }
    });

    $('.compare_footer .clear').click(function(){
      ClearCompare();
    })

    $('.compare_footer .clear.disabled').click(function(event){
      event.stopPropagation();
      event.preventDefault();
    });

  })
</script>

<div class="mattress_prod" id="mattress_prod">
  <div class="container">
    <div class="grid flex">

      
       <div class="grid__item  flex" >
        <div class="prod">
          <div class="prod_title">The Allswell</div>
          <div class="prod_img">
            

            
