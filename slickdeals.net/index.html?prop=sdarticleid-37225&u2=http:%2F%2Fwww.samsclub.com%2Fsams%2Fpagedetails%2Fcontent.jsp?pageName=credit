                .test(navigator.userAgent.substr(0, 4))) {
                isMobile = true;
            }
    
            if (isMobile) {
                $("#navLogo").on("click", function() {
                    var el = $(this);
                    if (/major([a-z]+)?/.exec(document.location)) {
                        // Mobile Test Environment
                        var testHomeUrl = "https://qa2-m.samsclub.com/?xid=hdr_logo";
                        el.attr("href", testHomeUrl);
    
                    } else {
                        //  Mobile Production Environment
                        var prodHomeUrl = "https://m.samsclub.com/?xid=hdr_logo";
                        el.attr("href", prodHomeUrl);
    
                    }
                });
    
    
                $(".lp-link-applyNow").on("click", function() {
                    var el = $(this);
    
                    ajaxSiteCatalyst("misc","sams-credit:apply-now")
    
                    if (/major([a-z]+)?/.exec(document.location)) {
                        // Mobile Test Environment
                        var testLoginUrl = "https://qa2-m.samsclub.com/api/node/samsapi/applyController/"
                        el.attr("href", testLoginUrl);
                    } else {
                        //  Mobile Production Environment
                        var prodLoginUrl = "https://m.samsclub.com/api/node/samsapi/applyController/"
                        el.attr("href", prodLoginUrl);
                    }
    
                    if (/refsite=([a-zA-Z0-9]+)?/.exec(document.location)) {
                        var newSrc = el.attr("href") + RegExp.$1;
                        el.attr("href", newSrc);
                    } else{
                        var newSrc = el.attr("href") + "mcrl";
                        el.attr("href", newSrc);
                    }
                });
                console.log('PHONE');
    
                $('.lp-link-learnMore').attr("href", "#lp_know_more_sec_mb")
                $('.topOmp.text-center.sc-pencil-ad').remove(); //SAINITIMPR-375
            } else {
                // Desktop
    
                $(".lp-link-applyNow").on("click", function() {
                    ajaxSiteCatalyst("misc","sams-credit:apply-now");
                    var el = $(this);
                    if (/refsite=([a-zA-Z0-9]+)?/.exec(document.location)) {
                        var newSrc = el.attr("href") + "&refsite=" + RegExp.$1 + "&xid=sams-credit_apply-now";
                        el.attr("href", newSrc);
                    } else {
                        var newSrc = el.attr("href") + "&refsite=" + "dcrl" + "&xid=sams-credit_apply-now";
                        el.attr("href", newSrc);
                    }
                });
                console.log('DESKTOP');
                $('.lp-link-learnMore').attr("href", "#lp_know_more_sec")
            }
    
            // Helper Functions
            function getCookie(cookieName) {
                var name = cookieName + "=";
                var cookies = document.cookie.split(';');
                var scCookie = "";
                for (var i = 0; i < cookies.length; i++) {
                    var cookie = $.trim(cookies[i]);
                    if (cookie.indexOf(name) === 0) {
                        scCookie = cookie.substring(name.length, cookie.length);
                        break;
                    }
                }
                return scCookie;
            }
    
            var firstName = getCookie("firstNameCookie");
            if (firstName) {
                $('.lp-first-name').text(firstName+", g");
                $('.fname-br1').css('display','block');
                $('.fname-br2').css('display','none');
            } else{
                $('.fname-br1').css('display','block');
                $('.fname-br2').css('display','block');
                $('.lp-line-br-mb').css('display','block');                
            }
            if ($('.mainHeader').css('content').indexOf("lp_tb_pov_jan") >= 0) {
                var tabLeft = ($(window).innerWidth() - parseInt($('.lp-first-name').css('width'))) / 2 + 'px';
                $('.lp-first-name').css('left', tabLeft);
            }
    
            if ($('.mainHeader').css('content').indexOf("lp_pov_jan") >= 0) {
                var mobLeft = ($(window).innerWidth() - parseInt($('.lp-first-name').css('width'))) / 2 + 'px';
                $('.lp-first-name').css('left', mobLeft);
            }
    
        })
    </script><div class="clear"></div>
