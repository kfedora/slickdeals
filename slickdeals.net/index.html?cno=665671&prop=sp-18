    carouselAttributes.pageIdent                = "category";
    carouselAttributes.carouselId               = "sep28-Category";
    carouselAttributes.carouselType             = "EDITORS_PICK";
    carouselAttributes.carouselViewTemplateType = "EDITORS_PICK";
