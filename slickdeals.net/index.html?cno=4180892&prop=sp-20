<script type="text/javascript">
    if (typeof msApp == null || typeof msApp === "undefined"){
        var msApp = {};
    }
    msApp.commonWebResourcePath = "//static.lenovo.com/fea";
    msApp.customerHost = "https://www.lenovo.com";
    msApp.contextPath = "/us/en";
    msApp.storeRootPath = "/us/en";
    msApp.country = "us";
    msApp.orgLanguage = "en_us";
    msApp.language =  "en";
    msApp.previewCmsHost = "http://global-cms.prod.online.lenovo.com";
    msApp.storename = "US Web Store";
    msApp.storeCode = "usweb";
    msApp.storeType = "public_consumer";

    var webRootlocation = msApp.storeRootPath;

    var commonResourcePath = '/_ui/desktop/common';

    var ACC = {config : {}};
    ACC.config.contextPath = msApp.storeRootPath;
    ACC.config.encodedContextPath = msApp.storeRootPath;
    ACC.config.commonResourcePath = "/_ui/desktop/common";
    ACC.config.themeResourcePath = "/_ui/desktop/theme-blue";
    ACC.config.siteResourcePath = "/_ui/desktop/site-uspublicsite";

    ACC.config.language = msApp.language;
    ACC.config.originLanguage = msApp.orgLanguage;

    ACC.config.rootPath = "/_ui/desktop";
    ACC.config.CSRFToken = "1510a5af-311d-4f34-b7c9-ad0b042c8eb3";
    ACC.config.siteName = "uspublicsite";

    ACC.config.isoCode = msApp.country.toUpperCase();

    ACC.pwdStrengthVeryWeak = 'Very weak';
    ACC.pwdStrengthWeak = 'Weak';
    ACC.pwdStrengthMedium = 'Medium';
    ACC.pwdStrengthStrong = 'Strong';
    ACC.pwdStrengthVeryStrong = 'Very strong';
    ACC.pwdStrengthUnsafePwd = 'password.strength.unsafepwd';
    ACC.pwdStrengthTooShortPwd = 'Too short';
    ACC.pwdStrengthMinCharText = 'Minimum length is 8 characters';
    ACC.accessibilityLoading = 'Loading... Please wait...';
    ACC.accessibilityStoresLoaded = 'Stores loaded';
    if (!String.prototype.startsWith) {
        String.prototype.startsWith = function(searchString, position) {
            position = position || 0;
            return this.indexOf(searchString, position) === position;
        };
    }
