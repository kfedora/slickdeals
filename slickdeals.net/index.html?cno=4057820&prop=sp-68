		function ShowCategory(Slide)
		{
			var Dropdown = $(".DropDown")
			var BlueBar = Dropdown.find(".BlueBar")
			var List = Dropdown.find(".List")
			
			// Empty it
			List.html("")
			
			// Fill it
			for (var x = 0 ; x < Prods.length ; x++)
			{
				var Cur = Prods[x]
				
				if (Cur.Slide == Slide)
				{
					// to Blue Bar
					//BlueBar.find(".Icon").css("background-position", "0px " + Cur.IconPosY)
					//BlueBar.find(".Text").html(Cur.Text)
					BlueBar.find(".Text").html(Cur.Category)
					
					if (Cur.Size)
						BlueBar.find(".Text").css("font-size", Cur.Size)
					else
						BlueBar.find(".Text").css("font-size", "26px")
					
					// To Header Category Bar
					$(".Header .Category").html(Cur.Category)
				}
				else
				{
					// to List
					var NewItem = $("<div>")
						.addClass("Item")
						.append($("<a>")
							.attr('slide',Cur.Slide)
							.attr("href", '#')
							/*
							.append($("<div>")
								.addClass("Icon")
								.css("background-position", "0px " + Cur.IconPosY)
							)
							*/
							.append($("<div>")
								.addClass("Text")
								.addClass("ProximaNova")
								.html(Cur.Category)
							)
							.append($("<div>")
								.css("clear", "both")
							)
						)
					
					List.append(NewItem)
				}
			}
		}

