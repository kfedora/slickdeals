<script>
  (function($){
    (function ( $ ) {
  $.fn.HSNEmailSignup = function(opt) {
    //console.log('HSNEmailSignup pre:');
    //input text/textarea fields only
    //david.kline@hsn.net
    if(!$(this).is('input[type="text"],input[type="email"],textarea')){
      window.console && window.console.error('HSNEmailSignup error: This element is not a text or email input field or textarea, you require a text or email input field or textarea');
      return this;
    }
  //this will end up needing to be changed to the basic email capture from the site, or maybe just include it as an option
  //-code standards, redo the new features with the edit template modal, except figure out how to handle the exceptions
  //--maybe have various preset options for layouts, need to get creative to finalize some designs
  //-figure out what is outstanding for the latest ext update, probably launched missing a few things
    //console.log('HSNEmailSignup go:');
    if(typeof(opt) == 'undefined'){ opt = {}; }
    
    var _this = this,
      //element = $(this).first(),
      elements = $(this),//.not(element),
      //params = {},
      button,
      thanks,
      errormsg,
      timeID = new Date().getTime(),
      isApp = !!$('.appPhone,.appTablet,.mobile.NativeApp,.mobile #app-content').length,
      isMobile = !!$('body.mobile').length, //probably not needed

      params = $.extend({
        type: '',
        button: false,
        errormsg: false,
        wtwSourceID: '91',
        sourceID: '57',
        attribute:'',
        webPID: '',
        source: '',
        thanks: 'Thank you for signing up!',
        //thanksElement: '',
        invalid: 'Invalid email address. Please try again.',
        //valid: '._emailsignup-thanks',
        valid: false,
        success: function (data) {
            if (data.success) {
              if (typeof(window.cmCreateManualLinkClickTag) == 'function'){
                var CM = "emailsignup-_-signup-submit-_-click"
                cmCreateManualLinkClickTag(window.location.pathname + "?cm_re=" + CM + "&cm_sp=" + CM, "");
              }
              //console.debug(data);
              HSN.Instance().GetPageIdModal().Show($('<div />').html('<div style="font-size:1.5em;padding:2%;">'+params.thanks+'</div>'));
                //width:250px;text-align:center;
              //window.location.href = "/myaccount/communicationpreferences?email=" + data.emailAddress;
                //thanks
                /*if(!!isApp){
                  alert(params.thanks);
                } else {
                  HSN.Instance().GetPageIdModal().Show('._emailsignup-thanks'+timeID);
                  //showThanks();
                }//*/
            } else {
                if (data.errors != null) {
                  var errorMessage = "* ";
                  $(data.errors).each(function (index) {
                      errorMessage += data.errors[index] + " ";
                  });
                  errormsg.show().html(errorMessage);
                } else {
                  alert("Unfortunately there was an error, please try again.");
                }
            }
        },
        error: function (jqXHR, textStatus, errorThrown) {
            alert("Unfortunately there was an error, please try again.");
            window.console && window.console.error(errorThrown);
        },
        wtwError: function (jqXHR, textStatus, errorThrown) {
          //alert(errorThrown);
          window.console && window.console.error(errorThrown);
        },
        wtwSuccess: function(data) {
          //var DI = HSN.Instance().DeviceInformation
          if(!!isApp){
            alert(params.thanks);
          } else {
            //HSN.Instance().GetPageIdModal().Show('._emailsignup-thanks'+timeID);
            HSN.Instance().GetPageIdModal().Show($('<div />').html('<div style="font-size: 1.75em; padding:5%;">'+data+'</div>'));
            //showThanks();
          }
          
          if (typeof(window.cmCreateManualLinkClickTag) == 'function'){
            var CM = "emailsignup-_-signup-submit-_-click"
            cmCreateManualLinkClickTag(window.location.href + "?cm_re=" + CM + "&cm_sp=" + CM);
          }
        }
      },opt);
      //cast both source IDs to strings
      params.wtwSourceID = params.wtwSourceID+'';
      params.sourceID = params.sourceID+'';
      //console.debug(elements);
      elements.data('HSNEmailSignup',params);
    if(!params.valid || !$(params.valid).length){
      thanks = $('<div class="_emailsignup-thanks'+timeID+'" style="padding: 5%;">'+params.thanks+'</div>')
        .appendTo($('<div class="_emailsignup-thanks-outer" style="display: none;" />')
            .insertAfter(elements));
    } else {
      thanks = $(params.valid);
    }

    if(!params.errormsg || !$(params.errormsg).length){
      errormsg = $('<div class="_emailsignup-errormsg" style="display: none; color: #ef3341; font-size: 12em;" />').insertAfter(elements);
    } else {
      errormsg = $(params.errormsg);
    }

    if(!params.button || !$(params.button).length){
      button = $('<input class="_emailsignup-submit" type="button" value="SUBMIT">').insertAfter(elements);
    } else {
      button = $(params.button);
    }

    function init(){
      //if this is a regular email
      if(params.type == 'wtw'){
        button.on("click", wtwSignup);
        elements.on('submit',wtwSignup);
      } else {
        button.on("click", emailSignup);
        elements.on('submit',emailSignup);
      }
    };

    function emailSignup(ev){
      ev.stopPropagation();
      ev.preventDefault();
      
      var emailAddress = elements.first().val().trim();
      
      //console.log('email: '+emailAddress);
      //var emailAddress = $("#email_address").val();
      
      if(emailAddress != null && emailAddress != "" && HSN.ValidateEmail(emailAddress)) {
        //$('#sale-alerts-panel-error').hide();
        //$('#sale-alerts-panel-error').html('');
        $.ajax({
          url: '/headerfooter/emailsignup',
          context: this,
          dataType: 'json',
          type: 'POST',
          data: {
            'EmailAddress': emailAddress,
            'ConfirmEmailAddress': emailAddress, //might need to actually put a confirm email field eventually...
            "SourceId" : params.sourceID,
            '_ts': new Date().getTime()
          },
          success: params.success,
          error: params.error
        });
      } else {
        alert(params.invalid);
      }
    };

    function wtwSignup(ev){
      ev.stopPropagation();
      ev.preventDefault();
      
      var emailAddress = elements.first().val().trim();
      
      //console.log('email: '+emailAddress);
      //var emailAddress = $("#email_address").val();
      
      if(emailAddress != null && emailAddress != "" && HSN.ValidateEmail(emailAddress)) {
        $.ajax({
            //context: this,
            type: "POST",
            url: "/signupemail/submit-email-signup?_=" + new Date().getTime(),
            dataType: "html",
            contentType: "application/x-www-form-urlencoded",
            data: {
              emailAddress: emailAddress,
              attributeId: params.attribute,
              //webpId: params.webPID,
              sourceId: params.wtwSourceID
            },
            success: params.wtwSuccess,
            error: params.error
          });
      } else {
        //alert("Invalid email address. Please try again.");
        alert(params.invalid);
      }
    };
    
    init();
    return this;
  }//plugin
  //process the deferred elements
  if(typeof(window.HSNEmailSignup) == 'object' && !!window.HSNEmailSignup.hasOwnProperty('length')){
    $.each(window.HSNEmailSignup, function(i,v){
      v.el.HSNEmailSignup(v.opt);
    });
    delete window.HSNEmailSignup;
  }
}( jQuery ));
      $('.lessman-wtw > span:last-child > div > input').HSNEmailSignup({
        type:'wtw',
        button:'.lessman-wtw .wtws',
        attribute: 129,
        wtwSourceID: 15,
        valid:'.lessman-wtw .wtwthx'
      });
  })(jQuery);
</script>

</div>
<div class="cms-cell-nopad first full" style="width:100.00000000000000000000000000%;" data-zone="Unknown" >
<!-- Brief:0- -->
</div>
<div class="clear"></div>


</div>

