<script type="text/javascript">
	(function($){
		// Helpers
		function toTitleCase(str) {
			return str.replace(/(?:^|\s)\w/g, function(match) {
				return match.toUpperCase();
			});
		}

		function GetCoremetric(InText)
		{
			var Output = toTitleCase(InText)
			
			var ToRemove = "~!@#$%^&*()_+-=[]{};':\",.<>/?\|"
			
			for ( var x = 0 ; x < ToRemove.length ; x++ )
			{
				var CurChar = ToRemove.substr(x, 1)
				var Replacer = new RegExp("\\" + CurChar, 'g');
				
				Output = Output.replace(Replacer, "")
			}
			
			Output = Output.replace(/ /g, "")
			Output = Output.replace(/\t/g, "")
			
			return Output
		}


		// Slides / Reviews
		var Prods = []
		var Reviews = []

		function ShowSlide(Slide,noCM)
		{
			ClearSlide()
			ShowProd(Slide)
			ShowReviews(Slide)
			ShowCategory(Slide)
			TrimReviews()
			
			// Coremetric (Does not exist on page load, skipping the first (automatic) call to this function intentionally)
			if (!noCM && typeof(window.cmCreateManualLinkClickTag) != 'undefined')
			{
				var Category = $(".DropDown .BlueBar .Text").html()
				var CM = "Customer-Reviews-_-Selector-_-" + GetCoremetric(Category)
				
				cmCreateManualLinkClickTag(window.location.pathname + "?cm_re=" + CM + "&cm_sp=" + CM, "Lessman");
			}
		}

		function AddProd(obj)
		{
			Prods[Prods.length] = obj
		}

		function ShowProd(Slide)
		{
			for (var x = 0 ; x < Prods.length ; x++)
			{
				var Cur = Prods[x]
				
				if (Cur.Slide == Slide)
					RenderProd(Cur)
			}
		}

		function RenderProd(curObj)
		{
			var CurProd = $(".Andrew .Slide .Product")
			var CurLink = "/products/andrew-lessman/" + curObj.WebID
			var CurCM = "Customer-Reviews-_-" + GetCoremetric(curObj.Category)
			
			// Img
			//CurProd.find(".ProdImage img").attr("src", curObj.Img)
			CurProd.find(".ProdImage img").attr("src", "https://i02.hsncdn.com/is/image/HomeShoppingNetwork/prodfull/~" + curObj.PFID + ".jpg?v=1")
			CurProd.find(".ProdImage img").attr("alt", curObj.Name)
			// Img Link
			var ImgLink = CurProd.find(".ProdImage > a")
			ImgLink.attr("href", CurLink)
			ImgLink.attr("manual_cm_re", CurCM + "-_-productimage")
			ImgLink.attr("manual_cm_sp", CurCM + "-_-productimage")
			
			// Product Details
			CurProd.find(".ProdDetails .Name").html(curObj.Name)
			CurProd.find(".ProdDetails .ActiveRating").css("width", curObj.Rating / 5.0 * 100 + "%")
			CurProd.find(".ProdDetails .RatingNum").html(curObj.RatingCount)
			
			// See All Reviews
			var SeeAllLink = CurProd.find(".ProdDetails .SeeAll")
			
			var DI = HSN.Instance().DeviceInformation
			
			if (DI.IsNativeApp)
				SeeAllLink.attr("href", CurLink)
			else
				SeeAllLink.attr("href", CurLink + '#product-reviews')
				
			SeeAllLink.attr("manual_cm_re", CurCM + "-_-productreview")
			SeeAllLink.attr("manual_cm_sp", CurCM + "-_-productreview")
			
			// Flag
			var CurFlag = CurProd.find(".lessman-flag")
			CurFlag.html("")
			
			if (curObj.Flag)
			{
				var NewFlag = $("<div>")
					.addClass("product-flag")
				
				switch(curObj.Flag)
				{
					case "CustPick":
						NewFlag.addClass("cust-pick");
						NewFlag.html("<div>Customer</div><div>Pick</div>");
						break;
						
					case "TS":
						NewFlag.addClass("todays-special");
						NewFlag.html("<div>Today's</div><div>Special</div>");
						break;
						
					case "DailyFix":
						NewFlag.addClass("daily-fix");
						NewFlag.html("<div>Daily</div><div>Fix</div>");
						break;
				}
				
				CurFlag.append(NewFlag)
			}
			
			// Label
			var CurLabel = CurProd.find(".Label")
			if (curObj.Label)
			{
				CurLabel.data("product-label-url", curObj.Label)
				CurLabel.show()
			}
			else
				CurLabel.hide()
			
		// Video
			var IsAndroid = (navigator.userAgent && navigator.userAgent.toString().toLowerCase().indexOf("android") >= 0)
			
			var CurVideo = CurProd.find(".Video")
			if (!!curObj.Video && !IsAndroid)
			{
				var Asset = curObj.Video
				var AssetType = curObj.DistType
				
				//var vAndroid = curObj.Video.toString()
				//var vIOS = curObj.Video.toString()
				//var vSRT = curObj.Video.toString()
				var vDesktop = curObj.Video.toString()

				CurVideo.data("product-id", curObj.WebID)
				CurVideo.data("product-name", curObj.Name)
				//CurVideo.data("android-url", "http://llvid.hsn.com/clips/ngr/" + vAndroid.substr(vAndroid.length - 2, 2) + "/" + vAndroid + ".mp4")
				//CurVideo.data("ios-url", "https://odv.hsn.com/clips/ipad/" + vIOS.substr(vIOS.length - 2, 2) + "/" + vIOS + ".m4v")
				
				if (AssetType == 1)
				{
					CurVideo.data("rtmp-url", "rtmp://cp87160.edgefcs.net/ondemand/mp4:ODS/clips/ipad/" + vDesktop.substr(vDesktop.length - 2, 2) + "/" + vDesktop + ".mp4")
					CurVideo.data("http-url", "https://odv.hsn.com/clips/ipad/" + vDesktop.substr(vDesktop.length - 2, 2) + "/" + vDesktop + ".mp4")
				}
				else if (AssetType == 2)
				{
					CurVideo.data("rtmp-url", "rtmp://cp87160.edgefcs.net/ondemand/mp4:ODS/customcontent/" + Asset + ".mp4")
					CurVideo.data("http-url", "https://odv.hsn.com/customcontent/" + vDesktop + ".mp4")
					CurVideo.data("cc-url", "https://odv.hsn.com/customcontent/" + vDesktop + ".srt")
				}
				
				CurVideo.data("disttypeid", AssetType)
				
				//CurVideo.data("cc-url", "https://odv.hsn.com/clips/ipad/" + vSRT.substr(vSRT.length - 2, 2) + "/" + vSRT + ".srt")
				CurVideo.data("analytic-cat", "LessmanVideo")
				CurVideo.data("analytic-name", GetCoremetric(curObj.Name))
				CurVideo.attr("youtube","false")
				CurVideo.show()
			}
			else if (!!curObj.Youtube){
				var youtube = curObj.Youtube;
				CurVideo.addClass('youtube-player-modal');
				CurVideo.data({"show-disclaimer":"true", "id": youtube, "autoplay":"1", "suggest-hd":"1", "category":"LessmanVideo", "name": curObj.name});
				CurVideo.attr({"data-show-disclaimer":"true", "data-id": youtube, "data-autoplay":"1", "data-suggest-hd":"1", "data-category":"LessmanVideo", "data-name": curObj.name, "youtube":"true"});
				CurVideo.show()
			} 
			else
				CurVideo.hide()
			
			// Favorite
			//var CurFav = CurProd.find(".favorite-container a")
			//CurFav.data("object-id", curObj.WebID)
			
			// Footer
			var CurFooter = $(".Andrew .Footer")
			var CurFooterButton = CurFooter.find(".Button")
			//var CurCM = "Reviews-_-Footer-_-" + GetCoremetric((/<b>(.*?)<\/b>/).exec(curObj.Footer)[1])
			//var CurCM = "Reviews-_-Footer-_-" + GetCoremetric(curObj.Category)
			var CurCM = "Customer-Reviews-_-ShopMoreProducts-_-" + GetCoremetric(curObj.Category)
			
			var CurCategory = curObj.Category
			
			// Make plural for a couple of them
			if (CurCategory == "Skeletal Vitamin" || CurCategory == "Multivitamin")
				CurCategory += "s"
			
			var DI = HSN.Instance().DeviceInformation
			
			if (DI.IsNativeApp)
			{
				$(".Footer .App > a").hide()
				$($(".Footer .App > a")[curObj.Slide - 1]).show()
			}
			else
			{
				//CurFooter.find(".Text").html(curObj.Footer.replace("CATEGORY", "<b class='ProximaNovaBold'>" + curObj.Category + "</b>"))
				CurFooterButton.html("SHOP ALL " + CurCategory)
				CurFooterButton.attr("href", curObj.FooterLnk)
				CurFooterButton.attr("manual_cm_re", CurCM)
				CurFooterButton.attr("manual_cm_sp", CurCM)
			}
		}

		function AddReview(obj)
		{
			Reviews[Reviews.length] = obj
		}

		function ShowReviews(Slide)
		{
			var Cnt = 1
			
			for (var x = 0 ; x < Reviews.length ; x++)
			{
				var Cur = Reviews[x]
				
				if (Cur.Slide == Slide)
					RenderReview(Cur, Cnt++)
			}
		}

		function RenderReview(curSlide, Cnt)
		{
			var CurRating = $(".Andrew .Slide .Rating" + Cnt)
			
			// Stars
			var RatingBar = $("<div>")
			var ActiveRatingBar = $("<div>")
			
			if (Cnt == 1)
			{
				RatingBar.addClass("Rating")
				ActiveRatingBar.addClass("ActiveRating")
			}
			else
			{
				RatingBar.addClass("RatingSm")
				ActiveRatingBar.addClass("ActiveRatingSm")
			}
			
			ActiveRatingBar.css("width", curSlide.Rating / 5.0 * 100 + "%")
			
			RatingBar.append(ActiveRatingBar)
			
			// Title
			var Title = $("<div>")
				.addClass("Title")
			
			if (Cnt == 1)
			{
				Title.addClass("SabonNextLTPro-Regular")
				Title.html("<div class='Quote ProximaNovaThin'>&ldquo;</div><div style='float: left;'> " + curSlide.Title + "</div><div class='Quote ProximaNovaThin'>&rdquo;</div>")
			}
			else
			{
				Title.addClass("SabonNextLTPro-Bold")
				Title.html(curSlide.Title)
			}
			
			// Text
			var Text = $("<div>")
				.addClass("Text")
				.addClass("SabonNextLTPro-Regular")
				.html(curSlide.Text)
			
			// User
			var User = $("<div>")
			User.addClass("UserDiv")
			
			if (Cnt == 1)
				User.addClass("First")
			
			// Image
			var UserImg = $("<img>")
				.attr("src", "https://img.hsni.com/images/prod/2014/other/andrew/User_Icon.jpg")
			
			if (Cnt == 1)
				UserImg.css({width: "44px", height: "44px"})
			else
				UserImg.css({width: "25px", height: "25px"})
			
			User.append(UserImg)
			
			var NameContainer = $("<div>")
				.addClass("NameContainer")
			
			// Name
			NameContainer.append($("<div>")
				.addClass("UserName")
				.addClass("ProximaNovaBold")
				.html("<b>" + curSlide.Name + "</b>")
			)
			
			// Date
			NameContainer.append($("<div>")
				.addClass("UserDate")
				.addClass("ProximaNova")
				.html(curSlide.Date)
			)
			
			User.append(NameContainer)
			
			// Append
			CurRating.append(RatingBar)
			CurRating.append(Title)
			CurRating.append(Text)
			CurRating.append(User)
		}

		function ClearSlide()
		{
			$(".Andrew .Slide .Rating1").html("")
			$(".Andrew .Slide .Rating2").html("")
			$(".Andrew .Slide .Rating3").html("")
		}

		var Flags = {
			CustPick: "CustPick",
			TS: "TS",
			DailyFix: "DailyFix"
		}

		var Benefits = {
			Cholesterol: "Cholesterol",
			Gas: "Gas"
		}

		// Products
		AddProd({
			Slide: 1,
			Name: "Healthy Hair, Skin & Nails",
