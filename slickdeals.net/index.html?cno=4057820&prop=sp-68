	var mannsChoice;
	$('.box26016 .seeresults').click(function(e){
	  if(selectedChoicesLength > 0) {
			//do whatever the submit function is
			//console.log(selectedChoices);
			
			if (window.cmCreateManualLinkClickTag)
			{
				// First Call
				var CM = "Vitamin-finder-_-See-results-_-click"
				cmCreateManualLinkClickTag(window.location.pathname + '?cm_re=' + CM + '&cm_sp=' + CM, 'Lessman');
				
				mannsChoice = ((oldChoice) ? oldChoice : choice).replace(/ /,'');
				//alert('mc: '+mannsChoice);

				// Second Call, sorted list of the attributes
				var CM = "Vitamin-finder-_-"+mannsChoice+"-_-"
				
				var CmArray = []
				
				$.each(selectedChoices, function(i, v){
					CmArray.push(GetCoremetric_v2(v))
				})
				
				CmArray.sort()
				
				var Len = CmArray.length
				for ( var x = 0 ; x < Len ; x++ )
				{
					if (x == Len - 1)
						CM += CmArray[x]
					else
						CM += CmArray[x] + ":"
				}
				
				cmCreateManualLinkClickTag(window.location.pathname + '?cm_re=' + CM + '&cm_sp=' + CM, 'Lessman');
			}
			
			//var getVar = 'how='+encodeURIComponent(((oldChoice) ? oldChoice : choice))+'&';
			var getVar = 'brand=129&how='+encodeURIComponent(((oldChoice) ? oldChoice : choice))+'&';
			var getVarArr = Array();
			
			$.each(selectedChoices, function(i,v){
		  	getVarArr.push('facet='+i);
			});
			
			// Native Apps
			if (window.HSN && HSN.Instance().DeviceInformation.IsNativeApp && HSN.Gap)
			{
				var url = 'http://'+window.location.host + '/content/vitamin-finder-results/171?ab_cd=9&' + getVar + getVarArr.join('&');
				var gap = new HSN.Gap({ isWin8Metro: isWin8Metro });
				
				gap.ExecCommand("hsn://HSNWebView.Goto//" + encodeURIComponent(url));
			}
			else
			{
				setTimeout("window.location = '/content/vitamin-finder-results/171?" + getVar + getVarArr.join('&') + "'", 150)
			}
			
	  } else {
			//do nothing or throw an error that isn't an annoying popup or disable the see results button until at least one option is chosen
			//alert('Please choose at least one thing.');
	  }
	});
	
	var check;
	function toggleSlide26016(type, oldType){
		if (window.cmCreateManualLinkClickTag)
		{
			var CM = "Vitamin-finder-_-StepOne-_-" + GetCoremetric_v2(type)
			cmCreateManualLinkClickTag(window.location.pathname + '?cm_re=' + CM + '&cm_sp=' + CM, 'Lessman');
		}
					
		$('.box26016 .slide').toggleClass('show');
	  //console.log('type: '+type);
	  $('.box26016 .checkContainer').html('').scrollTop(0);
	  if(type){
	  	var realType = (oldType) ? oldType : type;
	  	$('.howMain').text(realType);
	  	var bn = 1;
		$.each(buttonObj[realType.replace(/ /g,'').toLowerCase()], function(v,i){
		  	var rtcol = ((bn)%3 == 0) ? ' rtcol' : '';
		  	var tl = (/__/g).test(v) ? ' twoliner' : '';
		  	$('.box26016 .checkContainer').append('<div class="checkButton'+rtcol+tl+'"><input type="checkbox" class="checkButtonBox" how="'+v+'" hownum="'+i+'"><span>'+v.replace(/__/g,'<br>').replace(/_/g,' ')+'</span></div>'); //.replace(/_/g,' ')
		  	bn++;
		});
		
		$('.box26016 .checkButton').click(function(e){
		  	e.stopPropagation();
		  	e.preventDefault();
			if(!$(e.target).is('input')){
				//console.log('button click to input');
				//$(this).find('input').click();
				$(e.target).find('input').click();
			}
			return false;
		});
		$('.box26016 .checkButton input').click(function(e){
		  	e.stopPropagation();
		  	//e.preventDefault();
		  	//var check;
		  	//if(!$(e.target).is('input')){
			//  check = $(this).find('input');//.click();
			//} else {
			  //check = $(this);
				check = $(e.target);
				//console.log(this)
			//}
			
		  if(check.attr('disabled')){
			//e.preventDefault();
			return false;
		  } else {
		  	//check[0].checked = !check[0].checked;
		  	//console.log('ischecked: '+check.is(':checked'));
			if(check.is(':checked')){
			  selectedChoicesLength++;
			  selectedChoices[check.attr('hownum')] = check.attr('how');
			  $(this).addClass('checked');
			  $(this).parent().addClass('ischecked');
			} else {
			  selectedChoicesLength--;
			  delete selectedChoices[check.attr('hownum')];
			  $(this).removeClass('checked');
			  $(this).parent().removeClass('ischecked');
			}
			//console.log('scl: '+selectedChoicesLength);
			switch(true){
			  	case(selectedChoicesLength > 3):
					//disable non-checked boxes somehow
			  		$('.box26016 .checkButton input:not(.checked)').attr('disabled','disabled');
				break;
			  	case(selectedChoicesLength < 1):
					$('.box26016 .seeresults span').text('make a choice');
				break;
			  	default:
			  		$('.box26016 .checkButton input:not(.checked)').removeAttr('disabled');
					$('.box26016 .seeresults span').text('see results');
				break;
			}
		  }
		  
		});
	  } else {
	  	selectedChoicesLength = 0;
			selectedChoices = {};
			
			if (window.cmCreateManualLinkClickTag)
			{
				var CM = "Vitamin-finder-_-GoBack-_-click"
				cmCreateManualLinkClickTag(window.location.pathname + '?cm_re=' + CM + '&cm_sp=' + CM, 'Lessman');
			}
	  }
	}
	//_26016-ce

	
// Tablet
$(document).ready(function() {
	var DI = HSN.Instance().DeviceInformation
	
	if (DI.IsTablet && DI.IsNativeApp)
	{
		$(".LessmanVitaminFinder").removeClass("Desktop").addClass("Tablet")
		
		var Size = 1.0 / 1536.0 * $(".LessmanVitaminFinder").width()
		$(".LessmanVitaminFinder").css("font-size", Size)
		
		$(window).resize(function() {
			var Size = 1.0 / 1536.0 * $(".LessmanVitaminFinder").width()
			$(".LessmanVitaminFinder").css("font-size", Size)
		})
	}
})
