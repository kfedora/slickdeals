		$.each(selectedChoices, function(i,v){
		  	//getVar += i+'='+encodeURIComponent(v)+'&';
		  	//getVarArr.push('facet='+encodeURIComponent(v));
		  	getVarArr.push('facet='+i);
		});
		var prefix = 'https://www.hsn.com/';
		//var prefix = '';
		window.location = prefix+'/content/vitamin-finder-results/171?'+getVar+getVarArr.join('&');
		//console.log('getvar: '+getVar);
