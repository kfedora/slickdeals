<script>
	/*//_26016-cs*/
// Helpers
function toTitleCase_v2(str) {
	if (!str)
		return ""
	
	return str.replace(/(?:^|\s)\w/g, function(match) {
		return match.toUpperCase();
	});
}

function GetCoremetric_v2(InText)
{
	var Output = toTitleCase_v2(InText)
	
	var ToRemove = "~!@#$%^&*()_+-=[]{};':\",.<>/?\|"
	
	for ( var x = 0 ; x < ToRemove.length ; x++ )
	{
		var CurChar = ToRemove.substr(x, 1)
		var Replacer = new RegExp("\\" + CurChar, 'g');
		
		Output = Output.replace(Replacer, "")
	}
	
	Output = Output.replace(/ /g, "")
	Output = Output.replace(/\t/g, "")
	
	return Output
}

	(/iphone/i).test(navigator.userAgent) ? $('body').addClass('iphone') : '';
	$('.box26016')
		.append($('<span class="mobileOnly26016 ProximaNovaBold">Find The Perfect Vitamins And Supplements</span>'));

	$('.box26016')
		.append($('<div class="slide slide1 show"></div>'))
		.append($('<div class="slide slide2"></div>'));
				
	/*$('.box26016 .slide1')
		.append($('<div class="choiceText">I want to focus on…</div>'))
		.append($('<div class="buttonContainer"><div class="choiceButton choice" how="Benefit"><span>Benefit</span></div><div class="choiceButton choice" how="Health Concern"><span>Health goals</span></div><div class="choiceButton choice" how="Body Part"><span>Physical concerns</span></div><div class="choiceButton choice last" how="Ingredient"><span>Specifc ingredients</span></div></div>'));*/
	
	//<div class="choiceButton choice" how="Health Concern"><span>Health Concern</span></div>
	$('.box26016 .slide1')
		.append($('<div class="choiceText ">I want to focus on…</div>'))
		.append($('<div class="buttonContainer"><div class="choiceButton choice " how="Health goals" oldhow="health goals"><span>Health goals</span></div><div class="choiceButton choice " how="physical concerns"><span>Physical concerns</span></div><div class="choiceButton choice  last" how="specifc ingredients"><span>Specifc ingredients</span></div></div>'));

	$('.box26016 .slide2')
		.append($("<div class='choiceText '>Select up to 4 <span class='howMain'>physical concerns</span>, <span class='howRest'>and we'll<br>recommend vitamins and/or supplements for you.</span></div>"))
		.append($('<div class="checkContainer"></div>'))
		.append($('<div class="ctaContainer"><div class="ctaButton goback choice"><span>Go Back</span></div><div class="ctaButton seeresults"><span>I&#39;m ready</span></div></div>'))
	
	var choice, oldChoice;
	var buttonObj = {};

	$.ajax({
		url: '//images.hsn.com/images/prod/scripts/LessmanVitaminFindernew.js',
		dataType: 'JSONP',
		jsonpCallback: 'vitaminMatrix',
		success: function(data,ts,jq){
			//console.log('success:');
			//console.debug(data);
			buttonObj = data;
        },
        error: function(e){ /*console.log('error:'); console.debug(e); */ }
	});

	var selectedChoicesLength = 0;
	var selectedChoices = {};
	
	//button handlers, show the next slide, etc
	/*$('.box26016 .choice').click(function(e){
		choice = $(this).attr('how');
	  	//console.log('choice: '+choice);
	  	toggleSlide26016(choice);
	});*/
	
	//button handlers, show the next slide, etc
	$('.box26016 .choice').click(function(e){
		choice = $(this).attr('how');
		oldChoice = $(this).attr('oldhow');
	  	//console.log('choice: '+choice);
	  	toggleSlide26016(choice, oldChoice);
	});

	/*$('.box26016 .seeresults').click(function(e){
	  if(selectedChoicesLength > 0){
		//do whatever the submit function is
		//console.debug(selectedChoices);
		//var getVar = 'brand='+encodeURIComponent(choice)+'&';
		var getVar = 'brand=129&how='+encodeURIComponent(choice)+'&';
