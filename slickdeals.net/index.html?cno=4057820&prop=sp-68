<script>
        
    function getCookie(key) {
        var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
        return keyValue ? keyValue[2] : null;
    }

    jQuery(document).ready(function () {
        if (getCookie('lessman_header') == null) {

            document.cookie = 'lessman_header=true;path=/';

            setTimeout(function () {

                $("#navigation-wrapper2").slideUp(250, function () {
                    $("#navigation-wrapper2").css('display', 'none');

                    $("#navigation-wrapper").slideDown(750, function () {

                            

                        setTimeout(function () {
                            $("#navigation-wrapper").slideUp(500, function () {
                                $("#navigation-wrapper2").slideDown(250, 'linear', function () {
                                    $("#navigation-wrapper2").css('display', 'block');
                                });
                            });
                        }, 1000);
                    });
                });
            }, 1000);


        }

        $("#navigation-wrapper").mouseleave(function () {

            $("#navigation-wrapper").slideUp(350, function () {
                $("#navigation-wrapper2").slideDown(250, 'linear', function () {
                    $("#navigation-wrapper2").css('display', 'block');
                });
            });
        });
    });
</script>


