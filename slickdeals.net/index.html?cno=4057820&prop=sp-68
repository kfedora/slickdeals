<script>
	(function($){
		var _opt = { currentModal: false },
			_device = JSON.parse($('script.device-information').html()),
			slides = (!!_device.IsMobile && !_device.IsTablet && 1) || 3;
		$(document).ready(function(){
			$('.lessman-whybetter .lessman-whybetter-carousel ._items')
				//store the non-cloned slides for checking indexes later
				.on('init._lbv',function(ev,slick){
					_opt.slides = slick.$slideTrack.children(':not(.slick-cloned)');
				})
				.on('beforeChange._lbv', function(ev,slick,currentSlide,nextSlide){
					//workaround for slick clone data-lazy issue
					//if we are wrapping around to the first slide again
					if(nextSlide == 0){
						//get the img src of that slide
						$.each($(slick.$slides[nextSlide]).add($(slick.$slides[nextSlide]).nextUntil('[data-slick-index="'+slick.options.slidesToScroll+'"]')).find('[src]'),function(i,v){
							//get the src of the img
							var src = $(v).attr('src');
							//find the clone that has that img and set src, remove extra attrs
							$(slick.$slider)
								.find('.slick-cloned [data-lazy="'+src+'"]')
								.attr('src',src)
								.removeAttr('data-lazy data-srcset data-sizes');
						});
						//stop firing the beforechange event
						$(slick.$slider).off('beforeChange._lbv');
					}
				})
				.slick({
					slidesToShow:slides,
					slidesToScroll:slides,
					lazyLoad:'ondemand',
					infinite:true,
					appendArrows: '.lessman-whybetter-carousel ._controls',
					autoplay:(!!_device.IsMobile && !_device.IsTablet),
					autoplaySpeed:7000
				});
			//modal open callback
			HSN.Instance().Events.On(HSN.Modal.EVENT.MODAL_OPEN,function(){
				if(typeof(_opt.currentModal) != 'boolean'){ 
					//put the shoppable content next to the video
					$('.lessman-whybetter-slides ._slide')
						.eq(_opt.currentModal)
						//get the image before cloning
						.find('[data-lazy]:not([src])')
							//recenter once after img is loaded
							.one('load',function(ev){$(window).resize();})
							.attr('src',function(){ return this.attributes['data-lazy'].value; })
						.end().clone()
						.appendTo('.modal .modal-content-container');
					_opt.currentModal = false;
					//force the modal to recenter after the extra content has been added
					$(window).resize();
				}
			});
			//slide anchor click
			$('body:not(.appTablet):not(.mobile) .lessman-whybetter .lessman-whybetter-carousel')
				.on('click._lbv','a',function(ev){
					//store the current shoppable modal value for use in the modal open callback
					_opt.currentModal = _opt.slides.index(this.parentNode) ;
				});
		});
	})(jQuery);
</script>

</div>
