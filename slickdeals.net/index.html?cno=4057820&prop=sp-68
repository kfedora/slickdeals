<script type="text/javascript">
jQuery(document).ready(function() {
	jQuery(".aFlyoutHover").each(function() {
		var Cur = jQuery(this);
		//n = jQuery(this).children(".aFlyout")
		var n = $("#" + $(this).attr("data-container"));
		
		var r = new HSN.Flyout({
			cacheable: true,
			closeClass: "ir",
			closeTag: "li",
			flyout: n,
			trigger: this,
			url: "/namedcell?name=" + Cur.attr("data-flyout"),
			disableHover: Modernizr.touch,
			triggerEvent: Modernizr.touch ? HSN.Flyout.TRIGGER_EVENT.CLICK : HSN.Flyout.TRIGGER_EVENT.HOVER
		});
		
		r.Events.On(HSN.Flyout.EVENT.AFTER_LOAD, function(flyout){
			$.each(flyout.Flyout.find('a'),function(i,v){$(v).attr('href',$(v).attr('href').toLowerCase());});
		});
		
		
		HSN.Instance().FlyoutManager.Add(r);
	});
});
</script>


