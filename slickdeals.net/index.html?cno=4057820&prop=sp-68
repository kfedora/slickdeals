		// Label Support
		$(".Andrew").on("click", ".Label", function(e) {
			e.preventDefault()
			e.stopPropagation()
			
			var Width = $("#AndrewCustReview").width()
			
			var r = '<div style="width: ' + (Width - 30) + 'px; padding: 15px;"><img width="100%" src="' + $(this).data('product-label-url') + '" alt="product label" /></div>'
			
			HSN.Instance().GetPageIdModal().Show($("<div/>").append($("<div/>", {html: r})))

			cmCreateManualLinkClickTag(window.location.pathname +
				'?cm_re=Top-Rated-_-productlabel-_-NA&cm_sp=Top-Rated-_-productlabel-_-NA', 'Lessman');
		});

		// Video Support
		$(".Andrew").on("click", ".Video[youtube='false']", function(e) {
			e.stopPropagation();
			e.preventDefault();
			
			/*
			var i = $(this).data()
			var r = i.disttypeid == 2 ? 30 : 12
			var u = [{
				title: i.productName,
				source: [{
					src: i.rtmpUrl,
					type: "video/mp4"
				},
				
				{
					src: i.httpUrl,
					type: "video/mp4",
					rules: ["ipad_encoding"]
				},
				
				{
					src: i.iosUrl,
					type: "video/mp4",
					rules: ["iphone_encoding"]
				},
				{
					src: i.androidUrl,
					type: "video/mp4",
					rules: ["android_encoding"]
				}],
				track: [{
					kind: "captions",
					type: "application/x-subrip",
					srclang: "en",
					src: location.protocol + "//" + location.host + "/services/getcaptions/" + i.productId + "/" + r + ".srt"
				}]
			}];
			
			i.overrides = {
				rules: {
					android_encoding: {
						regexp: "Android"
					},
					iphone_encoding: {
						regexp: "iPhone|iPod"
					},
					ipad_encoding: {
						regexp: "iPad"
					}
				},
				media: u[0],
				autoplay: true,
				controls: {
					mode: "auto"
				},
				flash: {
					params: {
						bgColor: "#000"
					}
				},
				captioning: {
					enabled: true
				}
			};
			*/
			
			var i = $(this).data()
			var r = i.disttypeid == 2 ? 30 : 12
			var u = [{
				title: i.productName,
				source: [{
					src: i.rtmpUrl,
					type: "video/mp4"
				},
				{
					src: i.httpUrl,
					type: "video/mp4",
					rules: ["Mobiles"]
				}],
				track: [{
					kind: "captions",
					type: "application/x-subrip",
					srclang: "en",
					src: location.protocol + "//" + location.host + "/services/getcaptions/" + i.productId + "/" + r + ".srt"
				}]
			}];
			
			i.overrides = {
				rules: {
					Mobiles: {
						regexp: "Android|iPhone|iPod|iPad"
					}
				},
				media: u[0],
				autoplay: true,
				controls: { mode: "auto" },
				flash: { params: { bgColor: "#000" } },
				captioning: { enabled: true }
			};
			
			HSN.Instance().GetVideoModal().Show(i, 724, 408, true, false)
		})

		$(".Andrew").on("click", ".youtube-player-modal", function(e) {
			e.stopPropagation();
			e.preventDefault();
			var t = $(this).data("name")
			  , n = $(this).data("category")
			  , a = $(this).data("id")
			  , i = $(this).data("element")
			  , o = $(this).data("showDisclaimer")
			  , r = {
				videoId: a,
				element: i,
				name: t,
				category: n,
				playerVars: {
					autoplay: $(this).data("autoplay"),
					controls: 1,
					fs: 1,
					theme: "light",
					rel: 0
				}
			};
			if ($(this).hasClass("youtube-player-inline")) {
				new HSN.YoutubePlayer({
					application: HSN.Instance(),
					settings: r
				})
			} else {
				var s = HSN.Instance().GetYoutubeModal();
				r.element = "hsn-youtube-player",
				s.Show(r, 720, 408, o)
			}
			return !1
		})

		// DropDown
		$(".DropDown").click(function(){
			$(this).hide()
		})

		$(".DropDownClicker").click(function(e) {
			e.preventDefault()
			e.stopPropagation()
			
			$(".DropDown").show()
		})

		// DropDown AntiClicker
		$("body").click(function(e) {
			var DropDown = $(".DropDown")
			
			if (DropDown.css("display") == "block")
				DropDown.hide()
		})

		// See all reviews link
		$(".Andrew").on("click", ".ProdDetails", function(e) {
			//var Cur = $(this)
			var Cur = $(".Andrew .ProdDetails")
			Cur.unbind("click")
			Cur.find(".SeeAll")[0].click()
		})

		//show slides
		//"javascript: ShowSlide(" + Cur.Slide + ");"
		$(".DropDown .List").on('click',".Item a",function(ev){
			ev.preventDefault();
			ShowSlide($(this).attr('slide'));
			return false;
		});
		// Tablet
		function TrimReviews()
		{
			if (window.HSN && HSN.Instance().DeviceInformation.IsNativeApp)
			{
				var CurReviews = $(".Rating1 .Text, .Rating2 .Text, .Rating3 .Text")
				
				$.each(CurReviews, function(i){
					var Cur = $(this)
					var CurText = Cur.html()
					if (CurText.length > 410)
						Cur.html(CurText.substr(0, CurText.indexOf(" ", 400)) + "...")
				})
			}
		}

		$(document).ready(function() {
			var DI = HSN.Instance().DeviceInformation
			
			if (DI.IsTablet && DI.IsNativeApp)
			{
				$("#AndrewCustReview").removeClass("Desktop").addClass("Tablet")
				
				var Size = 1.0 / 1536.0 * $(window).width()
				$("#AndrewCustReview").css("font-size", Size)
				
				$(window).resize(function() {
					var Size = 1.0 / 1536.0 * $(window).width()
					$("#AndrewCustReview").css("font-size", Size)
				})
				
				//TrimReviews()
				
				// Fix links
				//var SeeAllLink = $("#AndrewCustReview .ProdDetails .SeeAll")
				//SeeAllLink.attr("href", SeeAllLink.attr("href").replace("#product-reviews", ""))
				
				if (navigator.userAgent && navigator.userAgent.toString().toLowerCase().indexOf("android") >= 0)
					$("#AndrewCustReview .Icons .Video").hide()
				
				// Footer button
				$(".Footer .Desktop").hide()
			}
			// Start main script
			// Automatically show slide 1
			ShowSlide(2,true)
		})
	})(jQuery);
</script>

</div>
