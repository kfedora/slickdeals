<script>
    (function ($) {
        $(document).ready(function () {

            var violators = [];
            var widgetContainer = $("#dynamic-carousel-27e7e64d01e34b25ada50c139b9b13c7");
            var isMobile = false;
            var isSingleCard = false;

            var carouselSelector = "#dynamic-carousel-27e7e64d01e34b25ada50c139b9b13c7 .module .product-list";
            var carouselContainer = $("#dynamic-carousel-27e7e64d01e34b25ada50c139b9b13c7 .module");
            var itemWidth = parseFloat($(carouselSelector).find("li").first().outerWidth());

            //do for every case except for single card mobile
            if (!isMobile) {//mobile is vertical carousel
                carouselContainer.css("width", itemWidth * 4);
            }

            var x = new HSN.Carousel({
                carousel : carouselSelector,
                touch : Modernizr.touchevents,
                vflags : violators,
                showPreview : true,
                slidesToShow : isMobile ? 1 : 4
            });

            
            // center
            var itemListWidth = parseFloat(carouselContainer.find("ul.product-list").first().outerWidth());
            var carouselWidth = parseFloat(carouselContainer.outerWidth());
            var innerWidth = itemListWidth < carouselWidth ? itemListWidth : carouselWidth;
            carouselContainer.css("margin-left", (parseFloat(widgetContainer.innerWidth()) - innerWidth) / 2);
            

            carouselContainer.css("overflow-x", "visible");
            widgetContainer.css("visibility", "visible");

            $('.carousel-container .lessman-pdcard-video').click(function () {
                cmCreateManualLinkClickTag('/Video?cm_re=Video-_-Product-_-LessmanCarousel&cm_sp=Video-_-Product-_-LessmanCarousel');
            });

            carouselContainer.find(".lessman-pdcard-video").click(function (e) {

                e.stopPropagation();
                e.preventDefault();

                var videoData = $(this).data();

                if (videoData.coremetrics) {
                    window.cmCreateManualLinkClickTag(window.location.pathname + '?cm_re=' + videoData.coremetrics + '&amp;cm_sp=' + videoData.coremetrics);
                }

                if (!isMobile) {

                    //array of videos that contain all the video types
                    var videos = [
                        {
                            title: videoData.productName,
                            source: [
                                { src: videoData.httpUrl, type: "video/mp4" },
                                { src: videoData.androidUrl, type: "video/mp4", rules: ["android_encoding"] }
                            ],
                            track: [
                                //tried relative but player didn't like it so used this location object... not good but works
                                { kind: "captions", type: "application/x-subrip", srclang: "en", src: "https://" + location.host + "/services/getcaptions/" + videoData.productId + "/" + videoData.assetid }
                            ]
                        }
                    ];

                    //video overrides object for akamai player.
                    videoData = {
                        rules: {
                            android_encoding: { regexp: "Android" }
                        },
                        media: videos[0],
                        autoplay: true,
                        controls: { mode: "auto" },
                        captioning: { enabled: true }
                    };

                }

                HSN.Instance().GetVideoModal().Show(videoData, 724, 408, true, false);

            });
        });
    })(jQuery);
</script>
