</main><script type="text/javascript" SPK="8840668578876">/* Show page inmediatly */

showPage();

function showPage() {
    document.body.removeChild(loader);
    document.querySelector('.body-inner-wrapper').style.opacity = 1;
    hideScrollBar('auto');
}

function hideScrollBar(value) {
    document.body.parentElement.style.overflowY = value;
}

var slideContainer = document.querySelector('.slide-container'),
    slideContainerMobile = document.querySelector('.hero-swiper-container'),
    slidesHero = document.getElementsByClassName('slide'),
    sliderBtnforDesktop = document.querySelectorAll('.hero-btn-desktop'),
    tracker = document.querySelector('.tracker'),
    slidePosition = "slide1",
    espotContainer = document.querySelector('.espot-container'),
    espotWrapper = document.querySelector('.espot-wrapper'),
    espotSlide = document.querySelectorAll('.espot-slide'),
    legalBtn = document.querySelector('.legal-btn'),
    legalCopy = document.querySelector('.legal-copy'),
    mainContent = document.getElementById('main-content'),
    espots = document.getElementById('espots'),
    brands = document.getElementById('brands'),
    categories = document.getElementById('categories'),
    mySwiperEspots,
    mbl,
    desktop;


function sliderForDesktop() {
    resetAttributes(slideContainerMobile, 'hero-swiper-container', slideContainer, 'slide-container', slidesHero, "");
    if (mbl) {
        setTimeout(function () {
            slideContainer.removeAttribute('style');
        }, 500);
        moveSlider('0', '0', '0');
    }

    $('.hero *').bind('pointerdown', false);


    [].forEach.call(sliderBtnforDesktop, function (el, index) {
        el.addEventListener('click', function () {
            if (index === 0) {
                slidePosition = 'slide1';
            } else if (index === 1) {
                slidePosition = 'slide2';
            } else {
                slidePosition = 'slide3';

            }


            if (slidePosition === "slide1") {
                moveSlider('0', '0', '0');
            } else if (slidePosition === "slide2") {
                moveSlider('-33.33333%', '50%', '-50%');
            } else {
                moveSlider('-66.66666%', '100%', '-100%');
            }
        });
    });

    mbl = false;
}



function sliderForMobile() {
    if (!mbl) {
        resetAttributes(slideContainerMobile, 'swiper-container hero-swiper-container', slideContainer, 'swiper-wrapper slide-container', slidesHero, "swiper-slide ");

        $('.hero *').unbind('pointerdown', false);

        var swiperHero = new Swiper(slideContainerMobile, {
            slidesPerView: 1,
            spaceBetween: 0,
            speed: 600,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
        });

        mainContent.style.opacity = 1;

    }
    mbl = true;
}


function resetAttributes(elem1, class1, elem2, class2, elem3, class3) {
    elem1.className = class1;
    elem2.className = class2;

    for (var i = 0; i < elem3.length; i++) {
        elem3[i].className = class3 + "slide slide-" + [i + 1];
        elem3[i].removeAttribute('style');
    }

}




function moveSlider(position, positionTracker1, positionTracker2, scrollPos) {
    slideContainer.style.transform = "translateX(" + position + ")";
    tracker.style.cssText = 'left:' + positionTracker1 + ';transform: translateX(' + positionTracker2 + '); -webkit-transform: translateX(' + positionTracker2 + ')';
}

function checkScreenSize() {
    window.innerWidth < 750 ? sliderForMobile() : sliderForDesktop();
}



function createEspot() {
    if (window.innerWidth < 750) {
        mySwiperEspots = new Swiper(espotContainer, {
            slidesPerView: 2.25,
            spaceBetween: 8,
            roundLengths: true
        });

        if (desktop) {
            espotContainer.className = 'swiper-container espot-container';
            espotWrapper.className = 'swiper-wrapper espot-wrapper';
        }

        desktop = false;

        for (var i = 0; i < espotSlide.length; i++) {
            espotSlide[i].className = "swiper-slide espot-slide";
        }

    } else {
        if (mySwiperEspots) {
            mySwiperEspots = undefined;
        }

        desktop = true;
        espotContainer.className = 'espot-container';
        espotContainer.removeAttribute('style');
        espotWrapper.className = 'espot-wrapper';
        espotWrapper.removeAttribute('style');

        for (var i = 0; i < espotSlide.length; i++) {
            espotSlide[i].className = "espot-slide";
            espotSlide[i].removeAttribute('style');
        }
    }
}

function createProducts() {

    var productsContainer = document.querySelector('.best-sellers-container'),
        espacio;
    window.innerWidth < 750 ? espacio = 8 : espacio = 21;
    swiper = new Swiper(productsContainer, {
        slidesPerView: 2.25,
        roundLengths: true,
        spaceBetween: espacio,
        breakpoints: {

            1366: {
                slidesPerView: 2.5,
            },
            1500: {
                slidesPerView: 4,
            },
            1765: {
                slidesPerView: 4.5,
            },
            1920: {
                slidesPerView: 5,
            }
        }
    });

}

function showAllCategories() {
    var categoriesBtn = document.querySelectorAll('.view-js'),
        categoriesHide = document.querySelectorAll('.categories-hide');


    [].forEach.call(categoriesBtn, function (btn, index) {
        btn.addEventListener('click', function () {
            if (index === 0) {
                addActiveClass(document.querySelectorAll('.view-js')[0], 'categories-hide');
                for (var i = 0; i < categoriesHide.length; i++) {
                    removeActiveClass(categoriesHide[i], 'categories-hide');
                    addActiveClass(categoriesHide[i], 'bounceIn');
                }
            } else {
                for (var i = 0; i < categoriesHide.length; i++) {
                    addActiveClass(categoriesHide[i], 'bounceOut');
                    categoriesHide[i].addEventListener('animationend', function () {
                        if (this.classList.contains('bounceOut')) {
                            addActiveClass(this, 'categories-hide');
                            removeActiveClass(this, 'bounceOut');
                            removeActiveClass(document.querySelectorAll('.view-js')[0], 'categories-hide');
                        }
                        document.documentElement.scrollTop = categories.offsetTop;
                        document.body.scrollTop = categories.offsetTop;
                    });
                }

            }
        });
    });
}

function checkPrices() {
    var contador = 0,
        intervalo = setInterval(function () {
            var prices = document.querySelectorAll('.price-hide');
            [].forEach.call(prices, function (price) {
                if (price.innerText.indexOf('$') > -1) {

                    price.classList.remove('price-hide');
                    contador++;

                }
                if (contador === 9) {
                    clearInterval(intervalo);
                }
            });
        }, 500);
}

function expandLegal() {
    legalBtn.addEventListener('click', function () {
        var legalText = this.children[0];
        if (legalText.innerText === "READ MORE") {
            legalText.innerText = "HIDE";
            addActiveClass(legalCopy, 'active')
        } else {
            legalText.innerText = "READ MORE";
            removeActiveClass(legalCopy, 'active');
        }
    })
}

function removeBrandsSectionForCA() {
    if (location.href.indexOf('/ca/') > -1) {
        brands.style.display = "none";
    }
}

function openModal() {
    var modalElements = document.querySelectorAll('.js-modal'),
        modalLegal = document.getElementById('modal-legales');

    [].forEach.call(modalElements, function (elemento, index) {
        elemento.addEventListener('click', function () {
            index === 0 ? (addActiveClass(modalLegal, 'active'), hideScrollBar('hidden')) : (removeActiveClass(modalLegal, 'active'), hideScrollBar('auto'));
        });
    });
}

function stylePopUp() {
    var headerMobile = parseInt(window.getComputedStyle(document.getElementsByClassName('dockNav')[0]).height);
    window.innerWidth < 750 ? document.getElementById('modal-legales').style.height = "calc(100vh - " + headerMobile + "px)" :
        document.getElementById('modal-legales').style.height = "100vh";
}

function reorderModules() {
    if (window.innerWidth < 750) {
        mainContent.insertBefore(brands, mainContent.children[5]);
        mainContent.insertBefore(espots, mainContent.children[4]);
    } else {
        mainContent.insertBefore(brands, mainContent.children[1]);
        mainContent.insertBefore(espots, mainContent.children[2]);
    }
}


function addActiveClass(elemento, clase) {
    elemento.classList.add(clase);
}

function removeActiveClass(elemento, clase) {
    elemento.classList.remove(clase);
}


removeBrandsSectionForCA();
checkScreenSize();
createEspot();
/*  stylePopUp(); */
showAllCategories();
createProducts();
expandLegal();
openModal();
checkPrices();
reorderModules();


window.addEventListener('resize', function () {
    checkScreenSize();
    createEspot();
    /*  stylePopUp(); */
    reorderModules();
    createProducts();
});</script>
</div>
<!-- Start content under tabs --> 
<!-- end search layout --> 
</div>
