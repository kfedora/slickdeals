<script type="text/mustache" id="{{DeliveryDatesOverlay}}">
<div class="as-deliverydatesoverlay clearfix large-centered">
    <div class="as-deliverydatesoverlay-heading-wrap">
        <h2 id="dudeOverlayHeading" class="as-deliverydatesoverlay-heading">{{overleyHeader}}</h2>
    </div>
