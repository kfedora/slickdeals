                    <script>
            function appendRecaptchaScript() {
                var script = document.createElement('script');
                script.src = 'https://www.google.com/recaptcha/api.js?render=explicit&onload=onRecaptchaLoadCallback';
                script.async = true;
                script.defer = true;
                document.body.appendChild(script);
            }
            if (window.addEventListener) {
                window.addEventListener('load', appendRecaptchaScript);
            } else if (window.attachEvent) {
                window.attachEvent('onload', appendRecaptchaScript);
            }
        </script>
                    
