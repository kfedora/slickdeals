								picOnLoad(true);
							}else{
							    image.onload = function(){
							    	picTimer1=new Date().getTime();
							    	picOnLoad(true);
							    };
							    image.onerror = function(){
									picOnError();
								};
							}
							image.onerror = function(){
								picOnError();
							};

							var backgroundImgTest = 'false';
