                /* if(redirectUrl.charAt(redirectUrl.length-1) == "/"){
                    redirectUrl=redirectUrl.substring(0,redirectUrl.length-1);
                }else{
                    redirectUrl=redirectUrl;
                } */
                //This program and mingfei, lixin, young confirmed
                var newWidnow =  window.open(redirectUrl,"_top","scrollbars=yes, resizable=yes");
                newWidnow.document.body.style.overflow = 'scroll';
                newWidnow.scrollTo(0,0);
                newWidnow.focus();
            }
		</script>
