    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script><script>
  window.jQuery || document.write('<script src="https://gamespot1.cbsistatic.com/vd40e26ca80e9836530ae677c4a1c8479/js/compiled/jquery.local.js">\x3C/script>')
