log: function (message) {
if (isLoggingActive) {
console.log(message);
}
}
};
if (data && attrs && id) {
client = window.optimizelyClient.createInstance({
datafile: data,
skipJSONValidation: true,
logger: logger,
logLevel: logLevel
});
