window.bvDCC = {
catalogData: {
locale: "en_US",
catalogProducts:[{"productId":"k181djue","productName":"K181 DJ UE","productDescription":"Developed based on input from professional DJs, the K181 DJ UE reference class DJ headphones provides all the features needed for those who own the crowd at the club.","productImageURL":"https://www.harmanaudio.com/dw/image/v2/AAUJ_PRD/on/demandware.static/-/Sites-masterCatalog_Harman/default/dwcdb3529f/AKG_K181DJ_UE_Standard_V7_ohneKabel_EarpadRetouch.jpg?sw=270&sh=330&sm=fit&sfrm=png","productPageURL":"https://www.harmanaudio.com/professional-headphones/K181DJUE.html","brandName":"akg","categoryPath":[{"id":"professional-headphones","Name":"Headphones"}],"family":"k181djue","inactive": false}]
}
