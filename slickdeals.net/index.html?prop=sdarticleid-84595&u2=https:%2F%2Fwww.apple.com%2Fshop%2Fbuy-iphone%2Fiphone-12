    <input type="text" id="ii_searchreset" aria-owns="{{dataUid}}_nl_ml" aria-autocomplete="list" aria-expanded="{{menuShowing}}" {{#selId}}aria-activedescendant='{{.}}'{{/selId}} value="{{chosenValue}}" data-ase-nl-input="{{dataUid}}_nl" {{#dataInputId}} id="{{.}}"{{/dataInputId}}
        {{#dataInputClass}}       class="{{.}}"{{/dataInputClass}}
        {{#dataInputName}}        name="{{.}}"{{/dataInputName}}
        {{#dataInputMaxlength}}   maxlength="{{.}}"{{/dataInputMaxlength}}
        {{#dataInputMaxlength}}   size="{{.}}"{{/dataInputMaxlength}}
        {{#dataInputPlaceholder}} placeholder="{{.}}"{{/dataInputPlaceholder}}
        {{#dataInputAutocomplete}}autocomplete="{{.}}"{{/dataInputAutocomplete}}
    >
</input-reset>

