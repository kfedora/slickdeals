    <script type="text/javascript">
		 function menuItemClick(obj){
			 /* begin : handled by conggb1 for NA-21650 */
			try{
				window.nemoFacetAdobe.handleClickLogicForAdobe();
				if($(obj).is(":radio")){
					window.location.href=$(obj).attr("location");
					return;
				}
			}catch(err){
				console.info("Error occurs : " + err);
			}
			/* end : handled by conggb1 for NA-21650 */
		var checkedLength = $("#facetedBrowseWrapper").find("input[type='checkbox']:checked").length;
		var listLength = $("#facet-area-clear-container").find("li").length;
		   console.log("listLength: " + listLength);
		  if(checkedLength == 0){
			var baseUrl = $("#facet-area-clear-container").find('a').attr('href');
			//added by WL for PPS-1634 start
			if(baseUrl)
		 	//added by WL for PPS-1634 end
				location.replace(baseUrl);
		  }else if( checkedLength > 0 ){  
			  $(obj).closest('form').submit();
		  }
		 }
		</script>
		</form>
    </div>
