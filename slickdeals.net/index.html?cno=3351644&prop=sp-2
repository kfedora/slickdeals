                    at_property: "b7ae968b-52b6-dfde-d9f3-ae9d30ce6f99"
                };


                Object.keys(values).map(function (key, index) {
                    var e = document.createElement('div');
                    e.innerHTML = values[key];
                    values[key] = e.childNodes.length === 0 ? "" : e.childNodes[0].nodeValue;
                });

                var params = [];
                var value;

                for (value in values) {
                    if (values.hasOwnProperty(value)) {
                        if (value !== "mboxName") {
                            params.push(value + "=" + values[value]);
                        } else {
                            params.push(values[value]);
                        }

                    }
                }

                try {
                    mboxDefine(mboxName, mboxName);
                    mboxUpdate.apply(undefined, params);
                } catch (e) {
                    if (console && console.log) {
                        console.log(e);
                    }
                }
            })();

    </script>

              <script type="text/javascript" src="//nexus.dell.com/dell/marketing/Bootstrap.js" defer></script>
                        <script data-perf="bodyGlobal">Dell.perfmetrics.end('bodyGlobal');</script>

          

        <script defer src="//apps.bazaarvoice.com/deployments/dellglobal/readonlyspecialevents/production/en_US/bv.js"></script>



          
