    ORW.filePath = ORW.filePath.split('/placeholder_100x.png')[0];
    ORW.formatMoney = function(money){
        // Reads in money in $104.00 format, not 104
        if(ORW.hideTrailingZeros){
            return money.replace(/((\,00)|(\.00))$/g, '');
        }else{
            return money;
        }
    };
