			];
			var hps_counter = 0;
			var hps_elem = document.getElementById('header-promo-static');
			// save instance in magnificPopup variable
			hpsChange();
			var hps_inst = setInterval(hpsChange, 6000);

			function hpsChange() {
				hps_elem.innerHTML = hps_text[hps_counter];
				hps_counter++;

				$('.promo_details_popup').click(function(e) {
					e.preventDefault()
					$.magnificPopup.open({
						items: {
							src: '#' + $(this).data('popup-id'),
							type: 'inline'
						},
						callbacks: {
							open: function() {
								$('body').css('max-width', 'initial');
							}
						}
					});
				});
				if (hps_counter >= hps_text.length) {
					hps_counter = 0;
					// clearInterval(inst); // uncomment this if you want to stop refreshing after one cycle
				}
			}
		</script>

		<!-- Start of LiveChat (www.livechatinc.com) code -->
		<script type="text/plain" class="optanon-category-C0003">
			$("#new-livechat-container").show();
			if(Response.viewportW() > 768) {
				window.__lc = window.__lc || {};
				window.__lc.license = 5251911;
				(function() {
					var lc = document.createElement('script'); lc.type = 'text/javascript'; lc.async = true;
					lc.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'cdn.livechatinc.com/tracking.js';
					var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(lc, s);
				})();
			}
		</script>
		<!-- End of LiveChat code -->

		<!-- Listrak Subscribe Code (on customer_service/health page) -->
		<script type="text/javascript">
			$(document).ready(function() {
				$('#form_subscribe_sms').submit(function(e) {
					e.preventDefault();
					var error = '';
					if ($('#frm_number').val() == '') {
						error = error + "Please enter your mobile phone number.\n";
						$('#frm_number').focus();
					}
					if ($('input[name="confirm[]"]:checked').length < 1) {
						error = error + "Please accept our SMS policies.\n";
						$('.terms').focus();
					}
					if (error == '') {
						$.post(this.action, $('#form_subscribe_sms').serialize(), function(data) {
							if (data.status == 'error') {
								alert(data.message);
							} else if (data.status == 'success') {
								var phone = data.phone;
								var site_location = $('input[name="location"]').val();
								$('#widget').html('Complete your subscription by responding to the text message we just sent you...');
								segmentTrack("Sms Capture Successful", {
									"phone": phone,
									"site_location": site_location
								});
								segmentTrack("Sms Capture Successful Listrak Auto", {
									"phone": phone,
									"site_location": site_location
								});
							}
						}, 'json');
					} else {
						alert(error);
					}
				});

				$('#checkout-sms-form').submit(function(e) {
					e.preventDefault();
					var error = '';
					if ($('#frm_number').val() == '') {
						error = error + "Please enter your mobile phone number.\n";
						$('#frm_number').focus();
					}
					if ($('input[name="confirm[]"]:checked').length < 1) {
						error = error + "Please accept our SMS policies.\n";
						$('.terms').focus();
					}
					if (error == '') {
						$.post(this.action, $('#checkout-sms-form').serialize(), function(data) {
							if (data.status == 'error') {
								alert(data.message);
							} else if (data.status == 'success') {
								var phone = data.phone;
								var site_location = $('input[name="location"]').val();
								$('#checkout-confirm-sms-cta').html('Complete your subscription by responding to the text message we just sent you...');
								segmentTrack("Sms Capture Successful", {
									"phone": phone,
									"site_location": site_location
								});
								segmentTrack("Sms Capture Successful Listrak Auto", {
									"phone": phone,
									"site_location": site_location
								});
							}
						}, 'json');
					} else {
						alert(error);
					}
				});
			});
		</script>
