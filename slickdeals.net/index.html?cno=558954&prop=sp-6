		<script type="text/javascript">
			picturefill();
			new LazyLoad({
				'threshold': 100
			});
			var MobileSwap = (function() {
				var className = 'mobile-swap';
				var getScreenWidth = function() {
					return (Math.max(document.documentElement.clientWidth, window.innerWidth || 0));
				};
				var interval = null;
				var frequency = 100;
				var update = function() {
					var screenWidth = getScreenWidth();
					var mobileImages = document.getElementsByClassName(className);
					var i = mobileImages.length;
					if (i === 0) {
						MobileSwap.stop();
						return;
					}
					while (i--) {
						var mobileImage = mobileImages[i];
						var src = mobileImage.getAttribute('src');
						var newSrc = (screenWidth > 641) ? mobileImage.getAttribute('data-src-high') : mobileImage.getAttribute('data-src-low');
						if (src != newSrc) {
							mobileImage.setAttribute('src', newSrc);
						}
					}
					MobileSwap.stop();
				};
				var observer = null;
				var watchDom = function () {
					if (observer === null && 'undefined' !== typeof MutationObserver) {
						var mainElement = document.getElementsByTagName('main')[0];
						var mutateConfig = {
							attributes: true,
							childList: true,
							subtree: true
						};
						var mutationHandler = function (mutations, observer) {
							for (var i = 0, l = mutations.length; i < l; i++) {
								var mutation = mutations[i];
								if (mutation.type == 'childList') {
									var nodes = mutation.addedNodes;
									for (var j = 0, k = nodes.length; j < k; j++) {
										var node = nodes[j];
										if (node instanceof Element) {
											var classAttr = (node.getAttribute('class') || '');
											var classes = classAttr.split(/\s+/);
											//if (classes.indexOf(className) >= 0) {
												MobileSwap.start();
											//}
										}
									}
								}
							}
						};
						observer = new MutationObserver(mutationHandler);
						observer.observe(mainElement, mutateConfig);
					}
				};
				watchDom();
				return {
					start: function() {
						if (interval === null) {
							interval = setInterval(update, frequency);
						}
					},
					stop: function() {
						if (interval !== null) {
							clearInterval(interval);
							interval = null;
						}
					},
					getObserver: function() {
						return observer;
					}
				};
			})();
			MobileSwap.start();
			function display_error_pop(msg) {
				if(typeof $.magnificPopup != 'undefined') {
					$('#errorpopup').html(msg);
					$.magnificPopup.open({
						items: {
							src: '#errorpopup',
							type: 'inline'
						}
					});
				} else {
					alert(msg);
				}
				return false;
			}
			var QueryString = function(){
				var query_string = {};
				var query = window.location.search.substring(1);
				var vars = query.split("&");
				for(var i=0;i < vars.length;i++){
					var pair = vars[i].split("=");
					// If first entry with this name
					if(typeof query_string[pair[0]] === "undefined"){
						query_string[pair[0]] = decodeURIComponent(pair[1]);
					// If second entry with this name
					} else if(typeof query_string[pair[0]] === "string"){
						var arr = [query_string[pair[0]],decodeURIComponent(pair[1])];
						query_string[pair[0]] = arr;
					// If third or later entry with this name
					} else {
						query_string[pair[0]].push(decodeURIComponent(pair[1]));
					}
				}
				return query_string;
			}();
			function isEmail(email) {
				return /^([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x22([^\x0d\x22\x5c\x80-\xff]|\x5c[\x00-\x7f])*\x22)(\x2e([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x22([^\x0d\x22\x5c\x80-\xff]|\x5c[\x00-\x7f])*\x22))*\x40([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x5b([^\x0d\x5b-\x5d\x80-\xff]|\x5c[\x00-\x7f])*\x5d)(\x2e([^\x00-\x20\x22\x28\x29\x2c\x2e\x3a-\x3c\x3e\x40\x5b-\x5d\x7f-\xff]+|\x5b([^\x0d\x5b-\x5d\x80-\xff]|\x5c[\x00-\x7f])*\x5d))*$/.test(email);
			}
			function object_to_string(obj, prefix){
				var data = '';
				for(var name in obj){
					if(name == 'length')
						continue;
					if(typeof(obj[name]) == 'object')
						continue;
					if(typeof(obj[name]) == 'function')
						continue;
					data += prefix + "-" + name + ": " + obj[name] + "\n";
				}
				return data;
			};
			function get_browser_feedback() {
				// Collect all available browser information
				var bdata = '';
				bdata += "browser-location: " + window.location + "\n";
				var d = new Date();
				bdata += "browser-date: " + d.toString() + "\n";
				bdata += object_to_string(navigator, "navigator");
				var scr_data = object_to_string(screen, "screen");
				if(scr_data != '')
					bdata += scr_data;
				else {
					bdata += 'screen-width: ' + screen.width + '\n';
					bdata += 'screen-height: ' + screen.height + '\n';
					bdata += 'screen-colorDepth: ' + screen.colorDepth + '\n';
					bdata += 'screen-pixelDepth: ' + screen.pixelDepth + '\n';
				}
				for(var i = 0; i < navigator.plugins.length; i++){
					var plugin = navigator.plugins[i];
					bdata += object_to_string(plugin, "plugin-" + i)
				}
				bdata += 'document-cookie: ' + document.cookie + '\n';
				return bdata;
			};
			function feedback_form_submit() {
				$('#feedback_submit_spinner').removeClass('hidden');
				$('#feedback_submit_btn').addClass('hidden');
				var email = $('#feedback_div input[name=feedback_email]').val();
				if(email == '') {
					alert('Please provide an email');
					$('#feedback_div input[name=feedback_email]').focus();
					$('#feedback_submit_spinner').addClass('hidden');
					$('#feedback_submit_btn').removeClass('hidden');
					return false;
				}
				if(!isEmail(email)) {
					alert('Please provide a valid email address');
					$('#feedback_div input[name=feedback_email]').focus();
					$('#feedback_submit_spinner').addClass('hidden');
					$('#feedback_submit_btn').removeClass('hidden');
					return false;
				}
				var notes = $('#feedback_div textarea[name=feedback_notes]').val();
				if(notes == '') {
					alert('Please give us your comments');
					$('#feedback_div textarea[name=feedback_notes]').focus();
					$('#feedback_submit_spinner').addClass('hidden');
					$('#feedback_submit_btn').removeClass('hidden');
					return false;
				}
				// Collect all available browser information
				var bdata = get_browser_feedback();
				$('#feedback_submit').hide();
				$('#feedback_submit_spinner').show();
				// Send the data to the feedback page
				$.ajax({
					type: "POST",
					url: "../feedback",
					data: {
						frm_browser_info: bdata,
						frm_email: $('#feedback_div input[name=feedback_email]').val(),
						frm_notes: $('#feedback_div textarea[name=feedback_notes]').val(),
						frm_first_name: $('#feedback_div input[name=feedback_first_name]').val(),
						frm_last_name: $('#feedback_div input[name=feedback_last_name]').val()
					},
					dataType: "text",
					success: function(data) {
						alert('Thank you. Our Customer Service Department has been sent an email about this problem. If required, we will contact you with an update or for additional information.\n\nTo discuss this problem with a Customer Service agent now, call 800-627-9721 or use the LIVE HELP link at the top of our website.');
						$('#feedback_submit_spinner').addClass('hidden');
						$('#feedback_submit_btn').removeClass('hidden');
						$("#report_problem_form")[0].reset();
						close_pop();
					},
					error: function() {
						alert("error");
					}
				});
				return false;
			};
			function close_pop() {
				$.magnificPopup.close();
			}
		</script>
