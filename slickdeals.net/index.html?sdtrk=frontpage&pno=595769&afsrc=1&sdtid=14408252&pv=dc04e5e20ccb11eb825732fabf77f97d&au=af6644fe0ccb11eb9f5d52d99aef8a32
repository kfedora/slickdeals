  /* make #a11y part of our design contract, semantic or bust */
  &[aria-hidden="true"] { display: none; }
}

/* used to hack in the cookie banner */
#gen-nav-commerce-header .l-header {
  position: relative;
}

@media (min-width: 1024px)  {
  /* also used to hack in the cookie banner */
  body.nav-version--v1 .header-wrapper {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1;
  }
}

/* used to lock scrolling for the nav + app when any overlay is active */
.overlay-active {
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}

/* used to prevent the branch.io banner from overlaying the mobile navigation */
@media(max-width: 639px) {
  #branch-banner-iframe {
      box-shadow: none !important;
      z-index: auto !important;
    }
}

.visually-hidden {
  position: absolute !important;
  height: 1px;
  width: 1px;
  overflow: hidden;
  clip: rect(1px 1px 1px 1px); /* IE6, IE7 */
  clip: rect(1px, 1px, 1px, 1px);
  white-space: nowrap; /* added line */
}
