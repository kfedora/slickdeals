var ii;
var xx = 0;
for (ii = 0; ii < coll.length; ii++) {
coll[ii].addEventListener("click", function(e) {
var content = document.getElementsByClassName("collapsible-mobile");
var content2 = document.getElementsByClassName("visually-hidden");
var xx = 0;
$('.collapsible-mobile').toggleClass("is-open");
if (content.style === "block" && content2.style ==="block") {
content.style = "none";
$('html, body').animate({
scrollTop: $("document").offset()
}, 100);
} else {
content.style = "block";
var scrollVal = $(document).scrollTop();
if(scrollVal < 600){
$('html, body').animate({
scrollTop: $(".products-counts-sorting").offset().top + 20
}, 100);
}
}
});
}
var coll2 = document.getElementsByClassName("collapsible-mobile2");
var ii2;
var xx2 = 0;
for (ii2 = 0; ii2 < coll2.length; ii2++) {
coll2[ii2].addEventListener("click", function(e) {
var content2 = this.nextElementSibling;
var xx2 = 0;
if (content2.style.display === "none") {
content2.style.display = "block";
$(e.target).addClass("up-cross-open");
$(e.target).removeClass("down-cross-open");
} else {
content2.style.display = "none";
$(e.target).addClass("down-cross-open");
$(e.target).removeClass("up-cross-open");
}
});
}
