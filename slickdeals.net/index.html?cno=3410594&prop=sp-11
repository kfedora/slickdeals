</script>






<script>
(function (window) {
// Collect dw pdict udo items
window.udoItems = window.udoItems || [];
udoItems = udoItems.concat([


{"page_type":"category","page_name":"Exclusive Flash Deals","page_language":"en_US"},


]);
// Flatten all udoItems into single object
var __utag_data = mergeUdoItems(window.udoItems);
// Update data if mismatched
__utag_data = normalizeUtagData(__utag_data);
// Make clone instead of reference so we can use as a cache later on (utag_data object gets mutated by utag.js as data is collected!)
window.utag_data = merge({}, __utag_data);
// Expose this so we can do a sanity check later
window.__utag_data = __utag_data;
if (console && console.log) {
console.log(__utag_data);
}
function mergeUdoItems(udoItemsArray) {
var rtn = {};
// Flatten and merge udoItems into utag_data for tealium
udoItemsArray.forEach(function (item){
rtn = merge(rtn, item, udoMergeCustomizer);
});
return rtn;
}
function merge (target, source, custom) {
var sourceKeys = Object.keys(source);
var targetKeys = Object.keys(target);
sourceKeys.forEach(function (key) {
if(target[key] !== 'undefined') {
if(custom) {
target[key] = custom(target[key], source[key], key);
}else {
target[key] = source[key];
}
}
});
return target;
}
function udoMergeCustomizer(objValue, srcValue, keyName) {
// Concat if Array
if (objValue && objValue.hasOwnProperty('length')) {
return objValue.concat(srcValue);
}
// Concat if Number
if (typeof keyName !== 'undefined' && keyName === 'search_results' && typeof objValue === 'number') {
return objValue + srcValue;
}
// Object, repeat merge
if (typeof objValue === 'object') {
return merge(objValue, srcValue, udoMergeCustomizer);
}
// just replace with src value for everything else
return srcValue;
}
/**
* This function aligns page_name, page_type, and other? data properties
* in the utag_data with the UDO implementation spec.
* @param {Object} the generated utag_data
* @return {Object} the modified utag_data
*/
function normalizeUtagData(data){
// Update Page Type and Page Title
var pageName = data.hasOwnProperty('page_name') ? data.page_name : '';
var pageType = data.hasOwnProperty('page_type') ? data.page_type.toLowerCase() : '';
var isCheckout = data.hasOwnProperty('enh_action') && data.enh_action === 'checkout';
var checkoutStep = data.hasOwnProperty('enh_checkout_step') ? data.enh_checkout_step : '';
var isPurchase = data.hasOwnProperty('enh_action') && data.enh_action === 'purchase';
if(isCheckout && checkoutStep !== ''){
// Checkout Specific UDO Page Types
switch(checkoutStep){
case '1':
pageName = 'STEP 1 - SHIPPING';
pageType = 'shipping info';
break;
case '2':
pageName = 'STEP 2 - BILLING';
pageType = 'billing info';
break;
case '3':
pageName = 'STEP 3 - CONFRIMATION';
pageType = 'confirmation';
break;
}
} else if (isPurchase) {
pageName = 'receipt';
pageType = 'checkout';
} else {
// Remap page name values based on UDO Implementation Guide
switch(pageName){
case 'Style Gallery':
pageType = 'social (#luckyyou)';
break;
}
// Remap page type values based on UDO Implementation Guide
switch(pageType){
// leave these as is
case 'search' :
case 'category' :
case 'featured shop' :
case 'account' :
case 'content' :
case 'checkout':
case 'wishlist' :
break;
// update these
case 'myaccount':
pageName = 'My Account';
pageType = 'account';
break;
case 'orderconfirmation':
pageType = 'confirmation';
break;
case 'cart' :
pageType = 'cart';
pageName = 'YOUR SHOPPING BAG';
break;
case 'storefront' :
pageType = 'homepage';
break;
case 'product' :
pageType = 'product detail';
break;
// if none is found, default to content
default :
pageType = 'content'
break;
}
}
data.page_type = pageType;
data.page_name = pageName;
return data;
}
}(window));
</script>


