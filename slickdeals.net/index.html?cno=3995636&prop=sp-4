		search_params.suggestedSearchText = '';
		search_params.persd = 'false';
		search_params.refs = '[{\"name\":\"tradeOnly\",\"value\":\"false\"},{\"name\":\"includedChannels\",\"value\":\"GS_US\"},{\"name\":\"isEmbargoed\",\"value\":\"false\"}]';
