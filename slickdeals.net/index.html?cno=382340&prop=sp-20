<script>
				//this file holds scripts that are important for the initial view of the header
//top bar cart ** chanegd to be included in html
(function(){
	var getCookie = function(cookie){
		if(document.cookieObj)return document.cookieObj[cookie];
		var parts = document.cookie.split(";"), i, parts, part, name, val;
		document.cookieObj = {}
		for(i=0;i<parts.length;i++){
			part = parts[i].split("=");
			name = part.shift().trim();
			val = part.join("=").trim();
			document.cookieObj[name] = val;
		}
		return getCookie(cookie);
	};

	var uui = getCookie("uui"),
		topBarCart = getCookie("TopBarCart"),
		ttlitm = document.getElementById("js-ttlitm"),
		jslogin = document.getElementById("js-login"),
		loginame = document.getElementById("js-loginname"),
		phonenum = document.getElementById("js-phonenum");
		window.userFirstName = uui ? uui.split('|')[1] : "",
		userName = "";
	
	if(topBarCart){
		topBarCart = topBarCart.split("|");
		ttlitm.textContent = ttlitm.innerText = topBarCart[0];
		ttlitm.setAttribute('data-total','total' + topBarCart[0]);
	}
	if(jslogin && uui){		
		uui = uui.split("|");
		if(phonenum)phonenum.textContent = phonenum.innerText = decodeURIComponent(uui[0]);
		if(uui[1]){
			username = decodeURIComponent(uui[1]);
			loginame.textContent = loginame.innerText = username;
			jslogin.className +=" js-loggedIn";
			document.getElementById("js-username").innerText = username;
			document.getElementById("my-account-guest-notification").style.display = "none";
			document.getElementById("my-account-user-notification").style.display = "block";			
				var xmlhttp;

			    if (window.XMLHttpRequest) {
			        xmlhttp = new XMLHttpRequest();
			    } else {
			        xmlhttp = new ActiveXObject("Microsoft.XMLHTTP"); // "ancient" ie browsers
			    }

			    xmlhttp.onreadystatechange = function() {
			        if (xmlhttp.readyState == XMLHttpRequest.DONE ) {
			           if(xmlhttp.status == 200){
						   var data = JSON.parse(xmlhttp.responseText);
						   window.preloadedNotifications = data;
						   if(data.hasOwnProperty("user")){
				           		document.getElementById("js-username").innerText = data.user;
						   }
		} 
	}
			    }

			    var apiUrl = location.protocol + "//" + location.host + "/bnh/controller/home?Q=json&A=GetNotifications&O=";
			    xmlhttp.open("GET", apiUrl, true);
			    xmlhttp.send();
		}

	}
}());
			</script>
</div>
