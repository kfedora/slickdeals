<script type="text/template" id="template-note-shabbosNote">
<div class="shabbos-note js-shabbosNote site-note js-note-item {{= item.className }} {{ if($.cookie('noteClosed')){ }} collapsed {{ }else{ }} expanded {{ } }}" id="js-note{{= alertId }}" data-id="{{= alertId }}">
	<div class="shabbos-note-inner page-width text-center clearfix" data-selenium="shabbos-mode">
			<strong class="shabbos-title  {{= item.titleClassName }}"><svg class="svg-error"><use xlink:href="#error-light"></use></svg>{{= item.title }}</strong>
		
			<span class="condensed-note c2 fs14 smbold toggle">{{= item.condensedNote }}</span>

			{{ if(!(alertId.indexOf("pre-") !== -1)){ }}
			<span class="js-expand js-noteBtn details-btn cursor-pointer">
