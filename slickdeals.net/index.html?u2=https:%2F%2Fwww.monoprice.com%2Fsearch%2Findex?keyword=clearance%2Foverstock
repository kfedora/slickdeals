            <script>
                $(function () {
                    $('input.input__field').each(function () {

                        var $this = $(this),
                            $parent = $this.parent();

                        // In case the input is already filled
                        if ($.trim($this.val()) !== '')
                            $parent.addClass('input--filled');

                        // Add events
                        $this
                            .on('focus', function () {
                                $parent.addClass('input--filled');
                            })
                            .on('blur', function () {
                                if ($.trim($this.val()) === '')
                                    $parent.removeClass('input--filled');
                            });
                    });
                });
            </script>

