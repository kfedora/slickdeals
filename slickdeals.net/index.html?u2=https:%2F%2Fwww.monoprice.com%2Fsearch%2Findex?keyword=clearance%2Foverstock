<script>
    (function ($) {
        $(document).ready(function () {
            $(window).on('load', function () {
                $('body').addClass('mp-loaded');
            });
        });
    })(jQuery);
    function UsZipcodeValidate(event) {

        var idZipcode = $("#Zipcode");
        var idxZip = $("#x_zip");
        var zipCode = idZipcode.length ? idZipcode : idxZip.length ? idxZip : "";
        var key = String.fromCharCode(event.charCode ? event.which : event.charCode);
        zipCode.attr('maxlength', '15');
        if ($("#Country").val() === "US") {
            zipCode.attr('maxlength', '10');
            var regex = new RegExp("^[0-9-]");
            if (zipCode.val().indexOf("-") >= 0) {
                if (zipCode.val().length >= 6)
                    regex = new RegExp("^[a-zA-Z0-9-]");
            }
            else if (zipCode.val().length >= 5) {
                regex = new RegExp("^[a-zA-Z0-9-]");
                if (zipCode.val().length === 5 && key !== "-") {
                    zipCode.val(zipCode.val() + "-");
                }

                if (zipCode.val().length >= 9 && key !== "-")
                    return false;
            }

            if (!regex.test(key)) {
                event.preventDefault();
                return false;
            }
        }

    }
    function UsZipcodeCheckingFormat() {
        var idZipcode = $("#Zipcode");
        var idxZip = $("#x_zip");
        var zipCode = idZipcode.length ? idZipcode : idxZip.length ? idxZip : "";
        var zipcodeFiveDigits = zipCode.val().substring(0, 5);

        if ($("#Country").val() === "US") {
            if (zipCode.val().length === 10 && zipCode.val().indexOf("-") >= 0 && zipCode.val().indexOf("-") !== 5) {
                var tempZipcode = zipCode.val().replace("-", "");
                tempZipcode = tempZipcode.substring(0, 5) + "-" + tempZipcode.substring(5);
                zipCode.val(tempZipcode);
            }
            if (zipCode.val().length > 5 && (zipcodeFiveDigits.indexOf("e") >= 0 || !$.isNumeric(zipcodeFiveDigits))) {
                zipCode.addClass("mp-has-error");
            }
            else {
                zipCode.removeClass("mp-has-error");
            }
        }
    }
</script>
