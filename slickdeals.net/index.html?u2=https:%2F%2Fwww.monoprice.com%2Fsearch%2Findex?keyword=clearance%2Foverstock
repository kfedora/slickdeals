    window.onload = function (e) {
        $('.global-adsense').show();
    }
    $('.hawk-volumepricing-tooltip').hover(function () {
        $(this).parent().find('.hawk-volumepricing-tooltip-content').toggleClass('active');
    });

    $('.hawk-volumepricing-tooltip-content').hover(function () {
        $(this).parent().find('.hawk-volumepricing-tooltip-content').toggleClass('active');
