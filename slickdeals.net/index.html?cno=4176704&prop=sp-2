                        <span id="starting-price" class="pdp-price-without-tt">
                            $479.00
                        </span>
                        <span class="pdp-price-with-tt">
                            <a js-tooltip js-is-clickable class="pdp-js-tooltip-trigger" pdp-close-label pdp-title="" pdp-message="" pdp-container="hero-details">$479.00</a>
                        </span>
