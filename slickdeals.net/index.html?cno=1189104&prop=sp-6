<script type="text/javascript">	
		
		
			var loadScriptHandler = function(){
				var loadScript = document.createElement("script");
				loadScript.src="/_ui/desktop/common/js/require-2.1.15.js";
		
				loadScript.onload = function(){ 
					if(!this.readyState||this.readyState === "loaded"|| this.readyState === "complete"){ 
						require(['//lenovo.resultspage.com/autocomplete/sli-rac.config.js'], function(){ 
						}); 
					} 
				};
				document.body.appendChild(loadScript);
			};	
			if (window.addEventListener)
				window.addEventListener("DOMContentLoaded", loadScriptHandler, false);
			else if (window.attachEvent)
				window.attachEvent("onload", loadScriptHandler);
			else window.onload = loadScriptHandler;
			
			function ajaxsearchsubmit(form) {
				var search = encodeURIComponent(form.text.value); 
				document.activeElement.blur(); 		
				// add by shiquanguang for new request 1116 from sli begin
				var isoCode="US";
				// modify by shiquanguang for PPS-12762
				
				// modify by shiquanguang for PPS-14913
				var isOutlet = "OUTLET";
	
				var sliRequestUrl = "";
				if(isoCode == 'CA'){
					sliRequestUrl = $("#sliRequestType").val() + "search3.ca.lenovo.com/search?w=" + search + "&fsearch=yes"; 
				}else if(isoCode == 'US' && isOutlet == 'OUTLET'){
					sliRequestUrl = $("#sliRequestType").val() +  "search3.outlet.lenovo.com/search?w=" + search + "&fsearch=yes"; 
				}
				else{
					sliRequestUrl = $("#sliRequestType").val() +  "search3.lenovo.com/search?w=" + search + "&fsearch=yes"; 
				}
				if(sliRequestUrl.indexOf("https") != -1){
					sliRequestUrl = sliRequestUrl.replace("https","http");
				}
	
				window.location = sliRequestUrl;
				// add by shiquanguang for new request 1116 from sli end
		
				return false; 
			}
			</script>
			<script type="text/javascript">	
			var codeVal ="US";
			if(codeVal == 'US' || codeVal == 'CA'){
				// TODO fusion code start
				var useFusionSearch = "false";
				var prefixLink = "/us/en/outletus";
				//TODO fusion code end
				if(!window.initBV && useFusionSearch == "true" && !window.isCTOPage)
				{
					// modify by huanghai4 for CONTENT-728 begin
					$(document).ready(function() {
						$("#inputSearchText").on('focus', function loadBV() {
                            // update by wenchen.liu for BROWSE-1475 start
                            var bvPath = '';
                            if(codeVal == 'US'){
                                bvPath = '//apps.bazaarvoice.com/deployments/Lenovo/main_site/production/en_US/bv.js';
                            } else if(codeVal == 'CA'){
                                bvPath = '//apps.bazaarvoice.com/deployments/Lenovo-EN_CA/main_site/production/en_CA/bv.js';
                            }
                            // update by wenchen.liu for BROWSE-1475 end
							var len=$("[src='" + bvPath + "']").length;
							if(len==0){
								console.log('#####bv.js is not loaded yet#####');
								//load bv script if not present on page for search
								window.isAsyncBVLoaded=false;
								var bvscript = document.createElement('script');
								bvscript.onload = function () {
									window.isAsyncBVLoaded=true;
									$("#inputSearchText").off('focus', loadBV);
								};
								bvscript.src = bvPath;
								document.head.appendChild(bvscript);	
							}else{
								console.log('#####bv.js is already loaded#####');
							}
						});
					});
					// modify by huanghai4 for CONTENT-728 end
					
					_hbSymbol = "&nbsp$&nbsp;"; //TODO: pull from platform
					_hbCurrency = $("meta[name='currencycode']").attr("content");
					_hbCC = $("meta[name='cc']").attr("content");
					_hbLC = $("meta[name='lc']").attr("content");	
				}
			}
	    </script>
