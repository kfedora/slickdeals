<script>
    //UPDATE FOOTER LINKS
    document.addEventListener("DOMContentLoaded", function() {
       var fuelPointsLink = document.getElementsByClassName('fuelPointsLink_js')[0];
       var logoUrl = document.getElementsByClassName('footer__logo__img_js')[0];
       
       //UPDATE FUEL POINTS LINK IN THE FOOTER
			fuelPointsLink.setAttribute("href", "https://giftcards.kroger.com/fuelpoints");
		
		//UPDATE CITY LOGO IMG IN THE FOOTER
			logoUrl.setAttribute("src", "https://productimages.nimbledeals.com/nimblebuy/7eb358475f41f9ba35142978a0a92bb4");
    });
    
    function getUrlParam(name, url) {
        if (!name) {
            return new Error('You should specify name params');
        }
        name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
        url = url || window.location.href;
           
        var regexS = "[\\?&]"+name+"=([^&#]*)";
        var regex = new RegExp( regexS );
        var results = regex.exec( url );
        return results == null ? "" : results[1];
    };
	//end
</script> <div class="main promotionsPage">
