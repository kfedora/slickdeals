    var XssUtils = new XssUtilsClass();

    function XssUtilsClass() {
        var FORBIDDEN_URI_CHARS = [
            '(', ')', '\'', '"', ';', '<', '>'
        ];
        var INVALID_CHARS_RE = new RegExp(
            '[' + FORBIDDEN_URI_CHARS.join('') + ']', 'g'
        );
        this.reload = function () {
            window.location.reload(true);
        };
        this.encodeUri = function (url) {
            return url
                .replace(INVALID_CHARS_RE, '')
                .replace(/\s/g, '%20');
        };
    };

    function SET_SELECTED_ELEMENT_TO_STORAGE(id){
        localStorage.setItem('selectedElementID', id);
    };




    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    };

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return false;
    };

    function createHttpRequest(method, url, callbackFun){
        var request = new XMLHttpRequest();
        request.onreadystatechange = function() {
            if (request.readyState === 4 && request.status === 200) {
                request.responseText ? callbackFun(JSON.parse(request.responseText)) : callbackFun(false);
            };
            if (request.statusText === 'Unauthorized' && request.status === 401) {
                callbackFun(false);
            };
        };
        request.open(method, url, true);
        request.send();
    };

    function createElementFromHTML(htmlString) {
        var div = document.createElement('div');
        div.innerHTML = htmlString.trim();
        return div.firstChild;
    }

    function addItemsToList(template, targetContainer, hideItemIds) {
        return function (list) {
            var itemTemplate = template.innerHTML;
            var allCategories = document.getElementsByClassName("allCategories_js")[0];

            list.forEach(function(item, index) {
                item.index = index;
                if(item.id == currentCategoryID){
                    item.isActive = true;
                };

                if(hideItemIds && hideItemIds.indexOf(item.id) != -1 || !item.hasLiveProducts){
                    return;
                }

                if(item.id == '327'){
                    item.categoryUrl = '/promotions';
                    hasPromotionsCategory = true;
                    document.getElementsByClassName('promotionsCategory_js')[0].classList.remove("hide");
                };

                if(item.id == '1119'){
                    item.categoryUrl = '/visa-gift-card';
                };

                listCategory.push(item);

                var newItem = createElementFromHTML(Mustache.render(itemTemplate, item));

                if (!(hideCategories.indexOf(item.id + '') >= 0)) {
                    targetContainer.insertBefore(newItem, allCategories);
                }
                //targetContainer.appendChild(newItem);
                //$(targetContainer).find('.allCategories_js').before(newItem);
            });

            isCategoriesLoad = true;
        };
    };

    function addItemsToListTemplate(template, targetContainer, hideItemIds, allClass, categoryId) {
        return function (list) {
            var itemTemplate = template.innerHTML;
            var allCategories = document.getElementsByClassName(allClass)[0];
            var categoriesList = [];
            
            list.forEach(function(occasions) {
                if (occasions.id == categoryId) {
                    categoriesList = occasions.categories;
                }
            })

            categoriesList.sort(function(a, b){
                if(a.name < b.name) { return -1; }
                if(a.name > b.name) { return 1; }
                return 0;
            })

            categoriesList.sort(function(a, b){
                return b.sortField - a.sortField;
            })

            categoriesList.forEach(function(item, index) {
                item.index = index;
                if(item.id == currentCategoryID){
                    item.isActive = true;
                };

                if(hideItemIds && hideItemIds.indexOf(item.id) != -1 || !item.hasLiveProducts){
                    return;
                }

                if(item.id == '327'){
                    hasPromotionsCategory = true;
                    document.getElementsByClassName('promotionsCategory_js')[0].classList.remove("hide");
                };

                if(item.id == '1119'){
                    item.categoryUrl = '/visa-gift-card';
                };

                listCategory.push(item);

                var newItem = createElementFromHTML(Mustache.render(itemTemplate, item));

                targetContainer.insertBefore(newItem, allCategories);
                //targetContainer.appendChild(newItem);
                //$(targetContainer).find('.allCategories_js').before(newItem);
            });

            isCategoriesLoad = true;
        };
    };

    function renderList(itemTemplate, container, urlOfGetterList, hideItemIds) {
        var newListCreator = addItemsToList(itemTemplate, container, hideItemIds);
        createHttpRequest('GET', urlOfGetterList, newListCreator);
    };

    function renderListTemplate(itemTemplate, container, urlOfGetterList, hideItemIds, allClass, categoryId) {
        var newListCreator = addItemsToListTemplate(itemTemplate, container, hideItemIds, allClass, categoryId);
        createHttpRequest('GET', urlOfGetterList, newListCreator);
    };

