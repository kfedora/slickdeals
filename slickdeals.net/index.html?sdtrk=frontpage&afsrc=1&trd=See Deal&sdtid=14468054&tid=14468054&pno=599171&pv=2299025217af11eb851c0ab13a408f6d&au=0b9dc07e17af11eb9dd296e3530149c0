    var featuredCategoryId = $('.featuredCategoryId').val();

    if(!!featuredCategoryId){
        updateFeaturedCard(featuredCategoryId, false)
    }

    function updateFeaturedCard(featuredCategoryId, isSubCategory){
        var filterCurentCategory = function(category){
            return category.id == currentCategoryID;
        }
        var currentCategory = listCategory.filter(filterCurentCategory)[0];
        if(isSubCategory){
            var filterCurentSubCategory = function(category){
                return category.id == featuredCategoryId;
            }
            var currentSubCategory = null;
            if(currentCategory.categories.length){
                var currentSubCategory = currentCategory.categories.filter(filterCurentSubCategory)[0];
                if(currentSubCategory.categories.length){
                    var filterCurentFeatureCategory = function(category){
                        return category.name == 'Featured';
                    }
                    var featureCategoryCategory = currentSubCategory.categories.filter(filterCurentFeatureCategory)[0];
                    if(featureCategoryCategory){
                        getFeatureCard(featureCategoryCategory.id);
                    }
                } else {
                    getFeatureCard(currentSubCategory.id);
                }

                currentSubCategory.description ? updateCategoryDescription(currentSubCategory.description) :  updateCategoryDescription(currentCategory.description)
            }
        } else {
            getFeatureCard(featuredCategoryId);
            if(currentCategory){
                updateCategoryDescription(currentCategory.description)
            }
        }

        function updateCategoryDescription(categoryDescription){
            var currentCategoryDescription = document.getElementById('currentCategoryDescription');
            if(currentCategoryDescription){
                currentCategoryDescription.innerHTML = categoryDescription;
            }
        }

        function getFeatureCard(categoryId){
            var getFeaturedCardParams = {
                publisherId: publisherId,
                cities: currentCityId,
                page: currentPage,
                sort: 'numberDealSold_desc',
                size: 1,
                categoriesIds: categoryId ? categoryId : currentCategoryID
            };
            $.get(productsLink, getFeaturedCardParams, function(resp){
                var featuredCardBox = document.getElementById('featuredCardContent'),
                    featuredCardTemplate = document.getElementById('featured-card-template').innerHTML;

                if(resp.content.length && featuredCardBox){
                    featuredCardBox.innerHTML = '';
                    var item = resp.content[0];
                    var cardBrandString = item.offers[0].brand.name.toString();
                    var updatedImageSrc = wirelessArtimeCards[cardBrandString];
                    updatedImageSrc ? item.imageUrl = updatedImageSrc : item.imageUrl = item.images[0].src;
                    function checkPromotion(promotion) {
                        return promotion == item.type;
                    }

                    var promotion = promotionTypes.find(checkPromotion);
                    if(promotion && hasPromotionsCategory) item.promotionCategory = '/promotions';

                    var newCardItem = createElementFromHTML(Mustache.render(featuredCardTemplate, item));
                    featuredCardBox.appendChild(newCardItem);
                }

            }, "json");
        }

    }

    (function checkCategories(){
        setTimeout(function(){
            isCategoriesLoad ? updateCategoryLists(listCategory) : checkCategories();
        }, 100);
    })();

    function updateCategoryLists(list){
        var homeCategorySlider = document.getElementById('home-category-slider'),
            mobileCategorySelector = document.getElementById('mobile-category-selector'),
            categoryCardSlider = document.getElementById('category-card-slider'),
            bottomCategorySlider = document.getElementById('bottom-category-slider'),
            categoriesBox = document.getElementById('category-box');

        if(bottomCategorySlider) {
            var bottomCategorySliderItemTemplate = document.getElementById('bottom-category-item-template').innerHTML;
            
            list.forEach(function(category, index) {
                if(isCategoryContinueShopping && defaultGetCardParams.categoriesIds == category.id){
                    category.active = true;
                }
                
                if(category.imageUrl !== null) {
                    var bottomCategoryItem = createElementFromHTML(Mustache.render(bottomCategorySliderItemTemplate, category));
                
                    bottomCategorySlider.appendChild(bottomCategoryItem);
                }
            });
        }

        if(homeCategorySlider){
            var homeCategorySliderItemTemplate = document.getElementById('home-category-item-template').innerHTML;
            var mobileCategorySelectorItemTemplate = document.getElementById('mobile-category-item-template').innerHTML;
            var hasCategory = false;
            list.forEach(function(category) {
                if(category.hasLiveProducts){
                    if(isCategoryContinueShopping && defaultGetCardParams.categoriesIds == category.id){
                        category.active = true;
                    }
                    var newItem = createElementFromHTML(Mustache.render(homeCategorySliderItemTemplate, category));
                    homeCategorySlider.appendChild(newItem);
                    hasCategory = true;

                    var newMobileSelectorItem = createElementFromHTML(Mustache.render(mobileCategorySelectorItemTemplate, category));
                    mobileCategorySelector.appendChild(newMobileSelectorItem);
                };
            });


            //remove after moving jQuery to bottom
            if(hasCategory){
                $('.icon-slider-container').removeClass('hide');
                $('.filter-select').removeClass('hide');
            }

            new Carousel('.card-slider', 4, false, true, 1, true);


            categorySwitching('categorySliderLink');
            mobileCategorySwitching('mobile-category-selector');
        }

        if(categoryCardSlider){
            var categoryCardSliderItemTemplate = document.getElementById('category-item-template').innerHTML;
            var bottomCategorySliderItemTemplate = document.getElementById('bottom-category-item-template').innerHTML;
            var mobileCategorySelectorItemTemplate = document.getElementById('mobile-category-item-template').innerHTML;
            var hasCategory = false;
            list.forEach(function(category) {
                if(category.categories.length && category.id == currentCategoryID){
                    category.categories.forEach(function(subCategory) {
                        if(subCategory.name == 'Featured') {
                            featureCategoryId = subCategory.id;
                        }
                        if(subCategory.hasLiveProducts && subCategory.name != 'Featured'){
                            if(isCategoryContinueShopping && defaultGetCardParams.categoriesIds == subCategory.id){
                                subCategory.active = true;
                            }
                            var newItem = createElementFromHTML(Mustache.render(categoryCardSliderItemTemplate, subCategory));

                            categoryCardSlider.appendChild(newItem);
                            hasCategory = true;

                            var newMobileSelectorItem = createElementFromHTML(Mustache.render(mobileCategorySelectorItemTemplate, subCategory));
                            mobileCategorySelector.appendChild(newMobileSelectorItem);
                        };
                    });
                };

                if(category.hasLiveProducts && bottomCategorySlider){
                    var bottomCategoryItem = createElementFromHTML(Mustache.render(bottomCategorySliderItemTemplate, category));

                    bottomCategorySlider.appendChild(bottomCategoryItem);
                };
            });

            if(featureCategoryId != null){

            }

            //remove after moving jQuery to bottom
            if(hasCategory){
                $('.icon-slider-container').removeClass('hide');
                $('.filter-select').removeClass('hide');
            }

            new Carousel('.card-slider', 4, false, true, 1, true);

            categorySwitching('categorySliderLink');
            mobileCategorySwitching('mobile-category-selector');
        };

        if(categoriesBox){
            var categoryBoxItemTemplate = document.getElementById('category-item-template').innerHTML;
            list.forEach(function(category) {
                if(category.hasLiveProducts){

                    var newItem = createElementFromHTML(Mustache.render(categoryBoxItemTemplate, category));
                    categoriesBox.appendChild(newItem);
                };
            });
        }

        if ($('.icon-slider__item').length < 8) {
            new Carousel('.icon-slider', $('.icon-slider__item').length, false, false, 0, true, false, 8);
        } else {
            new Carousel('.icon-slider', 8, false, true, 0, true, false, 8);
            categorySwitching('categorySliderLink');
        }

        $('.icon-slider').on('refreshed.owl.carousel', function() {
            categorySwitching('categorySliderLink');
        })


        //var lazyLoadCategoriesImages = new LazyLoadImagesClass('img.category-lazy-image', null).init();
    };

    function categorySwitching(categoryLinkClassName){
        var categoryLinks = document.getElementsByClassName(categoryLinkClassName);
        [].forEach.call(categoryLinks, function(categoryLink) {
            categoryLink.addEventListener("click", function(event){
                event.preventDefault();
                var self = this;
                var getSelectedCategoryId = this.getAttribute('data-category-id');
                var selectedCategoryId = getSelectedCategoryId ? getSelectedCategoryId : currentCategoryID;
                var hasSubCategory = false;

                if(!isLoadingContent && selectedCategoryId !== currentActiveCategoryId) {
                    var currentActiveTab = document.querySelectorAll(".categorySliderLink");

                    currentActiveCategoryId = selectedCategoryId;
                    currentPage = 1;
                    defaultGetCardParams.productName = '';
                    defaultGetCardParams.page = currentPage;
                    defaultGetCardParams.categoriesIds = selectedCategoryId;

                    if(selectedCategoryId != currentCategoryID){
                        hasSubCategory = true;
                    }
                    if(this.getAttribute('data-category-url') != ""){
                        history.pushState(null, 'Title', this.getAttribute('data-category-url'));		                            history.pushState(null, 'Title', this.getAttribute('data-category-url'));
                    }
                    setSelectedCategory(selectedCategoryId);
                    if(currentCategoryID != ''){
                        updateFeaturedCard(selectedCategoryId, hasSubCategory);
                    }
                    getCard(defaultGetCardParams, true);
                    initFiltering();
                };
            });
        });
    };

    function mobileCategorySwitching(categorySelectorId){
        var mobileCategorySelector = document.getElementById(categorySelectorId);

        mobileCategorySelector.addEventListener('change', function(){
            var getSelectedCategoryId= this.options[this.selectedIndex].getAttribute('data-category-id');
            var selectedCategoryId = getSelectedCategoryId ? getSelectedCategoryId : currentCategoryID;
            var hasSubCategory = false;

            currentPage = 1;
            defaultGetCardParams.productName = '';
            defaultGetCardParams.page = currentPage;
            defaultGetCardParams.categoriesIds = selectedCategoryId;

            if(selectedCategoryId != currentCategoryID){
                hasSubCategory = true;
            }

            setSelectedCategory(selectedCategoryId);
            if(currentCategoryID != ''){
                updateFeaturedCard(selectedCategoryId, hasSubCategory);
            }
            getCard(defaultGetCardParams, true);
            initFiltering();
        })
    };

    function setSelectedCategory(selectedCategoryId){
        var categoryLinks = document.getElementsByClassName('categorySliderLink'),
            mobileCategorySelector = document.getElementById('mobile-category-selector'),
            mobileCategorySelectorIcon = document.getElementsByClassName('filter-select__icon')[0];

        var optionToSelect =  [].filter.call(mobileCategorySelector.options, function(option) {
            var categoryId = option.getAttribute('data-category-id');

            return selectedCategoryId == categoryId
        });

        if(mobileCategorySelectorIcon && optionToSelect[0]){
            mobileCategorySelectorIcon.style.backgroundImage = "url(" + optionToSelect[0].dataset.icon +")";
            optionToSelect[0].selected = true;
        }


        [].forEach.call(categoryLinks, function(categoryLink) {
            var categoryId = categoryLink.getAttribute('data-category-id');

            categoryLink.classList.remove("active-filter");

            if (selectedCategoryId == categoryId) {
                categoryLink.classList.add("active-filter");
            }
        });
    }

    function initFiltering() {
        var $rdpFilterLink = $('button[id*=\"filterLink\"]');

        $rdpFilterLink.removeClass('active');
        $rdpFilterLink.first().addClass('active');

        $rdpFilterLink.on('click', function (e) {
            e.preventDefault();
            if(!isLoadingContent){
                var $eventTarget = $(e.target),
                    value = $eventTarget.data('url');
                $rdpFilterLink.removeClass('active');
                $eventTarget.addClass('active');
                currentPage = 1;
                defaultGetCardParams.page = currentPage;
                defaultGetCardParams.productName = value;

                if(!defaultGetCardParams.categoriesIds){
                    defaultGetCardParams.categoriesIds = currentCategoryID;
                }

                getCard(defaultGetCardParams, true);
            }

        });
    };

    initFiltering();

    function initSorting() {
        var $listSortingSelect = $('#listSortingSelect');
        $listSortingSelect.on('change', function (e) {
            e.preventDefault();
            var sortValue = $(this).val();

            currentPage = 1;
            defaultGetCardParams.page = currentPage;
            defaultGetCardParams.sort = sortValue;

            if(!defaultGetCardParams.categoriesIds){
                defaultGetCardParams.categoriesIds = currentCategoryID;
            }

            getCard(defaultGetCardParams, true);
        });
    };

    initSorting();

    /*
	function showMoreCards(){
		var showMoreCards = document.getElementsByClassName('showMoreCards')[0];
        if(showMoreCards){
           showMoreCards.addEventListener("click", function(event){
				event.preventDefault();
				if(!isLoadingContent){
					currentPage++;
					var params = defaultGetCardParams;
					params.page = currentPage;
					getCard(params, false)
				};
			});
        };
	};
	*/

    function showMoreCards(event){
        event.preventDefault();
        if(!isLoadingContent){
            currentPage++;
            var params = defaultGetCardParams;
            params.page = currentPage;
            refreshPrevPageHref();
            $('.showMoreCards').attr('href', lastPageDetection());
            refreshPrevLinkHref();
            refreshNextLinkHref();
            setCanonicalUrl();
            getCard(params, false)
        };
    };


    function getCard(params, isNewList){
        loadingContent(true);
        var showMoreBox = document.getElementsByClassName('showMoreBox')[0];

        $.get(productsLink, params, function(resp){
            totalPages = resp.totalPages;
            var cardList = document.getElementById('cardList'),
                cardItemTemplate = document.getElementById('card-item-template').innerHTML;

            if(isNewList){
                cardList.innerHTML = "";
            };

            var cardPositionStartingPoint = resp.number * resp.size;

            if (resp.totalPages == currentPage) {
                $('.showMoreCards').remove();
                $('link[rel="next"]').remove()
            } else {
                if(!$('.showMoreCards')[0]) {
                    createShowMoreBtn();
                }
                if (!$('link[rel="next"]')[0] && checkCanonicalUrl()) {
                    $('link[rel="canonical"]').after('<link rel="next" href="' +  lastPageDetection() + '">')
                }
            }

            if (currentPage == 1) {
                $('.prev-page').remove();
                $('link[rel="prev"]').remove()
            } else {
                if (!$('.prev-page')[0]) {
                    createPrevBtn();
                }
                if (!$('link[rel="prev"]')[0] && checkCanonicalUrl()) {
                    $('link[rel="canonical"]').after('<link rel="prev" href="' +  firstPageDetection() + '">')
                }
                checkCanonicalUrl();
            }

            if(resp.content.length){

                if(isNewList){
                    cardStorage.cards = [].concat.apply([], resp.content);
                    cardStorage.isFullList = true;
                    cardStorage.numberOfElements = 1; //I like magic
                } else{
                    cardStorage.cards = [].concat.apply(cardStorage.cards, resp.content);
                    cardStorage.numberOfElements = defaultGetCardParams.itemsPerPageDropDown;
                };

                cardStorage.page = resp.number;
                cardStorage.totalPages = resp.totalPages;

                resp.content.forEach(function(card) {
                    cardPositionStartingPoint++;
                    card.positionId = cardPositionStartingPoint;

                    var cardBrandString = card.offers[0].brand.name.toString();
                    if(wirelessArtimeCards[cardBrandString]){
                        var cardImgUrl = wirelessArtimeCards[cardBrandString];
                        card.images[0].src = cardImgUrl;
                    }

                    if(card.productDetailsUrl.indexOf("apple-itunes-code-34") !=-1){
                        card.productDetailsUrl = "/apple-itunes-code-34"
                    }


                    var newCardItem = createElementFromHTML(Mustache.render(cardItemTemplate, card));
                    cardList.appendChild(newCardItem);
                });

                cardClickHandler();

            } else {
                showMoreBox.classList.add('hide');
                cardList.innerHTML = "<center style='width: 100%'><p>Sorry, no results were found.</p></center>"
            }

            loadingContent(false);

        }, "json");

    };

    function getCardsFromStorage(cardStorageObj){
        var cardListBox = document.getElementById('cardList'),
            cardItemTemplate = document.getElementById('card-item-template').innerHTML,
            showMoreBox = document.getElementsByClassName('showMoreBox')[0];

        var cardPositionStartingPoint = cardStorageObj.numberOfElements;

        if(cardStorageObj.isFullList){
            cardListBox.innerHTML = "";
            cardPositionStartingPoint = 0;
        };

        cardStorageObj.totalPages == currentPage + 1  ? showMoreBox.classList.add('hide') : showMoreBox.classList.remove('hide');

        cardStorageObj.cards.forEach(function(card) {
            cardPositionStartingPoint++;
            card.positionId = cardPositionStartingPoint;
            var newCardItem = createElementFromHTML(Mustache.render(cardItemTemplate, card));
            cardList.appendChild(newCardItem);
        });

        initContinueShopping();

        cardClickHandler()
    }


    function loadingContent(isLoad) {
        isLoad ? blockingContent() : unblockingContent();
        if(progressBar){
            progressBar.toggleClass("hide");
        }


        function blockingContent() {
            isLoadingContent = true;
        };
        function unblockingContent() {
            isLoadingContent = false;
        };
    };

    function cardClickHandler() {
        var $rdpCardLink = $('.image-card');
        $rdpCardLink.on('click', function (e) {
            e.preventDefault();
            var $self = $(this);
            var url = $self.attr('href');
            var positionId = $self.data('wa-click');
            var activeSession = {
                cardPositionId: positionId,
                params: defaultGetCardParams,
                continueURL: window.location.href,
                cardStorage: cardStorage,
                isStorageRelevance: true
            };
            localStorage.setItem('activeSession', JSON.stringify(activeSession));
            window.location.href = XssUtils.encodeUri(url);
        });
    };

    cardClickHandler();

    function initContinueShopping() {
        if(isContinueShopping && isStorageRelevance){
            var dataString = "[data-wa-click='" + continueShoppingObj.cardPositionId + "']"
            var continueShoppingCard = document.querySelectorAll(dataString)[0];
            if(continueShoppingCard){
                continueShoppingCard.focus();
                /*
		        continueShoppingCard.scrollIntoView({
                    behavior: 'smooth'
                }).focus();
                */
                //continueShoppingCard.focus();
                isContinueShopping = false;
            }
        };
    };

    function continueShopping(){
        if(isContinueShopping && isStorageRelevance){
            var getCardParams = continueShoppingObj.params;
            getCardParams.size = (getCardParams.page + 1) * defaultGetCardParams.itemsPerPageDropDown;

            defaultGetCardParams.page = getCardParams.page;
            defaultGetCardParams.productName = getCardParams.productName;
            defaultGetCardParams.sort = getCardParams.sort;
            defaultGetCardParams.categoriesIds = getCardParams.categoriesIds;
            getCardParams.page = 0;

            var currentActiveTab = $(".active-filter");
            if(currentActiveTab && defaultGetCardParams.categoriesIds){
                currentActiveTab.removeClass("active-filter");
            };

            var $rdpFilterLink = $('a[id*=\"filterLink\"]');
            if($rdpFilterLink){
                $rdpFilterLink.removeClass('active')
                $rdpFilterLink.each(function(){
                    var value = $(this).attr('href');

                    if(value == getCardParams.productName){
                        $(this).addClass('active');
                    }
                })
            };

            var $listSortingSelect = $('#listSortingSelect');
            if($listSortingSelect){
                if(getCardParams.sort){
                    $listSortingSelect.val(getCardParams.sort);
                }
            };


            getCardsFromStorage(continueShoppingObj.cardStorage);
            //localStorage.removeItem('activeSession');
        };
    };

    continueShopping();

    var totalPages = 1;
    var urlParams = function (name) {
        var results = new RegExp('[\?&]' + name + '=([^&#]*)')
            .exec(window.location.search);

        return (results !== null) ? results[1] || 0 : false;
    }
    var hrefForPage =  window.location.pathname;

    var changeUrlWithoutReload = function(page) {
        if (page === 1) {
            window.history.pushState({},"", hrefForPage);
        } else {
            window.history.pushState({},"", hrefForPage + "?page=" + page);
        }
    };

    var firstPageDetection = function() {
        if (currentPage === 1 || currentPage - 1 === 1) {
            return 'https://' + window.location.hostname + window.location.pathname
        }
        return 'https://' + window.location.hostname + window.location.pathname + '?page=' + (currentPage - 1)
    }

    var lastPageDetection = function() {
        if (currentPage == totalPages) {
            return 'https://' + window.location.hostname + window.location.pathname + '?page=' + currentPage
        }
        return 'https://' + window.location.hostname + window.location.pathname + '?page=' + (Number(currentPage) + 1)
    }

    var refreshPrevPageHref = function() {
        $('.prev-page, .showMoreCards').attr('href', firstPageDetection());
    }

    var refreshPrevLinkHref = function() {
        $('link[rel="prev"]').attr('href', firstPageDetection());
    }

    var refreshNextLinkHref = function() {
        $('link[rel="next"]').attr('href', lastPageDetection());
    }

    var setCanonicalUrl = function() {
        if (currentPage === 1 ) {
            $('link[rel="canonical"]').attr('href','https://' + window.location.hostname + window.location.pathname)
        } else {
            if (currentPage <= totalPages) {
                $('link[rel="canonical"]').attr('href','https://' + window.location.hostname + window.location.pathname + '?page=' + currentPage)
            } else {
                $('link[rel="canonical"]').attr('href','https://' + window.location.hostname + window.location.hostname + window.location.pathname)
            }
        }
    }


    var switchPage = function(p) {
        if(!isLoadingContent){
            if (p === '+') {
                currentPage++
            }
            if (p === '-') {
                currentPage--
            }
            refreshPrevPageHref();
            refreshPrevLinkHref();
            refreshNextLinkHref();
            var params = defaultGetCardParams;
            params.page = currentPage;
            cardList.innerText = "";
            changeUrlWithoutReload(currentPage);
            setCanonicalUrl();
            getCard(params, false)
        };
    }

    var checkCanonicalUrl = function() {
        var isCanonical = true;
        var isPageQuery = 'page';
        var entries = window.location.search.split('&');

        entries.forEach(function (item) {
            if(item.indexOf(isPageQuery) < 0) {
                isCanonical = false;
                $('link[rel="prev"], link[rel="next"]').remove();

            }
        })
        return isCanonical
    }

    var goToPrevPage = function(ev) {
        ev.preventDefault();
        switchPage('-')
    };

    var goToNextPage = function(ev) {
        ev.preventDefault();
        switchPage('+')
    };

    var createPrevBtn = function() {
        $('.showMoreBox').prepend('<a class="custom-btn prev-page" onclick="goToPrevPage(event)" href="' +  firstPageDetection() + '" style="margin-right: 10px;">Show less</a>');
    };

    var createShowMoreBtn = function () {
        $('.showMoreBox').append('<a class="custom-btn showMoreCards" onclick="showMoreCards(event)" aria-label="Show more gift cards" href="' +  lastPageDetection() + '"> Show more</a>');
    }

    var currentPage = urlParams('page') > 0 ? urlParams('page') : 1;

    setCanonicalUrl()

    //$('.showMoreCards').after('<a class="custom-btn next-page" onclick="goToNextPage(event)" href="' +  lastPageDetection() +  '" style="margin-left: 10px">next</a>')

    $('.showMoreCards').attr('href', lastPageDetection());


    if (currentPage == 1) {
        $('.prev-page').hide();
    } else {
        if (currentPage <= totalPages) {
            if (!$('.prev-page')[0]) {
                createPrevBtn();
            }
            if (checkCanonicalUrl()) {
                $('link[rel="canonical"]').after('<link rel="prev" href="' +  firstPageDetection() + '">')
            }
        }
    }
    if (currentPage == totalPages) {
        $('.next-page').hide();
        $('.showMoreCards').remove();
    } else {
        if(currentPage < totalPages) {
            if (checkCanonicalUrl()) {
                $('link[rel="canonical"]').after('<link rel="next" href="' +  lastPageDetection() + '">')
            }
        }
    }

    /*Temporary fix NS-6566*/
    var select = document.querySelector('#listSortingSelect');
    var isIE11 = !!window.MSInputMethodContext && !!document.documentMode;
    if(select & !isIE11) {
        select.value = 'numberDealSold_desc';
        select.dispatchEvent(new Event('change'));
    }
    /*END*/

    var _createClass = function () { function defineProperties(target, props) { for (var i = 0; i < props.length; i++) { var descriptor = props[i]; descriptor.enumerable = descriptor.enumerable || false; descriptor.configurable = true; if ("value" in descriptor) descriptor.writable = true; Object.defineProperty(target, descriptor.key, descriptor); } } return function (Constructor, protoProps, staticProps) { if (protoProps) defineProperties(Constructor.prototype, protoProps); if (staticProps) defineProperties(Constructor, staticProps); return Constructor; }; }();

    function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }

    var Navigation = function () {
        function Navigation(el) {
            _classCallCheck(this, Navigation);

            this.nav = $('.header');
            this.subnav = this.nav.find('.has-submenu');
            this._init_events();
        }

        _createClass(Navigation, [{
            key: '_init_events',
            value: function _init_events() {
                var windowWidth = $(window).width();

                $('.has-submenu > a').click(function(){
                    $(this).closest('.has-submenu').toggleClass('menu-open');
                })
            }
        }]);

        return Navigation;
    }();

    var ArrangeNav = function () {
        function ArrangeNav(el) {
            _classCallCheck(this, ArrangeNav);

            this.items = $('.header__utility-nav__item:not(.header__basket, .location__nav__item)');
            this.locationNav = $('.location__nav__item');
            this.body = $('body');
            this._init_events();
        }

        _createClass(ArrangeNav, [{
            key: '_init_events',
            value: function _init_events() {
                var ArrangeNavObject = this;

                ArrangeNavObject.arrange_navigation(ArrangeNavObject);

                $(window).on('resize', function () {
                    ArrangeNavObject.arrange_navigation(ArrangeNavObject);
                });
            }
        }, {
            key: 'arrange_navigation',
            value: function arrange_navigation(element) {
                if ($('#mobile-test').is(':visible')) {
                    element.init_mobile_menu();
                } else {
                    element.init_desktop_menu();
                    element.close_mobile_nav();
                }
            }
        }, {
            key: 'init_mobile_menu',
            value: function init_mobile_menu() {
                this.items.detach().appendTo('.nav-main > ul').addClass('nav__item');
            }
        }, {
            key: 'init_desktop_menu',
            value: function init_desktop_menu() {
                this.items.detach().prependTo('.header__utility-nav').removeClass('nav__item');
                this.locationNav.prependTo('.header__utility-nav');
            }
        }, {
            key: 'close_mobile_nav',
            value: function close_mobile_nav() {
                this.body.removeClass('nav-open');
            }
        }]);

        return ArrangeNav;
    }();

    var NavTrigger = function () {
        function NavTrigger(el) {
            _classCallCheck(this, NavTrigger);

            this.trigger = $('#hamburger-button');
            this.html = $('html');
            this._init_events();
        }

        _createClass(NavTrigger, [{
            key: '_init_events',
            value: function _init_events() {
                var NavObject = this;

                this.trigger.on('click', this.on_click.bind(this));
            }
        }, {
            key: 'on_click',
            value: function on_click() {
                this.toggle_menu();
            }
        }, {
            key: 'toggle_menu',
            value: function toggle_menu() {
                this.trigger.toggleClass('open');
                this.html.toggleClass('nav-open');
            }
        }]);

        return NavTrigger;
    }();

    var Carousel = function () {
        function Carousel(el, num_slides, dots, loop, lazyLoadEager, checkVisibility, autoplay, slideBy) {
            _classCallCheck(this, Carousel);

            this.el = el;
            this.num_slides = num_slides;
            this.dots = dots;
            this.loop = loop;
            this.slideBy = slideBy || 1;
            this.autoplay = autoplay;
            this.checkVisibility = checkVisibility;
            this.lazyLoadEager = lazyLoadEager;
            this._init_events();
            this.rtl = false;
        }

        _createClass(Carousel, [{
            key: '_init_events',
            value: function _init_events() {
                $(this.el).addClass('owl-carousel');

                if ($(this.el)[0] && $(this.el)[0].id === 'category-card-slider') {
                    this.rtl = true;
                }

                $(this.el).owlCarousel({
                    responsive: {
                        0: {
                            items: 1,
                            mouseDrag: true
                        },
                        1024: {
                            items: this.num_slides,
                            mouseDrag: false
                        }
                    },
                    lazyLoad: true,
                    loop:this.loop,
                    lazyLoadEager: 1,
                    autoplayHoverPause: true,
                    slideBy: this.slideBy,
                    checkVisibility: this.checkVisibility,
                    lazyLoadEager: this.lazyLoadEager,
                    dots: this.dots,
                    autoplay: this.autoplay,
                    nav: true,
                    rtl: this.rtl
                });
            }
        }]);

        return Carousel;
    }();

    var Filter = function () {
        function Filter() {
            _classCallCheck(this, Filter);

            this.desktopEl = $('.icon-slider__item');
            this.mobileEl = $('.filter-select__field select');

            this._init_events();
        }

        _createClass(Filter, [{
            key: '_init_events',
            value: function _init_events() {
                this.desktopEl.on('click', this.on_click.bind(this));
                this.mobileEl.on('change', this.on_change.bind(this));
            }
        }, {
            key: 'on_click',
            value: function on_click(item) {
                var index = $(item.target).parent().index();

                this.desktopEl.removeClass('active-filter');
                $(item.target).addClass('active-filter');

                this.mobileEl.find('option:eq(' + index + ')').prop('selected', true);

                var icon = this.mobileEl.find('option:eq(' + index + ')').data('icon');
                //$('.filter-select__icon').css('background-image', 'url(images/icon--' + icon + '.svg)');
            }
        }, {
            key: 'on_change',
            value: function on_change(item) {
                var index = this.mobileEl.prop('selectedIndex');
                var icon = this.mobileEl.find('option:selected').data('icon');
                // $('.filter-select__icon').css('background-image', 'url(images/icon--' + icon + '.svg)');

                this.desktopEl.removeClass('active-filter');
                $(this.desktopEl).eq(index).addClass('active-filter');
            }
        }]);

        return Filter;
    }();

    new Navigation();
    new ArrangeNav();
    new NavTrigger();

    // Carousel args (element, number of slides, dots: true/false, loop: true/false, stagePadding)
    if ($('.homePage').length) {
        $('.hero').removeClass('hide');
        new Carousel('.hero', 1, true, true, 1, true, true);
    }

    new Filter();

    //SUBSCRIBE WIDGET
    var validateEmail = function (email){
        var regExp1   = /^(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|"(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21\x23-\x5b\x5d-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])*")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\[(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?|[a-z0-9-]*[a-z0-9]:(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21-\x5a\x53-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])+)\])$/i;
        var emailString = email;
        if(null != emailString &&  typeof emailString != 'undefined' && emailString.indexOf("proxymail.facebook.com") > -1) {
            return false;
        }
        return regExp1.test(emailString);
    };


    var processSubscription = function($subcribeForm){
        var $form = $subcribeForm,
            validate = function($form){
                var isValid = true,
                    email = $.trim($form.find("[name='email']").val());

                if (!validateEmail(email)){
                    isValid = false;
                    $form.find(".subscribeEmailError").fadeIn();
                }

                return isValid;
            },
            submit = function($el){
                var URL = $el.prop("action");
                if (!URL) return false;
                var $btn = $el.find("[type='submit']"),
                    SERVER_ERROR_MSG = "Sorry, something went wrong. Please, try again later!"
                var promise = $.ajax({
                    type: "POST",
                    dataType: "json",
                    url: URL + "?" + $el.serialize(),
                    beforeSend: function(){
                        return $btn.prop("disabled", true);
                    },
                    success: function(data){
                        if (!data.success) return showError(data.message);

                        var $success = $el.siblings(".subscribeSuccess");
                        $el.find(".subscribeEmailError").slideUp();
                        return $success.fadeIn().delay(2500).slideUp();
                    },
                    error: function(){
                        return showError(SERVER_ERROR_MSG);
                    },
                    complete: function(){
                        return $btn.prop("disabled", false);
                    }
                });
                return promise;
            };
        $form.on("submit", function(e){
            e.preventDefault();
            var $el = $(this);
            if (!validate($el)) return false;

            return submit($el);
        });
    };

    processSubscription($(".customSubscribeForm_js"));
    processSubscription($("#newsletter-card"));
    processSubscription($("#newsletter-banner"));

    var htmlEscape = function(str) {
        return String(str)
            .replace(/&/g, '&amp;')
            .replace(/"/g, '&quot;')
            .replace(/'/g, '&#39;')
            .replace(/</g, '&lt;')
            .replace(/>/g, '&gt;');
    };

    var isStringNotEmpty = function(element) {
        if(element != null && element != undefined && $.trim(element).length > 0){
            return true
        }
        return false;
    };

    var getProperty = function(key){
        return $("#" + key).html();
    };

    $("body").on("click", "[data-toggle-class]", function(e){
        var $el = $(this),
            className = $el.data("toggle-class"),
            $target = $($el.data("toggle-el")),
            isSafe = $el.data("toggle-safe");
        if (!isSafe){
            e.preventDefault();
        }
        $target = $target.length ? $target : $("html");
        return $target.toggleClass(className)
    });

    $("[data-parent-toggle-class]").on("click", function(e){
        var $el = $(this),
            className = $el.data("parent-toggle-class");
        $el.closest('.main-nav-item').toggleClass(className);
    });

    $("#lockContent").on("click", function(){
        var classNames = $(this).data("remove-classes");
        return $("html").removeClass(classNames);
    });

    var $categoriesFilter = $(".categoriesFilter"),
        $categoriesFilterTitle = $(".categoriesFilterTitle");

    $categoriesFilterTitle.on('click', function(){
        $categoriesFilter.toggleClass('categories-filter-opan');
    });

    function ProductSearchClass() {
        var self = this;
        var PRODUCT_AUTOSUGGEST_URL = "https://us4products.nimblecommerce.com/api/v1/products/search";
        var PRODUCT_DETAILS_PAGE_URI = "//giftcards.kroger.com/detailsGroupBuy.action";
        var highlightClass = "highlighted";

        this.initSearchForm = function($formEl){
            $formEl.submit(function(e){
                var self = this;
                var $searchField = $(self).find("input[type=search]");
                var $searchString = $searchField.val().replace(/^\s\s*/, '');
                var actionUrl = $(self).attr("action");
                e.preventDefault();

                if ($searchString.length > 1) {
                    window.location.href = XssUtils.encodeUri( actionUrl + "?searchV2Type=all&searchV2str=" ) + encodeURIComponent($searchString) + '&cities=' + currentCityId + '&itemsPerPageDropDown=99';
                } else {
                    return false;
                }
            });
        };

        this.initSearch = function search($formEl, searchString, searchType, searchRootTagId) {
            var actionUrl = $formEl.attr("action");
            var url = XssUtils.encodeUri(actionUrl + "?searchV2Type=" + searchType + "&searchV2str=") + encodeURIComponent(searchString);
            if (searchRootTagId != undefined) {
                url += "&searchV2RootTagId=" + searchRootTagId;
            }
            if(currentCityId){
                url += "&cities=" + currentCityId;
            }
            url += "&itemsPerPageDropDown=99";

            window.location.href = url;
        };

        this.initAutocomplete = function($formEl, $searchField, data){
            var searchFieldObj = $searchField.autocomplete({
                minLength: 2,
                highlightClass: highlightClass,
                source: function (request, response) {
                    var requestTerm = request.term.replace(/^\s\s*/, '');
                    if (requestTerm.length < 2) return false;
                    $.ajax({
                        url: PRODUCT_AUTOSUGGEST_URL,
                        dataType: "json",
                        type: "Get",
                        headers: { "AccessToken": token },
                        data: {
                            term: htmlEscape(requestTerm),
                            categoryId: currentCityId,
                            cities: currentCityId
                        },
                        success: function (data) {
                            response(data);
                        }
                    });
                },
                select: function (event, ui) {
                    var type = ui.item.type;
                    var searchString = ui.item.value.replace(/^\s\s*/, '');
                    var searchType;
                    var searchRootTagId;

                    if(type == "TOP_PRODUCT_TITLE") {
                        var detailsGroupBuyUrl = PRODUCT_DETAILS_PAGE_URI + "?gbId=" + ui.item.productId;
                        window.location.href = XssUtils.encodeUri( detailsGroupBuyUrl );
                        return;
                    }

                    switch (type) {
                        case "TAG_NAME":
                            searchType = "tn";
                            searchRootTagId = ui.item.rootTagId;
                            break;
                        case "PRODUCT":
                            searchType = "p";
                            break;
                        case "SUBMERCHANT":
                            searchType = "s";
                            break;
                        case "TERM":
                            searchType = "all";
                    }

                    self.initSearch($formEl, searchString, searchType, searchRootTagId);
                }
            });

            searchFieldObj.autocomplete("widget").addClass( "deal-search-autocomplete" );

            var searchFieldRender;
            if (typeof jQuery.ui != 'undefined' && /[1-9]\.[7-9].[1-9]/.test($.ui.version)){
                searchFieldRender = searchFieldObj.data.bind(searchFieldObj, "autocomplete");
            } else {
                searchFieldRender = searchFieldObj.autocomplete.bind(searchFieldObj, "instance");
            }

            searchFieldRender()._renderItem = function (ul, item) {
                var description,
                    hideSuggestedItem = false,
                    $searchField = $formEl.find("input[type=search]");

                var re = new RegExp("(" + this.term + ")", "gi"),
                    matchClass = this.options.highlightClass,
                    template = "<span class='" + matchClass + "'>$1</span>",
                    value = item.value.replace(re, template),
                    textJoiner = "in";

                switch (item.type) {
                    case "TAG_NAME":
                        description = item.tagName;
                        break;
                    case "PRODUCT":
                        description = "offers";
                        break;
                    case "SUBMERCHANT":
                        description = "merchants";
                        break;
                    default:
                        description = "";
                }
                description = isStringNotEmpty(description) ? " " + textJoiner + " <span class='description'>" + description + "</span>" : "";

                if ((item.type == "TOP_PRODUCT_TITLE" || item.type == "TERM") && !$(ul).has("hr").length &&
                    ($(ul).has("li.product").length || $(ul).has("li.submerchant").length || $(ul).has("li.tag_name").length)) {
                    $(ul).append("<hr>");
                }

                return $("<li class='search-autocomplete-item'>")
                    .addClass(item.type.toLowerCase())
                    .data("item.autocomplete", item)
                    .append("<a class='search-autocomplete-link'>" + value + description + "</a>")
                    .appendTo(ul);
            };
        };

        this.init = function($formEl, merchantId, localeSiteId, selectedCategoryId){
            var $searchField = $formEl.find("input[type=search]");
            var siteData = [];
            siteData.merchantId = merchantId;
            siteData.localeSiteId = localeSiteId;
            siteData.selectedCategoryId = selectedCategoryId;

            self.initSearchForm($formEl);

            if (!$searchField.length) return false;

            self.initAutocomplete($formEl, $searchField, siteData);
        }
    };

    var dealSearch = new ProductSearchClass();
    dealSearch.init($(".searchForm"));

    var $noDealsSearchForm = $(".noDealsSearchForm");
    if($noDealsSearchForm){
        var noDealsSearch = new ProductSearchClass();
        noDealsSearch.init($noDealsSearchForm);
    };
    /*
	//solutions from https://github.com/adobe-accessibility
    if($(window).width() > 992){
		(function initAccessibleMenu(){
			if($(window).width() > 992){
				setTimeout(function(){
					$('.main-nav-item').length ? accessibleMenu() : initAccessibleMenu();
				}, 100);
			};
		})();
	};

	function accessibleMenu(){
		$(".main-nav").accessibleMegaMenu({
			uuidPrefix: "accessible",
			menuClass: "main-nav-list",
			topNavItemClass: "main-nav-item",
			panelClass: "dropdown-main-nav",
			panelGroupClass: "dropdown-main-nav-list",
			hoverClass: "hover",
			focusClass: "focus",
			openClass: "open"
		});
	};
    */
    var $rdpFilterLink = $('a[id*=\"filterLink\"]');
    $rdpFilterLink.on('keydown', function (e) {
        if(e.keyCode === 13) {
            e.preventDefault();
            var $eventTarget = $(e.target),
                url = $eventTarget.attr('href');
            SET_SELECTED_ELEMENT_TO_STORAGE($eventTarget.attr('id'));
            window.location.href = XssUtils.encodeUri( url );
        }
    });
