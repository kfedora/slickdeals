</script>




        <script>
            if( !getCookie("riskified")){
                setCookie("riskified", Math.random().toString(36).substr(2, 9), 365);
            }
            var session_id = getCookie("riskified");
            var url = ('https:' == document.location.protocol ? 'https://' : 'http://')
                + "beacon.riskified.com?shop=" + window.location.hostname + "&sid=" + session_id;
            var s = document.createElement('script');
            s.type = 'text/javascript';
            s.async = true;
            s.src = url;
            var x = document.getElementsByTagName('script')[0];
            x.parentNode.insertBefore(s, x);
        </script>

