    function updateCounterItemSC(scData) {
        var shoppingCartCount = document.getElementsByClassName("shoppingCartCount_js")[0];

        if(scData){
            var getJSON = decodeURIComponent(scData),
                getObj = JSON.parse(getJSON),
                scCounter = 0;

            getObj.forEach(function(item) {
                scCounter += item.o[0].q;
            });

            shoppingCartCount.classList.remove('hide');
            shoppingCartCount.innerHTML = scCounter;
        }
        shoppingCartCount.classList.add("updated-count");
    }

    updateCounterItemSC(scCookie);

    function userAuthorization(){
        var userCookie = getCookie('u_a'),
            userUnauthorizedBox = document.getElementsByClassName("userUnauthorized")[0],
            userAuthorizedBox = document.getElementsByClassName("userAuthorized");

        userCookie ? checkAuthorization() : isUserAuthorized(false);

        function checkAuthorization() {
            var checkAuthorizationUserURL = hostURL + '/checkAuthorizationUser';
            var checkUserAuthorization = new createHttpRequest('POST', checkAuthorizationUserURL, isUserAuthorized);
        };

        function isUserAuthorized(authorize){
            authorize ? initAuthorization(authorize) : userUnauthorizedBox.classList.remove("visibility-hidden");
        };

        function initAuthorization(data) {
            var logoutLinks = document.getElementsByClassName("logOutLink"),
                savedDesignsLink = document.getElementsByClassName("saved-designs")[0];
            userUnauthorizedBox.classList.add("hide");
            
            for (var i = 0; i < userAuthorizedBox.length; i++) {
              userAuthorizedBox[i].classList.remove("hide");
            }

            if(data.isSavedCardsDesignsExist){
                savedDesignsLink.classList.remove("hide");
            }
            
            updatePDPafterAutorization(data);
            triggerSaleCycleEvenIfNeed(data);
            function userLogout(){
                [].forEach.call(logoutLinks, function(link) {
                    link.onclick = function() {
                        setCookie("saleCycle", "", 0);
                        var logoutFromNewWlURL = hostURL + '/logoutFromNewWl',
                            logout = new createHttpRequest('POST', logoutFromNewWlURL, logoutFromOldWl);
                    };
                });

                function logoutFromOldWl() {
                    var logoutFromOldWlURL = hostURL + '/signout.action',
                        logout = new createHttpRequest('POST', logoutFromOldWlURL, redirectAfterLogout);
                };

                function redirectAfterLogout(response) {
                    window.location.href = XssUtils.encodeUri( response.info.map.redirectUrl );
                };
            };

            userLogout();
        };
    };
    userAuthorization();
    function updatePDPafterAutorization(data){
        var userEmailBox = document.getElementsByClassName("ddpGcSelfUxUserEmail_js")[0];
        if(userEmailBox && data.email) {
            userEmailBox.innerHTML = data.email;
        }
    }

    function triggerSaleCycleEvenIfNeed(data){
        if(data.email)$(".userEmail").val(data.email);
        setTimeout(function(){
            if(!getCookie('saleCycle')){
                setCookie("saleCycle", true, 14);
                document.querySelector('.userEmail').dispatchEvent(new Event("change"));
            }
        }, 0);
    }
    function submitFormSorting(form, event){
        SET_SELECTED_ELEMENT_TO_STORAGE(event.target.id);
        form.submit();
    };

