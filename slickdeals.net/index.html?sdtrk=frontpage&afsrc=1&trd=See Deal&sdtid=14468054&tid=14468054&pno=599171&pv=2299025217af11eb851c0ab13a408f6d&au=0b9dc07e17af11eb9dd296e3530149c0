    var dynamicTagManager = new DynamicTagManager();

    function DynamicTagManager() {
        this.pushDataLayer = function(data) {
            if(!window.tmsDataLayer || !data) {
                return;
            }
            window.tmsDataLayer.push(data);
        };
    }


    $("body").on("click", "[wl-ajax-link]", function(e){
        e.preventDefault(); // avoid hash change for links processed by JS
    });

    $("[data-defer-el]").each(function(){
        var $el = $(this),
            className = $el.data("defer-el");
        return $el.removeClass(className);
    });

    $("[data-defer-attr]").each(function(){
        var $el = $(this),
            attr = $el.data("defer-attr");
        return $el.removeAttr(attr);
    });

