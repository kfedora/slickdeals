<script>
jQuery(function () {
console.log('24');
var callUsButton = jQuery('.callUsButton');
var toggleSelectors = [
    '.button-container',
    '.help-title',
    '.secondaryWrapper',
    '.myBBYrewardsRecipe',
    '.myAccountRecipe',
    '.bestBuyStoresRecipe',
    '.productRepairandSupportRecipe',
    '.otherRecipe',
    '.ordersPurchasesRecipe',
];

var options = {
    ordersPurchases: {
        chatQueue: undefined,
        showChatButton: false,
        visible: [
            '.secondaryWrapper',
            '.ordersPurchasesRecipe',
        ],
        track: 'Orders & Purchases',
    },
    productRepair: {
        chatQueue: 'Service-Queue',
        showChatButton: false,
        visible: [
            '.secondaryWrapper',
            '.productRepairandSupportRecipe',
        ],
        track: 'Product Repair & Support',
    },
    bbyStores: {
        chatQueue: undefined,
        showChatButton: false,
        visible: [
            '.secondaryWrapper',
            '.bestBuyStoresRecipe',
        ],
        track: 'Best Buy Stores',
    },
    other: {
        chatQueue: undefined,
        visible: [
            '.secondaryWrapper',
            '.otherRecipe',
        ],
        track: 'Other',
    },
    myAccount: {
        chatQueue: undefined,
        showChatButton: false,
        visible: [
            '.secondaryWrapper',
            '.myAccountRecipe',
        ],
        track: 'My Account',
    },
    rewards: {
        chatQueue: undefined,
        showChatButton: false,
        visible: [
            '.secondaryWrapper',
            '.myBBYrewardsRecipe',
        ],
        track: 'My Best Buy Rewards',
    },
};

var CONTACT_OPTION = 'contactOption';

var CHAT_NOW_DEFAULT_QUEUE = 'AccHelpTopics';
var chatNow = document.getElementById('chatNow')
var chatNowQueue = CHAT_NOW_DEFAULT_QUEUE;

Object.keys(options).forEach(function (option) {
    jQuery('#' + option).click(function () {
        navigateToOption(option, chatNowQueue);
    });
});

loadOption(getParams(), chatNowQueue);

function getParams() {
  var search = window.location.search.slice(1);
  return search.split('&').reduce(function (out, keyValue) {
    var keyValuePair = keyValue.split('=');
    out[keyValuePair[0]] = keyValuePair[1];
    return out;
  }, {});
}

function saveParams(params) {
  var search = '?' + Object.keys(params).reduce(function (out, key) {
    var keyValue = [key, params[key]].join('=');
    return out.concat(keyValue);
  }, []).join('&');
  history.pushState('', '', search);
}

function loadOption (params, defaultQueue) {
   option = params[CONTACT_OPTION];
  if (options.hasOwnProperty(option)) {
    navigateToOption(option, defaultQueue);
  }
}

function showIf(el, visible) {
    if (visible) {
        el.show();
    } else {
        el.hide();
    }
}

function navigateToOption(option, defaultQueue) {
    if (!options.hasOwnProperty(option)) {
        return;
    }

    var config = options[option];
    console.log('sending: ', config.chatQueue, chatNowQueue, CHAT_NOW_DEFAULT_QUEUE)
    setChatNowQueue(config.chatQueue || defaultQueue);
    toggleSelectors.forEach(function (selector) {
        showIf(jQuery(selector), config.visible.indexOf(selector) !== -1);
    });
    focusOnElement('second');

    var track = 'Contact: ' + config.track + ': ';
    jQuery('.chatBox-one').attr('data-track', track + 'Chat Now');
    jQuery('.chatBox-two').attr('data-track', track + 'Find a Store');
    callUsButton.attr('data-track', track + 'Call Us')

    console.log('show chat with: ', config.showChatButton);
    showChatButton(config.showChatButton)

    var params = getParams();
    params[CONTACT_OPTION] = option;
    saveParams(params);
}

// TODO - var naming!
var callEmHideOnExpandElements = jQuery("#callEm .hideOnExpand");
var bbyCallUsInfo = jQuery(".bbyCall");
var chatBoxThree = jQuery('.chatBox-three');

function showCallUs () {
  callEmHideOnExpandElements.hide();
  bbyCallUsInfo.show();
  chatBoxThree.addClass('expanded')
  callUsButton.attr('aria-expanded', 'true')
}
function hideCallUs() {
  callEmHideOnExpandElements.show();
  bbyCallUsInfo.hide();
  chatBoxThree.removeClass('expanded')
  callUsButton.attr('aria-expanded', 'false')
};

function showChatButton(state) {
    if (state) {
        jQuery('#chatNow').addClass('showChatButton')
    } else {
        jQuery('#chatNow').removeClass('showChatButton')
    }
}

jQuery(".backButton").click(function () {
  jQuery(".button-container").toggle();
  jQuery(".help-title").toggle();
  jQuery(".secondaryWrapper").toggle();
  hideCallUs();
  focusOnElement('first');
  showChatButton(false);
  var params = getParams();
  delete params[CONTACT_OPTION];
  saveParams(params);
});


callUsButton.click(function () {
  showCallUs();
});

var pageCookies = document	
  .cookie.split('; ')	
  .map(function (str) { return str.split('='); })	
  .reduce(function (out, keyValue) {	
    out[keyValue[0]] = keyValue[1];	
    return out;	
  }, {});	
if (pageCookies.ut) {	
  jQuery('#mngpaymentMethods').show();	
} else {	
  jQuery('#mngpaymentMethods_faq').show();
}

function focusOnElement (showing) {
  var element;
  if (showing === 'first') {
    element = jQuery('.firstViewWrapper .help-title');
  } else if (showing === 'second') {
    element = jQuery('.secondaryWrapper .okayOptions')
  }

  element.focus()
}



  function setChatNowQueue(queue) {
    chatNowQueue = queue || CHAT_NOW_DEFAULT_QUEUE;
    $('#chatNow').attr('data-queue', chatNowQueue);
  }
  
  

var pollForUnifiedChat = setInterval(detectUnifiedChat,300);

function detectUnifiedChat() {
    if (window.unifiedChat) {
        clearInterval(pollForUnifiedChat);
        setQueueAvailability();
    }
}

function getAvailabilityCall(queueToCheck, key) {
    window.unifiedChat.getAvailability({ provider: 'ecc', queue: queueToCheck })
        .then(function(value) {
            var isAvailable = value.isAvailable || false;
            if (isAvailable && $('#chatNow').attr('data-queue') == String(queueToCheck)) {
                showChatButton(true)
            }
            options[key].showChatButton = isAvailable;
        });
}

function setQueueAvailability() {
    Object.keys(options).forEach(function(key){
        var option = options[key];
        getAvailabilityCall(option.chatQueue || CHAT_NOW_DEFAULT_QUEUE, key)
    });
}


function openChatWindow(evt) {
  evt.preventDefault()
  window.unifiedChat.init({
    provider: 'ecc',
    queue: chatNowQueue,
    clientId: 'x-shop-contactus'
  })
}

chatNow.addEventListener('touchstart', openChatWindow)
chatNow.addEventListener('click', openChatWindow)

});
