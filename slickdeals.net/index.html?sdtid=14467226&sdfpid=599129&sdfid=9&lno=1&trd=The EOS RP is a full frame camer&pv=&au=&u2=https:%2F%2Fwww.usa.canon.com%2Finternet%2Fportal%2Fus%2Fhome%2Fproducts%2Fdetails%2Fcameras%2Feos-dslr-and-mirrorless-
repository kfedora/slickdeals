<script type="text/javascript">
	//Check for browser compatibility
	if ($('#devicetype-desktop').is(':visible')) { 
		$.reject();
	}else{
		//mobile device so check versions
		if(checkMobileVersion()){
			// if true version is old
			$.reject({
				reject:{all:true},
				browserShow: false,
				header: "Your browser is currently not supported.",
				paragraph1: "Your mobile device's operating system and/or browser are out of date and not supported. ",
				paragraph2: 'Please consider updating your phone.',   
			});
		}
	}
	
	//Check to show theme baner
	var showBanner = 'N';
	if(showBanner == 'Y') $('#themeBanner').show();
	
	
	
</script>

<script type="text/javascript">

	$(document).ready(function() {
		// Need to show ConfirmIt only on support pages
		if (window.location.href.indexOf("home/support") > -1) {
			var model = '';
			var category = '';
			if (window.location.href.indexOf("home/support/details") > -1) {				
					var loc = window.location.href;
					var splitLoc = loc.split('#');
					if(splitLoc.length > 1){
						var trimmedHREF = splitLoc[0];
						window.history.replaceState("data","Title",trimmedHREF);
					}
			}					
			if (useMaritzCXSurvey == "N") {
				ConfirmIt.openSurvey(encodeURIComponent(model),encodeURIComponent(category));
			} else {
				
				McxSiteInterceptOnExit.addUrlParameter("mcxModel", model); 
				McxSiteInterceptOnExit.addUrlParameter("mcxPCategory", category);
				//Survey ID
				McxSiteInterceptOnExit.addUrlParameter("mcxID", "M6BSBQ");
				McxSiteInterceptOnExit.addUrlParameter("mcxReferrerURL",encodeURIComponent(window.location.href));
				McxSiteInterceptOnExit.acceptSurvey();
				//$('#surveyModal').modal('show');
			}
		}
	});
</script>
