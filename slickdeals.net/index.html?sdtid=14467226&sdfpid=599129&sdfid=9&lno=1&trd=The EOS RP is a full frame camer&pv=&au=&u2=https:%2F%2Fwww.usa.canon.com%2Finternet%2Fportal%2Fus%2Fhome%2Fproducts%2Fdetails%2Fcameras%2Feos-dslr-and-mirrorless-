<script type="text/javascript" src="/NewWebThemeStatic/themes/NewWebTheme/js/min/jquery.waypoints.min.js"></script>



<script> 

var userType, loginStatus, pageType,userName, emailSubscriber, emailConsumerNewsFlag, emailProductInsightFlag, emailBusinessNewsFlag, newsLetterText, consumerNews, productInsight, businessNews, environmentVariableName;



 
        environmentVariableName = "PRODUCTION";
   


$( document ).ready(function() {
	
	userName = $('#userSection')[0].innerText;
	

	
	if (userName !== "") {
		userType = 'registered';
		loginStatus = 'true';
	} else {
		userType = 'guest';
		loginStatus = 'false';
	}

	if ($('#breadcrumb_list')[0].childNodes.length > 3) {
		pageType = $('#breadcrumb_list')[0].childNodes[3].innerText;
		if ($('#breadcrumb_list')[0].childNodes[3].innerText === "My Canon") {
			pageType = "Account";
		}
	} else {
		pageType = $('#breadcrumb_list')[0].childNodes[1].innerText;
	}


	if ($('#ProductTitle')[0] !== undefined) {
		pageType = 'Product Page (PDP)';
	}
	if ($('#filterProductsHeading')[0] !== undefined) {
		if ($('#filterProductsHeading')[0].innerText === "Filter Products") {
			pageType = 'Category Page (CLP)';
		}
	}

	if (loginStatus === "true")
	{
		
		var userID = $('#userSection')[0].innerText;//userid
		 
		
		
		
		
				
		var encryptedUserId = "itpO/s5KUQg=";
		emailSubscriber = $("input[name='emailSubscriber']").attr('value');
		consumerNews = $("input[name='newsConsumer']").attr('value');
		productInsight = $("input[name='newsProducts']").attr('value');
		businessNews = $("input[name='newsBusiness']").attr('value');


			
			if (emailSubscriber === 'true') {
				dataLayer.push({
					'userId' : encryptedUserId,
					'userType' : userType,
					'loginStatus' : loginStatus,
					'emailSubscriber': emailSubscriber,
					'emailConsumerNewsFlag': consumerNews,
					'emailProductInsightFlag': productInsight,
					'emailBusinessNewsFlag': businessNews,
					'pageType' : pageType,
					'pageId' : $('title').context.title,
					'contentType' : 'New Web',
		            'environmentName': environmentVariableName
				});
			}
			else if (emailSubscriber === 'false') {
				dataLayer.push({
					'userId' : encryptedUserId,
					'userType' : userType,
					'loginStatus' : loginStatus,
					'emailSubscriber': emailSubscriber,
					'pageType' : pageType,
					'pageId' : $('title').context.title,
					'contentType' : 'New Web',
					'environmentName': environmentVariableName
				});
			}
			else {
				dataLayer.push({
					'userId' : encryptedUserId,
					'userType' : userType,
					'loginStatus' : loginStatus,
					'pageType' : pageType,
					'pageId' : $('title').context.title,
					'contentType' : 'New Web',
					'environmentName': environmentVariableName
				});
			}
			

	}
	else {
		dataLayer.push({		
			'userType' : userType,
			'loginStatus' : loginStatus,
			'pageType' : pageType,
			'pageId' : $('title').context.title,
			'contentType' : 'New Web',
			'environmentName': environmentVariableName
		});
	}

	
});




</script> 
