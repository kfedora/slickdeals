<script type="text/javascript">
	// below code checks the canonusa.allegiancetech.com site response to add the javasctipt file to header
	if (window.location.href.indexOf("home/support") > -1) {
		$.ajax({
			url: maritzSurveyUrl,
			dataType: 'script',		
	        async: true,
			error: function(){
				// will fire when timeout is reached				
				console.log('error in  loading the javascript file from canonusa.allegiancetech.com');			
			},
			success: function(){
				//adding script to header			
				$('<script />', { type : 'text/javascript', src : maritzSurveyUrl}).appendTo('head');
			},
			timeout: 5000 // sets timeout to 5 seconds
		});
	}
</script>

