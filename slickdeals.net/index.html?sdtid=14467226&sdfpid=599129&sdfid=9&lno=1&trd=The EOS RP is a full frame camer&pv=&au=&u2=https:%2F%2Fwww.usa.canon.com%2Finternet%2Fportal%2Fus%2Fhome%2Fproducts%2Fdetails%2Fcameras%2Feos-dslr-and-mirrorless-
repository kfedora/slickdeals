<script type='text/javascript'>
  var init_video_disp_flag = 'false';
function loadIframe(id,url,invWrapperId) {
    var $iframe = $('#f4798d26-a6ac-4d31-ac3b-5e9941bac0bb_iframe');
    $('#'+invWrapperId).hide();
    $iframe.show();
    if ( $iframe.length ) {
        $iframe.attr('src',url);
        addNowPl(id);
        return false;
    }
    return true;
}
function addNowPl(id){
$('._videospan').html('');
$('#'+id+'_videospan').html('<strong class="cf-canon-red">Now Playing</strong>');
}
</script>
