<script type="text/javascript">
    $(document).ready(function() {
        var browseTypeId = 0;
        // Firefox 1.0+
        var isFirefox = typeof InstallTrigger !== 'undefined';

        if (isFirefox)
            browseTypeId = 0;

        var ua = navigator.userAgent.toLowerCase();

        // Safari
        var isSafari = !isChrome && (ua.indexOf('safari') != -1);

        if (isSafari)
            browseTypeId = 1;

        // Internet Explorer 6-11
        var isIE = /*@cc_on!@*/ false || !!document.documentMode;

        if (isIE)
            browseTypeId = 2;

        // Edge 20+
        var isEdge = !!window.chrome && (ua.indexOf('edg') != -1);;

        if (isEdge)
            browseTypeId = 3;

        // Chrome 1 - 71
        var isChrome = !isEdge && !!window.chrome && (ua.indexOf('chrome') != -1);

        if (isChrome)
            browseTypeId = 4;

        var browserTypeIns = $("#browser-specific-instructions .row");

        browserTypeIns.each(function(idx, li) {

            if (browseTypeId == idx) {
                $(this).attr('style', "display:block;");
            }
        });

        // Initialize the plugin	  
        $('#browser-specific-instructions').popup({
            scrolllock: true
        });

    });

    function openinstructions(isShow) {       
        if (isShow) {
            document.getElementById("PDFInstructionDiv").style.display = "block";
        }
        if( !/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
           $('#browser-specific-instructions').popup('show');
        }
    }

    function openinstructionswithdisclaimer(isShow, id) {
      if( !/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
        if (isShow) {
            document.getElementById("PDFInstructionDiv").style.display = "block";
        }
       if($('#chkAgree'+id).is(':checked'))
           $('#browser-specific-instructions').popup('show');
       else 
         return false;
       }
      
    }

    function closepopup() {
        $('#browser-specific-instructions').popup('hide');
    }
</script>

<!-- browser instructions code end-->
