	var carouselSlidesLeft = $( "#productThumbCarousel > .carousel-inner > .carousel-thumb-slide" ).slice(0,4).length;
	for (var i = 0; i < 5 && carouselSlidesLeft > 0 ; i ++) {
		console.log ("carouselSlidesLeft="+carouselSlidesLeft);
		$( "#productThumbCarousel > .carousel-inner > .carousel-thumb-slide" ).slice(0,4).wrapAll("<div class='item' />");
		carouselSlidesLeft = $( "#productThumbCarousel > .carousel-inner > .carousel-thumb-slide" ).slice(0,4).length;
	}
	
	if (numberOfProductSlides > 4) {
		$("#productThumbCarousel > .carousel-control").show();
