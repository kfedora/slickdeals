<script type="text/javascript">
$(".tinkerScript").remove();
// Javascript to enable link to tab
var hash = document.location.hash;
var hashText = hash.replace("#","");
if (hashText != "") {
	var $target = $('a.' + hashText);
	if ($target.length > 0) {
		$target.click();
		scrollToAnchor('productsupporttabs');
	}
}
</script>

