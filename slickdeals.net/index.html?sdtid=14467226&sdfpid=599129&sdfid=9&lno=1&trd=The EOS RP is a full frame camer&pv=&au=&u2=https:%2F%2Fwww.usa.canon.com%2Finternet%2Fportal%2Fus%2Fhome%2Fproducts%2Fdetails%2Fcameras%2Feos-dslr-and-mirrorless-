<script>
var stack = [];
var tabDisplay = '';

function setTabDisplay(tabName){
	if(tabDisplay == ''){
		tabDisplay += tabName;
	}else{
		tabDisplay += ' | ' + tabName;
	}
}

/*1 of 4 title:Cameras*/





if (window.location.href.indexOf("home/support/details") > -1) {
	
		
			$("#breadcrumb_list").append('<li class="cf-gray-1">Cameras</li>');
			stack.push('Cameras');
			setTabDisplay('Cameras');
		
		
	
	
}else{
	
		
		
			$("#breadcrumb_list").append('<li><a class="cf-gray-1" href="/internet/portal/us/home/products/groups/cameras/cameras">Cameras</a></li>');
			stack.push('Cameras');
			setTabDisplay('Cameras');
		
	
	
}



/*2 of 4 title:EOS DSLR and Mirrorless Cameras*/





if (window.location.href.indexOf("home/support/details") > -1) {
	
		
			$("#breadcrumb_list").append('<li class="cf-gray-1">DSLR and Mirrorless Cameras</li>');
			stack.push('DSLR and Mirrorless Cameras');
			setTabDisplay('DSLR and Mirrorless Cameras');
		
		
	
	
}else{
	
		
		
			$("#breadcrumb_list").append('<li><a class="cf-gray-1" href="/internet/portal/us/home/products/groups/cameras/eos-dslr-and-mirrorless-cameras/dslr-and-mirrorless-cameras">DSLR and Mirrorless Cameras</a></li>');
			stack.push('DSLR and Mirrorless Cameras');
			setTabDisplay('DSLR and Mirrorless Cameras');
		
	
	
}



/*3 of 4 title:Mirrorless*/





if (window.location.href.indexOf("home/support/details") > -1) {
	
		
			$("#breadcrumb_list").append('<li class="cf-gray-1">Mirrorless Cameras</li>');
			stack.push('Mirrorless Cameras');
			setTabDisplay('Mirrorless Cameras');
		
		
	
	
}else{
	
		
			$("#breadcrumb_list").append('<li><a class="cf-gray-1" href="/internet/portal/us/home/products/list/cameras/eos-dslr-and-mirrorless-cameras/mirrorless/mirrorless">Mirrorless Cameras</a></li>');
			stack.push('Mirrorless Cameras');
			setTabDisplay('Mirrorless Cameras');
		
		
	
	
}



/*4 of 4 title:EOS RP*/


$("#breadcrumb_list").append('<li>EOS RP</li>');stack.push('EOS RP');



</script>
