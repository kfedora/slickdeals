	<script type="text/javascript" SPK="8831629198396">var studentsBackToSchool = {

    init: function () {

        var getUrl = window.location.href;
        var getUrlHash = getUrl.split('#').pop();

        if (getUrlHash.indexOf('quiz') == 0) {
            $('html, body').animate({
                scrollTop: $(".student-about-you").offset().top
            }, 2000);
        }
        var instructions = document.getElementsByClassName('instructions')[0];
         instructions.classList.add('active');
    }
}

$(window).load(function () {
    setTimeout(studentsBackToSchool.init, 500);
    
