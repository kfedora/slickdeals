	<div class="column leads" id="lead-posts">
				<div class='slick'>
				<figure class="tile lead">
	<script type="application/ld+json">{
		"@context": "http://schema.org",
		"@type": "Article",
		"mainEntityOfPage": {
			"@type": "WebPage",
			"@id": "https://www.autoblog.com/2020/10/10/junkyard-gem-1979-chevrolet-nova-coupe/"
		},
		"headline": "Junkyard Gem: 1979 Chevrolet Nova Coupe",
		"alternativeHeadline": "The final model year for the rear-wheel-drive Chevy Nova",
		"url": "https://www.autoblog.com/2020/10/10/junkyard-gem-1979-chevrolet-nova-coupe/",
		"articleSection": "Featured",
		"datePublished": "2020-10-10T12:00:00-0400",
