<script>
	

;(window);

window.AUTOBLOG = window.AUTOBLOG || {};

AUTOBLOG.settings = {
    adBlockEnabled: false,
    adblockerAd: false,
	apiUrl: '',
	assetBase: '/v/b543f2024aad3a8392ebb2300919b2f808498013',
