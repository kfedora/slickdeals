	<script type="application/ld+json">{
		"@context": "http://schema.org",
		"@type": "Article",
		"mainEntityOfPage": {
			"@type": "WebPage",
			"@id": "https://www.autoblog.com/2020/10/14/lucid-air-range-406-miles/"
		},
		"headline": "Lucid Air base sedan targets Tesla Model S, with 406-mile range for $77,400",
		"alternativeHeadline": "That's $69,900 after the federal tax credit",
