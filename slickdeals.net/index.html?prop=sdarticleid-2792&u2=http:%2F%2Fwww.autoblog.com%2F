	<div class="columns">
	<div class="column leads" id="lead-posts">
				<div class='slick'>
				<figure class="tile lead">
	<script type="application/ld+json">{
		"@context": "http://schema.org",
		"@type": "Article",
		"mainEntityOfPage": {
			"@type": "WebPage",
			"@id": "https://www.autoblog.com/2020/10/12/2021-hyundai-santa-fe-engine-specs/"
		},
		"headline": "2021 Hyundai Santa Fe gets more powerful and efficient engines with its fresh styling",
		"alternativeHeadline": "It also gets a new more luxurious top trim level",
		"url": "https://www.autoblog.com/2020/10/12/2021-hyundai-santa-fe-engine-specs/",
		"articleSection": "Official",
		"datePublished": "2020-10-12T12:00:00-0400",
