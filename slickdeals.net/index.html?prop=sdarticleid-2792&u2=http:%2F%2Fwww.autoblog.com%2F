	devId: 'ao1atoBLg0975R',
	devmode: false,
    disableSpot: false,
    disableSpotRecirc: false,
    domain: window.location.hostname,
    isInternal: false,
    isSmartphone: false,
    locale: '',
	originBase: 'https://s.aolcdn.com',
	protocol: 'https',
	proxy: '',
	referrer: '',
    scarfSearch: {
        sponsored: '',
        sponsoredMake: '',
        sponsoredModel: ''
    },
    shownInterstitial: (document.cookie.match(/^(?:.*;)?\s*ab_interstitial\s*=\s*([^;]+)(?:.*)?$/)||[,null])[1],
    shownInterstitialReminded: (document.cookie.match(/^(?:.*;)?\s*ab_interstitial_reminded\s*=\s*([^;]+)(?:.*)?$/)||[,null])[1],
    spaceId: 1197802858
};

AUTOBLOG.settings.isLogging = !!('boolean' === typeof AUTOBLOG.settings.devmode && AUTOBLOG.settings.devmode);

AUTOBLOG.user = {
	authenticated: false,
	authPref: '',
    displayName: ''
};



AUTOBLOG.pageData = Boot.extend(AUTOBLOG.pageData || {}, {
	ampId: '',
	displayName: '',
	galleryId: '',
	isLive: '',
    
	pageVersion: 'v2017',
	pageId: 'home-new',
	parentPageId: '',
    
	postId: '',
	reporting: {
        comscore: {
            cs_ucfr: '0'
        },
        global: {
            pct: '',pt: 'home',
        },
        rapid: {
            pt: 'home',pct: '',
        }
	},
	topicId: ''
});

AUTOBLOG.usedgooglesearch = '';

AUTOBLOG.ads = {"tablet":{"ad-banner-btf":{"mn":"93463762","adType":"F","width":"LB","height":"LB","adSetInView":"1"},"ad-banner-amp-html":{"mn":"93523447","adType":"F","width":"320","height":"50"},"ad-btf-native":{"mn":"93446774","adType":"text","width":"1140","height":"280"},"ad-banner":{"mn":"93446775","adType":"F","width":"LB","height":"LB"},"ad-tile":{"mn":"93445673","adType":"F","width":"RR","height":"RR","adSetInView":"1"},"ad-btf-native-global":{"mn":"93440679","adType":"text","width":"320","height":"115"},"ad-tile-amp-html":{"mn":"93523446","adType":"F","width":"300","height":"250","adSetInView":"1"},"gemini-ad-native":{"mn":"963885156","adType":"text","width":"1140","height":"280"}},"desktop":{"ad-banner-btf":{"mn":"93463762","adType":"F","width":"LB","height":"LB","adSetInView":"1"},"ad-btf-native":{"mn":"93446774","adType":"text","width":"1140","height":"280"},"ad-banner":{"mn":"93373101","adType":"F","width":"LB","height":"LB"},"ad-tile":{"mn":"93310537","adType":"F","width":"RR","height":"RR"},"ad-btf-native-global":{"mn":"93440679","adType":"text","width":"320","height":"115"},"gemini-ad-native":{"mn":"963885156","adType":"text","width":"1140","height":"280"}},"mobile":{"ad-banner-btf":{"mn":"963860861","adType":"F","width":"MM","height":"MM","adSetInView":"1"},"ad-banner-amp-html":{"mn":"93523447","adType":"F","width":"320","height":"50"},"ad-btf-native":{"mn":"93446774","adType":"text","width":"1140","height":"280"},"ad-banner":{"mn":"93442577","adType":"F","width":"320","height":"50"},"ad-tile":{"mn":"963859641","adType":"F","width":"MM","height":"MM","adSetInView":"1"},"ad-btf-native-global":{"mn":"93429462","adType":"text","width":"320","height":"115"},"ad-tile-amp-html":{"mn":"93523446","adType":"F","width":"300","height":"250","adSetInView":"1"},"gemini-ad-native":{"mn":"963885156","adType":"text","width":"1140","height":"280"}}};
if (typeof location.ancestorOrigins != 'undefined' && location.ancestorOrigins.length) {
	var o = location.ancestorOrigins[0];
	if (!(	o.slice('ampproject.org'.length * -1) == 'ampproject.org' ||
			o.slice('ampproject.net'.length * -1) == 'ampproject.net' ||
			o.slice('cloudflare.com'.length * -1) == 'cloudflare.com' ||
			o.slice('stumbleupon.com'.length * -1) == 'stumbleupon.com' ||
			o.slice('googleweblight.com'.length * -1) == 'googleweblight.com' )) {
		if (o !== location.origin) {
			AUTOBLOG.ads = {};
		}
	}
}



AUTOBLOG.ads.adCall = function(ads, layout){
	

    if (typeof(adSetInView) !== "function") { return; }

	var devices = Boot.respond(),
        _loopNum = 0;

	adSetHtNmAT('https://at.atwola.com');

	if (typeof ads == 'string') { ads = AUTOBLOG.ads[ads]; } else if (ads == null) { ads = AUTOBLOG.ads.desktop; }

	AUTOBLOG.ads.adCleanup();

	for (var key in ads) {
		var obj = ads['mn'] && ads || ads[key] || {};

		if (obj.hasOwnProperty('mn')) {
			if (document.getElementById(key)) {
                if (ads[key]['adSetInView']) {
                    adSetInView(ads[key]['adSetInView']);
                }

                if (typeof adsEnableSandbox === 'function') {
                    adsEnableSandbox();
                }

                if (obj['adType'] !== 'text') {
                    if (typeof adsRotateMult !== 'undefined') {
                        

                        adsRotateMult(key);
                    }
                } else {
                    adsSetEAN(key, 'br');
                }

                if (key !== 'gemini-ad-native') {
                    if (AUTOBLOG.settings.isSmartphone && AUTOBLOG.pageData.pageId == 'permalink') {
                        var story_length = $('.post-contents .post-body br').length / 2,
                            paragraph_selector = 'br',
                            paragraph_location = 3,
                            max_paragraph_length = 8,
                            max_story_length = 18;

                        if (story_length <= 1) {
                            var story_length = $('.post-contents .post-body p').length,
                                paragraph_selector = 'p',
                                paragraph_location = 2,
                                max_paragraph_length = 6,
                                max_story_length = 4;
                        }

                        if (key === 'ad-tilea') {
                            var $node = $('#ad-tilea');

                            $node
                                .css({
                                    'margin-top': '30px'
                                });

                            if (story_length > 3) {
                                $node
                                    .insertBefore('.post-contents .post-body ' + paragraph_selector + ':not(".snippet"):eq(' + paragraph_location + ')');
                            } else if (story_length === 3 && paragraph_selector === 'p') {
                                $node
                                    .insertBefore('.post-contents .post-body ' + paragraph_selector + ':not(".snippet"):eq(2)');
                            } else {
                                $node
                                    .insertAfter('.post-contents .post-body');
                            }
                        } else {
                            if (key === 'ad-tileb' && story_length > max_paragraph_length) {
                                var $node = $('#ad-tileb');

                                $node
                                    .css({
                                        'margin-top': '30px'
                                    })
                                    .insertBefore('.post-contents .post-body br:eq(max_story_length)');
                            }
                        }
                    }

                    if (AUTOBLOG.settings.isSmartphone && (obj['width'] === 'MM' && obj['height'] === 'MM')) {
                        if (AUTOBLOG.pageData.pageId == 'permalink' && key === 'ad-tilea') {
                            adSetMobile('1', 'noscroll');
                        }
                    }

                    if (key !== '') {
                        htmlAdWH(obj['mn'], obj['width'], obj['height'], obj['adType'], key);
                    }
                } else {
                    AUTOBLOG.utils.waitAndFire(function() {
        				return $ && ($('#ad-btf-native').find('div[id^="yap"]').length > 0 || $('#gemini-ad-native').find('div[class^="yap"]').length > 0 || $('#native_highvis').length > 0);
        			}, function() {
        				htmlAdWH(obj['mn'], obj['width'], obj['height'], obj['adType'], key);
        			}, 1000);
                }

				if (key !== '') {
                    if (layout) {
                        $('#' + key)
                            .attr('data-adtype', layout);
                    }
                }

				if (AUTOBLOG.settings.debug) {
					var elem = document.getElementById(key),
                        val = key + ' ' + JSON.stringify(obj);

					elem.setAttribute('data-toggle', 'popover');
					elem.setAttribute('data-content', val);
					elem.setAttribute('data-placement', 'bottom');
				}
			}
		}

        _loopNum++;
	}
};

AUTOBLOG.ads.adCleanup = function(){
	var a = document.getElementById('ad-banner');

	if (a != null && a.hasAttribute('style')) {
		var els = [];

		while (a) {
			a.removeAttribute('style');
			els.unshift(a);
			a = a.parentNode;
		}
	}
};

if (!('htmlAdWH' in window)) {
    htmlAdWH = function () {
        AUTOBLOG.main.log('htmlAdWH: not defined', arguments);
    };
}

var testAd = document.createElement('div');
testAd.innerHTML = '&nbsp;';
testAd.className = 'adsbox ad advertisers';

document.body.appendChild(testAd);

if (testAd.offsetHeight === 0) {
    AUTOBLOG.settings.adBlockEnabled = true;
}

testAd.remove();

function _getLib(src, onload, customAttr) {
    if (!src) {
        return;
    }

    var js = document.createElement('script');

    if (typeof customAttr !== 'undefined' && (customAttr['name'] !== '' && customAttr['value'] !== '')) {
        js.setAttribute(customAttr['name'], customAttr['value']);
    }

    js.src = src;

    js.onload = function() {
        if (typeof onload === 'function') {
            onload();
        }
    };

    document.head.appendChild(js);
};
</script>

