	<div class="columns">
	<div class="column leads" id="lead-posts">
				<div class='slick'>
				<figure class="tile lead">
	<script type="application/ld+json">{
		"@context": "http://schema.org",
		"@type": "Article",
		"mainEntityOfPage": {
			"@type": "WebPage",
			"@id": "https://www.autoblog.com/2020/10/22/mercedes-benz-recall-light-up-logo-disables-power-steering-headlight/"
		},
