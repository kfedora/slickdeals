	<div class="column leads" id="lead-posts">
				<div class='slick'>
				<figure class="tile lead">
	<script type="application/ld+json">{
		"@context": "http://schema.org",
		"@type": "Article",
		"mainEntityOfPage": {
			"@type": "WebPage",
			"@id": "https://www.autoblog.com/2020/10/16/ford-bronco-raptor-warthog-spy-shots/"
		},
		"headline": "Spy photos seem to confirm 'Warthog' name for Ford Bronco Raptor",
		"alternativeHeadline": "Real deal, or elaborate troll effort? You decide",
		"url": "https://www.autoblog.com/2020/10/16/ford-bronco-raptor-warthog-spy-shots/",
		"articleSection": "Spy Shots",
