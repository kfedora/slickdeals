<section id="autoblog-autoblog-green" class="flex-module flex-6 autoblog-green">
			<h3>
							<a href="/green/">Green</a>
						</h3>
					<h2 class="title">
							<a href="/green/">Autoblog Green</a>
						</h2>
					<ul class="flex-container item-list" id="autoblog-autoblog-green-frame">
