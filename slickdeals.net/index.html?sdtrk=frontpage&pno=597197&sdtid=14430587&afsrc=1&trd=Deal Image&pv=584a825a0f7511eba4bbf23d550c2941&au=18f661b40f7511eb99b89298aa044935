    <script type="text/javascript">
        jQuery(document).ready(function () {
            var buttonSelectors = [
                '#options-button .crtfrm .btn-add',
                '#options-button .crtfrm .btn-NoSale',
                '#options-button > form#storeselector',
            ].join(',');
            var btnScrollOffset = jQuery(buttonSelectors).length ? (jQuery(buttonSelectors).offset().top + 50) : 450;

            jQuery(window).scroll(function () {
                var scrollTop = jQuery(window).scrollTop();
                if (scrollTop > btnScrollOffset) {
                    jQuery('.mm-t007-sticky-bar').addClass('mm-t07-show');
                } else {
                    jQuery('.mm-t007-sticky-bar').removeClass('mm-t07-show');
                }
            });
        });
        jQuery(document).on('click', '.mm-t007-totop', function () {
            jQuery('html, body').animate({ scrollTop: 0 }, jQuery(window).scrollTop() / 2);
        });

    </script>


                

   
