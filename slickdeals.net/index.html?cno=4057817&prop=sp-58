nav#primary-navigation .sticky-navigation #hamburger-open #hamburger-holder .hamburger-cell li:hover,
nav#primary-navigation .sticky-navigation #hamburger-open #hamburger-holder .hamburger-cell li.active {
    background: #0077c8;
}

