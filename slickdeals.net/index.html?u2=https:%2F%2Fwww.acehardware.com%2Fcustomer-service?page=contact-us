	function deferJquery(method) {
		if (window.jQuery) {
			method();
		} else {
			setTimeout(function() { deferJquery(method) }, 50);
		}
	}
	deferJquery(function () {
		if( !(jQuery(".mz-loginpage").length || jQuery(".ace-checkout").length || jQuery(".mz-cart").length || jQuery(".ace-rewards-landing").length || jQuery(".onlineOrderLandingPage").length || jQuery("#mz-drop-zone-tpsPage_category_templateStyles").length || jQuery("#mz-drop-zone-tpsPage_inspirationParent_templateStyles").length ) &&
		window.localStorage.getItem("global_holiday_101620") === null ){
			jQuery(".global_holiday_101620").css("display", "block");
		}

		jQuery(".global_holiday_101620 .theCloseButton").on("click",function(e){
			e.preventDefault();
			document.querySelectorAll(".global_holiday_101620")[0].style.display = "none";
			window.localStorage.setItem('global_holiday_101620', true);
		});
	});
