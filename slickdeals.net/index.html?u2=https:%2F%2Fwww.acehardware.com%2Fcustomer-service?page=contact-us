	window.onload = function() {
		var lastFocus;
		function modalShow () {
			lastFocus = document.activeElement;
		}
		function modalClose () {
			lastFocus.focus();
		}

		jQuery(".modalOpener").on("click",function(e){
			modalShow();
			// account for possibility of multiple modals
			myClasses=jQuery(this).attr("class").split(' ');
			modalName="";
			jQuery.each(myClasses,function(index,value){
				if(value.indexOf('modalOpener_') == 0) {
					modalName=value.substr(value.indexOf("modalOpener_")+12);
				}
			});
			jQuery(".modalOpenerModal.modalOpener_"+modalName).clone().appendTo("body").addClass("show").find(".modal_closer").focus();
		});

		jQuery("body").on('click', ".modalOpenerModal,.modal_closer", function(e){
			jQuery("body>.modalOpenerModal").remove();
			modalClose();
		});
		jQuery("body").on("click", ".modalOpenerModal *", function(e){
			e.stopPropagation();
		});
		jQuery("body").on("click", ".modalOpener", function(e){
			e.preventDefault();
		});
	}
</script></div></div></div></div></div></div></div></div></div></div><div class="clearfix" ><div class="mz-drop-zone-toggle" data-view="order-information"><h2 class="mz-cs-section-title"><a href="#mz-cs-top">Order Information </a><span class="mz-support-row-toggle pull-right"><i class="sprite-plusRed-3"></i><i class="sprite-minusRed-3"></i></span></h2><div class="mz-support-dropzone-container col-xs-12" id="order-information"><div id="mz-drop-zone-order-information" class="mz-drop-zone" ><div class="mz-cms-row" data-widget="{&quot;title&quot;:&quot;Dropzone&quot;}" ><div class="mz-cms-col-12-12"  style="width:"><div class="mz-cms-block" ><div class="mz-cms-content"><div class="mz-content-widget-wrap"><div class="mz-content-widget-inner-wrap"><style>
