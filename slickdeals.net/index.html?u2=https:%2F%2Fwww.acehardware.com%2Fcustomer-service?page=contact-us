</style><div class="customer-service-section" ><h2 class="customer-service-title">How Can We Help?</h2><p>Thanks for visiting us online. If you ever need any help, please contact us:
    <p><p>Phone: 888-827-4223
    <p>Email: carecenter@acehardware.com
    
</div></div></div></div></div></div></div></div></div></div><div class="mz-drop-zone-toggle" data-view="contact-us"><h3 class="mz-cs-subtitle col-xs-12"><a href="#mz-cs-top">Contact Us </a><span class="mz-support-row-toggle pull-right"><i class="sprite-plusRed-3"></i><i class="sprite-minusRed-3"></i></span></h3><div class="mz-support-dropzone-container col-xs-12" id="contact-us"><div id="mz-drop-zone-contact-us" class="mz-drop-zone" ><div class="mz-cms-row" data-widget="{&quot;title&quot;:&quot;Dropzone&quot;}" ><div class="mz-cms-col-12-12"  style="width:"><div class="mz-cms-block" ><div class="mz-cms-content"><div class="mz-content-widget-wrap"><div class="mz-content-widget-inner-wrap"><script>
(function() {
  'use strict';
  window.addEventListener('load', function() {
    // Fetch all the forms we want to apply custom Bootstrap validation styles to
    var forms = document.getElementsByClassName('needs-validation');
    window.formVal = window.formVal || {};

    function getFormValues(formId)
    {
      var formValues = {};
      var form = document.getElementById(formId);
      formValues.firstName = form.querySelector('input[name=firstName]').value;
      formValues.lastName = form.querySelector('input[name=lastName]').value;
      formValues.orderNum = form.querySelector('input[name=orderNum]').value;
      formValues.phone = form.querySelector('input[name=phone]').value;
      formValues.email = form.querySelector('input[name=email]').value;
      formValues.category = form.querySelector('select[name=category]').value;
      var emailMessage = form.querySelector('textarea[name=emailMessage]').value;
      
      // remove credit card / gift card information
      emailMessage = emailMessage.replace(/[0-9]{4}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4}/g, "");
      // Visa
      emailMessage = emailMessage.replace(/4(([0-9]{18})|([0-9]{3}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4})|([0-9]{12}))/g, "");
      // Mastercard
      emailMessage = emailMessage.replace(/(?:5[1-5][0-9]{2}(\s*|\s*-\s*)|222[1-9](\s*|\s*-\s*)|22[3-9][0-9](\s*|\s*-\s*)|2[3-6][0-9]{2}(\s*|\s*-\s*)|27[01][0-9](\s*|\s*-\s*)|2720(\s*|\s*-\s*))[0-9]{4}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4}/g, "");
      // American Express
      emailMessage = emailMessage.replace(/3[47][0-9]{2}((\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{3}|(\s*|\s*-\s*)[0-9]{6}(\s*|\s*-\s*)[0-9]{5})/g, "");
      // Discover
      emailMessage = emailMessage.replace(/6(?:011(\s*|\s*-\s*)|5[0-9]{2}(\s*|\s*-\s*))[0-9]{4}(\s*|\s*-\s*)[0-9]{4}(\s*|\s*-\s*)[0-9]{4}/g, "");
      
      formValues.emailMessage = emailMessage;
      formValues.confirmation = formId + '-confirmation';
      form.reset();
      window.formVal = formValues;
    }
    
    function validate(event) {
        event.preventDefault();
        var inputs = document.getElementsByClassName("form-control");
        for (var i = 0; i < inputs.length; i++) {
          inputs[i].classList.add("is-loading");
        }
        var formId = event.currentTarget.id;
        getFormValues(formId);
        window.grecaptcha.execute(document.getElementById( formId + '-recaptcha'));
    }

    // Loop over them and prevent submission
     var validation = Array.prototype.filter.call(forms, function(form) {
       form.addEventListener('submit', function(event) {
        if (form.checkValidity() === false) {
          event.preventDefault();
          event.stopPropagation();
          form.classList.add('was-validated');
         }
         else if(form.checkValidity() === true)
         {
          validate(event);
         }
       }, false);
    });
  }, false);
})();

function contactSubmitFinished(){
  document.getElementById('contactUsCategory').value = "";
  document.getElementById('messageLengthTxt').innerText = "0/1500 characters";
  document.getElementById('contact-us-form').classList.remove('was-validated');
  var inputs = document.getElementsByClassName("form-control");
  for (var i = 0; i < inputs.length; i++) {
    inputs[i].classList.add("placeholder-shown");
    inputs[i].classList.remove("is-loading");
  }
}

function onContactUsSubmit(captchaResponse) {
  window.formVal = window.formVal || {};
  var confirmation = document.getElementById(window.formVal.confirmation);

  if(window.formVal !== null)
  {
    require(['modules/api', 'hyprlive'], function(api, Hypr) {
      function clone(obj) {return JSON.parse(JSON.stringify(obj));}
      var payload = clone({email: window.formVal.email, firstName: window.formVal.firstName, lastName: window.formVal.lastName, 
          orderNumber: window.formVal.orderNum, phone: window.formVal.phone, category: window.formVal.category,
          emailMessage: window.formVal.emailMessage, 'g-recaptchaToken': captchaResponse});
       api.request(
        'post',
        'care-center/sendCareCenterEmail',
        payload
        )
        .then(function(response) {
          window.formVal = {};
          confirmation.innerText = "";
          var popupContainer = document.getElementById("contactUsPopupContainer");
          popupContainer.style.display = "block";
          contactSubmitFinished();
        })
        .catch(function(err) {
          confirmation.innerText = ((err && err.errorString) || 'Something went wrong. Please try again.');
          contactSubmitFinished();
        });
      }); 

  }
  window.grecaptcha.reset();
}
