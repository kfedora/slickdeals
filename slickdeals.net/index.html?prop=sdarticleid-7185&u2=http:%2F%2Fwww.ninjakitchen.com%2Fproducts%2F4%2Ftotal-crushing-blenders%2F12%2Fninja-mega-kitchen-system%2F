            function reviewChange(sku){
                $('[data-bv-product-id]').attr('data-bv-product-id',sku);
                $('[data-cmbv-id]').text(sku);
            }

            $(document).ready(function(){
                $('.manuals .trigger, .reviews .trigger').on('click', function() {
                    var $this = $(this);
                    $this.closest('.accordion').toggleClass('active');
                });
                $('.manuals .option, .reviews .option').on('click', function() {
                    var $this = $(this).closest('.accordion');
                    var val = $(this).attr('data-val');
                    if(typeof reviewChange == "function"){
                        reviewChange(val);
                    }
                    $this.find('.trigger .copy').text(val);
                    $('.manuals [data-prod],.reviews [data-prod]').addClass('hide');
                    $('.manuals [data-prod="' + val + '"],.reviews [data-prod="' + val + '"]').removeClass('hide');
                    $this.removeClass('active');
                    $this.find('.clear').removeClass('hide');
                });
                $('.manuals .clear, .reviews .clear').on('click', function() {
                    var $this = $(this).closest('.accordion');
                    var val = $this.find('.trigger .copy').attr('data-default');
                    $this.find('.trigger .copy').text(val);
                    $('.manuals [data-prod],.reviews [data-prod]').removeClass('hide');
                    $this.find('.clear').addClass('hide');
                });
            });
        </script><script type="text/javascript">
                function getBVCKY(){
                    return cookie.getCookie('cm.bv');
                }
                $(document).ready(function(){
                    if(getBVCKY() == null){
                        $('#BVViolator').addClass('active');
                    }
                    $(document).on('click', '#BVViolator .close', function(){
                        $('#BVViolator').removeClass('active');
                        cookie.setCookie('cm.bv', false);
                    });
                });
                $(window).on('scroll', function(){
                    var winScroll = window.pageYOffset;
                    var detHeight = $('#Details .seriesmodels').outerHeight();
                    if(winScroll > detHeight){
                        $('#BVViolator').removeClass('active');
                    }else{
                        if(getBVCKY() == null){
                            $('#BVViolator').addClass('active');
                        }
                    }
                });
