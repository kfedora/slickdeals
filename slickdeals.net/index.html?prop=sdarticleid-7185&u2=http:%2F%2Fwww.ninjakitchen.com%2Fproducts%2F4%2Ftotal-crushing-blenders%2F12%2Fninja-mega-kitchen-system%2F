        // TODO: Consolidate these functions into cm.jquery.datalayer.js
        function datalayerEvent(event, category, action, label, keyvals) {
            dataObj = {
                'event': event,
                'EventCategory': category,
                'EventAction': action,
                'EventLabel': label
            }
            dataLayer.push(Object.assign(dataObj, keyvals));
        }
        function gaEvent(category, action, label, value) {
            dataLayer.push({'event': 'epAnalyticsEventClick',
                            'epEventCategory': category,
                            'epEventAction': action,
                            'epEventLabel': label,
                            'epEventValue': value});
        }
        function cuEvent(event, category, action, label, text, content) {
            dataLayer.push({'event': event,
                            'EventCategory': category,
                            'EventAction': action,
                            'EventLabel': label,
                            'link_text': text,
                            'content_type': content});
        }
        function ctaEvent(event, category, action, label, text, content, bannerContent, prodCat) {
            dataLayer.push({'event': event,
                            'EventCategory': category,
                            'EventAction': action,
                            'EventLabel': label,
                            'link_text': text,
                            'content_type': content,
                            'banner_content': bannerContent,
                            'product_category': prodCat});
        }
        function footEvent(event, category, action, label, text, lcat, content) {
            dataLayer.push({'event': event,
                            'EventCategory': category,
                            'EventAction': action,
                            'EventLabel': label,
                            'link_text': text,
                            'link_category': lcat,
                            'content_type': content});
        }
        function promoEvent(name, id, creative, position) {
            dataLayer.push({'event': 'promotionClick',
                            'ecommerce': {
                                'promoClick': {
                                    'promotions': [{
                                        'name': name,
                                        'id': id,
                                        'creative': creative,
                                        'position': position
                                    }]
                                }
                            }});
        }
        function prodEvent(event, category, action, label, prodId, prodName, prodSection, value) {
            dataLayer.push({'event': event,
                            'EventCategory': category,
                            'EventAction': action,
                            'EventLabel': label,
                            'product_id': prodId,
                            'product_name': prodName,
                            'product_section': prodSection,
                            'link_text': value});
        }
        function catfilterEvent(event, category, action, label, act, filtCat, filtName, prodCat) {
            dataLayer.push({'event': event,
                            'EventCategory': category,
                            'EventAction': action,
                            'EventLabel': label,
                            'action': act,
                            'filter_category': filtCat,
                            'filter_name': filtName,
                            'product_category': prodCat});
        }
