<link href="https://cdn.staticsfly.com/css/sflystore/calendarWidget-v15030186940007081.css" rel="stylesheet"/>
<script type='text/javascript' src="/js/lib/jquery/plugins/jquery-ui-1.10.1.custom.js"></script>

<script>
    var datepickerId = '#datepicker';

    function addPrevYearButton (dp, year) {
        var prevYearBtn,
            $datepicker = $('#datepicker'),
            minDate = $datepicker.datepicker('option', 'minDate');

        if (minDate != null && year <= minDate.getFullYear()) {
            prevYearBtn = $('<a class="prev-year-btn ui-state-disabled"><i class="ui-icon"></a>');
            dp.addClass('disabled-btn-prev-year');
        } else {
            prevYearBtn = $('<a class="prev-year-btn"><i class="ui-icon"></a>');
            dp.removeClass('disabled-btn-prev-year')
        }

        prevYearBtn.bind('click', function () {
            $.datepicker._adjustDate($('#datepicker'), -1, 'Y');
        });

        prevYearBtn.insertBefore('.ui-datepicker-prev');
    }

    function addNextYearButton () {
        var nextYearBtn = $('<a class="next-year-btn"><i class="ui-icon"></a>');

        nextYearBtn.bind('click', function () {
            $.datepicker._adjustDate($('#datepicker'), +1, 'Y');
        });

        nextYearBtn.insertAfter('.ui-datepicker-next');
    }

    function addChangeYearButtons (id, dp, year) {
        setTimeout(function () {
            var prevYearBtnExists = $('.prev-year-btn').length > 0,
                nextYearBtnExists = $('.next-year-btn').length > 0;

            if (!prevYearBtnExists) {
                addPrevYearButton(dp, year);
            }
            if (!nextYearBtnExists) {
                addNextYearButton();
            }

            $(id).change();
        }, 0);
    }

    $(document).ready(function () {
        // need to add addChangeYearButtons to event handlers to persist next/prev year buttons
        $(datepickerId).datepicker({
			

            
            minDate: new Date(new Date().getTime() + 24 * 60 * 60 * 1000),
            
			
            

            showOn: 'focus',
            showOtherMonths: true,
            dayNamesMin: ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'],
            beforeShow: function (input, inst) {
                var date = new Date(input.value),
                    year = date.getFullYear() || new Date().getFullYear();
                addChangeYearButtons(datepickerId, inst.dpDiv, year);
            },
            onChangeMonthYear: function (year, month, inst) {
                addChangeYearButtons(datepickerId, inst.dpDiv, year);
            }
        });

        // disable focus to improve user experience on mobile devices (no keyboard popup, scrolling)
        $(datepickerId).attr('readonly', 'readonly');
        $(datepickerId).bind('focusin focus', function (e) {
            e.preventDefault();
        });
    });
</script>

    

