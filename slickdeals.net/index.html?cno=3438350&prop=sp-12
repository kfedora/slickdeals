            localStorage.setItem('FSPhotos', '1');
            localStorage.setItem('FSGalleon', '1');
        }
        
        var suid = "null";
        if (typeof suid !== 'undefined' && suid !== null && suid !== 'null' && !/^0*$/.test(suid)
