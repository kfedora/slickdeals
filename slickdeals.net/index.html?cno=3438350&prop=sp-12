	<script type="text/javascript">
        var BRANDS = {
            SHUTTERFLY: 'SFLY',
            TINYPRINTS: 'TP'
        };
        var footerDebugDataContent = JSON.parse('{"timeStamp":"10/18/20, 12:24 PM","hostname":"10"}');
        var redirectUri = '' || window.location.href;
        var currentBrand = false ? BRANDS.TINYPRINTS : BRANDS.SHUTTERFLY;

        function addEvent(e, t, o) {
            e.addEventListener ? e.addEventListener(t, o, false) : e.attachEvent && e.attachEvent("on" + t, o)
        }

        var initFooterComponent = function() {
            var e = {
                currentBrand: currentBrand,
                isFullVersion: true,
                footerDebugDataContent: footerDebugDataContent,
                redirectUri: redirectUri,
                showBrandsOnMobile : true
            };
            window.GlobalFooter(e, "responsive-footer")
        };
        addEvent(window, "load", function() {
            initFooterComponent()
        });
	</script>

	

	
	
	




<script type="text/javascript" async src="https://cdn.staticsfly.com/cncbundles/cncbundle.h237646814.js" charset="utf-8"></script>




  



  <link rel="stylesheet" type="text/css" href="https://cdn.staticsfly.com/cncbundles/cncbundle.h-460858563.css" charset="utf-8"/>
  <script type="text/javascript" src="https://cdn.staticsfly.com/cncbundles/cncbundle.h1706539177.js" charset="utf-8"></script>








    <script>
      $(function() {
        var $addToCartBtn = $('#cartCta');
        if($('#validationError').length) {
          $('html, body').animate({
            scrollTop: $("#datePickerFormSection").offset().top
          }, 100);
        }

        var $datepicker = $('#datepicker').datepicker('option', 'onSelect', function () {
          $('.error').removeClass('active');
          $('#validationError').remove();
        });

        $addToCartBtn.on('click', function() {
          if ($datepicker.is(':disabled')) {
            return;
          }
          if ($datepicker.val()  === '') {
            $('.error').addClass('active');
            $('#validationError').remove();
          } else {
            determineCartTypeAndExecute();
          }
        });

        function getUrlParameter(name) {
          name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
          var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
          var results = regex.exec(location.search);
          return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
        };

        function determineCartTypeAndExecute() {
          var isIcWebEnabled = ("false" == "true");
          var isLoggedIn = ("false" == "true");
          var showIcWebURI = window.location.origin + '/rest/ecom/showicweb';
          if (!isLoggedIn) {
              document.getElementById('sskForm').submit();
              window.location.href = '/forwardingSignin/start.sfly?forwardToURL=' + window.location.href + '&executeAddToCartAction=true&dateVal=' + $datepicker.val();
          } else {
            if (isIcWebEnabled) {
              prepareAddToIcCart();
            } else {
              addToSphinxCart();
            }
          }
        }

        function addToSphinxCart() {
          if (getUrlParameter('executeAddToCartAction') === 'true') {
            $datepicker.val(getUrlParameter('dateVal'));
          }
          document.getElementById('sskForm').submit();
        }

        function prepareAddToIcCart() {
          var userXMLURI = window.location.origin + '/userXML/query.sfly?command=getSessionUserInfo&version=v2';
          $.get(userXMLURI)
            .done(function(data) {
              var oauthV2AccessToken = data.getElementsByTagName('oauthV2AccessToken')[0];
              accessToken = oauthV2AccessToken ? oauthV2AccessToken.textContent : undefined;
              if (!accessToken) {
                throw new Error('Could not get access token.');
              }
              addToICWebCart(accessToken);
            })
            .fail(function() {
              console.error('Could not get access token.');
            });
        }

        if (getUrlParameter('executeAddToCartAction') === 'true') {
          determineCartTypeAndExecute();
        }

        function addToICWebCart(accessToken) {
          var apigeeApiKey = "uWrM911sdIvHivflYxyiHlGgmlgoaV0m";
          var wolCartUrl = "/cart";
          var apigeeHost = "api2.shutterfly.com";
          var cartUrl;
          var formattedDate;

          if (getUrlParameter('executeAddToCartAction') === 'true') {
            formattedDate = getUrlParameter('dateVal').split('/');
          } else {
            formattedDate = $datepicker.val().split('/');
          }

          cartUrl = "https://"+ apigeeHost +"/v1/web-cart-orch/cart/v1/carts/default/weddingKit";
          cartData = {
            "sku": "WPD_SAMPLE_KIT",
            "marriageDate": formattedDate[2]+"-"+formattedDate[0]+"-"+formattedDate[1],
            "occasionType": "WEDDING"
          };

          $.ajax({
            type: "POST",
            url: cartUrl,
            beforeSend: function(xhr) {
              xhr.setRequestHeader('SFLY-Apikey', apigeeApiKey);
              xhr.setRequestHeader('content-type', 'application/json');
              xhr.setRequestHeader('Authorization', 'Bearer ' + accessToken);
            },
            data: JSON.stringify(cartData),
            contentType: "application/json; charset=utf-8",
            dataType: "json"
          })
          .done(function(data) {
            location.href = wolCartUrl;
          })
          .fail(function(err) {
            console.log("Couldn't add to cart. Try again");
          });
        }
      });
    </script>
  



















<script type="text/javascript">_satellite.pageBottom();</script>










