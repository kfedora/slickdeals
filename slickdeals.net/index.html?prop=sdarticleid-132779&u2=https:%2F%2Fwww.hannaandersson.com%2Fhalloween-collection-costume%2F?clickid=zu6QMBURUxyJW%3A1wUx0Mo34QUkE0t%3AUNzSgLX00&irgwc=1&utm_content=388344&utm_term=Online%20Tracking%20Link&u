rewriteImageAPI = function(url){
	var version = Yo.utils.metaDataValue('resource.version'),
    cdn = Yo.utils.metaDataValue('cdn.hostname'),
    profileId = Yo.utils.metaDataValue('profile.id');
    var opt = (navigator.userAgent.indexOf('Chrome') >= 0 &&
               navigator.userAgent.indexOf('Edge') < 0) ? 'o~f_webp' : 'o~';
    return location.protocol+cdn+'/'+profileId+'/'+opt+'/'+version+'/'+url;
};

yo_aTagRewrite = function() {
	var aTags = document.getElementsByTagName('a');
	for (var x=0; x < aTags.length; x++) {
		if (aTags[x].hasAttribute('data-image') && aTags[x].getAttribute('data-image').indexOf('yottaa.net') == -1) {	
			aTags[x].setAttribute('data-image', rewriteImageAPI(aTags[x].getAttribute('data-image')));
		}
		if (aTags[x].hasAttribute('href') && aTags[x].getAttribute('href').indexOf('yottaa.net') == -1 && aTags[x].getAttribute('href').indexOf('pinterest') == -1 && aTags[x].getAttribute('href').indexOf('jpg') != -1) {	
			aTags[x].setAttribute('href', rewriteImageAPI(aTags[x].getAttribute('href')));
		}
	}
}

yo_rewriteJSON = function(obj){
    var data = $(obj).attr('data-swap-images');
    if (data && data != ''){
        json = JSON.parse(data);
        if (json.imageURLs ){
        	if (json.imageURLs.mobile && json.imageURLs.mobile.indexOf('yottaa.net') == -1) { 
            	json.imageURLs.mobile = Yo.utils.rewriteImageAPI(json.imageURLs.mobile);
            }
            if (json.imageURLs.tablet && json.imageURLs.tablet.indexOf('yottaa.net') == -1) { 
            	json.imageURLs.tablet = Yo.utils.rewriteImageAPI(json.imageURLs.tablet);
            }
            if (json.imageURLs.desktop && json.imageURLs.desktop.indexOf('yottaa.net') == -1) { 
            	json.imageURLs.desktop = Yo.utils.rewriteImageAPI(json.imageURLs.desktop);
            }
            if (json.imageURLs.highResolution && json.imageURLs.highResolution.indexOf('yottaa.net') == -1) { 
            	json.imageURLs.highResolution = Yo.utils.rewriteImageAPI(json.imageURLs.highResolution);
            }
        	$(obj).attr('data-swap-images', JSON.stringify(json));
		}
	}
}
