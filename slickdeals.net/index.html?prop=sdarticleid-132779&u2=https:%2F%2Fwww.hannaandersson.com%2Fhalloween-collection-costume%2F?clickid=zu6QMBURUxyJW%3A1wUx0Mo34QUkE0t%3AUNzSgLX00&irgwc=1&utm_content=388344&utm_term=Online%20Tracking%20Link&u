<script type="text/javascript">
// Record any PROMO impressions that are rendered in the page.
if(gtmAnalytics.promoImpressions.length > 0)
{
dataLayer.push({
"event" : "promotion-impression",
"ecommerce": {
"promo-impressions" : gtmAnalytics.promoImpressions
}
});
gtmAnalytics.promoImpressions = [];
}
// Record any PRODUCT impressions that are rendered in the page.
if(gtmAnalytics.productImpressions.length > 0)
{
var separateImpressions = {product: [], set: []};
gtmAnalytics.productImpressions.forEach(function(impression){
if (impression.type === 'set') {
separateImpressions.set.push(impression);
} else {
separateImpressions.product.push(impression);
}
});
dataLayer.push({
"event" : "product-impression",
"ecommerce" : {
"currencyCode" : "USD", // This will need to change in the future depending how they want to manage multiple currencies.
"impressions" : separateImpressions.product
}
});
gtmAnalytics.submitProductSetImpressions(gtmAnalytics.productImpressions);
gtmAnalytics.productImpressions = [];
gtmAnalytics.analyticsGridLoad++;
}
</script>

