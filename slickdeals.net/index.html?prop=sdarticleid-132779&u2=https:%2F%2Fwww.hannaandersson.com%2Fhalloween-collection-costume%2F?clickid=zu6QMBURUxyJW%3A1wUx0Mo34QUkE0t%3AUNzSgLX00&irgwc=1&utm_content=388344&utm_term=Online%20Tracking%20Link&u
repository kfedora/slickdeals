if($ptw.height() != $(".piv-main-image").height()) {
$ptw.height(_h);
$("#thumbnails").height(_h);
} else if(_imagesLoaded == $thumbs.length && $ptw.height() == _h || _iterations == 100) { // iterations is used to break out of a interval timer, super slow connections need lots of time.
$ptw.addClass("loaded").find(".mcs-button-arrow").animate({opacity:1}, 400, function() { });
clearInterval(_intvl1);
}
_iterations++;
}
} else {
var _h = $(".piv-thumbnail").first().height();
$ptw.height(_h);
$("#thumbnails").height(_h);
$ptw.addClass("loaded").find(".mcs-button-arrow").animate({opacity:1}, 400, function() { });
}
if($('.product-images-wrapper').hasClass('video-playing')) {
$('.mz-thumb:not(.video-link)').removeClass("mz-thumb-selected");
}
}
else
{
var _iterations=0;
if($(".more-to-love-recommendations__tiles.MagicScroll, .ha-magic-scroll").length > 0) {
var $msc = $("#"+_msid);
var _intvl2;
function adjustMSCarousel($container, slide, slideImg) {
var $vImg = $container.find(".mcs-item[data-item='0']").first().find(slideImg); // Gets first viewport visible image in carousel
var _mcsH = 0;
$container.find(slide).each(function() {
if(_mcsH < $(this).height()) {
_mcsH = $(this).height();
}
});
if($msc.height() != _mcsH && $vImg && $vImg[0] && $vImg[0].complete) { // Wait for image to be completely loaded
$container.height(_mcsH);
} else if(($msc.height() == _mcsH && $vImg && $vImg[0] && $vImg[0].complete) || _iterations > 100 ) { // Wait for image to be completely loaded or 100 iterations... (slow 3g takes 45 iterations or so...)
if($vImg) {
$container.find(".mcs-button-arrow").css({top: ($vImg.height()/2 - 15), margin: 0 }).animate({opacity:1}, 400, function() { }); // always center the arrows on the images not entire product tile height.
}
$container.parent().animate({opacity:1}, 400, function() { });
clearInterval(_intvl2);
}
_iterations++; //increment the counter
}
if ($msc.hasClass('ha-magic-scroll__scroller')) {
_intvl2 = setInterval(adjustMSCarousel.bind(null, $msc, '.ha-magic-scroll__item', 'img'), 300);
// Add data-clicked attributes if necessary
if ($msc.parent('[data-contentid]').length > 0) {
$msc.find(".mcs-button").each(function() {
$this = $(this);
var btnData = $this.hasClass("mcs-button-arrow-prev") ? ['Previous slide', 'left arrow'] : ['Next slide', 'right arrow'];
$this.attr("aria-label", btnData[0]).attr("data-clicked", btnData[1])
.off('click.gatracking').on('click.gatracking', function(e){
try { gtmAnalytics.submitPromotionClick($(e.target).closest('[data-contentid]'), $(e.target));
} catch (err) { console.log("gtmAnalytics error: " + err.message); }
});
})
}
} else if ($msc.hasClass('more-to-love-recommendations__tiles')) {
_intvl2 = setInterval(adjustMSCarousel.bind(null, $msc, '.product-tile', '.thumb-link img'), 300);
// ADA compliance
$msc.find(".more-to-love-recommendations__tile").each(function() {
// Remove id="{uuid}" on product-tiles for carousels
$(this).find(".product-tile").removeAttr("id");
});
}
// Arrow click analytics
$msc.find('.mcs-button-arrow').on('click', function(e) {
$('body').trigger('mcs-arrow-clicked', e);
})
}
}
}
};
var mzOptions = {
onUpdate: function() {
if($(arguments[2]).hasClass('mz-thumb')) {
$('#pdp-video-player').hide();
$('.product-image.main-image').show();
$('.product-images-wrapper').removeClass('video-playing');
}
// ADA compliance for primary MagicZoom product image
if ($(".mz-figure img").length && window.ha.pdpalt) {
$(".mz-figure img").attr("alt", window.ha.pdpalt);
}
},
onZoomReady: function() {
var target = document.getElementById(arguments[0]);
var targetImg = target.querySelector('.mz-lens img');
var observer = new MutationObserver(function(mutations) {
mutations.forEach(function(mutation) {
if (mutation.attributeName == 'src') {
MagicZoom.zoomIn(target);
}
});
});
observer.observe(targetImg, { attributes: true });
// ADA compliance for hidden/placeholder MagicZoom images
if ($(".mz-lens img, .mz-zoom-window img").length) {
$(".mz-lens img, .mz-zoom-window img").attr("alt", "");
}
// ADA compliance for A+ PDP pages
$('.pdp-aplus .mz-figure img, .pdp-aplus-qv .mz-figure img').each(function() {
var $this = $(this);
var altText = $this.attr('alt');
if (!altText) { return; }
var $zoomImg = $this.parents('.mz-figure').find('.mz-lens img');
if (!$zoomImg.length || $zoomImg.attr('alt').length > 0) { return; }
$zoomImg.attr('alt', altText + ' (zoom)');
});
if(window.innerWidth < 768) {
$("#thumbnails").attr("data-options", "orientation : horizontal; items: 5; step: 1;");
}
else
{
$("#thumbnails").attr("data-options", "orientation : vertical; step: 1; items: fit; height: "+$(".piv-main-image").height()+";" );
}
MagicScroll.refresh("thumbnails");
},
onZoomIn: function() {
var $zoom = $('.js-pdp-aplus-zoom');
if (!$zoom.length) { return; }
$zoom.addClass('js-zoomed-in');
$zoom.parents('.pdp-aplus-zoom').addClass('js-zoomed-in');
},
onZoomOut: function() {
var $zoom = $('.js-pdp-aplus-zoom');
if (!$zoom.length) { return; }
$zoom.removeClass('js-zoomed-in');
$zoom.parents('.pdp-aplus-zoom').removeClass('js-zoomed-in');
}
};
</script>
