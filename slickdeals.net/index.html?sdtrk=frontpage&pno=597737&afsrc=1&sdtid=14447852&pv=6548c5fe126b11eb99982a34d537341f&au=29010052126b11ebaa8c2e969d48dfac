	function send_adobe_tracking(){
	  s.pageName='US_20-2253_MobileOfferGiftCard/10152020';
	  s.server=document.location.hostname;
	  s.channel='Landingpage';
	  s.prop1='browsing';
	  s.prop11='US_20-2253_MobileOfferGiftCard/10152020';
	  s.prop23='MKT Promotion';
	  s.prop19=Web.StateManager.Cookies.get(Web.StateManager.Cookies.Name.NVTC);
	  s.prop24=Web.StateManager.Cookies.get(Web.StateManager.Cookies.Name.UTMA);


    
  /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
  try {
    var s_code = s.t();
    sentToPacketBeat(s);
    if (s_code) {
        document.write(s_code);
    }
  } catch (eee) {}
}

if(typeof s != 'undefined'){
  //we have adobe, just send out the tracking data
  send_adobe_tracking();
}else{
  //oops, adobe lib is not loaded yet. we check it every 0.1s
  var check_interval = setInterval(function(){
    if(typeof s != 'undefined'){
     //we have adobe, just send out the tracking data and stop checking
      send_adobe_tracking();
      clearInterval(check_interval);
    }
  }, 100);
}
</script> 
