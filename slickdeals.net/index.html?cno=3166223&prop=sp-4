              <script>
                function getYoutubeThumbHtml(data) {
                  const {
                    id,
                    width,
                    height,
                    title
                  } = data;
                  return `<div class="youtube-thumbnail" style="width: ${width}px; height: ${height}px; background-image: url('https://i.ytimg.com/vi/${id}/sddefault.jpg');" ><div class="title">${title}</div><div class="play"></div><div class="arrow">&#x25b6;</div></div>`
                }
                function embedYoutube(container, data) {
                  const el = document.createElement('iframe');
                  const {
                    id,
                    width,
                    height
                  } = data;
                  el.setAttribute('src', `https://www.youtube-nocookie.com/embed/${id}?rel=0&autoplay=1`);
                  el.setAttribute("width", width);
                  el.setAttribute("height", height);
                  el.setAttribute("frameborder", "0");
                  el.setAttribute("allowfullscreen", "1");
                  container.replaceChild(el, container.firstChild);
                }
                window.addEventListener('DOMContentLoaded', () => {
                  const players = document.getElementsByClassName('youtube-player');
                  for (const player of players) {
                    const container = document.createElement('div');
                    container.setAttribute("tabindex", "0");
                    container.innerHTML = getYoutubeThumbHtml(player.dataset);
                    container.addEventListener('click', evt => embedYoutube(container, player.dataset), {
                      once: true
                    });
                    container.addEventListener('keydown', evt => {
                      if (evt.keyCode === 13) {
                        embedYoutube(container, player.dataset)
                      }
                    }, {
                      once: true
                    });
                    player.appendChild(container);
                  }
                })
              </script> <!-- /HTML Content Wrapper -->

              <!-- End of ContentSpaceName: home_youtube_social_cs -->

