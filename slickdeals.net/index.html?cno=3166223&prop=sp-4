      function getCookieFS(name) {
        try {
          var match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
          if (match !== null && match.length === 3) {
            return match[2];
          } else {
            return null;
          }
        } catch (err) {
          console.warn("FS: unexpected exception in getcookie : " + err);
          return null;
        }
      }
