<script>
    (function(){
        var generateSlotId = function() {
            var nextSlotId = 0;

            return function () {
                return 'cmnad_' + nextSlotId++;
            }
        }();

        window.addEventListener('DOMContentLoaded', function(e) {
            var adSlots = document.querySelectorAll('[data-adtype]');

            Array.prototype.forEach.call(adSlots, function(adSlot) {
                var slotId = generateSlotId();
                var adType = adSlot.dataset.adtype;
                var lazyLoad = adSlot.dataset.lazy;

                adSlot.id = slotId;

                var adConf = {
                    containerId: slotId
                };

                if (lazyLoad) {
                    adConf['lazyLoad'] = lazyLoad == 'true' ? true : false;
                }

                CMNUNT.load(adType, adConf);
            });
        });
    })();
</script>
