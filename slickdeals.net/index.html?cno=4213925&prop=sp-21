<script type="text/javascript">
	$(document).ready(function() {
		loadAccessoryData();
	});
	function loadAccessoryData(){
		if($("#dataLoading").length > 0){
			var rootUrl = $("#pdp_rootUrl").val();
			var productCode = $("#pdp_productCode").val();
			var urlPath = rootUrl + "/p/" + productCode + "/productReferencesAjax/";
		    $.ajax({
		        type: "GET",
		        url: urlPath,
		        dataType: "html",
		        data:{
		        	showEOL:"false",
		        	saleTypeIndirecrt:"false",
		        	indirectSiteListPriceDisplay:"false",
		        	pageType:"longscroll", 
		        	isGaming:"",
		        	deliveryTimeToggleFlag:""
		        },
		        success: function(data){
		        	$("#dataLoading").hide();
		        	var compabilityLink = $('.viewcompability');
		        	if($.trim(data) == "emptyContent") {
		        		$("#dataNotExist").show();
		        		$("#dataShowListDiv").hide();
		        		$("#tab-li-currentmodels").after(compabilityLink);
		        		$("#tab-li-accessories_-_services").remove();
		        		$("#tab-li-nav-accessories_-_services").remove();
		        	} else {
		        		$("#tab-accessories_-_services").after(compabilityLink);
		        		if(window.innerWidth < 750 && $(".cascade_accessories_-_services").length>0) {
		        			$(".cascade_accessories_-_services").show();
		        		}
		        		$("#dataNotExist").hide();
		        		$("#dataShowList").html(data);
		        		$("#dataShowListDiv").show();
			        	adjustPageStyle();
			        	isImgLoad(adjustPageStyle);
			        	if(window.innerWidth < 750 && $(".cascade_accessories_-_services").length>0) {
		        			$(".cascade_accessories_-_services").hide();
		        			$(".viewcompability").hide();
		        		}
		        	}
		        }
		    });
		}
	}
	
	var t_img;
	var isLoad = true;
	function isImgLoad(callback){
		$("#dataShowList .tabbedBrowse-accessories-item-image img").each(function(){
	        if(this.height === 0){
	            isLoad = false;
	            return false;
	        }
	    });
	    if(isLoad){
	        clearTimeout(t_img);
	        callback();
	    }else{
	        isLoad = true;
	        t_img = setTimeout(function(){
	            isImgLoad(callback);
	        }, 200);
	    }
	}
	
	function adjustPageStyle(){
		var $accessoriesImages = $(".tabbedBrowse-accessories-item-image"),
			$accessoriesTitle = $(".tabbedBrowse-accessories-item-title"),
			$accessoriesDescription = $(".tabbedBrowse-accessories-item-description"),
			$accessoriesLearnMore = $(".tabbedBrowse-accessories-item .button-standard"),
			$accessoriesPricingSummary = $(".tabbedBrowse-accessories-item-container .pricingSummary"),
			$accessoriesItemColumns = $(".tabbedBrowse-accessories-item"),
			$accessoriesColumns = $(".tabbedBrowse-accessories-item-container"),
			_longscrollSettings = {
				activeTabClass             : "tab-nav-item-currentTab",
				hiddenClass                : "hidden",
				expandedContentTabClass    : "expandedContentTab",
				responsiveShowContentClass : "responsiveShowContent",
				stickyTabsClass            : "stickyTabs",
				smallBreakPointEnd         : 480,
				mediumBreakPointStart      : 481,
				mediumBreakPointEnd        : 848,
				largeBreakPointStart       : 849,
				iPadWidth                  : 1024,
				iPadHeight                 : 768,
				isInterstitial             : false
			},
			isGamingEnabled=$("#longscroll-subseries").hasClass("gaming");
		if ((_viewport().width > _longscrollSettings.mediumBreakPointEnd) && (!isGamingEnabled)) {
			equalColumnHeights($accessoriesImages);
			equalColumnHeights($accessoriesTitle);
			equalColumnHeights($accessoriesDescription);
			equalColumnHeights($accessoriesLearnMore);
			equalColumnHeights($accessoriesPricingSummary);
			equalColumnHeights($accessoriesItemColumns);
			equalColumnHeights($accessoriesColumns);
		}
		if(isGamingEnabled){
			equalColumnHeights($accessoriesLearnMore);
		}
	}
	
</script>
