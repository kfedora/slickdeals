<script>
/* add by SunYan for PPS-11978 start */
$(document).ready(function() {
	showEOLDiv();
});
/* add by SunYan for PPS-11978 end */
function showEOLDiv(){
	var EOLflag = false;
	if(EOLflag){
		//Make ColorBox responsive
		var cboxOptions = {
		  width: '95%',
		  maxWidth: '960px',
		  /*begin:add by yinlp1 for browse-1854*/
		  initialHeight: '244px',
		  transition:"elastic",
		  /*end:add by yinlp1 for browse-1854*/
		  inline:true,
		  href:$('.inline_content'),
		  onComplete:function() {
						var count = $(".seriesPreview-subtitle").length;
						$( ".seriesPreview-subtitle" ).each(function( index ) {
							var eolDescVar = $(this).parent().find(".eol-desc").text();
							$(this).text(eolDescVar);
							$(this).parent().find(".eol-desc").remove();
							if (index+1 === count) {
				 				equalheight($('.seriesPreview-title'));
				 				equalheight($('.seriesPreview-subtitle'));
							}
						});
					  }
		}
		$.colorbox(cboxOptions);
		$(window).resize(function(){
		    $.colorbox.resize({
		      width: window.innerWidth > parseInt(cboxOptions.maxWidth) ? cboxOptions.maxWidth : cboxOptions.width,
		    });
		});
	}
}
//window.onload=showEOLDiv;
</script>
