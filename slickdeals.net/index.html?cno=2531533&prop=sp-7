// eslint-disable-next-line no-unused-vars
function dlDate() {
    var d = new Date();
    var daysOfWeek = [
        'sunday',
        'monday',
        'tuesday',
        'wednesday',
        'thursday',
        'friday',
        'saturday'
    ];

    return {
        getDayOfWeek: function(dayInt) {
            return daysOfWeek[dayInt];
        },
        weekday: daysOfWeek[d.getDate()],
        currTime: d.getHours().toString() + ':' + d.getMinutes().toString(),
        date:
            d.getMonth().toString() +
            '/' +
            d.getDate().toString() +
            '/' +
            d.getFullYear().toString()
    };
}

// TODO: determine if this is still needed
// eslint-disable-next-line no-unused-vars
function mapToLowerCase(x) {
    return x.toLowerCase();
}

// TODO: determine if this is still needed
// eslint-disable-next-line no-unused-vars
function replaceAllWithEmpty(
    values,
    valueIdentifiers,
    valueIdentifiersToMatch
) {
    var newValues = values.map(function(currentValue, idx) {
        var newValue = currentValue;
        var valueIdentifier = valueIdentifiers[idx];
        var matchFound = false;

        for (var i = 0; i < valueIdentifiersToMatch.length; i++) {
            if (valueIdentifier === valueIdentifiersToMatch[i]) {
                matchFound = true;
                break;
            }
        }

        if (matchFound) {
            newValue = '';
        }

        return newValue;
    });

    return newValues;
}

function mapStockStatus(messageKey) {
    if (messageKey.indexOf('dtd') > -1) {
        var y = messageKey.split('.').pop();

        switch (y) {
            case 'inStock':
                return 'in stock';
            case 'onOrder':
                return 'on order';
            case 'lowStock':
                return '0 In Stock';
            case 'lowStockPercent':
                return 'low stock percent';
            case 'outOfStock':
                return '0 In Stock';
            case 'notAvailable':
                return 'not available';
            case 'specialOrderWCheckout':
                return 'special order with checkout';
            case 'specialOrderWOCheckout':
                return 'special order without checkout';
        }
    } else {
        return messageKey;
    }
}

// TODO: determine if this is still needed
// eslint-disable-next-line no-unused-vars
function getDrivingPriorities(priorityData) {
    var priorityMap = {
        '1': 'life of tire',
        '2': 'handling',
        '3': 'stopping distance',
        '4': 'comfort & noise'
    };
    if (priorityData !== null) {
        return (
            priorityMap[priorityData.priority1] +
            ', ' +
            priorityMap[priorityData.priority2] +
            ', ' +
            priorityMap[priorityData.priority3] +
            ', ' +
            priorityMap[priorityData.priority4]
        );
    } else {
        return null;
    }
}

// TODO: determine if this is still needed
// eslint-disable-next-line no-unused-vars
function mapTireLife(years, months) {
    var tireLifeValues = [];

    for (var i = 0; i < years.length; i++) {
        if (years[i] !== '' && months[i] !== '') {
            tireLifeValues.push(years[i] + 'yrs ' + months[i] + 'mos');
        } else {
            tireLifeValues.push('');
        }
    }
    return tireLifeValues;
}

// TODO: determine if this is still needed
// eslint-disable-next-line no-unused-vars
function getCombinedPromotions(entries) {
    var promotionMap = {
        name: [],
        amount: []
    };

    var combinedPromotionEntries = [];

    for (var i = 0; i < entries.length; i++) {
        if (entries[i].promotionData) {
            combinedPromotionEntries.push(entries[i]);
        } else if (
            entries[i].front &&
            entries[i].front.promotionData &&
            entries[i].rear &&
            entries[i].rear.promotionData
        ) {
            combinedPromotionEntries.push(entries[i].front);
            combinedPromotionEntries.push(entries[i].rear);
        }
    }

    for (var x = 0; x < combinedPromotionEntries.length; x++) {
        var promotionData = combinedPromotionEntries[x].promotionData;

        for (var y = 0; y < promotionData.length; y++) {
            var isMAPPromotion = promotionData[y].code
                .toLowerCase()
                .includes('map');
            var lineItemAmount = promotionData[y].lineItemDiscount
                ? promotionData[y].lineItemDiscount.formattedValue
                : '';
            var amount =
                isMAPPromotion && promotionData[y].mapTotalDiscount
                    ? promotionData[y].mapTotalDiscount.formattedValue
                    : lineItemAmount;

            promotionMap.name.push(promotionData[y].description);
            promotionMap.amount.push(amount);
        }
    }

    return promotionMap;
}

// used in tealium.tag
// eslint-disable-next-line no-unused-vars
function getIsPackage(entries) {
    var is_package = 0;

    for (var i = 0; i < entries.length; i++) {
        if (entries[i].hasOwnProperty('bundleProduct1')) {
            is_package = 1;
        }
    }

    return is_package;
}

/* Since the tire life value displayed on the PDP is calculated dynamically based on the user-driven miles-per-year input/slider,
 * this function replicates the calculation from the PDLData React component so the data layer knows what is displayed on page load */
// TODO: determine if this is still needed
// eslint-disable-next-line no-unused-vars
function getDefaultTireLifeString(medianMileage) {
    var defaultMilesDriven = 15000;
    var years = medianMileage
        ? Math.floor(parseInt(medianMileage) / defaultMilesDriven)
        : false;
    var months = medianMileage
        ? Math.floor(12 * ((parseInt(medianMileage) / defaultMilesDriven) % 1))
        : false;

    return years && months ? years + 'yrs ' + months + 'mos' : null;
}

function FeesCombiner() {
    this.fetFeesName = [];
    this.fetFeesPrice = [];
    this.environmentalFeesName = [];
    this.environmentalFeesPrice = [];
    this.disposalFeesName = [];
    this.disposalFeesPrice = [];
    this.requiredFeesName = [];
    this.requiredFeesPrice = [];
}

FeesCombiner.prototype.setFetFeesName = function(val) {
    this.fetFeesName = val;
};
FeesCombiner.prototype.setFetFeesPrice = function(val) {
    this.fetFeesPrice = val;
};
FeesCombiner.prototype.setEnvironmentalFeesName = function(val) {
    this.environmentalFeesName = val;
};
FeesCombiner.prototype.setEnvironmentalFeesPrice = function(val) {
    this.environmentalFeesPrice = val;
};
FeesCombiner.prototype.setDisposalFeesName = function(val) {
    this.disposalFeesName = val;
};
FeesCombiner.prototype.setDisposalFeesPrice = function(val) {
    this.disposalFeesPrice = val;
};
FeesCombiner.prototype.setRequiredFeesName = function(val) {
    this.requiredFeesName = val;
};
FeesCombiner.prototype.setRequiredFeesPrice = function(val) {
    this.requiredFeesPrice = val;
};
FeesCombiner.prototype.getNames = function() {
    return this.fetFeesName.concat(
        this.environmentalFeesName,
        this.disposalFeesName,
        this.requiredFeesName
    );
};
FeesCombiner.prototype.getPrices = function() {
    return this.fetFeesPrice.concat(
        this.environmentalFeesPrice,
        this.disposalFeesPrice,
        this.requiredFeesPrice
    );
};

function AddonCombiner() {
    this.looseName = [];
    this.looseQuantity = [];
    this.loosePrice = [];
    this.matchingName = [];
    this.matchingQuantity = [];
    this.matchingPrice = [];
    this.serviceName = [];
    this.serviceQuantity = [];
    this.servicePrice = [];
}

