    this.matchingName = val;
};
AddonCombiner.prototype.setMatchingQuantity = function(val) {
    this.matchingQuantity = val;
};
AddonCombiner.prototype.setMatchingPrice = function(val) {
    this.matchingPrice = val;
};
AddonCombiner.prototype.setServiceName = function(val) {
    this.serviceName = val;
};
AddonCombiner.prototype.setServiceQuantity = function(val) {
    this.serviceQuantity = val;
};
AddonCombiner.prototype.setServicePrice = function(val) {
    this.servicePrice = val;
};
AddonCombiner.prototype.getNames = function() {
    return this.looseName.concat(this.matchingName, this.serviceName);
};
AddonCombiner.prototype.getQuantities = function() {
    return this.looseQuantity.concat(
        this.matchingQuantity,
        this.serviceQuantity
    );
};
AddonCombiner.prototype.getPrices = function() {
    return this.loosePrice.concat(this.matchingPrice, this.servicePrice);
};

function ProductCombiner() {
    this.isStaggered = false;
    this.frontId;
    this.frontPrice;
    this.frontSize;
    this.frontStockStatus;
    this.frontBrand;
    this.frontCategory;
    this.frontClass;
    this.frontName;
    this.frontRatingAverage;
    this.frontRatingCount;
    this.frontImageUrl;
    this.frontUrl;
    this.frontQuantity;
    this.rearId;
    this.rearPrice;
    this.rearSize;
    this.rearStockStatus;
    this.rearBrand;
    this.rearCategory;
    this.rearClass;
    this.rearName;
    this.rearRatingAverage;
    this.rearRatingCount;
    this.rearImageUrl;
    this.rearUrl;
    this.rearQuantity;
    this.looseAccessoryId;
    this.looseAccessoryPrice;
    this.looseAccessorySize;
    this.looseAccessoryStockStatus;
    this.looseAccessoryBrand;
    this.looseAccessoryCategory;
    this.looseAccessoryClass;
    this.looseAccessoryName;
    this.looseAccessoryUrl;
    this.looseAccessoryQuantity;
    this.frontGbb;
    this.rearGbb;
    this.tireLifeYears;
    this.tireLifeMonths;
    this.milesPerDollar;
    this.purchaseDecisionFront;
    this.purchaseDecisionRear;
    this.productSpeedFront;
    this.productSpeedRear;
    this.coreProductFront;
    this.coreProductRear;
}

ProductCombiner.prototype.buildAttrs = function(flag, frontAttrs, rearAttrs) {
    if (frontAttrs === undefined) {
        frontAttrs = [];
    }

    if (rearAttrs === undefined) {
        rearAttrs = [];
    }

    var arr = [],
        x = 0,
        addRearAttr = flag === undefined || flag === true ? true : false;

    if (frontAttrs && frontAttrs.length > 0) {
        for (x; x < frontAttrs.length; x++) {
            arr.push(frontAttrs[x]);
            if (addRearAttr) {
                arr.push(rearAttrs[x]);
            }
        }
    }

    return arr;
};

ProductCombiner.prototype.combine = function(frontAttrs, rearAttrs) {
    if (frontAttrs === undefined) {
        frontAttrs = [];
    }

    if (rearAttrs === undefined) {
        rearAttrs = [];
    }
    var newArr = [];

    if (rearAttrs !== undefined && this.isStaggered) {
        newArr = this.buildAttrs(true, frontAttrs, rearAttrs);
    } else if (rearAttrs === undefined && this.isStaggered) {
        rearAttrs = frontAttrs.map(function(x) {
            return x;
        });
        newArr = this.buildAttrs(true, frontAttrs, rearAttrs);
    } else {
        newArr = this.buildAttrs(false, frontAttrs, rearAttrs);
    }
    return newArr;
};

ProductCombiner.prototype.populate = function(frontVal, rearVal, bothVal) {
    var set = [],
        s = 0,
        pos = '';

    if (this.frontId !== undefined && this.rearId !== undefined) {
        for (s; s < this.frontId.length; s++) {
            pos = (s + 1).toString();
            if (
                this.frontId[s] !== undefined &&
                this.frontId[s] !== '' &&
                this.rearId[s] !== undefined &&
                this.rearId[s] !== ''
            ) {
                set.push(frontVal || pos);
                set.push(rearVal || pos);
            } else {
                set.push(bothVal || pos);
            }
        }
    }
    return set;
};
ProductCombiner.prototype.set = function(attr, value) {
    this[attr] = value;
};
ProductCombiner.prototype.populateProductSet = function() {
    return this.populate('1', '1', '0');
};
ProductCombiner.prototype.populateProductPosition = function() {
    return this.populate();
};
ProductCombiner.prototype.populateAxleType = function() {
    return this.populate('front', 'rear', 'both');
};
ProductCombiner.prototype.getIds = function() {
    return this.combine(this.frontId, this.rearId);
};
ProductCombiner.prototype.getPrices = function() {
    return this.combine(this.frontPrice, this.rearPrice);
};
ProductCombiner.prototype.getSizes = function() {
    return this.combine(this.frontSize, this.rearSize);
};
ProductCombiner.prototype.getStockStatus = function() {
    return this.combine(this.frontStockStatus, this.rearStockStatus);
};
ProductCombiner.prototype.getBrands = function() {
    return this.combine(this.frontBrand, this.rearBrand);
};
ProductCombiner.prototype.getCategories = function() {
    return this.combine(this.frontCategory, this.rearCategory);
};
ProductCombiner.prototype.getClasses = function() {
    return this.combine(this.frontClass, this.rearClass);
};
ProductCombiner.prototype.getNames = function() {
    return this.combine(this.frontName, this.rearName);
};
ProductCombiner.prototype.getRatingAverages = function() {
    return this.combine(this.frontRatingAverage, this.rearRatingAverage);
};
ProductCombiner.prototype.getRatingCounts = function() {
    return this.combine(this.frontRatingCount, this.rearRatingCount);
};
ProductCombiner.prototype.getImageUrls = function() {
    return this.combine(this.frontImageUrl, this.rearImageUrl);
};
ProductCombiner.prototype.getUrls = function() {
    return this.combine(this.frontUrl, this.rearUrl);
};
ProductCombiner.prototype.getQuantities = function() {
    return this.combine(this.frontQuantity, this.rearQuantity);
};
ProductCombiner.prototype.getGoodBetterBests = function() {
    return this.combine(this.frontGbb, this.rearGbb);
};
ProductCombiner.prototype.getTireLifeYears = function() {
    return this.tireLifeYears;
};
ProductCombiner.prototype.getTireLifeMonths = function() {
    return this.tireLifeMonths;
};
ProductCombiner.prototype.getMilesPerDollar = function() {
    return this.milesPerDollar;
};
ProductCombiner.prototype.getPurchaseDecisionValues = function() {
    return this.combine(this.purchaseDecisionFront, this.purchaseDecisionRear);
};
ProductCombiner.prototype.getProductSpeedValues = function() {
    return this.combine(this.productSpeedFront, this.productSpeedRear);
};
ProductCombiner.prototype.isCoreProduct = function() {
    return this.combine(this.coreProductFront, this.coreProductRear);
};

