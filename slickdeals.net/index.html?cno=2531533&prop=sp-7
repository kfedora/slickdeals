<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900&amp;display=swap" rel="stylesheet">

	<script>
		( function() {
			
			function loadCss( resource ) {

				var insertBeforeCriticalCss = resource.beforeCriticalCss || false;
				var head = document.getElementsByTagName( 'head' )[ 0 ];
				var title = document.getElementsByTagName( 'title' )[ 0 ];
			    var link = document.createElement( 'link' );
			    link.type = 'text/css';
			    link.rel = 'stylesheet';
			    link.href = resource.url;

			    if ( insertBeforeCriticalCss ) {
			    	title.insertAdjacentHTML( 'afterend', '<link rel="stylesheet" href="'+ resource.url +'" />' );
				} else {
					head.appendChild( link );
				}
			}

			var cssResources = [
				{ url: '/_ui/responsive/theme-dt-common/dist/fonts/font-awesome.min.css', beforeCriticalCss: true },
				{ url: '/_ui/responsive/theme-dt-common/dist/css/theme.min.css?ts=2020-10-15_01-43-53' },
				{ url: '/_ui/responsive/theme-dt-common/dist/css/theme.bundle.min.css?ts=2020-10-15_01-43-53' }
			];

			for ( var c = 0; c < cssResources.length; c++ ) {
				loadCss( cssResources[ c ] );
			}

