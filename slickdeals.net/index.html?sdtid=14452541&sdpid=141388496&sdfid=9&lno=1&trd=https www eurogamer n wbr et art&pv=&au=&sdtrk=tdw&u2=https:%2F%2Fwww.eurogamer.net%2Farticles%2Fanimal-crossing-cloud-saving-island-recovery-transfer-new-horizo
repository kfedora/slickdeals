								<script>
							function checkBBVMS() {
								function youtube_parser(url){
									var regExp = /^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#\&\?]*).*/;
									var match = url.match(regExp);
									return (match&&match[7].length==11)? match[7] : false;
								}

								jQuery('article iframe').each(function(){
									src = jQuery(this).attr('src');
									if(src) {
										check_yt = youtube_parser(src); 
										if(check_yt){
											jQuery(this).attr('id', 'yt-' + check_yt);
											jQuery(this).attr('data-yt-id', check_yt);
											jQuery(this).addClass('yt-embed');
										}
									}
								});
								var promises = [];
								jQuery('article iframe.yt-embed').each(function() {
									check_yt = jQuery(this).attr('data-yt-id');
									(function(replace_yt) {
										promises.push(jQuery.getJSON( "https://gamernetwork.bbvms.com/json/search?query=%22" + replace_yt + "%22&context=all", function( data ) {
											if(typeof data['items'][0] !== 'undefined' && typeof data['items'][0].id !== 'undefined') {
												yt_video = jQuery('#yt-' + replace_yt);
												jQuery(yt_video).attr('data-bbvms-id', data['items'][0]['id']);
											};
										}));
									})(check_yt);
								});
								Promise.all(promises).then(function( data ) {
									first_yt = true;
									jQuery("[data-bbvms-id]").each(function() {
										yt_video_wrapper = jQuery(this).closest('figure.video');
										if(first_yt) {
											playout_id = 'eurogamer_auto_';
											first_yt = false;
										} else {
											playout_id = 'eurogamer';
										}
										jQuery('> div', yt_video_wrapper).append('<div class="bb_wrapper"><script type="text/javascript" src="//gamernetwork.bbvms.com/p/' + playout_id + '/c/' + jQuery(this).attr('data-bbvms-id') + '.js" async="true"><\/script></div>');
										jQuery('.video-container', yt_video_wrapper).remove();
										jQuery('.like-and-subscribe', yt_video_wrapper).html('<a href="https://www.youtube.com/watch?v=' + jQuery(this).attr('data-yt-id') + '" class="button wide" target="_blank" onclick="ga(\'send\',\'event\',\'Video\',\'Click YT Link\',\'' + jQuery(this).attr('data-yt-id') + '\')">Watch on YouTube</a>');
									});
								});

							}
