  <script>
    var stickyTOC = {
      stickyTOC_after: 80,
      init: function() {
        this.scroll();
        this.events();
      },
      scroll: function() {
        var toc = document.getElementById("toc");
        if (toc == null) { return; }
        if (window.pageYOffset > this.stickyTOC_after) {
          if (toc.className != "toc-fixed") { toc.className = "toc-fixed"; }
        } else {
          if (toc.className != "") { toc.className = ""; }
        }
      },
      events: function() {
        window.addEventListener("scroll", this.scroll.bind(this), {passive: true});
      }
    };
    // When the page is ready we initialize and bind the toc
    document.addEventListener("DOMContentLoaded", function () {
      stickyTOC.init.bind(stickyTOC)()
      // We initialize the TOC
      var tocLinks = document.querySelectorAll('#toc > li > a');
      var olList = document.querySelectorAll('#toc li ol');
      function showList(e) {
        var curol = this.parentElement.querySelector('ol');
        for(i = 0; i < olList.length; i++){
          ol = olList[i];
          if (ol === curol) { continue; }
          olList[i].className = 'toc-hide-list';
        }

        if (curol != null) {
          if (curol.className == 'toc-hide-list') {
            curol.className = " ";
          } else {
            curol.className = 'toc-hide-list';
          }
        }
      }

      for(i = 0; i < tocLinks.length; i++){
        var el = tocLinks[i];
        if (el.addEventListener) {
          el.addEventListener('click', showList, false);
        } else {
          el.attachEvent('onclick', showList);
        }
      }
    });
  </script>
