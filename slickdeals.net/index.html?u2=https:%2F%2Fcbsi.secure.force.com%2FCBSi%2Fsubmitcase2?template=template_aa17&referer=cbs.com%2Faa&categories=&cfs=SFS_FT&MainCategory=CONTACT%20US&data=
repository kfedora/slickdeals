    function removePicklistValue(){
        var cat = document.getElementById("j_id0:j_id1:j_id37:j_id38:theform:j_id69:5:j_id71");
        var subCat = document.getElementById("j_id0:j_id1:j_id37:j_id38:theform:j_id69:6:j_id71");
        for (var i = 0;i < cat.length; i++) 
        {
            if (cat.options[i].value == "Wrong Number")
                cat.remove(i);
        }
        for (var i = 0;i < subCat.length; i++) 
        {
            if (subCat.options[i].value == "Student Discount")
                subCat.remove(i);
            if (subCat.options[i].value == "Unaware of Service")
                subCat.remove(i);    
            if (subCat.options[i].value == "Death of Subscriber")
                subCat.remove(i);                  
            if (subCat.options[i].value == "Episode Unavailable (Prime)")
                subCat.remove(i);    
            if (subCat.options[i].value == "Missing Entitlements")
                subCat.remove(i);
            if (subCat.options[i].value == "Streaming Issues (Prime)")
                subCat.remove(i);
            if (subCat.options[i].value == "CF Seeing Commercials")
                subCat.remove(i);                
        }
    }
