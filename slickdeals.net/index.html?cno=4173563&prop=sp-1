        <script>

            if (!window.jQuery) {
                var script = document.createElement('script');
                script.type = "text/javascript";
                script.src = "/assets/javascripts/jquery.min.js";
                document.getElementsByTagName('head')[0].appendChild(script);
            }
        </script>
        <script>
            function func_subscribe() {
                $("#subscribetoemail").prop("disabled", true);
                var vemailaddress = $("#input-signup-email").val();
                $.ajax({
                    type: "POST",
                    url: '/Home/EmailSubcription?email_address=' + vemailaddress,
                    success: function (result) {
                        linkTrack('emailSubscription');
                        //$("#mp-footer-email-form").css('display', 'none');
                        $("#mp-footer-email-message").css('display', 'block');
                        $("#mp-footer-email-message").html(result);
                        $("#mp-footer-email-message").focus();
                        $("#subscribetoemail").prop("disabled", false);
                    },
                    error: function () {
                        $("#subscribetoemail").prop("disabled", false);
                        return true;
                    }
                });

                return false;
            }
        </script>
        <script>
            function updateitemcart(idx) {
                var param = idx;
                $('#monoMiniContent').html('<div class="loading-box"><img src="https://images.monoprice.com/assets/images/loading-img.gif" alt="loading-img" class="icn-loading" /></div>');
                $('#monoMiniContent').focus();
                $.ajax({
                    type: "POST",
                    url: '/minicart/removeitem' ,
                    data: "ca_idx=" + param,
                    success: function (result) {
                        $("#monoMiniContent").html(result.miniCart);
                        $("#monoMiniContent").focus();
                        $(window).scrollTop($('#monoMiniContent').offset().top);
                        $("#myBagCount").html(" " + result.itemCount + " items");
                        $("#myBagSubTotal").html(result.subTotal);
                        $("#top-header-item-count").html(result.itemCount);
                        $(".checkout__button").html('<i class="glyphicon glyphicon-shopping-cart"></i> ' + result.itemCount);
                        if (!$('#monoMini').hasClass('animated')) {
                            if ($('#monoMini').css('display') == "block") {
                                $('#monoMini').css('display', 'block');
                            } else {
                                $('#monoMini').css('display', 'none');
                            }
                            var contentheight = $("#monoMiniContent").height() + $("#monoMiniBottom").height();
                            var miniheight = 390;
                            if (contentheight < 390) {
                                miniheight = contentheight + 33;
                            }
                            $('#monoMini').dequeue().stop().animate({ height: miniheight + "px" }, 100);
                        }
                    },
                    error: function () {

                    }
                });
                return false;
            }

            (function () {
                var load = function () {
                    $('.js-update').click(function () {
                        $(document).ajaxComplete(function (event, xhr, settings) {
                            if (settings.url.indexOf("CartUpdatePartial") > 0) {
                                updateitemcart('');
                            }
                        });
                    });
                    updateitemcart('');
                    $('#monoMini').css('display', 'none');
                    $("ul#carttab li#cart-tab").hover(function () {
                        if ($("#top-header-item-count").html() > 0)
                            if (!$('#monoMini').hasClass('animated')) {
                                $('#monoMini').css('display', 'block');
                                var contentheight = $("#monoMiniContent").height() + $("#monoMiniBottom").height();
                                var miniheight = 390;
                                if (contentheight < 390) {
                                    miniheight = contentheight + 33;
                                }
                                $('#monoMini').dequeue().stop().animate({ height: miniheight + "px" }, 100);
                            }
                    }, function () {
                        $('#monoMini').addClass('animated').animate({ height: "0" }, 100, function () {
                            $('#monoMini').css('display', 'none');
                            $('#monoMini').removeClass('animated').dequeue();
                        });
                    });
                };

                if (window.addEventListener) {
                    window.addEventListener('load', load);
                } else {
                    window.attachEvent('onload', load);
                }

            }());
        </script>
    


<script>
    function func_addtocart(form) {
        var vqty = form.qty.value;
        var vpid = form.p_id.value;
        var intItemCountHeader = 0;
        var intItemCountHeaderNew = 0;

        var itemCountHeader = $("#top-header-item-count").text();
        if (itemCountHeader != "") intItemCountHeader = parseInt(itemCountHeader, 10);
        var itemCountHeaderNew = $(".checkout__button").text();
        if (itemCountHeaderNew != "") intItemCountHeaderNew = parseInt(itemCountHeaderNew, 10);

        if (vqty != "" && vpid != "" && $.isNumeric(vqty)) {
            var itemCount = parseInt($("#top-header-item-count").text(), 10);
            var itemCountNew = parseInt($(".checkout__button").text(), 10);
            var intvqty = parseInt(vqty, 10);
            if (intvqty > 0) {
                $.ajax({
                    type: "POST",
                    url: '/cart',
                    data: "qty=" + vqty + "&p_id=" + vpid,
                    success: function (result) {
                        updateitemcart('');
                        if (!$('#monoMini').hasClass('animated')) {
                            setTimeout(function () {
                                $('#monoMini').css('display', 'block');
                                var contentheight = $("#monoMiniContent").height() + $("#monoMiniBottom").height();
                                var miniheight = 390;
                                if (contentheight < 390) { miniheight = contentheight + 33; }
                                $('#monoMini').dequeue().stop().animate({ height: miniheight + "px" }, 100);

                                setTimeout(function () {
                                    $('#monoMini').addClass('animated').animate({ height: "0" }, 100, function () {
                                        $('#monoMini').css('display', 'none');
                                        $('#monoMini').removeClass('animated').dequeue();
                                    });
                                }, 2000);

                            }, 500);
                        }

                        itemCount += intvqty;
                        itemCountNew += intvqty;

                        $("#top-header-item-count").html(itemCount);
                        $("#top-header-item-count").focus();
                        $(".checkout__button").html('<i class="glyphicon glyphicon-shopping-cart"></i> ' + itemCountNew);
                    },
                    error: function () {
                        return true;
                    }
                });

                var B2BMiniCart = 0;
                $(document).ajaxComplete(function (event, xhr, settings) {
                    if (settings.url.indexOf("RemoveItem") > 0) {
                        if (B2BMiniCart == "0") $.slidebars.open("right");

                        var itemCounters = $("#myBagCount").text().replace(" items", "");
                        var intItemCountHeaderLast = 0;
                        var intItemCountHeaderLastNew = 0;
                        if (itemCounters != "") intItemCountHeaderLast = parseInt($("#top-header-item-count").text(), 10);
                        if (itemCounters != "") intItemCountHeaderLastNew = parseInt($(".checkout__button").text(), 10);
                        if (intItemCountHeader == intItemCountHeaderLast || intItemCountHeaderLast == intItemCountHeaderLastNew) {
                            $("#add-item-count-message").css('display', 'none');
                        }
                        else {
                            var addqty = 0;
                            addqty = intItemCountHeaderLast - intItemCountHeader;
                            if (isNaN(addqty)) addqty = intItemCountHeaderLastNew - intItemCountHeaderNew;

                            if (addqty > 0) {
                                var dpQty = addqty;
                                if (dpQty == "1") {
                                    dpQty = "1 item";
                                } else {
                                    dpQty += " items";
                                }

                                $("#add-item-count").html(dpQty + " added to Cart");
                                $("#add-item-count-message").css('display', 'block');
                            }
                            else {
                                $("#add-item-count-message").css('display', 'none');
                            }
                        }
                    }
                });
            }
        }
        return false;
    }
</script>


<script src="/assets/javascripts/jquery.min.js?v=3.0.2.112"></script>
<script src="/assets/js/ada.js?v=3.0.2.112"></script>



