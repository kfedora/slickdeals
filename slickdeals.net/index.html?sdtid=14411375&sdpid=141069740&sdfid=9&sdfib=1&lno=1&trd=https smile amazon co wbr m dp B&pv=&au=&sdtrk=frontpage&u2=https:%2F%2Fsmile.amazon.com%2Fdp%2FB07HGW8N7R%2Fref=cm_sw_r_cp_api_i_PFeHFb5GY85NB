  <script type="text/javascript"> typeof uet === 'function' && uet('bb', 'hveNavSwmCountdownWidget', {wb: 1}); </script>

  <div class="nav-swm-countdown-wrapper" style="height: 40px; width: 350px;">
    <div id="hveNavSwmCountdownWidget-50fe5686-8ed3-43bf-8a78-721cabc9d379" >
      <div id= "hveNavSwmCountdownWidget-50fe5686-8ed3-43bf-8a78-721cabc9d379-internal" style='background: url("https://images-na.ssl-images-amazon.com/images/G/01/Events/2020/APDHF2/XCM_CUTTLE_1249787_1306925_US_PD20_3285283_350x40_2X_US_en._CB403782982_.jpg") 0% 0% / 350px 40px no-repeat; max-width: 350px; height: 40px; overflow: hidden; margin: 0px auto; position: relative;'></div>
