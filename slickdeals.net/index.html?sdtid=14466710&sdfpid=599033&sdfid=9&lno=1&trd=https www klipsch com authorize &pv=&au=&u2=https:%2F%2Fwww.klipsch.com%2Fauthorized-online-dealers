<script type="text/javascript">

    function ready(fn) {
        if (document.readyState != 'loading'){
            fn();
        } else {
            document.addEventListener('DOMContentLoaded', fn);
        }
    }

    var Shopatron = {};

    
    // scripts to load after the doc is loaded and the other external scripts are loaded.
    function postLoad() {
        
        let searchOptions = {
            appId: 'VFG70POBU4',
            apiKey: 'b1277b78777db178be329bfc21bc85bf',
            indexPrefix: 'prod_',
            regionDomain: 'klipsch', 
        };
        Klipsch.app.headerSearchBoxInit(searchOptions);


        // Hide ecommerce elements if applicable
        
        // load shopping cart
                    ready(function() {
                var options = options || {};
                options.baseUrl = 'https://www.klipsch.com/';
                options.baseEcommerceUrl = 'https://checkout.klipsch.com/';
                Klipsch.app.magentoInit(options);
            });
        
        
        // powerReviews
        POWERREVIEWS.display.render(powerReviewsComponents);
    }

    function loadScripts(array, callback) {
        var loader = function(src, handler) {
            var script = document.createElement('script');
            script.src = src;
            script.onload = script.onreadystatechange = function() {
                script.onreadystatechange = script.onload = null;
                handler();
            };
            var head = document.getElementsByTagName('head')[0];
            (head || document.body).appendChild(script);
        };
        (function() {
            if (array.length != 0) {
                loader(array.shift(), arguments.callee);
            } else {
                callback && callback();
            }
        })();
    };

    // load deferred scripts
    function loadDeferredScripts() {

        var scriptList = [];

            
        loadScripts(scriptList,function(){ postLoad(); });
    }

    // attach listener for documant loaded, and trigger the deferred js loading.
    if (window.addEventListener)
        window.addEventListener("load", loadDeferredScripts, false);
    else if (window.attachEvent)
        window.attachEvent("onload", loadDeferredScripts);
    else
        window.onload = loadDeferredScripts;

</script>




