<script>
    require([
        'jquery',
        'domReady!'
    ], function($) {
        function getUrlParameter(requestedParam) {
            var pageUrl = decodeURIComponent(window.location.search.substring(1)),
                urlParams = pageUrl.split('&'),
                param;

            for (var i = 0; i < urlParams.length; i++) {
                param = urlParams[i].split('=');

                if (param[0] === requestedParam) {
                    return param[1] === undefined ? true : param[1];
                }
            }

            return '';
        };

        if (window.location.pathname.indexOf('checkout/cart') === -1) {
            var sid = getUrlParameter('sid') || getUrlParameter('SID');
            if (sid) {
                $.ajax({
                    url: 'https://www.rockler.com/urltrackingcoupon/coupon/add/',
                    type: 'POST',
                    data: {
                        'sid': sid
                    },
                    dataType: 'json'
                }).success(function (data) {
                });
            }
        }
    });
