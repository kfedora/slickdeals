                        dots: true,
                        speed: 300,
                        infinite: true,
                        slidesToShow: 3,
                        slidesToScroll: 1,
                        prevArrow:'<button class="slick-prev slick-arrow" aria-label="' + $.mage.__("Previous") + '" title="' + $.mage.__("Previous") + '" type="button"><span class="arrow" aria-hidden="true"></span><span>' + $.mage.__("Previous") + '</span></button>',
                        nextArrow: '<button class="slick-next slick-arrow" aria-label="' + $.mage.__("Next") + '" title="' + $.mage.__("Next") + '" type="button"><span class="arrow" aria-hidden="true"></span><span>' + $.mage.__("Next") + '</span></button>',

                        responsive: [
                            {breakpoint: 1400, settings: {slidesToShow: 6, slidesToScroll: 6}},
                            {breakpoint: 1024, settings: {slidesToShow: 5, slidesToScroll: 5}},
                            {breakpoint: 768, settings: {slidesToShow: 2, slidesToScroll: 2, arrows: false}},
                            {breakpoint: 400, settings: {slidesToShow: 1, slidesToScroll: 1, arrows: false}}
                        ]
                    };

                    var body = document.getElementsByTagName('body').item(0),
                        classes = body && body.classList ? body.classList.value : '';

                    // Check all body CSS class defined within `layoutVariations`
                    // if there's a match, then rewrites default `responsive` configuration
                    // if multiple keys are found, first match will be applied.
                    Object.getOwnPropertyNames(layoutVariations).some(function (variation) {
                        // if class name on `layoutVariation` matches, then rewrite configuration
                        if (classes.indexOf(variation) >= 0) {
                            slickConfiguration['responsive'] = layoutVariations[variation];
                            return true;
                        }
                    });
                }

                // we know this phtml template only renders one section at a time,
                // but it's cleaner implementing in this way
                // instead of overwriting variables
                slicks.forEach(function (sliderSelector) {
                    $(sliderSelector).slick(slickConfiguration);

                    /**
                     * Enable and Disable tabindex for Rating Reviews
                     *
                     * Given that Rating reviews `.rating-summary` link contains a `tabindex=0`
                     * to enable screen readers read each rating, hidden slides needs to be updated
                     * in order to remove the attribute. Otherwise, while navigating through slides
                     * `.rating-summary` element from hidden/cloned slides will be picked up by the
                     * Screen Reader (despite the slide is hidden, and wrapper element contains
                     * a negative `tabindex` value.
                     */
                    // Remove all tabindex from hidden slides
                    $('.slick-slide[aria-hidden=true]', sliderSelector).each(function (i,e) {
                        $(e).find('.rating-summary').removeAttr('tabindex');
                    });

                    // Update all tabindex from visible and hidden slides
                    $(sliderSelector).on('afterChange', function (event, slick) {
                        $('.slick-slide[aria-hidden=true] .rating-summary', slick.$slider).removeAttr('tabindex');
                        $('.slick-slide[aria-hidden=false] .rating-summary', slick.$slider).attr('tabindex', '0');
                    });
                    // END customisation for Rating Reviews
                });
            });
        });
    </script>
        </div>
    </div>
</div>
