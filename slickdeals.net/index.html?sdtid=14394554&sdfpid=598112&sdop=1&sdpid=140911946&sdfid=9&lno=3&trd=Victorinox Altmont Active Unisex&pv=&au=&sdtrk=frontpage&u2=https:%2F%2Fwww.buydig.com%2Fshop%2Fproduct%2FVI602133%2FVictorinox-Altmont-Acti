        function OpenWhy(p_strPage) {
            var strFeatures;
            var p_strPage;
            strFeatures = "status=no,toolbar=no,menubar=no,location=no,scrollbars=yes,resizable=no,width=470,height=250";
            oWindow = window.open(p_strPage, "FreeShipping", strFeatures);
            oWindow.focus();
            return false;
        }

