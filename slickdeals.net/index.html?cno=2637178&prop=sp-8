<script type="text/javascript">
$(document).ready(function(){
	var isTransient = 'true';
	var getUrlParameter = function getUrlParameter(sParam) {
	    var sPageURL = decodeURIComponent(window.location.search.substring(1)),
		sURLVariables = sPageURL.split('&'),
		sParameterName,
		i;

	    for (i = 0; i < sURLVariables.length; i++) {
		sParameterName = sURLVariables[i].split('=');

		if (sParameterName[0] === sParam) {
		    return sParameterName[1] === undefined ? true : sParameterName[1];
		}
	    }
	};
	var open = getUrlParameter('open');
	if(isTransient == 'true' && open == 'true') {
	  openLoginModal();
	}
});
</script>

<script>
	$(document).ready(function() {
		function prepareAutocomplete(element, referenceElementClass, isMobile) {
			var $referenceElement = $("." + referenceElementClass);
				return {
				/** BEGIN - EDIT ENDECA PREDICTIVE SEARCH PARAMS **/
				/* serviceUrl - url to get the initial json response */
				serviceUrl: '/eps/search?N=0&term=',
				/* paramName - the url for the form submission */
				paramName: '/s?Dy=1&Nty=1&Ntt=',
				/* recsServiceUrl - url for calling the product recommendations */
				recsServiceUrl: '/eps',
				/** /END - EDIT ENDECA PREDICTIVE SEARCH PARAMS **/
				appendTo: $referenceElement,
				referenceElement: $referenceElement,
				isMobile: isMobile, /* used to determine max height */
				fallBackServiceUrl: "",
				forceFallBack: false,
				width:false,
				triggerSelectOnValidInput:false,
				caseSensitive:false,
				lookupLimit: 8,
				minChars:3,
				autoSelectFirst: true,
				onSelect:function(){
					element.parents("form").submit();
				},
				showRecommendations: true,
				doClickEvents: function(){
					/* unbind any events first */
					$("body").unbind("click.search-container touchstart.search-container");
					/* bind event to listen for clicking outside the results to close the layer */
					$("body").bind("click.search-container touchstart.search-container", function(e){
						var $searchRoot = $referenceElement;
						if($searchRoot.has(e.target).length == 0) {
							$(".autocomplete-suggestions").hide();
							$("body").unbind("click.search-container touchstart.search-container");
						}
					});
				},
				/* markup configuration */
                dataConfig: epsHeaderConfig

			}
		}
		
		/* Initiate Endeca Predictive Search */
		$(document).ready(function(){
			var $keywordEl = $("#searchText");
			var $keywordElMobile = $("#searchText_mobile");
			if($.fn.autocomplete) {
				/* There are 2 calls which initiate EPS for release 2, one for both desktop and mobile **/
				$keywordEl.autocomplete(prepareAutocomplete($keywordEl, 'jsSearchRoot', false));
				$keywordElMobile.autocomplete(prepareAutocomplete($keywordElMobile, 'jsSearchRoot_mobile', true));
			}
			
			$(document).on("click" , ".eps-recommendation", function() {
				$.ajax({
					type: "post",
					url: "/global/gadgets/resetBreadcrumbs.jsp",
					error: function(error) {
						console.log("Reset Breadcrumbs");
						console.error(error)
					}
				});
			});
		});
	});
</script>


               
            
            
		   
              
             
             
				
					
			
				
					
						
					
			
            
            
            	
            	
            	
            	
            	
            	
            	
            	
            	
            	
            	
                
                
                
