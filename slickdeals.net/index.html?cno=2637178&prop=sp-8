<script>

function gotoGoogleDirections(longitude, latitude) {
          var startPoint = jQuery("#from").val() || "Current+Location";
          window.open("https://maps.google.com/maps?saddr=" + startPoint + "&daddr=" + longitude + "," + latitude);
         }

  

</script>

