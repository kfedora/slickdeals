	$('.jcarousel')
    .on('jcarousel:create jcarousel:reload', function() {
        var element = $(this), width = element.innerWidth();
		var prev = $('.jcarousel-control-prev');
        var next = $('.jcarousel-control-next');

        if (width > 768) {
            width = width / 4;
        } else if (width > 576) {
            width = width / 3;
        } else {
            width = width / 2;
        }

        element.jcarousel('items').css('width', width + 'px');
    	})
    .jcarousel({
        animation: 'slow',
		wrap: 'circular'
    });
	
	$(window).on("resize", function () {
		if (matchMedia('only screen and (min-width: 624px)').matches) {
			$('.jcarousel-control-next').jcarouselControl({
				target: '+=1'
			});
			$('.jcarousel-control-prev').jcarouselControl({
				target: '-=1'
			});
		}
		else if (matchMedia('only screen and (min-width: 448px) and (max-width: 624px) ').matches) {
			$('.jcarousel-control-next').jcarouselControl({
				target: '+=1'
			});
			$('.jcarousel-control-prev').jcarouselControl({
				target: '-=1'
			});
		}
		else if (matchMedia('only screen and (min-width: 100px) and (max-width: 448px) ').matches) {
			$('.jcarousel-control-next').jcarouselControl({
				target: '+=1'
			});
			$('.jcarousel-control-prev').jcarouselControl({
				target: '-=1'
			});
		}
		else {
			$('.jcarousel-control-next').jcarouselControl({
				target: '+=1'
			});
			$('.jcarousel-control-prev').jcarouselControl({
				target: '-=1'
			});
		}
	}).resize();
$('.jcarousel').jcarouselAutoscroll({
    autostart: true
});


$('.jcarousel').each(function() {

var carousel = $(this);

carousel.swipe({
swipeLeft: function(event, direction, distance, duration, fingerCount) { carousel.jcarousel('scroll', '+=1'); },
swipeRight: function(event, direction, distance, duration, fingerCount) { carousel.jcarousel('scroll', '-=1'); }
});
});

