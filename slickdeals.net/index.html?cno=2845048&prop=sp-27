} else if (width <= 1023) {
experienceType = 'sm';
} else if (width <= 1279) {
experienceType = 'md';
} else {
experienceType = 'xl';
}
return experienceType;
})();
for (var i = 0; i < dataLayer.length; i++) {
if (dataLayer[i].event === 'Page Loaded') {
dataLayer[i].page.experienceType = experienceType;
break;
}
}
}
</script>
