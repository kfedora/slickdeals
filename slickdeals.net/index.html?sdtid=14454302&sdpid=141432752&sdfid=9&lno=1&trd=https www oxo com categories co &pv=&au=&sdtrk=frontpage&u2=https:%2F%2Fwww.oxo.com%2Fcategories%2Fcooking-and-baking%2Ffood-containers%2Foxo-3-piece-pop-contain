    <a class="title" data-bind="text: name, attr: {href: url}, highlight"></a></script>
<script type="text/javascript">
        window.wordpress_config = {"wordpress_base_url":"https:\/\/www.oxo.com\/blog\/"};</script>
<script type="text/javascript">
        requirejs([
            'jquery',
            'Mirasvit_SearchAutocomplete/js/autocomplete',
            'underscore',
            'mage/utils/wrapper',
            'slick'
        ],
            function ($, autocomplete, _, wrapper) {
                var getBreakpoint = function () {
                    var breakpoint;
                    if (window.getComputedStyle) {
                    breakpoint = window.getComputedStyle(
                        document.querySelector('body'), ':before'
                    ).getPropertyValue('content');

                    breakpoint = parseInt(breakpoint.replace("'", "").replace('"', ''));
                    } else { 
                    breakpoint = $(window).width();
                    }
                    return breakpoint;
                };

                var searchInit = function () {
                    var nav = $('[data-search-id]'),
                        id = nav.data('search-id');

                    
                    if (nav.length) {
                        var active = $('[data-search-slick]:not(:empty):first'),
                            parent = active.closest('.searchautocomplete__index'),
                            id = parent.attr('id');

                        parent.addClass('active');
                        $('[data-search-id="' + id + '"]').addClass('active');
						
						/* This svript for special price on search popup */
						jQuery("#search_magento_catalog_product .searchautocomplete__list .searchautocomplete__item .item-column--content").each(function(index) {
						   var mainAutoSearchPriceChk = 'priceAutoSearchChk-00'+index;
						   jQuery(this).attr("id", mainAutoSearchPriceChk);
							jQuery("#"+mainAutoSearchPriceChk+" span[data-bind]").each(function() {  
								var elem = jQuery(this);
								var getVal = elem.data("bind");
								if(getVal=='attr: {class: special_price_class}')
								{
									elem.addClass('old-price');
									jQuery("#"+mainAutoSearchPriceChk+" .price[data-bind]").addClass('display-special-price');
								}
							})	
						});
						/* End this svript for special price on search popup */
                    }

                    
                    $('[data-search-slick]').each(function () {
                        var slider = $(this),
                            type = slider.data('search-slick'),
                            slides1 = (type != 'external_wordpress_post') ? 2 : 1,
                            slides2 = (type != 'external_wordpress_post') ? 3 : 1,
                            slides3 = (type != 'external_wordpress_post') ? 4 : 2,
                            slides4 = (type != 'external_wordpress_post') ? 4 : 3;

                        settings = {
                            arrows: false,
                            dots: false,
                            infinite: false,
                            mobileFirst: true,
                            responsive: [
                                {
                                    breakpoint: 767,
                                    settings: {
                                        slidesToShow: slides4
                                    }
                                },
                                {
                                    breakpoint: 599,
                                    settings: {
                                        slidesToShow: slides3
                                    }
                                },
                                {
                                    breakpoint: 479,
                                    settings: {
                                        slidesToShow: slides2
                                    }
                                }
                            ],
                            rows: 0,
                            slidesToShow: slides1,
                            speed: 200,
                            touchThreshold: 150,
                            useCSS: false
                        }

                        if (getBreakpoint() < 1024) {
                            slider.slick(settings);
                        }

                        $(window).on('resize', function () {
                            if (getBreakpoint() < 1024) {
                                if (!slider.hasClass('slick-initialized')) {
                                    slider.slick(settings);
                                }
                            } else {
                                if (slider.hasClass('slick-initialized')) {
                                    slider.slick('unslick');
                                    setTimeout(function () {
                                        $(window).trigger('resize');
                                    }, 100);
                                }
                            }
                        });
                    });

                    
                    $(document).on('click', '[data-search-id]', function () {
                        var el = $(this),
                            id = el.data('search-id'),
                            active = $('#' + id);

                        if (active.length) {
                            el.addClass('active').siblings().removeClass('active');
                            active.addClass('active').siblings().removeClass('active');
                        }

                        return false;
                    });

                    
                    $('#search_mini_form').on('click', '.show-all-results a', function(e){
                        var id = $('[data-search-id].active').data('search-id'),
                            urlId = typeof window.btoa === "function" ? btoa(id) : id;
                        $(this).attr('href', $(this).attr('href') + '&index=' + urlId);
                    });
                };

                var parseBlogPosts = _.debounce(function() {
                    var container = $('#searchautocomplete__autocomplete .wrapper'),
                        postIds = container.find('[data-blog-post-id]').not('.rendered').map(function(){ return $(this).data('blog-post-id'); }).get();

                    if(postIds.length > 0 && window.wordpress_config.wordpress_base_url) {
                        var endpoint = window.wordpress_config.wordpress_base_url.trim('/') + '/wp-json/wp/v2/posts?per_page=50&include[]=' + postIds.join('&include[]=') + '&_embed';
                        $.getJSON(endpoint, function(data){
                            $.each(data, function(key, post){
                                var el = container.find('[data-blog-post-id="' + post.id + '"]');
                                el.addClass('rendered');
                                el.find('[data-blog-post-image]').css('background-image', "url('" + post._embedded['wp:featuredmedia'][0].source_url + "')");
                                el.find('[data-blog-post-category]').html(post._embedded['wp:term'][0][0].name);
                            });
                        });
                    }
                }, autocomplete.config.delay);

                var newMethod = wrapper.wrap(autocomplete.processApplyBinding, function(_super, data) {
                    _super(data);
                    parseBlogPosts();
                    searchInit();
                });

                autocomplete.processApplyBinding = newMethod;

                $(document).ready(function () {
                    var $body = $('body');
                    var $input = $('input#search');

                    autocomplete.init({"query":"","priceFormat":{"pattern":"$%s","precision":2,"requiredPrecision":2,"decimalSymbol":".","groupSymbol":",","groupLength":3,"integerRequired":false},"minSearchLength":3,"url":"https:\/\/www.oxo.com\/searchautocomplete\/ajax\/suggest\/","storeId":"1","delay":300,"layout":null,"popularTitle":"Hot Searches","popularSearches":[],"isTypeaheadEnabled":null,"typeaheadUrl":"https:\/\/www.oxo.com\/searchautocomplete\/ajax\/typeahead\/","minSuggestLength":2});

                    $input.on('keyup click focus', function () {
                        $body.addClass('searchautocomplete__active');

                        var result = autocomplete.search();

                        setTimeout(function () {
                            if (result) {
                                $('.searchautocomplete__autocomplete').addClass('_active');
                            } else {
                                $('.searchautocomplete__autocomplete').removeClass('_active');
                            }
                        }, 200)
                    });

                    $(document).click(function (event) {
                        if (!$(event.target).closest('#search_mini_form').length) {
                            $body.removeClass('searchautocomplete__active');
                            $('.searchautocomplete__autocomplete').removeClass('_active');

                            $input.blur();

                            $('#search_mini_form label').removeClass('active');
                        }
                    });
                });
            }
        );</script>
<script>
require([
  'fancybox',
  'jquery',
  'modernizr',
  'Magento_Customer/js/customer-data',
  'fastclick',
  'mage/translate',
  'jquery/jquery.cookie',
  'easing',
  'elevatezoom',
  'slick',
  'intersectionobserver'
], function (fancybox, $, Modernizr, customerData, FastClick) {
  $(function () {
    
    
    var el = document.documentElement,
      userAgent = navigator.userAgent;

    el.setAttribute('data-useragent', userAgent);
    el.setAttribute('data-platform', navigator.platform);

    
		FastClick.attach(document.body);

    
    Modernizr.addTest('mobile', function () {
      return navigator.userAgent.match(/(iPad|iPhone|iPod|Android|webOS|BlackBerry|IEMobile|Opera Mini)/g);
    });

    
    Modernizr.addTest('ios', function () {
      return navigator.userAgent.match(/(iPad|iPhone|iPod)/g);
    });

    
    Modernizr.addTest('iphone', function () {
      return navigator.userAgent.match(/(iPhone)/g);
    });

    
    Modernizr.addTest('safari', function () {
      return !navigator.userAgent.match(/Chrome/i)&&!navigator.userAgent.match(/iPhone|iPad|iPod/i)&&!!navigator.userAgent.match(/Safari/i);
    });

    
    Modernizr.addTest('firefox', function () {
      return navigator.userAgent.match(/firefox/i);
    });

    
    var gridOverlay = document.querySelector('[data-grid-overlay]');
    if (gridOverlay && (window.location.hostname === 'localhost' || window.location.hostname === 'oxo.test')) {
      $(document).on('keyup', function(e) {
        if (e.ctrlKey && e.keyCode == 76) {
          gridOverlay.classList.toggle('grid--off');
        }
      });
    }

    $(document).on('keydown', function(e){
      if (e.shiftKey) {
        $('body').addClass('hint-padding');
      }
    });

    $(document).on('keyup', function(e){
      $('body').removeClass('hint-padding');
    });

     
     $(document).on('click', 'a[href^=\\#]:not([href=\\#])', function(e) {
         e.preventDefault();
         var destination = $($(this).attr('href')),
             header;

         if (destination.length > 0) {
             header = $('header.page-header');

             $('body, html').animate({
                 scrollTop: ((destination.offset().top - header.outerHeight()) - parseInt(header.css('margin-top'))) - 10
             }, 700, 'easeOutQuad', function() {
                 
                 
                 destination.focus();
             });
         }
     });

    
    $('a[target=_blank]').each(function () {
      var rel = $(this).attr('rel');

      if (typeof rel === typeof undefined || rel === false) {
        $(this).attr('rel', 'noopener noreferrer');
      } else {
        rel = rel.replace(/noopener/g, '').replace(/noreferrer/g, '').replace(/^\s+|\s+$/gm, '');
        var value = rel.length ? ' noopener noreferrer' : 'noopener noreferrer';
        $(this).attr('rel', rel + value);
      }
    });

    
    $(document).on('click', '[data-fancybox]', function () {
      $('html').css('overflow', 'hidden');
    });

    $('[data-fancybox]').fancybox({
      afterClose : function() {
        $('html').css('overflow', '');
      }
    });


	
	$('.ContactUsPopup').fancybox({
        trapFocus: false,
		beforeLoad : function() {
			$('.loading-image').show();
			$('.captcha-reload').trigger('click');
      $(".contact-what-help-usa").val('').change();
      $(".contact-what-help").val('').change();

		},
		afterLoad : function() {
			$('.loading-image').hide();
			$(':input','#enquire_usa').not(':button, :submit, :reset, :hidden').val('').removeAttr('checked').removeAttr('selected');
			$(':input','#enquire_international').not(':button, :submit, :reset, :hidden').val('').removeAttr('checked').removeAttr('selected');
		}
	});
    
	$('a, :button').on('click touchend', function () {
        var captchaInterval = setInterval(function(){
            var $reCaptchaIframe = $('iframe[title="recaptcha challenge"]').last();
            $reCaptchaIframe.parent().css('position', 'fixed');
        }, 500);

        
        setTimeout(function(){
            clearInterval(captchaInterval)
        }, 1500)
    })

    
    $(document).on('swatch.initialized', function () {
      var swatches = $('.swatch-opt .product-color__list .product-color__item');
      if('URLSearchParams' in window) {
        var params = new URLSearchParams(window.location.search);
        if(params.get('color')) {
          var swatch = swatches.find('[title="' + params.get('color') + '"]').first().parent();
          if(swatch.length > 0) {
            return swatch.trigger('click');
          }
        }
		else if(window.location.hash)
		{
			var optionLabelCustom = window.location.hash.substr(1);
			console.log(optionLabelCustom);
			var swatch = $('.swatch-opt .product-color__list').find('li.product-color__item[data-name="' + optionLabelCustom + '"]').first();
			  if(swatch.length > 0) {
				return swatch.trigger('click');
			  }
		}
      }
      swatches.first().trigger('click');
    });

    
    var addthis_config = addthis_config || {};
    var addthisScript = document.createElement('script');
    addthis_config.pubid = 'ra-5b60bfc823a84458';
    addthis_config.data_track_clickback = true;
    addthisScript.setAttribute('src', 'https://s7.addthis.com/js/300/addthis_widget.js#domready=1');
    document.body.appendChild(addthisScript);

    
    var getBreakpoint = function () {
      var breakpoint;
      if (window.getComputedStyle) {
        breakpoint = window.getComputedStyle(
          document.querySelector('body'), ':before'
        ).getPropertyValue('content');

        breakpoint = parseInt(breakpoint.replace("'", "").replace('"', ''));
      } else { 
        breakpoint = $(window).width();
      }
      return breakpoint;
    };

    var dataToggle = function (e) {
      var speed = 400,
        toggleData = e.data('toggle'),
        container = e.closest(':has(' + toggleData + ')');

      
      if (e.data('toggle-siblings')) {
        var siblings = e.closest(e.data('toggle-siblings')).siblings().find(toggleData);
        siblings.slideUp(speed).closest('.active-toggle').removeClass('active-toggle');
      }

      
      e.closest(container).toggleClass('active-toggle').find(toggleData).slideToggle(speed);
    };

    $('[data-toggle-class]').on('click', function () {
      var el = $(this),
        className = el.data('toggle-class'),
        
        target = typeof (el.data('toggle-class-on')) === 'undefined' ? $('html') : $(el.data('toggle-class-on'));

      
      target.toggleClass(className);
      return false;
    });

    $('[data-toggle]').on('click keydown', function () {
      var keyCode = (event.keyCode ? event.keyCode : event.which);

      
      if (keyCode === 1 || keyCode === 13) {
          var toggleMaxWidth = typeof ($(this).data('toggle-max-width')) !== 'undefined' ? $(this).data('toggle-max-width') : null;
          var window_width = getBreakpoint();

          if(isNaN(window_width)) {
              var window_width = $(window).width();
          }

          
          if (toggleMaxWidth !== null && window_width < toggleMaxWidth || toggleMaxWidth === null) {
            dataToggle($(this));
            return false;
          }
      }
    });

    
    $(document).on('click', '.header.content > .nav-toggle', function () {
      $('html').addClass('menu-active menu-transition');
      setTimeout(function () {
        $('html').removeClass('menu-transition');
      }, 300);
    });

    
    $(document).on('click', '.nav-sections-item-title, [data-close-mobile-menu], .menu-active .page-header', function () {
      $('html').addClass('menu-transition');
      $('html').removeClass('menu-active');
      setTimeout(function () {
        $('html').removeClass('menu-transition');
      }, 300);
    });

    
    $(document).on('click', '.navigation__item--parent > a', function () {
      $('html').addClass('submenu-active');
      $(this).closest('.navigation__item--parent').children('.navigation__column').addClass('navigation__column--active');
  		var cal_window_width =	getBreakpoint();
  		if(isNaN(cal_window_width))
  		{
  			var cal_window_width =	$(window).width();
  		}

      if($(this).closest('.navigation__item--parent').is('.hover') && cal_window_width >= 800) {
        return true;
      }
      return false;
    });

    
    $(document).on('click', '.navigation__column > a', function () {
      $(this).closest('.navigation__column').removeClass('navigation__column--active');
      setTimeout(function () {
        $('html').removeClass('submenu-active');
      }, 300);
      return false;
    });

    
    $(document).on('click', '.navigation__inner-item--level1.navigation__inner-item--parent > a', function () {
      var cal_window_width_toggle =  getBreakpoint();
      if(isNaN(cal_window_width_toggle))
      {
        var cal_window_width_toggle =  $(window).width();
      }
      console.log('Mobile Menu - Sub Slide Toggle ', cal_window_width_toggle);
      if (cal_window_width_toggle < 800) {
        var link = $(this);
        link.toggleClass('active-toggle');
        link.next('.navigation__inner-list--level2').slideToggle();
        return false;
      }
    });

    
    $('.navigation__item--parent').each(function () {
      if (getBreakpoint() >= 800) {
        var nav = $(this),
          link = nav.find('> a'),
          drop = nav.find('.navigation__column'),
          timer;

        link.on('mouseenter', function () {
          var li = $(this).closest('li');

          li.siblings().removeClass('hover');

          timer = setTimeout(function(){
            li.addClass('hover');
          }, 600);
        });

        link.on('mouseleave', function () {
          li = $(this).closest('li');

          li.removeClass('hover');
          clearTimeout(timer);
        });

        link.on('touchstart', function () {
          var dropdown = $(this).next('.navigation__column'),
            li = $(this).closest('li');

          li.siblings().removeClass('hover');
          li.toggleClass('hover');

          if (dropdown.length) {
            return false;
          }
        });

        drop.on('mouseenter', function () {
          li = $(this).closest('li');

          li.addClass('hover');
        });

        drop.on('mouseleave', function () {
          li = $(this).closest('li');

          li.removeClass('hover');
        });
      }
    });

    
    $(window).on('resize', function () {
      $('.navigation__inner-list--level1').each(function () {
        var list = $(this),
          item = list.find('.navigation__inner-item--level1'),
          columnizeList = list.find('.navigation__columnize-list');

        var createColumns = function () {
          var columnCountTarget = getBreakpoint() < 1024 ? 3 : 4,
            currentColumnCount = list.attr('data-menu-columns'),
            columnItem = item.not('.navigation__inner-item--all'),
            itemCount = columnItem.length,
            startWrap = 2, 
            endWrap = itemCount,
            average = itemCount / columnCountTarget;

          if (itemCount > 2) {
            
            if (columnItem.last().find('img').length == 0) {
              endWrap++;
            } else {
              average = (itemCount - 1) / columnCountTarget;
            }

            
            
            if (columnItem.eq(1).find('img').length && itemCount <= 5) {
              endWrap++;
            }

            if (columnizeList.length == 0) {
              item.slice(startWrap, endWrap).wrapAll('<ul class="navigation__columnize-list"><li class="navigation__columnize-item"></li></ul>');
            }

            columnizeList = list.find('.navigation__columnize-list');
            var columnizeItem = columnizeList.find('.navigation__inner-item--level1'),
              columnizeItemCount = columnizeItem.length;

            if (columnCountTarget != currentColumnCount) {
              var column = columnizeList.find('.column');

              list.attr('data-menu-columns', columnCountTarget);

              if (column.length) {
                column.find('.navigation__inner-item--level1').unwrap();
              }

              
              
              if (columnItem.eq(1).find('img').length) {
                list.addClass('navigation__inner-list--images');
                columnizeItemCount = 4;
                average = 1;
              }

              for (var i = 0; i < columnizeItemCount; i += average) {
                columnizeItem.slice(i, i + average).wrapAll('<ul class="column" />');
              }
            }
          }
        };

        var deleteColumns = function () {
          columnizeList = list.find('.navigation__columnize-list');
          var nestedItem = columnizeList.find('.navigation__inner-item'),
            column = columnizeList.find('.column');

          list.attr('data-menu-columns', 0);
          list.removeClass('navigation__inner-list--images');

          if (column.length) {
            column.find('.navigation__inner-item--level1').unwrap();
          }

          if (columnizeList.length) {
            nestedItem.first().unwrap();
            nestedItem.first().unwrap();
          }
        };

        var hideImageColumn = function () {
          var columnItem = item.not('.navigation__inner-item--all'),
            itemCount = columnItem.length;

          if (columnItem.last().find('img').length) {
            columnItem.last().addClass('navigation__inner-item--hide-mobile');
          }

          
          if (columnItem.eq(1).find('img').length && itemCount <= 5) {
            columnItem.last().removeClass('navigation__inner-item--hide-mobile');
          }
        };

        if (typeof list.attr('data-menu-columns') === 'undefined') {
          list.attr('data-menu-columns', 0);
        }

        if (getBreakpoint() >= 800) {
          createColumns();
        } else {
          deleteColumns();
          hideImageColumn();
        }
      });
    }).resize();

    
    $('header.page-header').each(function () {
      var header = $(this);

      $(window).on('resize scroll', function () {
        var body = $('body'),
          html = $('html'),
          content = header.find('.header.content'),
          contentOffset = content.offset().top,
          windowTop = $(window).scrollTop(),
          position = Math.ceil(contentOffset - windowTop),
          panel = header.find('.panel.header'),
          panelHeight = panel.length ? panel.outerHeight() : 0,
          headerHeight = header.outerHeight();

        
        if (position <= 0) {
          body.css('margin-top', headerHeight);
          html.addClass('header-fixed');
          header.css('margin-top', -panelHeight);
        }

        
        if (windowTop <= panelHeight) {
          body.css('margin-top', '');
          html.removeClass('header-fixed');
          header.css('margin-top', '');
        }
      }).resize();
    });

    
    $('.sales-order-history').each(function () {
      var el = $(this),
        base = el.find('.block-base'),
        total = $('.block-last-order').length,
        x = 2;

      if (total > x) {
        base.show();
      }

      $(document).on('click', '.block-base__button', function () {
        var increment = 2;
        x = (x + increment <= total) ? x + increment : total;

        $('.block-last-order:lt(' + x + ')').show();

        if (x >= total) {
          base.hide();
        }
      });
    });

    
    $(document).on('click touchstart', 'label.sorter-label', function () {
      if (getBreakpoint() < 800) {
        var html = $('html');

        html.addClass('sort-transition');
        html.toggleClass('sort-active');

        setTimeout(function () {
          html.removeClass('sort-transition');
        }, 300);
      }
    });

    
    $(document).on('click touchstart', '[data-product-sort]', function () {
      var sort = $(this);

      if (getBreakpoint() >= 800) {
        sort.toggleClass('hover');
      }
    });

    $('html').on('click', function (e) {
      var el = $(e.target),
        sort = $('[data-product-sort]');

      if (getBreakpoint() >= 800) {
        if (el.hasClass('') === false && el.attr('class') != 'sort__title') {
          sort.removeClass('hover');
        }
      }
    });

    
    $(document).on('click', '[data-product-filter-close], [data-product-filter-open]', function () {
      var html = $('html');

      html.addClass('filter-transition');
      html.toggleClass('filter-active');

      setTimeout(function () {
        html.removeClass('filter-transition');
      }, 300);
    });

    
    $(document).on('click', '[data-filter-toggle]', function () {
      if (getBreakpoint() >= 800) {
        var el = $(this);

        el.parent().toggleClass('active-toggle');
        el.next().slideToggle();
      }
    });
    if($('[data-filter-level]').data('filter-level') > 2) {
      $('[data-filter-toggle]').trigger('click');
    }

    
    $('[data-toolbar-sorter]:first').each(function () {
      var sorter = $(this);

      $(window).on('resize scroll', function () {
        sorter.removeClass('sorter--fixed');

        var header = $('.header.content'),
          sorterOffset = sorter.offset().top,
          windowTop = $(window).scrollTop(),
          position = Math.ceil(sorterOffset - windowTop),
          headerHeight = header.outerHeight();

        if (position <= headerHeight) {
          sorter.addClass('sorter--fixed');
        } else {
          sorter.removeClass('sorter--fixed');
        }
      }).resize();
    });

    
    $('[data-product-hero-main]').each(function () {
      var slider = $(this);

      
      var zoom = function () {
        $('.zoomContainer').remove();

        if (getBreakpoint() >= 768) {
          $('.slick-current').find('[data-zoom-image]').elevateZoom({
            zoomType: 'inner'
          });
        }
      };

      $(document).on('zoom:reInit', function () {
        zoom();
      });

      var pause = function(slick) {
        slick.$slides.find('video').each(function () {
          this.pause();
        });
      }

      var play = function(slick) {
        slick.$slides.filter('.slick-active').find('video').each(function () {
          this.play();
        });
      }

      
      var dots = function () {
        var item = slider.find('li'),
          count = parseInt(item.length),
          width = 100 / count;

        if (count > 1) {
          item.css({ width: width + '%' });
        } else {
          item.css({ width: 0 });
        }
      };

      slider.on('afterChange', function (event, slick, currentSlide, nextSlide) {
        zoom();
        play(slick);
      });

      slider.on('beforeChange', function (event, slick, currentSlide, nextSlide) {
        $('[data-product-hero-thumb-id]').removeClass('active');
        $('[data-product-hero-thumb-id="' + nextSlide + '"]').addClass('active');
        pause(slick);
      });

      slider.on('init reInit', function (event, slick, currentSlide, nextSlide) {
        dots();
        slider.removeClass('product-hero-main--loading');
        zoom();
        pause(slick);
        play(slick);
      });

      slider.slick({
        adaptiveHeight: true,
        dots: true,
        fade: true,
        infinite: false,
        mobileFirst: true,
        rows: 0,
        slidesToShow: 1,
        speed: 200,
        useCSS: false,
        prevArrow: '<button type="button" class="slick-prev" aria-label="Previous"><span class="visually-hidden">Previous</span></button>',
        nextArrow: '<button type="button" class="slick-next" aria-label="Next"><span class="visually-hidden">Next</span></button>'
      });

      
      $('.product-hero-thumb-column').on('click', '[data-product-hero-thumb-id]', function () {
        var thumb = $(this),
          id = thumb.data('product-hero-thumb-id');

        slider.slick('slickGoTo', id);
      });

      
      $('[data-product-hero-thumb]').on('click', '[data-fancybox]', function () {
        pause(slider.slick('getSlick'));
      });

      var resizeId;
      $(window).on('resize', function () {
        clearTimeout(resizeId);
        resizeId = setTimeout(zoom, 500);
      }).resize();
    });

    
    $(document).on('click', '[data-modal-page]', function () {
      $.getJSON($(this).data('modal-page'), function(data) {
		var $pageContentHTML = $.parseHTML(data.content)[0].innerHTML;
		var $pageContent = $($pageContentHTML).text();
        $.fancybox.open('<div class="modal-content">' + $pageContent + '<div class="modal-content-close"><button class="modal-content-close__btn" data-fancybox-close>Back to Shopping</button></div></div>', {touch: false});
      });
      return false;
    });

    
    $('[data-product-hero-thumb]').each(function () {
      var slider = $(this),
        item = slider.find('.product-hero-thumb__item');

      if (item.length > 6) {
        var settings = {
          adaptiveHeight: true,
          arrows: true,
          dots: false,
          infinite: false,
          mobileFirst: true,
          slidesToShow: 6,
          speed: 300,
          touchThreshold: 150,
          useCSS: false,
          vertical: true,
          verticalSwiping: true,
          prevArrow: '<button type="button" class="slick-prev" aria-label="Previous"><span class="visually-hidden">Previous</span></button>',
          nextArrow: '<button type="button" class="slick-next" aria-label="Next"><span class="visually-hidden">Next</span></button>'
        };

        $(window).on('resize', function () {
          if (getBreakpoint() >= 768) {
            if (!slider.hasClass('slick-initialized')) {
              slider.slick(settings);
            }
            slider.slick('setPosition');
          } else {
            if (slider.hasClass('slick-initialized')) {
              slider.slick('unslick');
            }
          }
        }).resize();
      }
    });

    
    $('[data-product-social]').each(function () {
      var sort = $(this);

      sort.on('mouseenter', function () {
        sort.addClass('hover');
      });

      sort.on('mouseleave', function () {
        sort.removeClass('hover');
      });

      sort.on('touchstart', function () {
        sort.toggleClass('hover');
      });
    });

    
    $('.product-info-main.product-info-main--no-mobile').attr('data-sticky-menu', '');

    
    $('[data-product-hero-thumb-id="0"]').each(function () {
      var klass = 'product-detail-mobile-sticky-nav',
        el,
        tmpl = '<div class="' + klass + '" data-' + klass + '>' +
          '<div class="' + klass + '__product-wrapper">' +
            '<img data-sticky-image class="' + klass  + '__image" alt="Product Image" />'+
            '<h4 data-sticky-title class="' + klass + '__title"></h4>' +
            '<button class="' + klass + '__button"><span data-sticky-button></span></button>' +
          '</div>' +
          '<div class="' + klass + '__nav-wrapper" data-active-scroll-watch>' +
          '</div>' +
        '</div>';

      el = $(tmpl);
      el.find('[data-sticky-image]').attr('src', $('[data-product-hero-thumb-id="0"] img').attr('src'));
      el.find('[data-sticky-title]').text($('[data-ui-id="page-title-wrapper"]').first().text());

      if ($('#product-addtocart-button').length == 0 || $('#product-addtocart-button').is(':disabled')) {
        el.find('[data-sticky-button]').parent().remove();
      } else {
        el.find('[data-sticky-button]').text($('#product-addtocart-button').text()).on('click', function () {
          $('#product-addtocart-button').trigger('click');
          $('body, html').animate({
            scrollTop: 0
          }, 700, 'easeOutQuad');
        });
      }

      el.find('[data-active-scroll-watch]').html($('.product-info-main-container--links').children().html());

      $('body').append(el);

      observer = new IntersectionObserver(function(entries, observer) {
        $.each(entries, function(key, entry){
          if(entry.intersectionRatio > 0.5) {
            $('body').removeClass('product-detail-mobile-sticky-nav--visible');
          } else {
            $('body').addClass('product-detail-mobile-sticky-nav--visible');
          }
        });
      }, {threshold: 0.5, rootMargin: '-60px 0px 0px 0px'});

      observer.observe($('.product-hero-main__image').get(0));

      $(document).on('zoom:reInit', function () {
        el.find('[data-sticky-image]').attr('src', $('[data-product-hero-thumb-id="0"] img').attr('src'));
      });
    });

    
    (function(){
      var empty = $('.catalog-product-view .block-product-detail-top');
      if(empty.length == 0) {
        $('.product-details-columns').addClass('product-details-columns--no-border');
      }
    })();

    
    $('[data-sticky-menu]').each(function () {
      var el = $(this),
        breakpoint = getBreakpoint(),
        parent = el.parent(),
        stopper = $('[data-sticky-menu-limit]'),
        win = $(window),
        position = 'top',
        offsetTop = $('header').outerHeight(),
        stopAt,
        debounce,
        offset;

      
      
      stopper = stopper.eq(Math.min(2, stopper.length) - 1);

      

      
      setInterval(function () {
        var stopPosition = stopper.offset();
        if (stopAt.top !== stopPosition.top) {
          
          reset();
          win.trigger('scroll');
        }
      }, 2500);

      var reset = function () {
        
        
        parent.css('height', '');
        
        var classes = el.attr('class');
        
        el.removeClass('product-info-main--fixed product-info-main--absolute');
        
        stopAt = stopper.offset();
        
        el.css({top: '', right: '', width: '', height: ''});
        
        height = el.outerHeight();
        width = el.outerWidth();
        offset = el.offset();
        
        if(breakpoint >= 768) {
          parent.css('height', parent.outerHeight());
        }
        
        el.attr('class', classes);
        el.css('width', width);
        
        offsetTop = $('header').outerHeight();
        
        if (position === 'bottom') {
          bottom();
        }
        if (position === 'fixed') {
          stick();
        }
      }

      
      var stick = function () {
        if (breakpoint < 768) {
          return;
        }
        
        
        el.removeClass('product-info-main--absolute');
        el.addClass('product-info-main--fixed');
        el.css({
          top: '',
          right: breakpoint >= 1920 ? win.width() - offset.left - width : ''
        });
        position = 'fixed';
      }

      
      var top = function () {
        
        position = 'top';
        el.removeClass('product-info-main--fixed');
        
        reset();
      }

      
      var bottom = function () {
        if (breakpoint < 768) {
          return;
        }
        
        el.removeClass('product-info-main--fixed');
        el.addClass('product-info-main--absolute');
        el.css({
          top: stopAt.top - el.outerHeight() - offset.top + 40,
          right: ''
        });
        position = 'bottom';
      }

      reset();

      win.on('sticky:reset', reset);

      
      win.on('resize', function (e) {
        clearTimeout(debounce);
        debounce = setTimeout(function () {
          breakpoint = getBreakpoint();
          reset();
          win.trigger('scroll');
        }, 200);
      });

      
      $(document).on('swatch.initialized', function () {
        win.trigger('resize');
      });

      
      win.on('scroll', function (e) {
        var scroll = win.scrollTop();

        
        if (position == 'top') {
          if (scroll + offsetTop > offset.top) {
            return stick();
          }
        }

        
        if (position == 'bottom') {
          if (scroll + height + offsetTop < stopAt.top) {
            return stick();
          }
        }

        if (position == 'fixed') {
          
          if (scroll + offsetTop <= offset.top) {
            return top();
          }

          
          if (scroll + height + offsetTop >= stopAt.top) {
            return bottom();
          }
        }
      });
    });

    
    $('.no-touchevents').each(function () {
      $(document).on('mouseenter mouseleave', '[data-toggle-cart-tooltip]', function (e) {
        var el = $(this),
          type = e.type;

        if (type == 'mouseenter') {
          el.parent().addClass('active-tooltip');
        } else {
          el.parent().removeClass('active-tooltip');
        }
      });

      $(document).on('mouseenter mouseleave', '[data-cart-tooltip]', function (e) {
        var el = $(this),
          type = e.type;

        if (type == 'mouseenter') {
          el.parent().addClass('active-tooltip');
        } else {
          el.parent().removeClass('active-tooltip');
        }
      });
    });

    $('.touchevents').each(function () {
      $(document).on('touchstart', '[data-toggle-cart-tooltip]', function () {
        var el = $(this),
          type = el.data('toggle-cart-tooltip');

        if (type == 'close') {
          el.parent().parent().removeClass('active-tooltip');
        } else {
          el.parent().toggleClass('active-tooltip');
        }
      });
    });

    
    $('.checkout-cart-index button.action.checkout span').each(function () {
      $(this).text('Checkout');
    });

    
    $(document).on('click', '.mfp-container .action.remind', function () {
      $('.mfp-container').addClass('mfp-container--forgot');
    });

    $(document).on('click', '.mfp-container .action.back', function () {
      $('.mfp-container').removeClass('mfp-container--forgot');
    });

    
    $('.footer.content .copyright').each(function () {
      var copyright = $(this),
        container = $('.footer-bottom-links');

      if (typeof container !== 'undefined') {
        container.append(copyright);
      }
    });

    
    $('[id]').each(function (i) {
      var el = $('[id="' + this.id + '"]:gt(0)'),
        label = el.next('label'),
        newId = this.id + '_' + i;

      el.attr('id', newId);

      if(!label.length) {
        label = $('[for="' + this.id + '"]:gt(0)');
      }

      if (label.length) {
        label.attr('for', newId);
      }
    });

    
    $('[data-end-date]').each(function () {
      var el = $(this),
        date = el.attr('data-end-date'),
        hideDate = Date.parse(date);

      if (date && hideDate > new Date().getTime()) {
        el.addClass('promo-banner--active');
      }
    });

    $('[data-promo-banner]').each(function () {
      var promoBanner = $(this);

      if ($.cookie('hide-bluefoot-' + promoBanner.attr('data-promo-banner'))) {
        promoBanner.removeClass('promo-banner--active');
      } else {
        promoBanner.addClass('promo-banner--active');
      }
    });

    $(document).on('click', '[data-promo-banner-close]', function () {
      var el = $(this),
        container = el.closest('[data-promo-banner]');

      container.slideUp();
      $.cookie('hide-bluefoot-' + $(this).closest('[data-promo-banner]').data('promo-banner'), 1, {'expires': 365});
    });

    
    (function() {
      var sections = $('[data-active-scroll-watch]').find('a[href^="#"]'),
          observer;

      observer = new IntersectionObserver(function(entries, observer) {
        $.each(entries, function(key, entry){
          if(entry.intersectionRatio > 0.5) {
            $('[data-active-scroll-watch] [href="#' + $(entry.target).attr('id') + '"]').addClass('active').siblings('.active').removeClass('active');
          }
        });
      }, {threshold: 0.5});

      sections.each(function() {
        var el = $($(this).attr('href'));
        if(el.length > 0) {
          observer.observe(el.get(0));
        } else {
          $(this).remove();
        }
      });
    })();

    
    if($('body').is('.checkout-cart-index')) {
      $(document).on('click', '[data-configurable-edit]', function(e){
        
        
        var container = $(this).closest('[data-configurable-item]');
        $(this).hide();
        container.find('[data-configurable-controls]').show();
        container.find('[data-configurable-label]').hide();
        container.find('select').hide();

        return false;
      }).on('click', '[data-configurable-cancel]', function(e) {
        
        
        var container = $(this).closest('[data-configurable-item]'),
            val = container.find('select').val();
        container.find('[data-configurable-value="' + val + '"]').trigger('click');
        container.find('[data-configurable-controls]').hide();
        container.find('[data-configurable-edit]').show();
        container.find('[data-configurable-label]').show();

        return false;
      }).on('click', '[data-configurable-update]', function(e){
        
        
        var container = $(this).closest('[data-configurable-item]'),
            active = container.find('.active [data-configurable-value]');
        container.find('select').val(active.data('configurable-value'));
        container.find('[data-configurable-cancel]').trigger('click');

        return false;
      }).on('click', '[data-configurable-value]', function(e){
        
        var container = $(this).closest('[data-configurable-item]');
        container.find('[data-configurable-option]').removeClass('active');
        $(this).closest('[data-configurable-option]').addClass('active');

        return false;
      });
    }

    $(document).on('hover', '.navigation__item--parent', function() {
        if ($('.checkout-onepage-success').length) {
            $(this).toggleClass('hover');
        }
    });

    var navigation = $('.navigation');
    $(document).on('click', '.navigation__button', function(e) {
      var target = $(e.target),
          currentParent = target.closest('.navigation__item--parent'),
          link = $('.navigation__link', currentParent);
      $('.navigation__item--parent', navigation).not(currentParent).removeClass('hover');
      $('.navigation__button', navigation).not(target).attr('aria-expanded', 'false');
      $('.navigation__link', navigation).not(link).attr('aria-expanded', 'false');
      if (currentParent.hasClass('hover')) {
          target.attr('aria-expanded', 'false');
          link.attr('aria-expanded', 'false');
      } else {
          target.attr('aria-expanded', 'true');
          link.attr('aria-expanded', 'true');
      }
      currentParent.toggleClass('hover');
  });


    
    $(document).on('click', '.checkout-index-index button.action.showcart', function () {
      var section = $('.opc-sidebar');

      if (section.length > 0) {
        var header = $('header.page-header');
        $('body, html').animate({
          scrollTop: ((section.offset().top - header.outerHeight()) - parseInt(header.css('margin-top'))) - 10
        }, 700, 'easeOutQuad');

        return false;
      }
    });

    
    $(document).on('click', '[data-shipping-address-select]', function () {
      $(this).toggleClass('shipping-address-items-holder--active');
    });

	$('body').click(function(){
		$('[data-shipping-address-select]').removeClass('shipping-address-items-holder--active');
	});

	$(document).on('click', '.shipping-address-items li', function () {
		
		setTimeout(function(){
		$("[data-shipping-address-select]").removeClass();
		$('[data-shipping-address-select]').addClass('shipping-address-items-holder');
		}, 500);
	});

    
    $('body.checkout-onepage-success').each(function () {
      var el = $(this),
        column = el.find('.column.main'),
        title = el.find('.page-title-wrapper'),
        message = el.find('.checkout-success'),
        registration = el.find('#registration'),
        selectTitle = el.find('.info .box-title');

      
      column.prepend('<div class="order-message"/>');
      $('.order-message').append(title).append(message);

      
      selectTitle.on('click', function () {
        $(this).parent().toggleClass('active-toggle');
      });

      
      if (registration.length) {
        registration.insertAfter('.confirmation-copy');
      }
    });

    
    $('.search__index-external-wordpress-post').each(function () {
      $('body').addClass('catalogsearch-result-articles');
    });

    var setFooterSearch = function() {
      $('[data-footer-search] :input[name="q"]').each(function() {
        if($(this).is(':visible')) {
          $(this).removeProp('disabled');
        } else {
          $(this).prop('disabled', true);
        }
      });
    }
    $(window).on('resize', setFooterSearch);
    setFooterSearch();

    
    $(document).on('click', '.locations .location', function () {
      var el = $(this);
      el.addClass('active').siblings().removeClass('active');
    })

	$(document).ready(function() {
		
		
		$('.factsGallerySlider .text-split-carousel-bottom .factsGalleryText').addClass('text-split-carousel-bottom__copy');
	});
	
	$(document).on("keydown", "input[id^='cart-item']", function(event) {
		if(event.keyCode == 40) {
			if(jQuery(this).val() < 1) {
				return false;
			}
		}
	});


	
	$(document).on('click', '[data-search-trigger="modal-popup-header-search"]', function (e) {
	    $('[data-fancybox-close]').trigger('click');
    });

  });
});</script>
