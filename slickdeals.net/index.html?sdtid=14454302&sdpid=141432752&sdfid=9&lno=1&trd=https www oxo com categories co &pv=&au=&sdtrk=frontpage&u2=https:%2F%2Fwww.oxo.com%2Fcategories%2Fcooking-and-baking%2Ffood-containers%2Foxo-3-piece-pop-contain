<script>
    function getCookie(cname)
    {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function setCookie(cname, cvalue)
    {
        var d = new Date();
        
        d.setTime(d.getTime() + (24*60*60*1000))
        var expires = "expires="+ d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }


    var dataLayer = dataLayer || [];

    var currentPage = window.location.href;
    var path = window.location.pathname;
    var pageType = "content-page";

    var currentCategoryPath = "";
    var slashCount;

    
    if(currentPage.indexOf("/categories") !== -1){
        pageType = "category-page";

        currentCategoryPath = currentPage.split('/categories/')[1];
        slashCount = currentCategoryPath.split("/").length - 1;

        if(slashCount >= 3)
        {
            pageType = "pdp-page";
        }
    }

    
    if(currentPage.indexOf("/more-oxo") !== -1){
        pageType = "category-page";

        currentCategoryPath = currentPage.split('/more-oxo/')[1];
        slashCount = currentCategoryPath.split("/").length - 1;

        if(slashCount >= 1)
        {
            pageType = "pdp-page";
        }
    }

    
    if(currentPage.indexOf("/whats-new") !== -1){
        pageType = "category-page"

        currentCategoryPath = currentPage.split('/whats-new/')[1];
        slashCount = currentCategoryPath.split("/").length - 1;

        if(slashCount >= 1)
        {
            pageType = "pdp-page";
        }
    }

    
    if(currentPage.indexOf("/checkout/cart/") !== -1){
        pageType = "cart-page";
    }

    
    if(currentPage.indexOf("/checkout/") !== -1 && currentPage.indexOf("/checkout/cart/") == -1){
        pageType = "checkout-page";
    }

    
    if(currentPage.indexOf("/checkout/onepage/success/") !== -1){
        pageType = "purchase-page";
    }

    if(path.length == 0 || path.length == 1)
    {
        pageType = "home-page";
    }

    dataLayer.push({
        "pageType": pageType
    });

    setCookie("pageType",pageType);</script>
