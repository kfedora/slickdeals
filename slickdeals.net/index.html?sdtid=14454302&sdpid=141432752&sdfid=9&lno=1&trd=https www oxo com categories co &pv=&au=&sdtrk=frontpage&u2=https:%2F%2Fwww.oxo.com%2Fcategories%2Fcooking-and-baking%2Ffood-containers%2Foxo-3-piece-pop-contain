<script type="text/javascript">
    function addToCart() {
        var quantitySelect = document.getElementById("qty");
        var quantityValue = quantitySelect.options[quantitySelect.selectedIndex].value;

        var currentSku = document.getElementById("gtmSku").innerHTML.trim();

        var product = {};
        var products = [];
        var cartProduct;
        var fullCategory = getCookie("currentCategory");
        var currentCategory = fullCategory.replace('OXO 3-Piece POP Container Variety Set', '').slice(0, -1)

        product.id = currentSku;
        product.name = 'OXO 3-Piece POP Container Variety Set';
        product.price = '29.99';
        product.brand = 'Good Grips';
        product.category = currentCategory;
        product.quantity = quantityValue;
        product.contentIds = '2028';
        products.push(product);

        cartProduct = currentSku +"|"+ 'OXO 3-Piece POP Container Variety Set' +"|"+ '29.99' +"|"+ 'Good Grips' +"|"+ currentCategory;
        setCookie(currentSku, cartProduct);

        pushProductAddToCart(products);
        document.getElementsByClassName('message-success')[0].remove();
    }</script>
