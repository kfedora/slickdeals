<script type="text/javascript">
require(['jquery', 'jquery/ui'], function($){
  jQuery(document).ready( function() {
    
    jQuery("#newsletter-signup-button").click(function(e){
       var inputnews = jQuery("#newsletter").val();
        if(inputnews == 'NULL' || inputnews == '')
        {
            jQuery('.newsletter_custom_error').html('<p><div for="newsletter" generated="true" class="mage-error" id="newsletter-error" style="display: block;">This is a required field.</div></p>');
            e.preventDefault();
            return false;    
        }
        
    });

  });
});</script>
<script type="text/javascript">

    function pushPromotionView(promotions)
    {
        dataLayer.push({
            'ecommerce':{
                'promoView': {
                    'promotions': promotions
                }
            }
        });
    }

    function pushPromotionClick(promotion)
    {
        dataLayer.push({
            'event': 'promotionClick',
            'ecommerce':{
                'promoClick': {
                    'promotions': promotion
                }
            }
        });
    }

    function pushImpressions(products)
    {
        dataLayer.push({
            'event': 'impression',
            'ecommerce':
                {
                    'impressions': products
                }
        });
    }

    function pushProductClick(products, listName)
    {
        dataLayer.push({
            'event': 'productClick',
            'ecommerce': {
                'click':
                    {
                        'actionField':
                            {
                                'list': listName
                            },
                        'products': products
                    }
            }
        });
    }

    function pushProductDetail(products, listName)
    {
        dataLayer.push({
            'event': 'productDetail',
            'ecommerce':
                {
                    'detail':
                        {
                            'actionField': {'list': listName},
                            'products': products
                        }
                }
                ,
            'pdp-page':'true'
        });
    }

    function pushProductAddToCart(products)
    {
        dataLayer.push({
            'event': 'addToCart',
            'ecommerce': {
                'currencyCode': 'USD',
                'add': {
                    'products': products
                }
            }
        });
    }

    function pushProductAddToWishlist(products)
    {
        dataLayer.push({
            'event': 'addToWishlist',
            'products': products
        });
    }

    function pushProductRemoveFromCart(products)
    {
        dataLayer.push({
            'event': 'removeFromCart',
            'ecommerce': {
                'remove': {
                    'products': products
                }
            }
        });
    }

    function pushCheckout(products, step, option)
    {
        dataLayer.push({
            'event': 'checkout',
            'ecommerce': {
                'checkout': {
                    'actionField': {'step':step, 'option': option},
                    'products': products
                }
            }
        });
    }

    function pushCheckoutStepOption(step, option)
    {
        dataLayer.push({
            'event': 'checkoutOption',
            'ecommerce': {
                'checkout_option': {
                    'actionField': {'step': step, 'option': option}
                }
            }
        });
    }

    function pushVirtualPageview(url, title)
    {
        dataLayer.push({
            'event':'virtualPageview',
            'virtualPageURL': url,
            'virtualPageTitle': title
        });
    }

    function pushPurchase(products, transactionId, affiliation, revenue, tax, shipping)
    {
        dataLayer.push({
            'event': 'purchase',
            'currencyCode': 'USD',
            'ecommerce': {
                'purchase': {
                    'actionField': {
                        'id': transactionId,
                        'affiliation': affiliation,
                        'revenue': revenue,
                        'tax': tax,
                        'shipping': shipping,
                    },
                    'products': products
                }
            }
        });
    }

    function onClickProduct(listName, name, sku, price, brand, category, position, id)
    {
        var product = {};
        var products = [];

        product.id = sku;
        product.name = name;
        product.price = price;
        product.brand = brand;
        product.category = category;
        product.position = position;
        product.contentIds = id;
        products.push(product);

        pushProductClick(products, listName);
    }

    function onClickAddToCartProduct(sku, name, price, brand, category, quantity, id)
    {
        var product = {};
        var products = [];

        product.id = sku;
        product.name = name;
        product.price = price;
        product.brand = brand;
        product.category = category;
        product.quantity = quantity;
        product.contentIds = id;
        products.push(product);

        pushProductAddToCart(products);
    }

    function onClickAddToWhishlistProduct(sku, name, price, id)
    {
        var product = {};
        var products = [];

        product.id = sku;
        product.name = name;
        product.price = price;
        product.contentIds = id;
        products.push(product);

        pushProductAddToWishlist(products);
    }

    function testing()
    {
        alert("working");
    }</script>
