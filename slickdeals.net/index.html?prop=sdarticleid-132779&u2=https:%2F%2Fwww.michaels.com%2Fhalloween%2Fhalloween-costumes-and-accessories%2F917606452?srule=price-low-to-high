    <script type="text/javascript">
    if(window.location.href.indexOf("https")<0){ // Check if URL is on Http
        var form = document.getElementById("promotions-join"); // Form Element
        var signUp = document.getElementById("signup"); // Button Element

        var signUpLink = signUp.dataset.action; // Sign Up Link
        var formLink = form.action; // Form Link

        formLink = formLink.replace("https","http"); // Replace form Https link to Http
        signUpLink = signUpLink.replace("https","http"); // Replace button link Https to Http
        document.getElementById("promotions-join").action = formLink; // Replace form link to the new http link
        document.getElementById("signup").dataset.action = signUpLink; // Replace button link to the new http link
    }
    </script>

