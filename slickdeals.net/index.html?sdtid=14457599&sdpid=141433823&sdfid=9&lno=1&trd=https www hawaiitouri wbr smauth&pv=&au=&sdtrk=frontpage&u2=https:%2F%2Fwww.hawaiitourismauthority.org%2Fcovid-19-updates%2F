				$(".search-form").submit(function (event) {
					event.preventDefault();
					var query = $(this).find("input").val();
					window.location = "/search-results?#stq=" + query;
				});

				$("#ResearchSearchForm").submit(function (event) {
					event.preventDefault();
					var query = $("#researchSearchTerm").val();
					window.location = "/research-search-results?#stq=" + query;
				});

				$("a").each(function () {
					var hrefHost = $(this).prop('hostname');
					var href = $(this).attr('href');
	
					if (hrefHost.indexOf("www.") !== -1) {
						var wwwIndex = hrefHost.indexOf("www.");
						hrefHost = hrefHost.substring(wwwIndex + 4, hrefHost.length)
					}

					if (hrefHost !== host && $.inArray(hrefHost, allowList) == -1 && !window.location.pathname.includes(speedbump)) {
						var target = speedbump + '?targetUrl=' + encodeURI(href);
						$(this).attr("href", target);
					}
				})
			});
