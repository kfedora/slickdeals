    <script>$(document).ready(function(){var r=[{pattern: "deals/2-in-1-laptops", tabName: "2-in-1"},{pattern: "deals/legion-gaming-pcs", tabName: "Gaming"},{pattern: "deals/flash-sale", tabName: "Flash Sale"},{pattern: "deals/laptops", tabName: "Laptops"},{pattern: "deals/thinkpad-laptops", tabName: "ThinkPad"},{pattern: "deals/desktops", tabName: "Desktops"},{pattern: "deals/thinkcentre-desktops", tabName: "ThinkCentre"},{pattern: "deals/tablets", tabName: "Tablets"},{pattern: "deals/computer-monitors", tabName: "Monitors"},{pattern: "deals/computer-accessories", tabName: "Accessories"},{pattern: "deals/clearance", tabName: "Clearance"},{pattern: "lenovo-outlet-sale", tabName: "Outlet"},{pattern: "quick-ship", tabName: "Quick Ship"},{pattern: "deals/doorbusters", tabName: "Doorbusters"},{pattern: "deals/workstation", tabName: "Workstations"}], s="Generic"; for (var o in r){var n=r[o]; if (location.href.indexOf(n.pattern) > -1){s=n.tabName; break}}var row=1, col=1; $('.product').each(function(index, elem){var element={}; element.row=row; element.col=col; element.trackers=[]; element.pn=$(elem).find('.title h3 a').attr('href').split('en/p/')[1]; element.trackers.push($(elem).find('.gallery-content')); element.trackers.push($(elem).find('.title a')); element.trackers.push($(elem).find('.lnv-cta a.lnv-button')); bindElement(element); if ($(elem).parents('.halo').length){row ++;}else if ($(elem).parents('.secondaries').length){console.log('sec'); if (col < 2){col ++;}else{col=1; row ++;}}else{if (col < 4){col ++;}else{col=1; row ++;}}}); function bindElement(elem){for (var i=0; i < elem.trackers.length; i++){$(elem.trackers[i]).click(function(e){if (!lmd.espot){lmd.espot={};}lmd.espot.placementName=elem.row + 'x' + elem.col; lmd.espot.tabName=s; lmd.productid=elem.pn; _satellite.track("eSpot_clicked");});}}});</script><script>
    function shopNow(element) {
        var oid = $(element).attr('data-article');
        $(element).parents('.product').find('.accessories .accessory[data-selected=true]').each(function() {
            var accesory = $( this ).find('.price').attr('data-article');
            accessories.put(oid, accesory);
        });
        //accessories.save();

        //location.href = 'p/' + oid + '?ipromoID=LEN767706#tab-currentmodels';
        // no anchor to Models on each prod page.
        location.href = 'p/' + oid + '?ipromoID=LEN767706';
    };
    var max500 = window.matchMedia("(max-width: 500px)");
    function activateOnMobile(max500){
        if(max500.matches){
            partNumbers = document.getElementsByClassName('partNumber')
            for(let i = 0; i < partNumbers.length; i++){
                partNumbers[i].innerText = partNumbers[i].innerText.replace('Part Number: ','')
            }
        }
    }
    $(function() {
        $( "div.product " ).each(function() {
            var processor = $( this ).find( ".tech-specs li.feature" ).first();
            var inventory = $( this ).find( "li.inventory" ).text();
            if (processor.text() == '') {
                $( this ).find( ".tech-specs").css('display','none');
                $( this ).find( ".see-full-specs a").css('display','none');
            }
            if(inventory.endsWith("available")) {
                $( this ).find( "li.inventory" ).css('visibility','hidden');
            }
            if ($(this).find('li.instantSaving').length && $(this).find('li.minPrice-label').text() == 'After eCoupon:'){
              $(this).find('li.instantSaving').css('display', 'none');
              $(this).find('li.instantSaving-label').css('display', 'none');
            }

            var siteRoot = $('meta[name=siteRoot]').attr('content');
            var productcode = $(this).find('.title h3 a').attr('href').split('/p/')[1];
            if ($(this).find('form').length) {
                $(this).find('form').html('<div class="lnv-cta centered heightAnchor"><a onclick="shopNow(this)" data-article="' + productcode + '" class="lnv-button">Shop Now</a></div>');

                //$(this).find('form').after('<a class="financing-link" href="' + siteRoot + '/landingpage/lenovo-financing-options/">Credit & Leasing Options ></a>');
            }

            $(this).find('.title h3').after('<div class="partNumber">Part Number: &nbsp;' + productcode + '</div>');
        });
        if ($('meta[name="storeinfo.storetype"]').attr('content') == 'AFFINITY') {
            $('#navigation .nav-box a[href*="lenovo-outlet"]').parent().css('display','none');
        }
         if(window.location.href.indexOf("smbpro") > -1) {
            $('#navigation .nav-box a[href*="lenovo-outlet"]').parent().css('display','none');
            }
        /* $("div.product .title h3 a").click(function(e){
            e.preventDefault();
            var href = $(this).attr('href');
            location.href = href + '#tab-currentmodels';
        }); */
        activateOnMobile(max500);
    });

    $(document).ready(function() {

                var siteRoot = $('meta[name=siteRoot]').attr('content');
        $('div.lnv-cta.heightAnchor').after('<a class="financing-link" href="' + siteRoot + '/landingpage/lenovo-financing-options/">Credit & Leasing Options ></a>')
        if (location.href.indexOf("/deals/computer-accessories") > -1) {
            $('.discountLabel').hide();
        }
    });
    
    $(document).ready(function(){
	   if($("input[loyaltyProductInfo=true]").length>0){
		   $("[data-toggle='popover']").popover();
		   var productList = new Array();
		   $("input[loyaltyProductInfo=true]").each(function(){
			   var product = new Object();
			   product.productCode = $(this).attr("productCode");
			   product.productPrice = $(this).attr("productPrice");
			   productList.push(product);
		   });
		   var url = ACC.config.contextPath + "/p/caculatePointToMoneyForSubseries";
		   $.ajax({
		   url: url,
		   type: 'GET',
		   data: "productsObj=" + JSON.stringify(productList),
		   dataType: "json",
		   traditional: true,
		   timeout : 10000,
		   success: function (data) {
			   if(data.success == "true" && data.pointMappingMoneyItems){
				   for(var index in data.pointMappingMoneyItems){
					   var item = data.pointMappingMoneyItems[index];
					   if(item.multiplier && item.multiplier != 1){
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=1]").addClass("hiddenInfo"); //hidden default info
        			   }
        			   if(item.multiplier && item.multiplier >= 6){
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=6] .loyaty-value").text(item.pointMappingMoney);
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=6]").removeClass("hiddenInfo");
        			   }else if(item.multiplier && item.multiplier > 1){
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=" + item.multiplier + "] .loyaty-value").text(item.pointMappingMoney);
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=" + item.multiplier + "]").removeClass("hiddenInfo");
        			   }else{
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=1] .loyaty-value").text(item.pointMappingMoney);
        				   $(".loyalty_productCode_" + item.itemId + " span[multiplier=1]").removeClass("hiddenInfo");
        			   }
				   }
			   }
		   },
		   error: function(XMLHttpRequest, textStatus, errorThrown){
			   console.error("caculatePointToMoneyForSubseries fail");
		   }
	       });
	   }
	});
</script>
