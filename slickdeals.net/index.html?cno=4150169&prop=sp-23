<script>
$(window).on('load', function() {
    var featProducts = $("#halo .product");
    featProducts.each(function() {
        var klarna = document.createElement('klarna-placement');
        var startingAt = $(this).find(".minPrice span").text();
        var avoid = "$";
        var startingAtNew = startingAt.split(avoid).join('');
        klarna.setAttribute('data-id', 'bc4420fe-c00e-4d4c-b870-f0409e5fd089');
        klarna.setAttribute('data-inline', 'true');
        klarna.setAttribute('data-purchase_amount', (parseFloat(startingAtNew.replace(/,/g, "")).toFixed(0) * 100));
        $(this).find('.financing-link').before(klarna);
    });
    var secProducts = $("#secondaries .product");
    secProducts.each(function() {
        var klarna = document.createElement('klarna-placement');
        var startingAt = $(this).find(".minPrice span").text();
        var avoid = "$";
        var startingAtNew = startingAt.split(avoid).join('');
        klarna.setAttribute('data-id', 'bc4420fe-c00e-4d4c-b870-f0409e5fd089');
        klarna.setAttribute('data-inline', 'true');
        klarna.setAttribute('data-purchase_amount', (parseFloat(startingAtNew.replace(/,/g, "")).toFixed(0) * 100));
        $(this).find('.financing-link').before(klarna);
    });
    var products = $("#grid .product");
    products.each(function() {
        var klarna = document.createElement('klarna-placement');
        var startingAt = $(this).find(".minPrice span").text();
        var avoid = "$";
        var startingAtNew = startingAt.split(avoid).join('');
        klarna.setAttribute('data-id', 'bc4420fe-c00e-4d4c-b870-f0409e5fd089');
