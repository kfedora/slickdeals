    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.4.2/gsap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.4.2/ScrollTrigger.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TimelineMax.min.js"
        integrity="sha512-lJDBw/vKlGO8aIZB8/6CY4lV+EMAL3qzViHid6wXjH/uDrqUl+uvfCROHXAEL0T/bgdAQHSuE68vRlcFHUdrUw=="
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TweenMax.min.js"
        integrity="sha512-8Wy4KH0O+AuzjMm1w5QfZ5j5/y8Q/kcUktK9mPUVaUoBvh3QPUZB822W/vy7ULqri3yR8daH3F58+Y8Z08qzeg=="
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/plugins/CSSPlugin.min.js"
        integrity="sha512-ht40uOoiTef4nKq0THVzjIGh3VS108J577LVVgNXnQLXza3doXjoM3owin2vd+Hm6w88k12RIrePIVY2WNzz6Q=="
        crossorigin="anonymous"></script>

        <script>
window.addEventListener("load", function () {
    var customerContainer = document.querySelector(".customer-container"),
        dots = document.querySelectorAll(".dots");

    customerContainer.addEventListener("scroll", function () {
        let cardScroll = customerContainer.scrollLeft;
        console.log(cardScroll);

        if (cardScroll >= 0 && cardScroll <= 260) {
            resetDots();
            dots[0].classList.add("dot-active");
        } else if (cardScroll > 260 && cardScroll <= 560) {
            resetDots();
            dots[1].classList.add("dot-active");
        } else if (cardScroll > 560 && cardScroll <= 794) {
            resetDots();
            dots[2].classList.add("dot-active");
        } else {
            resetDots();
            dots[3].classList.add("dot-active");
        }
    });

    function resetDots() {
        dots.forEach(function (dot) {
            if (dot.classList.contains("dot-active")) {
                dot.classList.remove("dot-active");
            }
        });
    }

    let faqQuestion = document.querySelectorAll(".question");

    faqQuestion.forEach((question) => {
        question.addEventListener("click", function () {
            if (question.nextElementSibling.classList.contains("answer-show")) {
                question.nextElementSibling.classList.remove("answer-show");
            } else {
                question.nextElementSibling.classList.add("answer-show");
            }

            if (question.lastElementChild.classList.contains("answer-active")) {
                question.lastElementChild.classList.remove("answer-active");
            } else {
                question.lastElementChild.classList.add("answer-active");
            }
        });
    });

    /*Animation for steps for Mobile and tablet - Purple lines only mobile*/
    gsap.registerPlugin(ScrollTrigger);

    ScrollTrigger.matchMedia({
        "(max-width: 1279px)": function () {
            const step1 = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step1",
                    start: "top+=100 center+=100",
                    end: "top top",
                    toggleActions: "play complete complete complete",
                },
            });

            step1
                .from(".step1", 0.5, {
                    y: -100,
                    opacity: 0,
                    ease: Power1.easein,
                })

                .to("#lineMobile1", 1, {
                    strokeDashoffset: 0,
                });
        },
    });

    ScrollTrigger.matchMedia({
        "(max-width: 1279px)": function () {
            const step2 = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step1",
                    start: "top+=200 center+=100",
                    end: "bottom top",
                    toggleActions: "play complete complete complete",
                },
            });

            step2
                .from(".step2", 0.5, {
                    y: -100,
                    opacity: 0,
                    ease: Power1.easein,
                })

                .to("#lineMobile2", 1, {
                    strokeDashoffset: 0,
                });
        },
    });

    ScrollTrigger.matchMedia({
        "(max-width: 1279px)": function () {
            const step3 = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step2",
                    start: "top+=100 center+=100",
                    end: "bottom top",
                    toggleActions: "play complete complete complete",
                },
            });

            step3.from(".step3", 0.5, {
                y: -100,
                opacity: 0,
                ease: Power1.easein,
            });
        },
    });

    /*End ofAnimation for steps for Mobile*/

    /*Animation for half banner cont Mobile and tablet */
    ScrollTrigger.matchMedia({
        "(max-width: 4200px)": function () {
            const halfBannerCont = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: "#half-banner-cont",
                    start: "top-=100 center",
                    end: "bottom center",
                    toggleActions: "play complete complete complete",
                },
            });

            halfBannerCont
                .to("#half-banner-cont img", 0.5, {
                    opacity: 1,
                    scale: 1,
                    ease: Power1.easein,
                })

                .to(
                    "#half-banner-cont .half-banner-copy",
                    0.5,
                    {
                        y: 0,
                        opacity: 1,
                        ease: Power1.easein,
                    },
                    "+=.2"
                );
        },
    });

    /*Animation for promo espots Mobile and tablet */
    ScrollTrigger.matchMedia({
        "(max-width: 749px": function () {
            ScrollTrigger.batch(".espot", {
                interval: 0.5,
                batchMax: 1,
                start: "center bottom",
                onEnter: (batch) =>
                    gsap.to(batch, {
                        opacity: 1,
                        y: -20,
                        stagger: 0.3,
                    }),
            });
        },

        "(min-width: 750px": function () {
            ScrollTrigger.batch(".espot", {
                interval: 0.3,
                //batchMax: 1,
                start: "center-=10 bottom",
                onEnter: (batch) =>
                    gsap.to(batch, {
                        opacity: 1,
                        y: -20,
                        stagger: 0.3,
                    }),
            });
        },

        "(min-width: 1280px": function () {
            ScrollTrigger.batch(".espot", {
                interval: 0.3,
                batchMax: 3,
                start: "bottom bottom",
                end: "top top",
                onEnter: (batch) =>
                    gsap.to(batch, {
                        opacity: 1,
                        y: -20,
                        stagger: 0.3,
                    }),
            });
        },
    });

    ScrollTrigger.matchMedia({
        "(min-width: 750px)": function () {
            const step1Tablet = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step1",
                    start: "top+=100 center",
                    end: "top top",
                    toggleActions: "play complete complete complete",
                },
            });

            step1Tablet.to(
                "#lineTablet1",
                1,
                {
                    strokeDashoffset: 0,
                },
                "+=.5"
            );
        },
    });

    ScrollTrigger.matchMedia({
        "(min-width: 750px)": function () {
            const step2Tablet = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step2",
                    start: "top+=100 center",
                    end: "top top",
                    toggleActions: "play complete complete complete",
                },
            });

            step2Tablet.to("#lineTablet2", 1, {
                strokeDashoffset: 0,
            });
        },
    });

    ScrollTrigger.matchMedia({
        "(min-width: 750px)": function () {
            const step3Tablet = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step3",
                    start: "top+=100 center",
                    end: "top top",
                    toggleActions: "play complete complete complete",
                },
            });

            step3Tablet.to("#lineTablet3", 1, {
                strokeDashoffset: 0,
            });
        },
    });

    ScrollTrigger.matchMedia({
        "(min-width: 1280px)": function () {
            const desktopSteps = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".step1",
                    start: "top bottom",
                    end: "bottom+=200px center",
                    toggleActions: "play complete complete complete",
                },
            });

            desktopSteps
                .to(".step1", 1, {
                    y: 0,
                    opacity: 1,
                })

                .to("#lineDesktop1", 0.7, {
                    strokeDashoffset: 0,
                })

                .to(
                    ".step2",
                    1,
                    {
                        y: 0,
                        opacity: 1,
                    },
                    "-=.7"
                )

                .to("#lineDesktop2", 0.7, {
                    strokeDashoffset: 0,
                })

                .to(
                    ".step3",
                    1,
                    {
                        x: 0,
                        opacity: 1,
                    },
                    "-=1"
                )

                .to("#lineDesktop3", 0.7, {
                    strokeDashoffset: 0,
                });
        },
    });

    const tradeIn = gsap.timeline();

    tradeIn
        .to(
            "#tradeIn",
            0.5,
            {
                transformOrigin: "50% 50%",
                rotation: -180,
            },
            ".5"
        )

        .from(
            "#top-laptop",
            1,
            {
                x: "50% ",
            },
            "-=.5"
        )

        .from(
            "#bottom-laptop",
            1,
            {
                right: "35% ",
            },
            "-=1"
        );

    /* ScrollTrigger.matchMedia({
        "(max-width: 1279px)": function () {
            const customer = gsap.timeline({
                paused: true,
                scrollTrigger: {
                    trigger: ".customer1",
                    horizontal: true,
                    start: "left right",

                    toggleActions: "play complete complete complete",
                },
            });

            customer.to(".customer1", 1, {
                scale: 2,
            });
        },
    });*/

    ScrollTrigger.matchMedia({
        "(min-width: 0px": function () {
            ScrollTrigger.batch(".customer", {
                interval: 1,
                batchMax: 3,
                start: "center+=20 bottom",
                onEnter: (batch) =>
                    gsap.to(batch, {
                        opacity: 1,
                        y: -20,
                        stagger: 0.2,
                    }),
            });
        },
    });
});
</script></div>
