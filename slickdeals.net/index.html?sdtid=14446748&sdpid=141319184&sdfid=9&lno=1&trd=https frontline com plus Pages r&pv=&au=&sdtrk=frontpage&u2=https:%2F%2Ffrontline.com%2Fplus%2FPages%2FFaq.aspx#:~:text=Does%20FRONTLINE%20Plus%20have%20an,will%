                                <div webpartid="4f6c9446-ae33-460e-882b-eaac9304b234" haspers="false" id="WebPartWPQ1"
                                     width="100%" class="ms-WPBody" allowdelete="false" style="">
                                    <!-- add body class name -->
                                    <script>
                                      document.body.className = 'faq';
                                    </script>
                                    <script type="text/javascript">
                                      $(function () {
                                        $("header nav ul li.faq").addClass("active");

                                        $(".print-page").click(function () {
                                          window.print();
                                          return false;
                                        });

                                        $(".find-vet-submit").click(function () {
                                          FindVet();
                                          return false;
                                        });

                                        function FindVet() {
                                          var pathArray = location.href.split('/');
                                          host = pathArray[2];

                                          var vzip = document.getElementById('zipcode').value;
                                          if (vzip == "") {
                                            var vzip = document.getElementById('zipcodetablet').value;
                                          }
                                          window.location.href = "https://" + host + "/plus/Pages/where-to-buy?zip=" + vzip;
                                        }
                                      });
                                    </script>
                                </div>
