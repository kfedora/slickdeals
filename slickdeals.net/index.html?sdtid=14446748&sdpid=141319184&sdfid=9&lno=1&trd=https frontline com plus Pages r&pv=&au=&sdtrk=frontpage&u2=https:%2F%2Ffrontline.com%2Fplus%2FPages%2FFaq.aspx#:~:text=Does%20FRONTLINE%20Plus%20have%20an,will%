        <div id="ctl00_PlaceHolderMain_ctl03__ControlWrapper_RichHtmlField" class="ms-rtestate-field"
             style="display:inline" aria-labelledby="ctl00_PlaceHolderMain_ctl03_label">
            <section class="section1">
                <div class="section-content">
                    <div class="left-sect">
                        <img class="hidden-phone" src="/img/faq-header-d.png" alt="answers to how, when &amp; why">
                    </div>
                    <div class="right-sect">
                        <img class="header-image hidden-phone" src="/img/faq-header-img-d.png" alt="question marks">
                    </div>
                    <div class="header-divider hidden-phone"> Â </div>
                    <div class="visible-phone">
                        <img src="/img/faq-header-m.png" alt="answers to how, when &amp; why">
                    </div>
