<script type="text/javascript">

	function tempSubmit() {
		var valResult = valFormUpdated();
		if (valResult != false)
		{
			return sendHPCIMsg();
		}
		return valResult;
    }

    function tempNonCC() {
        return valFormUpdated();
    }

        function checkout() {
            //hideErrorMessgae();
            sendHPCIMsg();
            //showWaitMessage();
            return false;
        }

        function showWaitMessage() {
            document.getElementById("submitButton").disabled = true;
            //document.getElementById("waitingMessage").style.display = "block";
        }

        function hideWaitMessage() {
            document.getElementById("submitButton").disabled = false;
            //document.getElementById("waitingMessage").style.display = "none";
        }

        function submitForm() {
            //document.getElementById("ccType").value = document.getElementById("PurchaseViewModelObject_CreditCardType").value;
            //document.getElementById("expMonth").value = document.getElementById("PurchaseViewModelObject_ExpirationMonth").value;
            //document.getElementById("expYear").value = document.getElementById("PurchaseViewModelObject_ExpirationYear").value;
            //alert("ccNum.value: " + document.getElementById("ccNum").value);
            document.getElementById("purchaseForm").submit();
        }
