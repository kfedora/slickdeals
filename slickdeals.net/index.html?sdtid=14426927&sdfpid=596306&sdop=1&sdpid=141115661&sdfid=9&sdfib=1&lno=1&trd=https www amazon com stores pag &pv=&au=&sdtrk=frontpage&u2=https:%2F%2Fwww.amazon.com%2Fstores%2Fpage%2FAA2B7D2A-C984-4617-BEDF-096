    if ( window.P && window.P.AUI_BUILD_DATE ) {
        P.load.js("https://m.media-amazon.com/images/G/01/vap/video/airy2/prod/2.0.1454.0/js/airy.skin._TTW_.js");
        P.load.css("https://m.media-amazon.com/images/G/01/vap/video/airy2/prod/2.0.1454.0/css/beacon._TTW_.css");
    }
</script>
