	}];
	cq_params.categoryId = 'sale';
	cq_params.refinements = '[{\"name\":\"isRefurbished\",\"value\":\"false\"},{\"name\":\"isSupport\",\"value\":\"false\"},{\"name\":\"Category\",\"value\":\"sale\"}]';
