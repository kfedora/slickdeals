<script>
  $(function(){
    $('.product-item').each(function(){
      $(this).find('.overlay .quick').hoverIntent({
        over: function(){
          $(this).parent().find('.variants').fadeIn();
        },
        out: function(){
          $(this).parent().find('.variants').fadeOut();
        },
        interval: 20
      });
      
      $(this).find('.overlay .quick a').click(function(){
        $(this).closest('form').find('[name=id]').val($(this).data('variant-id'));
        addToCart($(this).closest('form'));
        return false;
      });
    });
  });
</script>
  
  
  <i class="scroll-up fa fa-angle-up"></i>
  <script>
    $(function(){
      $('.scroll-up').click(function(){
        $.scrollTo(0, 1000, {offset: 0});
      });
    });
  </script>
</div>
