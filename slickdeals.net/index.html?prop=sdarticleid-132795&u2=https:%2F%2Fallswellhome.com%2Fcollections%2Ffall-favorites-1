  <script>
    $(function() {

      $(window).on("orientationchange", function(){
        setTimeout(function(){
          $('#side-cart .footer').css('bottom', '+=1').css('margin-bottom', '-=1');
        }, 500);
      });

      var itemsCount = 0;
      $('#side-cart .quantity input[type=number]:visible').each(function(){
        itemsCount += parseInt($(this).val());
      });
      $('.top-utils .cart-wrap .count').html(itemsCount);
      if(itemsCount>0) {
        $('.top-utils .cart-wrap .count').show();
      } else {
        $('.top-utils .cart-wrap .count').hide();
      }

      function updateCart(){
        $('#loading-overlay').show();

        $.ajax({
          type: 'POST',
          url: '/cart/update.js',
          data: $('#side-cart form').serialize(),
          dataType: 'json',
          error: function(){
            console.log('error');
          },
          success: function(){
            addToCartSuccess();
            $('#add-cart-overlay').fadeOut();
          },
        });
      }

      $('#side-cart .remove').click(function(){
        var item = $(this).closest('.item')
        item.find('input[type=number]').val(0);
        var bundleId = item.data('bundle-id');
        if(bundleId) {
          $('#side-cart [data-bundle-id='+bundleId+'] [name="updates[]"]').val(0);
        }
        updateCart();
      });

      $('#side-cart input[type=number]').change(function(){
        updateCart();
      });

      $('#side-cart .item.hide .quantity input[type=number]').val(0).change();
    });
  </script>
  <link href='https://fonts.googleapis.com/css?family=Raleway:300' rel='stylesheet' type='text/css'>

  
  <div class="wrapper empty">
    <div class="header clearfix">
      <h1>Shopping Cart</h1>
      <a href="#" class="close" onclick="hideSideCart();return false;"><i class="lnr animation lnr-cross"></i></a>
    </div>

    <div>
      
        <div class="empty-text text-center">Your cart is empty</div>
      
    </div>
  </div><div id="shopify-section-cart_recommendations" class="shopify-section"></div></div></div>
