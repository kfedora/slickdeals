<body class="templateCollection"><script>
  //var CCPA_API_URL = 'https://sls-eus-dev-ccpa.azurewebsites.net/api/';
  var CCPA_API_URL = 'https://bonobos-ccpa.azure-api.net/';
  //var CCPA_WALMART_URL = 'https://ccpa-ui-dev.walmart.com/';
  var CCPA_WALMART_URL = 'https://cpa-ui.walmart.com/';
  var oneTrustClosed = false;

  var ccpaGetAccountType = function(email, callback, attempt){
    var accountType;
    if(!attempt) {
      attempt = 0;
    }$.ajax({
        url: '/a/smpl/custom_ccpa_proxy/validateAccount/'+email,
      }).success(function(data){
        if(data.hasAccount) {
          accountType = "1";
        } else {
          if(data.emailExists) {
            accountType = "2";
          } else {
            accountType = "3";
          }
        }
        callback(accountType);
      }).error(function(){
        if(attempt==3) {
          location.href = "/pages/ccpa-error";
        } else {
          attempt++;
          ccpaGetAccountType(email, callback, attempt);
        }
      });};

  // Function to update OptanonConsent cookie value
  function updateTargeting(ID) { 
    var cookie = decodeURIComponent(Cookies.get('OptanonConsent'));
    for (var i = 0; i < ID.length; i++) {
      cookie = cookie.replace(',' + ID[i] + ':0,', "," + ID[i] + ":0,");
      cookie = cookie.replace(',' + ID[i] + ':1,', "," + ID[i] + ":0,");
      OptanonActiveGroups = OptanonActiveGroups.replace("," + ID[i] + ",", ",");
      OnetrustActiveGroups = OnetrustActiveGroups.replace("," + ID[i] + ",", ",");
    }
    Cookies.set('OptanonConsent', encodeURI(cookie), {domain: 'allswellhome.com'});
  }

  $(function(){
    

    

    


    $('[href="#request-personal-information"]').click(function(){
        $('#loading-overlay').show();

        var sendRequest = function(attempt) {
          if(!attempt) {
            attempt = 0;
          }
          $.ajax({
            url: '/cart?view=timestamp&cache=false',
            cache: false
          }).success(function(time){
            $.ajax({
              url: CCPA_API_URL+'encryptParams',
              data: JSON.stringify({  timeStamp: time}),
              method: 'POST'
            }).success(function(data){
              data = JSON.parse(data);
              location.href = CCPA_WALMART_URL+"affirmation?brandCode=AWL&requestType=ACCESS&params=" + data.encryptedValue;
            }).error(function(){
              if(attempt==3) {
                location.href = "/pages/ccpa-error";
              } else {
                attempt++;
                sendRequest(attempt);
              }
            });
          }).error(function(){
            if(attempt==3) {
              location.href = "/pages/ccpa-error";
            } else {
              attempt++;
              sendRequest(attempt);
            }
          });
        };
        sendRequest();
      
        return false;
    });

    

    $('[href="#dont-sell-personal-information"]').click(function(){
        $('#loading-overlay').show();

        var sendRequest = function(attempt) {
          if(!attempt) {
            attempt = 0;
          }

          $.ajax({
            url: '/cart?view=timestamp&cache=false',
            cache: false
          }).success(function(time){
            $.ajax({
              url: CCPA_API_URL+'encryptParams',
              data: JSON.stringify({  timeStamp: time}),
              method: 'POST'
            }).success(function(data){
              data = JSON.parse(data);
              location.href = CCPA_WALMART_URL+"affirmation?brandCode=AWL&requestType=OPTOUT&params=" + data.encryptedValue;
            }).error(function(){
              if(attempt==3) {
                location.href = "/pages/ccpa-error";
              } else {
                attempt++;
                sendRequest(attempt);
              }
            });
          }).error(function(){
            if(attempt==3) {
              location.href = "/pages/ccpa-error";
            } else {
              attempt++;
              sendRequest(attempt);
            }
          });
        };
        sendRequest();
      
        return false;
    });
  });
</script>

