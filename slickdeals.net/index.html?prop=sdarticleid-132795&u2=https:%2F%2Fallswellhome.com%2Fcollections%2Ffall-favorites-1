<script>
  $(function(){
    if($('.top-banner .text p').length>1) {
    	$('.top-banner .text .read-more').css('display', 'inline-block');
    }
    $('.top-banner .read-more').click(function(){
      	if($(this).text()=='Read More') {
          	$(this).text('Show Less');
        	$(this).closest('.text').find('p').show();
        } else {
          	$(this).text('Read More');
          	$(this).closest('.text').find('p:gt(0)').hide();
        }
      	return false;
    });
  });
</script>

