    <script>
        var zoom = "";
        if (navigator.userAgent.toLowerCase().indexOf("ipad") > -1) {
            zoom = ", initial-scale=1";
        }
        if (window.matchMedia) {
            var viewport = document.querySelector("meta[name=viewport]"),
                isPortrait = !window.matchMedia("(orientation: portrait)").matches,
                getOrientationValue = function () {
                    isPortrait = !isPortrait;
                    if (isPortrait) {
                        viewport.setAttribute('content', 'width=768' + zoom);
                    } else {
                        viewport.setAttribute('content', 'width=1024' + zoom);
                    }
                };
            getOrientationValue();
            window.addEventListener("orientationchange", getOrientationValue, false);
        }
    </script>


