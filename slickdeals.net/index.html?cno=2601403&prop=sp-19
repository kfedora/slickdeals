<script>
function handleHeightPostRefresh() {
console.log("Inside handleHeightPostRefresh");
	 require(["jquery"], function($){
	 	$(document).ready(function(){
	 	
                  if ($('body').find('.lto-match-height').length > 0) {
                  try{
                  	$('body').find('.lto-match-height').matchHeight();
                  	}catch(e){
                  		console.log('exception in calling matchHeight');
                  	}
                  } else {
                   console.log('matchHeight false');
                  }
                  //setting role to shipping popup
                 // $("#resultsPerSelection").attr({'role':'alert'});
                  $("#resultsPerSelection1").attr({'aria-live':"assertive"});
                  
                  $('.shippingInfoST').attr({'role':'dialog','tabindex':'0'});
            });
	   });
	}
	
function tfdeviceLinkPostRefresh() {
console.log("Inside tfdeviceLinkPostRefresh");
	 require(["jquery"], function($){
	 	$(document).ready(function(){
            	$(".tf-lto-device-listing").click(function() {
				  window.location = $(this).find("a.tf-lto-device-url").attr("href"); 
				  return false;
				});
			
            });
	   });
	}	

handleHeightPostRefresh();

if(jQuery(".tf-lto-device-listing") != null)
	tfdeviceLinkPostRefresh();

</script>


