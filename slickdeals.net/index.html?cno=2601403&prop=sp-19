    });


dojo.require("dojo.back");
			dojo.back.init();
			dojo.addOnLoad(function(){
				SearchBasedNavigationDisplayJS.init('https://shop.totalwireless.com/shop/CategoryNavigationResultsView?pageSize=12&manufacturer=&searchType=&resultCatEntryType=&catalogId=12051&categoryId=22001&langId=-1&seoFacet=brand_apple&storeId=20003&sType=SimpleSearch&filterFacet=');
				shoppingActionsJS.initCompare('');
			});

			dojo.addOnLoad(function() {
				shoppingActionsServicesDeclarationJS.setCommonParameters('-1','20003','12051');
				 shoppingActionsJS.setCommonParameters('-1','20003','12051','G','$');
			          
				 var phoneNumber = '';
			     var storeName = 'TotalWireless'
			     var isMigrationToolEnabled = 'true';
			     var migrationGroupFromContext = '';
			     var zipCodeFromContext = '';
			          console.log(zipCodeFromContext);
			     var isSafeLinkPagePlusStore = false;
			           if((storeName == 'SafeLinkUpgrades' || storeName == 'PagePlusCellular')){
			             isSafeLinkPagePlusStore = true;
			            }
			        if( isSafeLinkPagePlusStore && zipCodeFromContext === ''){
			        	if(phoneNumber != null && !(phoneNumber == '')){
			        	 	dojo.byId('phoneNumber').value = phoneNumber;
			        	 	if(isMigrationToolEnabled){
			            		shoppingActionsJS.serviceCallUpgradePhoneNumberZipToTech(storeName,true);
			            	}else{
			            	shoppingActionsJS.serviceCallToValidatePhoneNumber(storeName);
			            
			            	}
			            }
			         }else if(!isSafeLinkPagePlusStore && phoneNumber != null && !(phoneNumber == '')){
			            dojo.byId('phoneNumberUpg').value = phoneNumber;
			           
			            if(isMigrationToolEnabled){
			            	shoppingActionsJS.serviceCallUpgradePhoneNumber(storeName,true);
			            }
			          }
				});
				
				jQuery(document).on('focusout','#main-content',function(e){
					var banner = jQuery('.catalog_banner_espot a')[0];
	 				if(!!banner){
	 					e.preventDefault();
	 					jQuery(banner).focus();
	 				}
	 				var maincontent = jQuery('#main-content')[0];
	 				if(!!maincontent){
	 					jQuery(maincontent).removeAttr('tabindex');
	 				}
				});
				jQuery(document).on('click','#skip-main',function(e){
					//e.preventDefault();
	 				var maincontent = jQuery('#main-content')[0];
	 				if(!!maincontent){
	 					jQuery(maincontent).attr('tabindex','0');
	 				}
				});
				
