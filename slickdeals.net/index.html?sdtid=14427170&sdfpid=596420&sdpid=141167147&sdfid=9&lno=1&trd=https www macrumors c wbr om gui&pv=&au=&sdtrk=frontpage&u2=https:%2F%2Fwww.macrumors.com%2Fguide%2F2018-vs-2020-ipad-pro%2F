                } else if (mrDark == 2) {
                    
                        document.write('<link rel="stylesheet" type="text/css" href="https://www.macrumors.com/dist/chunk-0.css?ver=8fe912ec9c5968381f3f" data-colorscheme="light" />');
                    
                } else {
                    try {
                        var light = window.matchMedia("(prefers-color-scheme: light)");
                        var dark = window.matchMedia("(prefers-color-scheme: dark)");
                        if (light.matches || dark.matches) {
                            
                                document.write('<link rel="stylesheet" type="text/css" href="https://www.macrumors.com/dist/chunk-0.css?ver=8fe912ec9c5968381f3f" media="(prefers-color-scheme: light)" data-colorscheme="light" />');
