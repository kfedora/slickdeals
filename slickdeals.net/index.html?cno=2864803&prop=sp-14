		(function(){
			function loadScript(url, callback) {
				var script = document.createElement("script");
				script.type = "text/javascript";
				script.async = true;
				if (script.readyState) {
					script.onreadystatechange = function() {
						if (script.readyState == "loaded" || script.readyState == "complete") {
							script.onreadystatechange = null;
							if (callback && typeof callback === "function") {
								callback()
							}
						}
					}
				} else {
					script.onload = function() {
						if (callback && typeof callback === "function") {
							callback();
						}
					}
				}
				script.src = url;
				(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(script);
			}

			var hostPath = 'https://cdn.staticsfly.com/thumbnailstatic/builds/stylestore-20190716-735-58a0db75-ecb2/dist/';

			loadScript(hostPath + 'react.min.js', function() {
				loadScript(hostPath + 'react-dom.min.js', function() {
					loadScript(hostPath + 'photobooks-style-store.min.js');
				});
			});
		})();


	</script>



