                        var loopStatus;

                        if ($('#m1') && $('#m1')[0].childElementCount == 1) {
                            loopStatus = false;
                        }
                        else {
                            loopStatus = true;
                        }

                        var homepage_a_m1 = $("#m1").owlCarousel({
                            loop: loopStatus,
                            items: 1,
                            video: true,
