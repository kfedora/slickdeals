                        //console.log('Chat COUNT '+count);
                    });
                    embedded_svc.addEventHandler("onIdleTimeoutOccurred", function (data) {
                        if (count < 3) {
                            window.location = window.location.href;
                        }
                        count = 0; //reset count
                    });
                    embedded_svc.addEventHandler("onChatEndedByAgent", function (data) {
                        if (count < 3) {
                            window.location = window.location.href;
                        }
                        count = 0; //reset count
                    });
                    embedded_svc.addEventHandler("onChatEndedByChasitor", function (data) {
                        if (count < 3) {
                            window.location = window.location.href;
                        }
                        count = 0; //reset count
                    });
                    embedded_svc.addEventHandler("onHelpButtonClick", function (data) {
                        //console.log('Click HelpButton');
                    });
                    embedded_svc.addEventHandler("afterMaximize", function (data) {
                        console.log('afterMaximize');
                        $(function () {
                            // replace label for MX
                            if (COOKIE_POSTFIX == "MX") {
                                $('ul.fieldList label').each(function () {
                                    if (this.htmlFor === 'FirstName') {
                                        $(this).children('span').each(function (idx, item) {
                                            if (item.innerText === 'FIRST NAME')
                                                item.innerText = "Last Name";
                                        });
                                    }
                                    else if (this.htmlFor === 'LastName') {
                                        $(this).children('span').each(function (idx, item) {
                                            if (item.innerText === 'LAST NAME')
                                                item.innerText = "First Name";
                                        });
                                    }
                                    else if (this.htmlFor === 'Email') {
                                        $(this).children('span').each(function (idx, item) {
                                            if (item.innerText === 'EMAIL')
                                                item.innerText = "CORREO ELECTRÓNICO";
                                        });
                                    }
                                    else if (this.htmlFor === 'Order__c') {
                                        $(this).children('span').each(function (idx, item) {
                                            if (item.innerText === 'ORDER #')
                                                item.innerText = "Order #";
                                        });
                                    }
                                    else if (this.htmlFor === 'Subject') {
                                        $(this).children('span').each(function (idx, item) {
                                            if (item.innerText === 'HOW CAN WE HELP YOU?')
                                                item.innerText = "¿COMO PODEMOS AYUDARTE?";
                                        });
                                    }
                                });
                            }
                            /* Focus and focusout  for embedded pre chat forms */
                            $('.fieldList input').each(function () {
                                /* Focus and focusout for input */
                                $(this).focus(function () {
                                    $(this).parent('div').children('label').addClass('focus');
                                }).blur(function () {
                                    if ($(this).val() == '') {
                                        $(this).parent('div').children('label').removeClass('focus');
                                        $(this).parent('div').find('.icon_delete').hide();
                                    }
                                });

                                if ($(this).val() == '') {
                                    $(this).parent('div').children('label').removeClass('focus');
                                    $(this).parent('div').find('.icon_delete').hide();
                                }
                                else {
                                    $(this).parent('div').children('label').addClass('focus');
                                    $(this).parent('div').find('.icon_delete').show();
                                }

                                $(this).click(function () {
                                    $(this).focus();
                                    $('.fieldList .embeddedServiceSidebarFormField').each(function () {
                                        if ($(this).find('input').val() == '') {
                                            $(this).find('label').removeClass('focus');
                                            $(this).find('.icon_delete').hide();
                                        }
                                    });
                                    $(this).parent('div').children('label').addClass('focus');
                                });

                                if ($(this).is(":focus")) {
                                    $(this).parent('div').children('label').addClass('focus');
                                }
                            });
                        });
                    });
                    embedded_svc.addEventHandler("onInviteAccepted", function (data) {
                        console.log('onInviteAccepted');
                    });

                    if (!embedded_svc.isDesktop()) {
                        //fnAddA11yHooks();
                    }

                    embedded_svc.settings.enabledFeatures = ['LiveAgent'];
                    embedded_svc.settings.entryFeature = 'LiveAgent';
                    embedded_svc.init(
                        _sfChatBaseCoreURL,
                        _sfChatCommunityEndpointURL,
                        gslbBaseURL,
                        _sfChatOrgId,
                        _sfChatEswConfigDevName_1st,
                        {
                            baseLiveAgentContentURL: _baseLiveAgentContentURL,
                            deploymentId: _deploymentId,
                            buttonId: _buttonId_1st,
                            baseLiveAgentURL: _baseLiveAgentURL,
                            eswLiveAgentDevName: _eswLiveAgentDevName_1st,
                            isOfflineSupportEnabled: false
                        }
                    );

                    //
                    document.getElementById('closeInvite').onclick = function () {
                        embedded_svc.inviteAPI.inviteButton.rejectInvite();
                        fnInit2ndInvitation(); // reset for 2nd invitation
                    };
                    document.getElementById('rejectInvite').onclick = function () {
                        embedded_svc.inviteAPI.inviteButton.rejectInvite();
                        fnInit2ndInvitation(); // reset for 2nd invitation
                    }; // use this API call to reject invitations
                    document.getElementById('acceptInvite').onclick = function () { embedded_svc.inviteAPI.inviteButton.acceptInvite(); }; // use this API call to start chat from invitations
                    document.addEventListener('keyup', function (event) { if (event.keyCode == 27) { embedded_svc.inviteAPI.inviteButton.rejectInvite(); } })

                    fnSetSFInviteOrdervalue();

                    var fnInit2ndInvitation = function () {
                        // check if current live chat init is 1st invitation
                        if (embedded_svc.settings.eswConfigDevName == _sfChatEswConfigDevName_1st) {
                            // reset for 'Forever21AutomaticInvitation2_US'
                            embedded_svc.init(
                                _sfChatBaseCoreURL,
                                _sfChatCommunityEndpointURL,
                                gslbBaseURL,
                                _sfChatOrgId,
                                _sfChatEswConfigDevName_2nd,
                                {
                                    baseLiveAgentContentURL: _baseLiveAgentContentURL,
                                    deploymentId: _deploymentId,
                                    buttonId: _buttonId_2nd,
                                    baseLiveAgentURL: _baseLiveAgentURL,
                                    eswLiveAgentDevName: _eswLiveAgentDevName_2nd,
                                    isOfflineSupportEnabled: false
                                }
                            );
                        }
                    };
                };

                var toggleInert = function (disabled) {
                    var bodyElem = document.getElementById("body-sfchat-content");
                    if (bodyElem) {
                        bodyElem.inert = disabled;
                    }
                }
                var fnAddA11yHooks = function () {
                    embedded_svc.addEventHandler('afterMaximize', function (data) {
                        toggleInert(true);
                    });
                    embedded_svc.addEventHandler('afterDestroy', function (data) {
                        toggleInert(false);
                    });
                    embedded_svc.addEventHandler('afterMinimize', function (data) {
                        toggleInert(false);
                    });
                }
                if (!window.embedded_svc) {
                    var _scriptURL = '';
                    if ("US" === "US") {
                        //_scriptURL = 'https://forever21callcenter--f21scpart.my.salesforce.com/embeddedservice/5.0/esw.min.js'; // staging
                        _scriptURL = 'https://forever21callcenter.my.salesforce.com/embeddedservice/5.0/esw.min.js'; // prod
                    }
                    else if ("US" === "MX") {
                        //_scriptURL = 'https://forever21callcenter--f21scpart.my.salesforce.com/embeddedservice/5.0/esw.min.js'; // staging
                        _scriptURL = 'https://forever21callcenter.my.salesforce.com/embeddedservice/5.0/esw.min.js'; // prod
                    }

                    var s = document.createElement('script');
                    s.setAttribute('src', _scriptURL);
                    s.onload = function () {
                        fnInitESW(null);
                    };
                    document.body.appendChild(s);
                } else {
                    fnInitESW('https://service.force.com');
                }
            </script>
            <!-- Salesforce Live Chat E -->
    <script type="text/javascript">
        (function () {
            var qtm = document.createElement('script'); qtm.type = 'text/javascript'; qtm.async = 1;
            qtm.src = 'https://cdn.quantummetric.com/qscripts/quantum-forever21.js';
            var d = document.getElementsByTagName('script')[0];
            !window.QuantumMetricAPI && d.parentNode.insertBefore(qtm, d);
        })();
    </script>
</body>
</html>