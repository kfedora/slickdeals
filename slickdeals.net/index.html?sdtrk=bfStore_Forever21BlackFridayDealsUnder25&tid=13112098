        var fnPriceFormat = function (listPrice, originalPrice) {
            var price = parseFloat(listPrice).toFixed(2);
            var original = '';
            if (originalPrice != undefined && originalPrice != null)
                original = parseFloat(originalPrice).toFixed(2);

            if (price !== original && original != '') {
                return '<span class="p_old_price pl_5">' + fnSetPriceFormat(original) + '</span> <span class="p_price">' + fnSetPriceFormat(price) + '</span>';
            } else {
                return '<p class="p_price">' + fnSetPriceFormat(price) + '</p>';
            }
        }
        var transient = {};
        var fnCallbackCertonaForHomePage = function (result) {
            //console.log(result);

            var objJSON = '{"value" : ' + result.Resonance["Response"][0].output.replace('Best Sellers', '') + '}';
            $('#hiddenCertonaResult')[0].value = objJSON;

            try {
                var objJSON = $('#hiddenCertonaResult')[0].value;
                var convertJSON = $.parseJSON(objJSON.replace(/[\n\r]+/g, ''));

                $(document).ready(function () {
                    // Push "Best Sellers" list to GTM
                    _cpua.productListLoaded('home best sellers certona', $.map(convertJSON.value,
                        function (product) {
                            return {
                                brand: product.Brand,
                                category: product.Category,
                                id: product.ParentGroupID,
                                name: product.ProductName,
                                originalPrice: parseFloat(product.ListPrice).toFixed(2),
                                price: parseFloat(product.SalePrice).toFixed(2)
                            };
                        }));
                });
                fnLoadTemplate('/catalog/productrecommendation.html'
                    , function (data) {
                        fnRenderHTML($('#CarouselAreaCertona'), data, convertJSON.value,
                            {
                                priceFormat: fnPriceFormat,
                                checkCountry: fnCheckCountry,
                                checkCookie: fnCheckCookieLang
                            },
                            function () {

                                $('#CarouselAreaCertona').owlCarousel({
                                    //margin: 10,
                                    loop: true,
                                    responsive: {
                                        0: {
                                            items: 3,
                                            slideBy: 3,
                                            nav: false,
                                            dots: false
                                        },
                                        600: {
                                            items: 3,
                                            slideBy: 3,
                                            nav: true,
                                            dots: false
                                        },
                                        1000: {
                                            items: 6,
                                            slideBy: 6,
                                            nav: true,
                                            dots: false
                                        }
                                    }
                                });

                        var chkContainer;

                        chkContainer = setInterval(function () {
                            if ($('.homepage_carousel').length == 4) {
                                    $('#certonaCarouselContainer').insertAfter($('.homepage_carousel').first());
                                    //$('.homepage_carousel').last().hide();
                                    if (convertJSON.value.length > 0) {
                                        //hide inhouse best sellers
                                        $('.homepage_carousel:eq(2)').hide();
                                    } else {
                                        $('#certonaCarouselContainer').hide();
                                    }
                            }

                            clearInterval(chkContainer);

                        }, 2000);

                        if (convertJSON.value.length > 0) {
                                        $('#home_rr').show();
                        }
                    },
                                null);
                    if ('US' == 'US') {
                        (fnOpenQuickViewPopupByButton());
                    }
                },
                null, $.home.data.catalogResource);
                }
                catch (err) {
                    $('#certonaCarouselContainer').hide();
                    //console.log(err);
                }
            };

        function fnCheckContainerHome() {
            var d = new Date();
            var t = d.toLocaleTimeString();
            document.getElementById("demo").innerHTML = t;
        }

        function myStopFunction() {
            clearInterval(myVar);
        }
        </script>

        <script type="text/javascript">
        var fnShowLanguagePopup = function () {
            var langpop = getCookie('_slpu' + COOKIE_POSTFIX);
            if (window.location.href.indexOf("/eu/") >= 0 && !isBotOrCrawler() && langpop === '') {
                fnLoadTemplate('/info/LanguagePop.html', function (data) {
                    fnNoscrollClassEvent('add');
                    $('body').prepend(data);
                    $('#langPop').show();
                });
            }
        }
        var fnCloseLanguagePopup = function () {
            //setCookie('_slpu' + COOKIE_POSTFIX, '1', 10 * 365);
            fnNoscrollClassEvent('remove');
            $('#langPop').hide();
        }
        var fnChangeLanguagePop = function (language) {
            language = language.toLowerCase();
            if (language === 'en-us')
                language = 'en';
            setCookie('_cl' + COOKIE_POSTFIX, language, 365);
            //setCookie('_slpu' + COOKIE_POSTFIX, '1', 10 * 365);
            if (window.location.href.indexOf("lang=") > -1) {
                window.location.href = window.location.href.substring(0, window.location.href.indexOf("lang=")) + "lang=" + language;
            } else {
                if (window.location.href.indexOf("?") > -1) {
                    window.location.href = window.location.href + "&lang=" + language;
                }
                else {
                    window.location.href = window.location.href + "?lang=" + language;
                }
            }
        }

