            var allowedScripts = [];
            function checkIfNotNecessaryCookie(name) {
                switch (name) {
                    case 'DC':
                    case '_clUS': case 'UPUS': case 'us_privacy':
                    case 'OptanonConsent': case 'OptanonAlertBoxClosed':
                    case 'LoggedInUS': case 'LoggedUserUS': case 'LoggedEmailUS': case 'LoginCheckUS': case 'LoggedGuestUS':
                        return false; break;
                    default: return true;
                }
            }
            function deleteOldCookies() {
                var cookies = document.cookie.split(';');
                for (var i = 0; i < cookies.length; i++) {
                    var name = cookies[i].split('=')[0].trim();
                    if (checkIfNotNecessaryCookie(name)) {
                        var expireDate = new Date();
                        expireDate.setDate(expireDate.getDate() - 1);
                        document.cookie =
                            name
                            + "= ; expires="
                            + expireDate.toGMTString()
                            + (name === 'crl8.fpcuid' || name === 'SafariEventTrigger'
                                ? "; path=" + location.pathname.slice(0, location.pathname.lastIndexOf('/')) : "; path=/");
                    }
                }
            };
            function ccpaGetCookie(name) {
                var cookies = document.cookie.split(';');
                    for (var i = 0; i < cookies.length; i++) {
                        var cname = cookies[i].split('=')[0].trim();
                        if (cname === name) return cookies[i].split(name)[1].trim();
                    }
                return '';
            }
            function performanceCallback() {
                !function waiting(i) {
