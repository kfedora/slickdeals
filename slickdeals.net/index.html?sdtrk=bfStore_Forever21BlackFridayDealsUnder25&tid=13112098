    <script>
        var checkIfEUUK = function () {
            switch (GlobalE.Country) {
                case 'AT':
                case 'BE': case 'BG':
                case 'CY': case 'CZ':
                case 'DE': case 'DK':
                case 'EE': case 'ES':
                case 'FI': case 'FR':
                case 'GB': case 'GR':
                case 'HR': case 'HU':
                case 'IE': case 'IT':
                case 'LT': case 'LU': case 'LV':
                case 'MT':
                case 'NL':
                case 'PO': case 'PT':
                case 'RO':
                case 'SE': case 'SI': case 'SK':
                case 'UK':
                    return true;
                default: return false;
            }
        }
        $(document).ready(function () {
            !function handleCCPAInternational() {
                try {
                    var load = true;
                    $.each(Consent, function (i, option) {
                        if (!checkOption("C0002", option)) {
                            load = false;
                        } else if (!checkOption("C0003", option)) {
                            load = false;
                        } else if (!checkOption("C0004", option)) {
                            load = false;
                        } else if (checkOption("C0005", option)) { }
                    });
                    (!!GlobalE.Country && !!GlobalE.Currency) ?
                        ((getCookie('isEUUK') !== 'Y' && load && !!Optanon) ? (
                            CONSENT_PRELOAD = true,
                            $.getScript("/us/shop/_common/20201013_28649/js/cookies/performanceCookies.js", function () {
                                $.ccpa.performance.google.ga.init();
                                $.ccpa.performance.google.gtm.init();
                                $.ccpa.performance.microsoft.init();
                            }),
                            $.getScript("/us/shop/_common/20201013_28649/js/cookies/functionalCookies.js", function () {}),
                            $.getScript("/us/shop/_common/20201013_28649/js/cookies/targetingCookies.js", function () {
                                $.ccpa.targeting.salesforce.init();
                                $.ccpa.targeting.dynamicyield.init();
                                if (location.href.indexOf('/mx/shop') < 0) {
                                    $.ccpa.targeting.curalate.init();
                                }

                                $.ccpa.targeting.friendbuy.init();
                            }),
                            $.getScript("/us/shop/_common/20201013_28649/js/data/bloomreach.js", function () {
                                if (typeof fnSendBloomreachData != 'undefined' && $.isFunction(fnSendBloomreachData)) {
                                    fnSendBloomreachData('init');
                                }
                            })) : false)
                        : setTimeout(handleCCPAInternational, 100);
                } catch (e) { setTimeout(handleCCPAInternational, 100); }
            }();
            $('a.UsableNetAssistive').on('click', function () { // This will enable the OneTrust banner to show up one more time for ADA users
                setCookie('_lb', 1, 1);
            });
            $("#onetrust-pc-btn-handler").ready(function () {
                !function swap() {
                    setTimeout(function () { ((userDevice.isMobile && $("#onetrust-pc-btn-handler").length > 0) ? ($("#onetrust-pc-btn-handler").before($("#onetrust-accept-btn-handler")), console.log('swap')) : swap()) }, 100);
                }();
            });
            $("#onetrust-accept-btn-handler").ready(function () {
                if (userDevice.isMobile) {
                    !function observerOtBanner() {
                        setTimeout(function () {
                            var acceptBtn = $('#onetrust-accept-btn-handler');
                            if (acceptBtn.length > 0) {
                                var target = document.getElementById('onetrust-banner-sdk');
                                var observer = new MutationObserver(function (mutations) {
                                    mutations.forEach(function (mutation) {
                                        if (mutation.oldValue.indexOf('visibility: hidden') > 0 && $('#branch-banner-iframe').length > 0 ) {
                                            document.body.style.marginTop = '76px';
                                            observer.disconnect();
                                        }
                                    });
                                });
                                var config = {
                                    attributes: true,
                                    attributeOldValue: true
                                };
                                (observer) ? observer.observe(target, config) : '';
                            } else {
                                getCookie('OptanonAlertBoxClosed').length > 0 ? '' : observerOtBanner();
                            }
                        }, 1000);
                    }();
                }
            });
        });
