        function checkReferrer() {
            return ((document.referrer.indexOf('forever21.com') < 0
                  && document.referrer.indexOf('localhost') < 0
                  && document.referrer.indexOf('forever21us.') < 0)
                || getCookie('isEUUK') === 'Y'
                || getCookie('_lb') == '1'); // Bypass for GDPR on referrer ** requested to not hide on gdpr
        }
        var h = false, otBanner = '#onetrust-banner-sdk';
        $(otBanner).ready(function () {
            ((!checkReferrer() || getCookie('us_privacy').indexOf('1YY') > -1) ? !function hideot(a) {
                ((typeof Optanon != 'undefined'
                    && $('#onetrust-banner-sdk').length > 0
                    && a > 0) ? (
                        Optanon.Close(),
                        setCookie('_dnc', '1', '1'),
                        $('#onetrust-banner-sdk').remove(),
                        setTimeout(hideot, 10, a - 1)
                    ) : setTimeout(hideot, 10, a - 1));
            }(30) : deleteCookie('_lb'))

        });
        $(window).on('load', function () {
            setTimeout(function () {
                if (location.href.indexOf('/mx/shop') > -1 && getCookie('OptanonConsent') != '') {
                    Optanon.Close();
                }
            }, 2000);

        });
        window.dataLayer = window.dataLayer || [];
        window._cpua = window._cpua || {};
        // For ABTest
        var fnGoCategoryPage = function (brand, category)
        {
            if (fnRecommendABTest() === 'B') {
                var abTestCategory = "";
                        var arrList = abTestCategory.split(';');
                if ($.inArray(category, arrList) > -1) {
                    location.href = SITE_PREFIX + '/catalog/category/' + brand + '/' + category + '_b';
                }
                else {
                    location.href = SITE_PREFIX + '/catalog/category/' + brand + '/' + category;
                }
            }
            else
            {
                location.href = SITE_PREFIX + '/catalog/category/' + brand + '/' + category;
            }
        }
        var fnRecommendABTest = function () {
            var abtype = "A";
                        //Checking Cookie value
                        var abtype = getCookie("ABT");
            if (abtype == "") {
                //Create Cookie
                abtype = "A";
                //50%
                var ranVal = Math.floor((Math.random() * 2) + 1);
                if (ranVal == 2) {
                    abtype = "B";
                }
                setCookie("ABT", abtype, 0.05);
            }
            return abtype;
        }
