<script>
    //$('#txt_promoCode').val(getCookie("SOCOUPON"));
    var // Trigger for expanding on promo pop up
        expand = true,
        // Text
        allOffersText = fnReplaceTextUsingResourceData(COMMONRESOURCE, '<%lbAllSpecialOffers%>') + ' (<%seeAllOffers%>)',
        promoError = 'An error occured with this promo code.',
        promoSuccess = 'Discount Applied!',
        // Device Info
        device = {},
        carouselOptions = {
            loop: true, nav: true, dots: false,
            items: 1, slideBy: 1,
            touchDrag: true, mouseDrag: false,
            autoplayHoverPause: true,
            smartSpeed: 1000, slideSpeed: 1000
        };
    /* Page Load & Render S */
    $(document).ready(function () {
        if (userDevice.isMobile) {
            $('#container_topbar_c').insertAfter('.bg_white.container.row');
            $('.mainsearch').addClass('open');
            $('#ihKeyword, #search_placeholder').on('click', function () {
                $('#tspbanner').hide();
                $('.mainsearch').hide();
                $('input#ihKeyword').trigger('open.brm_suggest');
                $('#brRecentTrending').show();
                
                $('.mainsearch').attr('style', 'position:absolute !important;top:86% !important;');
                $('.mainsearch').show();
            });
            $('.slider_close, .m_menu_cover').on('click', function () {
                $('.mobilesearch').removeClass('open');
            });
        }
        handleLoggedInUser();
        fnCheckDeviceType(device);
        var specialOffersData;
        if (typeof (Storage) !== "undefined") {
            specialOffersData = sessionStorage.getItem('specialOffers_' + COOKIE_POSTFIX);
            if (specialOffersData === null) { // if session storage has no special offers data
                executeAJAX("/Info/GetSpecialOffers", "POST", "JSON",
                    { sort: 2, page: 1, pageSize: 30 }, !0
                    , function (response) {
                        response.Custom01 = new Date(); // set time.
                        sessionStorage.setItem('specialOffers_' + COOKIE_POSTFIX, JSON.stringify(response));
                        renderOffers(response);
                    }
                    , console.log, false);
            }
            else {
                var jsonData = JSON.parse(specialOffersData);
                if (new Date() - new Date(jsonData.Custom01) <= 1800000) { // 1000 * 60 * 30
                    renderOffers(jsonData); // use session storage data
                } else {
                    // reset data every 30 minutes
                    executeAJAX("/Info/GetSpecialOffers", "POST", "JSON",
                        { sort: 2, page: 1, pageSize: 30 }, !0
                        , function (response) {
                            response.Custom01 = new Date(); // set time.
                            sessionStorage.setItem('specialOffers_' + COOKIE_POSTFIX, JSON.stringify(response));
                            renderOffers(response);
                        }
                        , console.log, false);
                }
            }
        } else {
            executeAJAX("/Info/GetSpecialOffers", "POST", "JSON",
                { sort: 2, page: 1, pageSize: 30 }, !0, renderOffers, console.log, false);
        }
        applyEvents();
        

        //$('.d_new_mega_menu.hover_p').css('color:white');
        $('#rr_logo')[0].src = '/us/shop/_common/images/rr_white.png';
        $('#FirstDepth_rr').hover(
            function () {
                $('#rr_logo')[0].src = '/us/shop/_common/images/rr.png';
            },
            function () {
                $('#rr_logo')[0].src = '/us/shop/_common/images/rr_white.png';
            }
        );
        //if (location.href.indexOf('/Search') > -1) {
        //    if (userDevice.isMobile) {
        //        $('.mainsearch').attr('style', 'position: initial !important;');
        //    }
        //    $('.mainsearch').hide();
        //}
    });
    var handleLoggedInUser = function () {
        var device = {}; fnCheckDeviceType(device);
        if (!device.isMobile) {
            if (fnIsLoggedIn()) {
                $('#userfName').html('Hi, ' + fnGetAcctInfoFromCookie().FirstName + '!');
                $('#userfName').on('click', function () {
                    location.href = SITE_PREFIX + (fnIsLoggedIn() ? '/account' : '/account/signin');
                });
            } else {
                $('.top_new_account').removeClass('signedin');
            }
        } else {
            $('#userfName').on('click', function () {
                location.href = SITE_PREFIX + (fnIsLoggedIn() ? '/account' : '/account/signin');
            });
        }
    }
    // using Global-E, load lang selection only for US
    //var fnSetTopbarLanguageByGE = function () {
    //    !function waiting(i) {
    //        try {
    //            var targets = ["US"];
    //            if (!fnCheckCountry('us')) return;
    //            var runSetTopbarLanguage = function (country) {
    //                var found = false;
    //                found = targets.indexOf(country.toUpperCase()) > -1;
    //                if (found) {
    //                    //desktop
    //                    if ($('#divTopbarLangSelection').length > 0) {
    //                        $('#container_topbar_r').removeClass('col_l_3_of_16').addClass('col_l_4_of_16');
    //                        $('#container_topbar_c').removeClass('col_l_8_of_16').addClass('col_l_7_of_16');
    //                        $('#divTopbarLangSelection').show();
    //                    }
    //                    //mobile
    //                    $('#divMobileLangSelection').show();
    //                    var owlCarousel = $('#tspbanner').closest('.owl-carousel').data('owl.carousel');
    //                    if (typeof owlCarousel != "undefined") owlCarousel.refresh();
    //                    return;
    //                }
    //            }
    //            var geCookie = getCookie('GlobalE_Data') !== '' ? $.parseJSON(decodeURIComponent(getCookie('GlobalE_Data'))) : null;
    //            if (geCookie !== null && geCookie.countryISO !== null) {
    //                runSetTopbarLanguage(geCookie.countryISO);
    //            } else if (typeof GlobalE != "undefined" && !!GlobalE.Country) {
    //                runSetTopbarLanguage(GlobalE.Country);
    //            } else { setTimeout(waiting, 500); }
    //        } catch (e) { }
    //    }();
    //}
    var applyEvents = function () {
        $('.expand_p').off('click');
        $('.expand_p').on('click', function () {
            if (location.href.indexOf('/catalog/product/') > -1) { // fixed the bug that expand/collapse all content
                $(this).next().slideToggle();
            }
            else {
                $(this).next('div.expand_c, dd.expand_c').slideToggle();
            }
            $(this).toggleClass('active');
        });
        $(".m_menu_cover, .slider_close").on("click", function () {
            $('#txtBloomreachSearch').val('');
        });
        $('.top_new_menu').on('click', function () {
            closeSearch();
            if (userDevice.isMobile && $('#container_topbar_c').css('display') == 'none') {
                $('#container_topbar_c, .mainsearch, .tabbed').hide();
            }
            $('#search_placeholder_mobile').show();
        });
        if (device.isMobile) {
            $('#offMenu').remove();
        } else {
            $('#offMenu').on('click', function () {
                if ($('.mainsearch.open').length > 0) {
                    closeSearch();
                    $('#offMenu').hide();
                }
            });
        }
        $('#txtBloomreachSearch, #ihKeyword').bind('keydown', function (e) {
            if (e.keyCode == 13) {
                $('.slider_close, .m_menu_cover, .container.clearfix.bg_white').removeClass('show').addClass('hide');
                $('.l_container').removeClass("open");
                fnNoscrollClassEvent('remove');
                $('#txtBloomreachSearch, #ihKeyword').val('');
                $('#offMenu').remove();
                // Please dont remove this 
                $('.mainsearch').hide();
            }
        });
        $('.mainsearchsugg a').on('click', function () {
            $('.mainsearch').hide();
        });
        $('input.mobileleftsearch').on('change paste keyup', function (e) {
            if ($(e.target).val() == '') {
                $('.mobilesearch').find('.clearfix.table').show();
                setTimeout(function () { $('.mobileleftsearch').trigger('click'); }, 1000);
            }
        });
    }
    var renderOffers = function (result) {
        if (result.ReturnCode === "00" && result.specialOffers !== null) {
            renderBanner(result);
            renderPopup(result);
        }
    }
    var renderBanner = function (data) {
        var dataForBanner = [];
        for (var i = 0; i < data.specialOffers.length; i++) {
            if (data.specialOffers[i].ShowOnTopBanner === 'Y')
                dataForBanner.push(data.specialOffers[i]);
        }

        fnLoadTemplate('/info/promoSlider.html', function (template) {
            fnRenderHTML('#tspbanner', template, dataForBanner, { truncateText: truncateText, fnCheckGlobalECountry: fnCheckGlobalECountry }, function () {
                $('#allOffersLink').html(allOffersText.replace('<%seeAllOffers%>', data.specialOffers.length));
                $.extend(carouselOptions, { autoplay: dataForBanner.length > 1 });
                $.extend(carouselOptions, {
                    onInitialized: function () {
                        try {
                            /// remove duplicate ID generated by carousel
                            if ($('[id="GlobaleFreeShippingBanner_Standard"]') && $('[id="GlobaleFreeShippingBanner_Standard"]').length > 1) {
                                $('.cloned [id="GlobaleFreeShippingBanner_Standard"]').remove();
                            }
                            if ($('[id="GlobaleFreeShippingBanner_Express"]') && $('[id="GlobaleFreeShippingBanner_Express"]').length > 1) {
                                $('.cloned [id="GlobaleFreeShippingBanner_Express"]').remove();
                            }
                        } catch (e) { }
                    }
                });
                $('#tspbanner').owlCarousel(carouselOptions);
                if (device.isMobile) {
                    $('#tspbanner').find('.owl-prev').hide();
                    $('#tspbanner').find('.owl-next').hide();
                } else {
                    $('#tspbanner').find('.owl-prev').addClass('owl-prev_trans').removeClass('owl-prev').html('');
                    $('#tspbanner').find('.owl-next').addClass('owl-next_trans').removeClass('owl-next').html('');
                }
                // temp fix TODO _ck
                setTimeout(function () { $('#tspbanner').find('.owl-item').addClass('pr_20'); }, 1000);
            });
        }, function (e) { console.log('Unable to load banner template:', e); }, null);
    }
    var renderPopup = function (bannerData) {
        fnLoadTemplate('/info/promoBannerPop.html', function (template) {
            fnRenderHTML('#offerpop', template, bannerData, {}, function () {
                $('#seeAllOffers').on('click', function () {
                    $.flexModal.open('#offerpop');
                    $('#offerpop').find('.expand_p').off('click');
                    $('#offerpop').find('.expand_p').on('click', function () {
                        (!expand) ? expand = true
                            : ($(this).hasClass('active')) ? ($(this).removeClass('active'), $(this).next().slideToggle())
                                : ($(this).addClass('active'), $(this).next().slideToggle());
                    });
                });
            });
        }, function () { }, null);
    }
    var truncateText = function (text, max, other, otherMax) {
        var device = {}; fnCheckDeviceType(device);
        if (device.isMobile || true) {
            var ellipses = '...';
            if (typeof other === 'undefined' || typeof otherMax === 'undefined' || other === '') {
                if (text.length > max + otherMax)
                    return text.slice(0, (max + otherMax) - ellipses.length) + ellipses;
            } else {
                var extra = otherMax - other.length;
                if (extra > 0)
                    max += extra;
                text = text.slice(0, max - ((text.length > max) ? ellipses.length : 0)) + ((text.length > max) ? ellipses : '');
            }
        }
        return text;
    }
    var fnCheckGlobalECountry = function () {
        var result = false;

        // check GlobalE selected country
        !function waiting(i) {
            var geCookie = getCookie('GlobalE_Data') !== '' ? $.parseJSON(decodeURIComponent(getCookie('GlobalE_Data'))) : null;
            //geCookie = $.parseJSON('{"countryISO":"HN","currencyCode":"HNL","cultureCode":"es"}');
            if (geCookie !== null && geCookie.countryISO !== 'US') {
                result = true;
            } else if (typeof GlobalE != "undefined" && !!GlobalE.Country && GlobalE.Country != "US") {
                result = true;
            } else if (i > 0) setTimeout(waiting(i - 1), 500);
        }(50);

        return result;
    }
    /* Page Load & Render E */
    /* Events S */
    var fnApplyPromoToCart = function (promo, pos) {
        expand = false;
        //fnPreloadForLoggedin(false, SITE_PREFIX + '/Account/SignIn');
        executeAJAX('/Checkout/ApplyDiscount', "POST", "JSON", {
            userid: fnGetUserId(),
            promoCode: promo,
            restrictedCodeName: promo,
            creditCardListName: ''
        }, true,
            function (result) {
                if (location.href.indexOf('/checkout/basket') > 0) {
                    $("#txt_promoCode").val(promo);
                    if (result.ReturnCode == "00") {
                        $.basket.data.basketData.LineItems = result.LineItems;
                        $.basket.data.basketData.OrderSummaryField = result.Summary;
                        $.basket.data.basketData.ShippingInfoField.IsFreeShippingAvailable = result.ShippingInfoField.IsFreeShippingAvailable;
                        $('#err_promoCode').hide();
                        if (!$('#divPromoArea > .expand_p').hasClass('active')) {
                            $('#divPromoArea > .expand_p').click();
                            $('#txt_promoCode').parent().addClass('focus');
                        }
                        fnBindBasket($.basket.data.container, $.basket.data.basketData);
                    } else {
                        if ($("#txt_promoCode").val().toLowerCase().indexOf("00005901011") == 0) {
                            result.ErrorMessage = "To select your free gift, add the qualifying item available <u><a href='/us/shop/catalog/category/f21/promo-beauty-gwp'>here</a></u> to your shopping bag.";
                        }
                        if (!$('#divPromoArea > .expand_p').hasClass('active')) {
                            $('#divPromoArea > .expand_p').click();
                            $('#txt_promoCode').parent().addClass('focus');
                        }
                        $("#divPromoInfo").hide();
                        $("#err_promoCode").html(result.ErrorMessage);
                        $("#err_promoCode").show();
                    }
                    $('#txt_promoCode').next().trigger('click');
                    showPromoMessage(result.ReturnCode == "00" ? fnReplaceTextUsingResourceData(COMMONRESOURCE, '<%lbPromoMessage1%>') : result.ErrorMessage, result.ReturnCode != "00", pos);
                } else {
                    if (result.ErrorMessage.indexOf('name: index') > -1) {
                        showPromoMessage(fnReplaceTextUsingResourceData(COMMONRESOURCE, '<%lbPromoMessage1%>'), false, pos);
                    } else {
                        showPromoMessage(result.ReturnCode == "00" ? fnReplaceTextUsingResourceData(COMMONRESOURCE, '<%lbPromoMessage1%>') : result.ErrorMessage, result.ReturnCode != "00", pos);
                    }
                }
            }, function (error) {
                showPromoMessage(error, true, pos);
            }, true);
    }
    var showPromoMessage = function (message, isError, pos) {
        var device = {}; fnCheckDeviceType(device);
        var withModal = $('article#offerpop').hasClass('flex-modal-item--visible'),
            id = withModal ? (isError ? '#noapplied' + pos : '#applied' + pos) : (isError ?  '#sbe>.noapplied' : '#sbe>.applied');
        $(id).html('');
        $(id).append('<i class="fas ' + (isError ? 'fa-exclamation-circle' : 'fa-check') + 'pr_10" style="font-size:11px;" id="promomsgtop"><%msg%></i>');
        $('#promomsgtop,' + id).text(device.isMobile ? (isError ? 'Error' : 'Success') : message.replace('Basket is empty.', fnReplaceTextUsingResourceData(COMMONRESOURCE, '<%lbPromoMessage2%>')));
        $(id).fadeToggle();
        setTimeout(function () { $(id).fadeToggle(500); }, 1500);
    }
    var openSearch = function (showPlaceholder, isLeftMobile) {
        if (userDevice.isMobile) {
            $('html, body').scrollTop(0);
        }
        //$('.mobilesearch').show();
        updateSearchTextBox('', showPlaceholder);
        if (!userDevice.isMobile) {
            $('div.search_exp').addClass('open');
            
        }
        
        if (isLeftMobile) {
            fnNoscrollClassEvent('add'); // CK's test code
            $('.mobilesearch').addClass('open');
            $('input#txtBloomreachSearch').trigger('open.brm_suggest');
            $('input#ihKeyword').trigger('open.brm_suggest');
            $('#txtBloomreachSearch').focusout(function (e) { // Mobile iphone 'done' fix
                if ($('#txtBloomreachSearch').val() != '') {
                    $('#search_placeholder_mobile').hide();
                }
            });
            $('.mainsearch').hide();
            // CK's test code
            //// mobile test temp
            //$('html, body').on('touchmove', function (e) {
            //    console.log('asdasd');
            //    e.preventDefault();
            //    $('html, body').scrollTop(0);
            //});
            //$('.m_menu_cover').on('click', function () {
            //    $('html, body').off('touchmove');
            //});
        } else {
            $('.mainsearch').show();
            $('#search_placeholder').show();
            if (!userDevice.isMobile) {
                $('input#ihKeyword').trigger('open.brm_suggest');
                $('input#txtBloomreachSearch').trigger('open.brm_suggest');
                setTimeout(function () {
                    $('#ihKeyword').trigger('focus');
                    $('#ihKeyword').trigger('click');
                    $('#brRecentTrending').show();
                }, 100);
            }
        }

        $('#offMenu').show();
    }
    var openQuickSearch = function (showPlaceholder) {
        updateSearchTextBox('', showPlaceholder);
        //$('div.search_exp').addClass('open');
        $('#ihKeyword').trigger('open.brm_suggest');
    }
    var closeSearch = function () {
        //$('html, body').off('touchmove'); // CK's test code
        updateSearchTextBox('', true);
        //$('#search_exp_mobile, #seach_exp_desk, .search_exp.fc').removeClass('open');
        if (!userDevice.isMobile) {
            $('#search_exp_mobile, #seach_exp_desk, .search_exp.fc').removeClass('open');
            $('.mainsearch').hide();
        } else {
            //fnNoscrollClassEvent('remove'); // CK's test code
            $('#tspbanner').show();
            $('#brRecentTrending').hide();
            $('.mainsearch').attr('style', 'position:initial !important;');
            $('#container_topbar_c, .mainsearch, .tabbed').hide();
            $('#tspbanner').show();
        }
        $('div#recentSearches').hide();
        $('#offMenu').hide();
        if (userDevice.isMobile) {
            $('#brRT').hide();
            $('.mainsearchsugg').hide();
        }
    }
    var updateSearchTextBox = function (text, showPlaceHolder) {
        if (showPlaceHolder && !userDevice.isMobile)
            $('label#search_placeholder, #search_placeholder_mobile').show();
        else
            $('label#search_placeholder, #search_placeholder_mobile').hide();
        $('input#ihKeyword').val(text);
    }
    var clearRecentSearches = function () {
        deleteCookie('recentsearch_' + COOKIE_POSTFIX);
        $('div#recentSearches').html('');
    }
    var searchRecentTerm = function (term) {
        $('label#search_placeholder, #search_placeholder_mobile').hide();
        $('input#ihKeyword, #txtBloomreachSearch').val(term);
        $('.slider_close').trigger('click');
        updateSearchTextBox(term, false);
        fnSearch();
        closeSearch();
    }
    var brUserInputEvent = function () {
        $('input#ihKeyword').on('input', function (e) {
            if ($(e.target).val() != '') {
                $('label#search_placeholder, #search_placeholder_mobile').hide();
            } else {
                $('div#recentSearches').show();
            }
        });
    }
    var openBcEvent = function () {
        $('.slider_close').trigger('click');
    }
    /* Events E */
</script>

