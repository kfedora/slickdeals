            function pushAllowedScripts(script) {
                allowedScripts.push(script);
            }
            function checkForScript(type) {
                return allowedScripts.indexOf(type) > -1;
            }
            function checkOption(category, option) {
                if (typeof option == 'undefined') return false;
                return option.indexOf(category) > -1 && option.indexOf(':1') > -1;
            }

            function OptanonWrapper() { // Callback OneTrust fires
