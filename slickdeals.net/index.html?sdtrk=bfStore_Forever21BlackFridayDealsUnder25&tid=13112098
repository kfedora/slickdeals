        var pos = 0;
        $(window).on('load', function () {
            if (userDevice.isMobile) {
                setTimeout(function () {
                    $('div.owl-carousel').on('touchmove', function (e) {
                        if (e.target.localName === 'img'
                            && (e.target.className != 'product_loading_image' && e.target.className != 'product_image')) {
                            e.preventDefault();
                            $('html, body').scrollTop($(window)[0].scrollY);
                        }
                        $('div.owl-carousel').on('drag.owl.carousel', function (event) {
                            document.ontouchmove = function (e) {
                                return true;
                            }
                        });
                        $('div.owl-carousel').on('dragged.owl.carousel', function (event) {
                            document.ontouchmove = function (e) {
                                return true;
                            }
                        });
                    });
                }, 2000);
            }
        });
    </script>
    <script>
        $(document).ready(function () {
            $.extend({
                ccpa: {
                    debug: false,
                    removeOtClassFix: function () {
                        if (userDevice.isMobile) {
                            $('#onetrust-banner-sdk').removeClass('otfixsm');
                            $('#onetrust-banner-sdk').removeClass('otfixbg');
                        }
                    },
                    addOtClassFix: function (small) {
                        if (userDevice.isMobile) {
                            $('#onetrust-banner-sdk').addClass('otfix' + (small ? 'sm' : 'bg'));
                        }
                    },
                    hasDoNotSellActivated: function () {
                        return getCookie('us_privacy').indexOf('1YY') >= 0;
                    },
                    setPrivacyString: function (value) {
                        console.log('iabccpa', value);
                        setCookie('us_privacy', '1Y' + value + 'N', 365);
                    },
                    log: function (msg, category) {
                        if ($.ccpa.debug)
                            console.log("CCPA - " + category + ":", msg);
                    }
                }
            });
            if (getCookie('isCA') !== 'Y')
                $('#cadns, #mcadns').remove();
        });
        if (userDevice.isMobile) {
            $(window).on('load', function () {
                if (location.href.indexOf('/catalog/product/') > -1) {
                    $.ccpa.addOtClassFix(true);
                }
            });
        }
