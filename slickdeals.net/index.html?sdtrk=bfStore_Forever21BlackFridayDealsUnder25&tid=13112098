            // setting smartbanner style - Branch
            function reformatter() {
                var journeysBanner = document.getElementById('branch-banner-iframe');
                if (typeof journeysBanner != 'undefined' && journeysBanner != null) {
                    try {
                        var style = getComputedStyle(journeysBanner);
                        if (journeysBanner && journeysBanner.style) {
                            if ((style.getPropertyValue('top') < '0px' || style.getPropertyValue('top') == '0px') && style.getPropertyValue('height') == '76px') {
                                journeysBanner.style.setProperty('padding', '0em');
                                var body = document.getElementsByTagName('body')[0];
                                body.style.setProperty('margin-top', '76px');
                            }
                        }
                    } catch (e) {
                        console.log(e)
                    }
                }
            }

            function checkBranchSDK() {
                !function waiting(i) {
                    if (typeof branch != 'undefined' && branch != null) {
                        branch.addListener('willShowJourney', reformatter);
                        var element = document.getElementById('branch-banner-iframe');
                        if (element) {
                            branch.h[0].listener();
                        }
                    } else {
                        setTimeout(waiting, 1000);
                    }
                }();
            }

