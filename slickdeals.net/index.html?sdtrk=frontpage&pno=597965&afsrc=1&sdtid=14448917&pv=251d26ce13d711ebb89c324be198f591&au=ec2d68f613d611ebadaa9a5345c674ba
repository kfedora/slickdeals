	<script type="text/javascript">
	require(["jquery"], function($){
	
		$test = $;
		var ie9 = (navigator.userAgent.toLowerCase().indexOf("msie 9.0") > -1);
	
		
			
			$test(window).resize();
			var invAvail = 'Available';
			var catName = 'Phones';
			if (invAvail != 'Available' && catName == 'SimCards'){
				shoppingActionsJS.hideContentForOOS();
			}
			var select_your_service_plan_display_val = "false";
			if(select_your_service_plan_display_val != null && select_your_service_plan_display_val != undefined && select_your_service_plan_display_val == 'true'){
				$test(".select_your_service_plan_display").show();
				$test('.product_handset_content').hide();
				var planPrice = jQuery('.js-plan.active .choose_plan_lto_price').text();
				if(planPrice != null){
					if(planPrice.indexOf('$') == 0)
						planPrice = planPrice.split('$')[1]; 
				}
				if(planPrice == "" || planPrice == undefined || planPrice == null){
					planPrice="0";
				}				
	    		var planCatalogEntryId = "";
		    		
		    		if(jQuery('.js-plan.active').val() != undefined){
		    			planCatalogEntryId= jQuery('.js-plan.active').attr('id').split('choose_your_plan_section_')[1];
		    		}
		    		if (planCatalogEntryId !== "" && dojo.byId('specialOfferPlanPrice_'+planCatalogEntryId) != null) {
	        		var specialOfferPlanPrice= parseFloat((dojo.byId('specialOfferPlanPrice_'+planCatalogEntryId).value));
	            	if(specialOfferPlanPrice != null && specialOfferPlanPrice != undefined){
	            		planPrice = specialOfferPlanPrice;
	            	}
	        	}
	    		var catalogEntryId = jQuery('.js-plan.active').attr('id');
				var noPlanCatalogEntryId = jQuery('.js-plan.lto-no-plan.no-plan').text();
				
				if(catalogEntryId && catalogEntryId != null && catalogEntryId != undefined){
					console.log("Plan Exists !!");
				}else if(noPlanCatalogEntryId && noPlanCatalogEntryId != null && noPlanCatalogEntryId != undefined){
					console.log("Plan doesnt exists  - select no plan !!");
					// jQuery('.js-plan.lto-no-plan.no-plan').addClass("active");
				}
				var planDecPrice = jQuery('.js-plan.active .choose_plan_lto_price_sup').text();
				var featurePhonePlanPrice = jQuery('.js-plan.active .choose_plan_lto_decimal_price').text();
				if(planDecPrice != undefined && planDecPrice != "" && planPrice!="0"){
		    		jQuery(".price.lto_selected_plancost").html('$'+planPrice+'.'+planDecPrice);
				} else if(featurePhonePlanPrice != undefined && featurePhonePlanPrice != ""){
		    		jQuery(".price.lto_selected_plancost").html('$'+featurePhonePlanPrice);
		    	} else {
					jQuery(".price.lto_selected_plancost").html('$'+planPrice);
				}
				jQuery("a.lto_selected_plan_continue_url").attr("href",jQuery(".js-plan.active a.lto_selected_plan_url").attr("href"));
				var selectedPaymtOption = jQuery("#lto_selected_payment_options").val();
		    	if(selectedPaymtOption == 'SP1') {
		    	jQuery('.accessoriesCarousel').hide();
		    	jQuery('#accessoriesCaroselESpot').hide();
		    		var deviceCost =parseFloat(jQuery("#estMonthlyAmountForGood").val())+ parseFloat(planPrice);    		
		    		jQuery(".price.lto_selected_devicecost").html('$'+deviceCost.toFixed(2)); 
		    		jQuery(".price.lto_selected_plancost").html('$'+deviceCost.toFixed(2));
		    	} else if(selectedPaymtOption == 'SP2'){
		    	jQuery('.accessoriesCarousel').hide();
		    	jQuery('#accessoriesCaroselESpot').hide();
		    		var deviceCost =parseFloat(jQuery("#estMonthlyAmountForBuilding").val())+ parseFloat(planPrice);    		
		    		jQuery(".price.lto_selected_devicecost").html('$'+deviceCost.toFixed(2));  
		    		jQuery(".price.lto_selected_plancost").html('$'+deviceCost.toFixed(2));
		    	} else if(selectedPaymtOption == 'FRP'){
		    		//jQuery('#accessoriesCaroselESpot').html('Donttttttttt');
		    	}			
		    	}else{
				var hash = location.hash;
				if(window.location.hash && hash == '#CHANGEPLAN'){ 
       			//var hash = location.hash;
      			   $test("#pdpSection").hide();
		    	   $test("container_content_rightsidebar").show();
		    	   $test('.product_handset_content').hide();
		    	   $test('#outOfStockDiv').hide();
		    	   $test('#OOSEmailNotifyDiv').hide();
		    	
       		  }else{
       		  	$test(".select_your_service_plan_display").hide();
       		  }
			}
			
			// Hide the other details on change plan flow
			
			var hash = location.hash;
			if(window.location.hash && hash == '#CHANGEPLAN'){ 
       			//var hash = location.hash;
      			   $test("#pdpSection").hide();
		    	   $test("container_content_rightsidebar").show();
		    	   $test('.product_handset_content').hide();
		    	   $test('#outOfStockDiv').hide();
		    	   $test('#OOSEmailNotifyDiv').hide();
		    	
       		  }else{
       		  	$test('#outOfStockDiv').hide();
				$test('#OOSEmailNotifyDiv').hide();       		  
				}
			
		});
