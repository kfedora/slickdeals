	<script type="text/javascript">
			function LoadJSOnDOMReady() {
	
				var jsAssetsDir = '/wcsstore/TFAuroraStorefrontAssetStore/' + 'javascript/';	
				var jslist = 	[
									"libs.js?tf_ver_37.0","scripts.js?tf_ver_37.0","jquery/js/modernizr.custom.js?tf_ver_37.0","jquery/js/jquery.dlmenu.js?tf_ver_37.0","jquery/js/jquery.openmenu.js?tf_ver_37.0","LazyLoad/jquery.lazy.min.js?tf_ver_37.0","/Tealeaf/tealeafWC.js?tf_ver_37.0"								
								];
		
				var jsListLength = jslist.length;
				var i,jsfilepath;
				for (i = 0; i < jsListLength; i++) {
				   jsfilepath =  jsAssetsDir + jslist[i];
				   	var element = document.createElement("script");
					element.src = jsfilepath;
					document.body.appendChild(element);
				 }
	
				// Load BV
				var bvscriptpath = 'https://apps.bazaarvoice.com/deployments/totalwireless/main_site/production/en_US/bv.js';				
				if('true' === 'true')
				{
					var element = document.createElement("script");
					element.src = bvscriptpath;
					document.body.appendChild(element);
			}
						}

			if (window.addEventListener)
			window.addEventListener("load", LoadJSOnDOMReady, false);
			else if (window.attachEvent)
			window.attachEvent("onload", LoadJSOnDOMReady);
			else window.onload = LoadJSOnDOMReady;
		</script>
			
