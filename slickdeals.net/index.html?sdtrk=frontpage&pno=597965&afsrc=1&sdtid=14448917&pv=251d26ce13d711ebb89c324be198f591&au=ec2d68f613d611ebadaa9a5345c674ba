		require(["jquery"], function($) {
		jQuery(document).ready(function(){
			$('body').on('click','#Compare_Popup_Close', function(e){
				var errorId = $("#ErrorMessageId").text();
				if (errorId.indexOf("_ERR_TOO_MANY") >= 0 || errorId.indexOf("_ERR_THRESHOLD_SHOPPING_CART_QUANTITY") >= 0 || errorId.indexOf("_ERR_GENERIC") >=0){
					$('a#lto_header_shopping_cart')[0].click();
				}
			});

		});
	});
