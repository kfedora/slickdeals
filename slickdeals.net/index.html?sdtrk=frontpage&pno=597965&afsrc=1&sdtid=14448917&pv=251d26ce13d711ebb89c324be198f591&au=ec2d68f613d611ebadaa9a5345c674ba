		<script type="text/javascript">
		    function collapseProductDetailsOnMobile() {
		    	if(getScreenSize() == 'xs') {
		                     jQuery('#productDetails').removeClass('in');
		                     jQuery('.icon-angle-up').removeClass('icon-angle-up').addClass('icon-angle-down');
		        }
		    }
		
		    function getScreenSize() {
		        if(jQuery('body div#screensize').length == 0) {
		                     jQuery('body').append('<div id="screensize"><div class="visible-xs"></div><div class="visible-sm"></div><div class="visible-md"></div><div class="visible-lg"></div></div>');              
		        }
		        var screenSize = jQuery('#screensize div:visible').attr('class');
		        return screenSize.replace('visible-','');
		    }
		</script>
	
		
