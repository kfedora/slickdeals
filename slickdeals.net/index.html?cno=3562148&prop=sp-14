$.fn.goTo = function (fudgeFactor) {
  var scrollTopPosition = (fudgeFactor === 'middle')
    ? $(this).offset().top / 2 + 'px'
    : $(this).offset().top - (fudgeFactor || 0) + 'px';

  $('html, body').animate({
      scrollTop: scrollTopPosition
  }, 'fast');
  return this; // for chaining...
};

$(document).ready(function (){
    $("[itemprop=description] a, .pdp-article-specs-link").click(function (){
        $($(this).attr('href')).goTo(100);
    });
});
</script>
<script>

    ( function ( greenskyLibUrl )
    {
        //Load CSS Library for the Modal Dialog
        var gsstyle = document.createElement( 'link' );
        gsstyle.rel = 'stylesheet';
        gsstyle.href = greenskyLibUrl + '/aslowas/gs-styles.css' + '?ts=' + new Date().getTime();
        var firstStyle = document.getElementsByTagName( "link" )[ 0 ];
        firstStyle.parentNode.insertBefore( gsstyle, firstStyle );

        // Load GS as Low as Library
        var script = document.createElement( "script" );
        script.type = "text/javascript";
        // Enable Async to not stop the page load
        script.async = !0;
        //URL to the actual GreenSky javascript file that contains logic for AsLowAs
        script.src = greenskyLibUrl + '/aslowas/gs-api-min.js' + '?ts=' + new Date().getTime();
        //Insert the script at the top of the page
        var firstScr = document.getElementsByTagName( "script" )[ 0 ];
        firstScr.parentNode.insertBefore( script, firstScr );

        if ( script.readyState )
        { //IE
            script.onreadystatechange = function ()
            {
                if ( script.readyState == "loaded" ||
                    script.readyState == "complete" )
                {
                    script.onreadystatechange = null;
                    callback();
                }
            };
        }
        else
        { //Others
            script.onload = function ()
            {
                callback();
            };
        };

        function callback()
        {
            if (window.location.pathname !== '/checkout') {
                setTimeout( function ()
                {
                    gsa_validateMerchant();
                }, 150 );
            }
        }

        // Load Iovation scripts
        var script = document.createElement( "script" );
        script.type = "text/javascript";
        //URL to the actual GreenSky javascript file that contains logic for AsLowAs
        // script.src = 'https://ci-mpsnare.iovation.com/snare.js';
        script.src = 'https://mpsnare.iesnare.com/snare.js';
        document.head.appendChild( script );

        var script = document.createElement( "script" );
        script.type = "text/javascript";
        //URL to the actual GreenSky javascript file that contains logic for AsLowAs
        script.src = greenskyLibUrl + '/aslowas/iovation_static_wdp.js';
        document.head.appendChild( script );

        var script = document.createElement( "script" );
        script.type = "text/javascript";
        //URL to the actual GreenSky javascript file that contains logic for AsLowAs
        // script.src = 'https://ci-first.iovation.com/latest/dyn_wdp.js';
        script.src = 'https://first.iovation.com/latest/dyn_wdp.js';
        document.head.appendChild( script );

    } )( GS_JS_URL );
</script>
