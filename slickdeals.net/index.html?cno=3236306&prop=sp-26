            var pinwheelEl = document.querySelector('.as-pinwheel14');
            if(window.as && window.as.CarouselPinwheel && pinwheelEl) {
                window.as.CarouselPinwheel({});
            }

            if(window.as && window.as.CarrierPromotionOverlay) {
                window.as.CarrierPromotionOverlay.init({
                    tabBtnSel: '.as-carrierpromotion-button'
                });
            }
        });
        </script>

        
