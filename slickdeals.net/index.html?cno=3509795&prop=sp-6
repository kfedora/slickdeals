$('.collapsible-mobile').toggleClass("is-open");
if (content.style === "block" && content2.style ==="block") {
content.style = "none";
$('html, body').animate({
scrollTop: $("document").offset()
}, 100);
} else {
content.style = "block";
var scrollVal = $(document).scrollTop();
if(scrollVal < 600){
$('html, body').animate({
scrollTop: $(".products-counts-sorting").offset().top + 20
}, 100);
}
}
});
}
