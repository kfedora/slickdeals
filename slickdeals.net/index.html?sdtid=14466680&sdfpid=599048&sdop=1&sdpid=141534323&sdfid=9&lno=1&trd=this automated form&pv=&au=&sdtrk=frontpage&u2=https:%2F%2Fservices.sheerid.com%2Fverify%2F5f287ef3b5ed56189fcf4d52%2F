    <script>
        // Create a program at my.sheerid.com
        var myProgramId = '5f287ef3b5ed56189fcf4d52';
        var baseUrl = 'https://my.sheerid.com';

                        var pathname = window.location.pathname;
        if (myProgramId.indexOf('templateId') > -1 && pathname && pathname.length > 0) {
            if (!window.location.origin) {
                window.location.origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
            }
            var fallbackTemplateId = pathname.match(/[a-z|\d]{24}/g);
            myProgramId = fallbackTemplateId[0];
            baseUrl = window.location.origin;
        }

        // trigger iframe-1.2.js to fade-in the contents of the modal:
        window.parent.postMessage(JSON.stringify({
            action: "updateHeight",
            height: 650,
        }), "*");

        var sheerid = window.sheerid;
        sheerId.setOptions({
            useFingerprinting: true,
            logLevel: 'error',
            restApi: {
                serviceUrl: baseUrl || ''
            },
        });

                    try {
                sheerId.addHook({
                    name: 'ON_VERIFICATION_STEP_CHANGE',
                    callback: function(verificationResponse) {
                        dataLayer.push({
                            'event':'step-change',
                            'step-name': verificationResponse.currentStep,
                            'verificationId': verificationResponse.verificationId
                        });
                    }
                });
            } catch(e) {
            }
        
                                                
        var myDiv = document.getElementById('my-form');
        var myForm = new sheerId.VerificationForm(myDiv, myProgramId);
    </script>
