	           }      
	       		   	
		}else{
			if(navigator && navigator.userAgent && navigator.userAgent.indexOf("Opera") != -1) {
				setTimeout(function(){document.location.hash = document.location.hash.substring(1);},50);
			}
		}
	});
	
