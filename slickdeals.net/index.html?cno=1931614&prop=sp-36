            $(document).ready(function () {
                $(".productSearchCount").html(33);


        });


          

        ga(function () {

            a1clientId = ga.getByName('mc').get('clientId');
            var a1TargetPage = $A1.Target({
                c_clientId: a1clientId,
                SearchTerm: ""

            });


            var event = $A1.Event({
                type: "onsiteSearch",
                customer: currentVisitor,

                targets: a1TargetPage



            }).send();
        });

          


