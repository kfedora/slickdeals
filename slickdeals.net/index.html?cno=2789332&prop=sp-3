                            img: '//static.lenovo.com/na/landing-pages/weekly-sale/nav/v2/lenovo-server-icon.svg',
                            img_hover: '//static.lenovo.com/na/landing-pages/weekly-sale/nav/v2/lenovo-server-icon-hover.svg',
                            link: siteRoot + '/d/deals/servers'
                        }
                    ],
                    /* ------------- Methods------- */
                    forSMB: function () {
                        if (store === 'smbpro') {
                            this.tabs.splice(1, 0, {
                                title: 'Business Deals',
                                img: '//static.lenovo.com/na/landing-pages/weekly-sale/nav/v2/lenovo-business-icon.svg',
                                img_hover: '//static.lenovo.com/na/landing-pages/weekly-sale/nav/v2/lenovo-business-icon-hover.svg',
                                link: siteRoot + '/d/deals/business'
                            }, {
                                title: 'ThinkPad Deals',
                                img: '//static.lenovo.com/na/landing-pages/weekly-sale/nav/v2/lenovo-thinkpad-icon.svg',
                                img_hover: '//static.lenovo.com/na/landing-pages/weekly-sale/nav/v2/lenovo-thinkpad-icon-hover.svg',
                                link: siteRoot + '/d/deals/thinkpad-laptops'
                            });
                            this.tabs.pop();
                        }
                    },
                    /* create style tag */
                    createStyle: function () {
                        var style = this.cElement('style');
                        style.innerHTML =
                            "#hero+.rich-text{padding:0}.searchResultContainer .searchTab .facetFilter .facetFilter-wrapper .facetFilter-item:first-of-type{border-top:none}.navigation-container{font - size:0;border-bottom:solid 1px #E3E3E3;max-width:100%}.left-arrow,.right-arrow{display:none}.nav-img{min-height:50px; max-width:50px;margin:0 auto 6px}.nav-img img:last-of-type{display:none}.ul-navigation{list-style:none;display:-webkit-box;display:flex;flex-wrap:nowrap;overflow-x:auto;margin:0;padding:12px 0 0 10px;position:relative}.track-bar{position:absolute;width:96px;height:8px;bottom:0;left:0;opacity:0;background:#1976A1}.track-bar.show{opacity:1}.track-bar.active{-webkit - transition:-webkit-transform .3s ease-in-out;transition:transform .3s ease-in-out;opacity:1}.nav-box{min-width:120px;max-width:150px;padding-bottom:15px;position:relative}.nav-box:hover .button-text,.nav-box.active .button-text{color:#1976A1}.nav-box:hover .nav-img img:last-of-type,.nav-box.active .nav-img img:last-of-type{display:block}.nav-box:hover .nav-img img:first-of-type,.nav-box.active .nav-img img:first-of-type{display:none}.button-text{font-size:13px;line-height:1.2;text-align:center;color:#808084}@media(hover:none){.ul-navigation::-webkit-scrollbar{display:none}.ul-navigation{scrollbar - width:none}}@media(min-width:750px){.nav-box{min-width:150px;max-width:150px;padding-bottom:20px}.button-text{max - width:85%;margin:auto;font-size:14px}.ul-navigation{padding-top:20px}.track-bar{width:120px}}@media(min-width:1180px){.navigation-container{margin:auto;width:calc(100% - 40px)}.left-arrow,.right-arrow,.navigation-inner-container{display:inline-block}.left-arrow,.right-arrow{width:50px;position:relative;height:127px}.left-arrow svg,.right-arrow svg{width:15px;display:inline-block;position:absolute;top:50%;-webkit-transform:translateY(-50%);transform:translateY(-50%);fill:#CECFCE}.left-arrow::after,.right-arrow::after{content:'';position:absolute;width:30px;height:128px;right:-25px;background:-webkit-gradient(linear,left top,right top,color-stop(5%,rgba(255,255,255)),to(transparent));background:linear-gradient(to right,rgba(255,255,255) 5%,transparent);z-index:999}.right-arrow::after{background:-webkit-gradient(linear,right top,left top,color-stop(5%,rgba(255,255,255)),to(transparent));background:linear-gradient(to left,rgba(255,255,255) 5%,transparent);right:auto;left:-15px;width:30px}.left-arrow.enabled svg,.right-arrow.enabled svg{fill:#1976A1;cursor:pointer}.right-arrow svg{right:0;-webkit-transform:translateY(-50%) rotate(180deg);transform:translateY(-50%) rotate(180deg)}.navigation-inner-container{width:calc(100% - 100px);vertical-align:top;overflow:hidden}.ul-navigation{overflow:hidden;width:200%;-webkit-box-pack:justify;justify-content:space-between;padding-left:0;-webkit-transition:all .5s ease;transition:all .5s ease}.ul-navigation.slide{-webkit - transform:translateX(-50%);transform:translateX(-50%)}}@media(min-width:1920px){.navigation - container{padding:0;max-width:1730px}.ul-navigation{width:" +
                            150 * this.tabs.length + "px}.ul-navigation.slide{transform:translateX(-" + 150 * (this
                                .tabs
                                .length - 11) + "px)}}" + this.disableArrows() + this.hideScrollBarTouchDevices();
                        document.head.appendChild(style);
                        this.createNavBar(document.querySelector('#hero').nextElementSibling);
                    },
                    /* styles when there are not arrows */
                    disableArrows: function () {
                        var additionalCss = "";
                        if (this.tabs.length < 12) {
                            additionalCss =
                                "div.left-arrow,div.right-arrow{display:none}ul.ul-navigation,div.navigation-inner-container{width:100%}ul.ul-navigation,ul.ul-navigation.slide{-webkit-transform:translateX(0);transform:translateX(0);overflow:auto}";
                            if (this.tabs.length < 7) {
                                additionalCss +=
                                    "@media(min-width: 960px){ul.ul-navigation{-webkit-box-pack: center;-ms-flex-pack: center;justify-content: center;}}";
                            }
                        }
                        return additionalCss;
                    },
                    /* disable scrollbar for touch devices */
                    hideScrollBarTouchDevices: function () {
                        if (/Android|webOS|iPhone|iPad|iPod|Windows Phone|Opera Mini/i.test(navigator.userAgent)) {
                            return ".ul-navigation::-webkit-scrollbar{display:none}.ul-navigation{scrollbar - width:none;-ms-overflow-style:none}";
                        }
                    },
                    /* create navbar */
                    createNavBar: function (container) {
                        if (container === null || container === "undefined") {
                            try {
                                container = document.querySelector('[data-componentid="navigation-bar-NA"]');
                            } catch (e) {
                                window.navBarDlpLog = e;
                            }
                        }
                        var navigationBar = document.createElement("section"),
                            navigationContainer = container;
                        navigationBar.id = "navigation";
                        navigationContainer.appendChild(navigationBar);
                        navigationContainer.innerHTML =
                            "<div class=\"navigation-container\"><div class=\"left-arrow\">" + this.arrow +
                            "</div><div class=\"navigation-inner-container\"><ul class=\"ul-navigation\"><div class=\"track-bar\"></div></ul></div><div class=\"right-arrow enabled\">" +
                            this.arrow + "</div></div>";

                        /* -------- create list items --------- */
                        for (var i = 0; i < this.tabs.length; i++) {
                            /* Momentary conditional to avoid showing Tablets in Public */
                            
                                this.createNavBox(this.tabs[i].link, this.tabs[i].img, this.tabs[i].img_hover, this
                                    .tabs[i]
                                    .title);
                            
                        }

                        /* check if navigation ExtensionScriptApis, if not append again */

                        setTimeout(function () {
                            if (!document.getElementsByClassName('ul-navigation')[0]) {
                                var navCont = navBar.cElement('div'),
                                    productListingPage = document.getElementById('productListingPage');
                                navBar.track = null;
                                productListingPage.insertBefore(navCont, document.querySelector('#hero')
                                    .nextElementSibling);
                                navBar.createNavBar(navCont);
                                console.log('there was an error, navbar created again');
                            }
                        }, 1000);
                        this.slideNavigation();
                        if (this.activeTab) {
                            this.moveTrackBar();
                            this.addActiveClass();
                            this.scrolltoTabMobile();
                        }
                        navBar.navCreated = true;
                        console.log('navigation bar created successfully');
                    },
                    /* create list item */
                    createNavBox: function (link, img, imghover, title) {
                        var navBox = this.cElement('li');
                        navBox.className = "nav-box";
                        navBox.innerHTML = "<a href=" + link + "><div class=\"nav-img\"><img src=" + img +
                            " alt=\"" +
                            title + "\"><img src=" + imghover + " alt=\"" + title +
                            "\"></div><div class=\"button-text\">" + title + "</div></a>";
                        document.querySelector('.ul-navigation').appendChild(navBox);

                        if ((link.split('deals/')[1]).split('?')[0] === this.newlocation()) {
                            this.aClass(navBox, 'active');
                            this.activeTab = document.getElementsByClassName('nav-box active')[0];
                        }
                    },
                    /* arrow function: arrows are only available if there are 12 tabs or more*/
                    slideNavigation: function () {
                        var arrows = document.getElementsByClassName('navbar-arrow'),
                            arrowsContainer = document.querySelectorAll(
                                '.navigation-container>div:not(:nth-of-type(2))');
                        this.ulNavigation = document.getElementsByClassName('ul-navigation')[0];
                        [].forEach.call(arrows, function (elem, index) {
                            elem.addEventListener('click', function () {
                                if (index === 0) {
                                    navBar.rClass(navBar.ulNavigation, 'slide');
                                    navBar.rClass(arrowsContainer[0], "enabled");
                                    navBar.aClass(arrowsContainer[1], "enabled");
                                } else {
                                    navBar.aClass(navBar.ulNavigation, 'slide');
                                    navBar.aClass(arrowsContainer[0], "enabled");
                                    navBar.rClass(arrowsContainer[1], "enabled");
                                }
                            })
                        })
                    },
                    /* move trackbar to active position */
                    moveTrackBar: function () {
                        var trackBar = document.getElementsByClassName('track-bar')[0];
                        trackBar.style.transform = "translateX(" + (this.activeTab.offsetLeft + (this.activeTab
                            .clientWidth / 10)) + "px)";
                        navBar.aClass(trackBar, "show");
                        if (this.track) {
                            navBar.aClass(trackBar, "active");
                        }
                        this.track = true;
                        window.addEventListener('resize', function () {
                            setTimeout(function () {
                                navBar.moveTrackBar();
                            }, 200);
                        });
                    },
                    /* scroll to active position mobile */
                    scrolltoTabMobile: function () {
                        this.ulNavigation.scrollLeft = this.activeTab.offsetLeft - this.activeTab.clientWidth;
                    },
                    /* change active class on tab click */
                    addActiveClass: function () {
                        var tabs = document.getElementsByClassName('nav-box');
                        for (var i = 0; i < tabs.length; i++) {
                            tabs[i].addEventListener('click', function () {
                                for (var x = 0; x < tabs.length; x++) {
                                    navBar.rClass(tabs[x], 'active');
                                }
                                navBar.aClass(this, 'active');
                                navBar.activeTab = document.getElementsByClassName('nav-box active')[0];
                                navBar.moveTrackBar();
                            });
                        }

                    },

                    /* custom function to create DOM element */

                    cElement: function (elem) {
                        return document.createElement(elem);
                    },

                    /* custom function to add class */
                    aClass: function (elem, clase) {
                        elem.classList.add(clase);
                    },
                    /* custom function to remove class */
                    rClass: function (elem, clase) {
                        elem.classList.remove(clase);
                    },
                    /* init navbar */
                    init: function () {
                        this.forSMB();
                        this.createStyle();
                    }
                };
            /* Flag for bottom banner on Desktops DLP NWP-10924 */
            window.bottomBannerFlag = null;
            window.facetResultPostRenderListener = function () {
                /* NWP-10924 bottom banner inclusion - desktops only */
                try {
                    if (typeof insertDesktopsLowerBanner !== "undefined" && window.bottomBannerFlag === null) {
                        insertDesktopsLowerBanner();
                        window.bottomBannerFlag = 1;
                    }
                } catch (e) {
                    window.errorLogBottomBan = e;
                }
                if (!navBar.navCreated) {
                    navBar.init();

                }

                /* ---------------- KLARNA BANNER LAPTOPS ----------------------- */

                if (window.location.href.indexOf('/laptops') > 0) {
                    if (!b_klarna) {
                        insertBanner();
                    }
                }


                function insertBanner() {
                    var smbDlp =
                        '<div class="klarnaDealsBnr"> <div class="klarnaDealsBnrCont"> <img alt="Klarna Logo" src="https://static.lenovo.com/na/landing-pages/military-program/lenovo-klarna-logo.png"> <div class="klarnaHead"><strong>Buy Now, Pay Later! </strong><br>Financing as low as 0% APR.</div><div><a class="klarnaCta" href="' + siteRoot + '/lenovo-klarna-credit/?IPromoID=LEN700177">Learn More</a></div></div></div><a href="' + siteRoot + '/lenovo-klarna-credit/?IPromoID=LEN700177" class="klarnaDealsMobBnr"> <div class="klarnaDealsMobBnrCont"> <div class="klarnaHead"><strong>Buy Now, Pay Later!</strong><br>Financing as low as 0% APR. </div><div class="klarnaDealsMobBnrCta">Shop Now</div></div></a><style type="text/css"> .klarnaDealsMobBnrCta{color: #fff; font-size: 16px; font-weight: bold;}.klarnaDealsMobBnr{display: none;}.klarnaHead br{display: none;}.klarnaDealsBnrCont img{height: 38px;}.klarnaHead{line-height:1; color: #fff; font-size: 29px;}.klarnaDealsBnr{margin:auto; background-color: #B01554; height: 100px; max-width: 1732px; display: flex; margin-bottom: 10px;}.klarnaDealsBnrCont{display: flex; width: 90%; margin: auto; max-width: 1180px; justify-content: space-around; align-items: center;}.klarnaCta{background: #fff; border: 1px solid #000; border-radius: 4px; color: #000; padding: 0 24px; line-height: 45px; height: 45px; transition: .3s ease-out; text-decoration: none; display: inline-block; width: auto; cursor: pointer; text-align: center; vertical-align: middle; white-space: nowrap; margin-bottom: 0; font-size: 16px;}.klarnaCta:hover{background: #e3e3e3; color: #757575; border: 1px solid;}.klarnaCta:active{background: #545454; color: #fff; border: 1px solid #fff;}@media (max-width: 1180px){.klarnaDealsBnrCont img{display: none;}}@media (max-width: 1180px){.klarnaHead br{display: block;}}@media (max-width: 600px){.klarnaDealsMobBnr{display: flex; background-color: #B01554; height: 100px;}.klarnaDealsMobBnrCont{width: 80%; margin: auto; display: flex; f