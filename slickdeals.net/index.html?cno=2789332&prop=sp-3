                                        {{~^~}}
                                          {{~#ifCond ../code '==' 'facet_Rating'~}}
                                            {{~facetRating this.originValue this.name~}}
                                          {{~^~}}
                                            {{~this.name~}}
                                          {{~/ifCond~}}
                                        {{~/ifCond~}}
                                        {{~#ifCond ../code '!=' 'facet_Rating'~}}&nbsp;({{this.count}}){{~/ifCond~}}
                                    </span>
                                </label>
                            </li>
                        {{/each}}
                    </ol>
                    {{#ifCond ../currentDeviceType '==' ../deviceTypes.desktop}}
                        {{#ifCond this.show_all '!=' true}}
                            {{#ifCond this.values.length '>' this.show_limit }}
                                <div class="facetFilter-item-morefilter">
                                    <span class="facetFilter-item-maxfilter {{#if expandMore}}hide{{/if}}">{{fetchByOrder ../rsbs.[text.showMore] 'Show more'}}</span>
                                    <span class="facetFilter-item-mixfilter {{#unless expandMore}}hide{{/unless}}">{{fetchByOrder ../rsbs.[text.showLess] 'Show less'}}</span>
                                </div>
                            {{/ifCond}}
                        {{/ifCond}}
                    {{/ifCond}}
                </div>
            </div>
        {{/ifContainsInArr}}
    {{/each}}

    {{#if saleDiscountFacets.facets}}
        <div class="facetFilter-item" data-facet-name="{{saleDiscountFacets.facets.[0].name}}" data-facet-code="{{saleDiscountFacets.facets.[0].code}}">
            <div class="facetFilter-item-header saleDiscount">
                <span class="facetFilter-item-title" data-facetName_expanded="{{saleDiscountFacets.facetName_expanded}}" data-facetName_collapsed="{{saleDiscountFacets.facetName_collapsed}}">{{saleDiscountFacets.facetName_expanded}}</span>
            </div>
            <div class="facetFilter-item-content">
                <ol class="facetFilter-item-menu">
                    {{#each saleDiscountFacets.facets.[0].values}}
                        <li class="facetFilter-item-list" data-facet-name="{{../saleDiscountFacets.facets.[0].name}}" data-facet-origin-name="{{this.facetOriginName}}" data-facet-code="{{../saleDiscountFacets.facets.[0].code}}" data-facet-origin-code="{{this.facetOriginCode}}" data-value-name="{{this.name}}" data-value-originValue="{{this.originValue}}" data-value-count="{{this.count}}">
                            <label>
                                <label class="facetFilter-item-list-checkbox {{#if this.selected}}facetFilter-checkbox-checked{{/if}}"></label>
                                <span class="facetFilter-item-list-option">{{this.name}} ({{this.count}})</span>
                            </label>
                        </li>
                    {{/each}}
                </ol>
                {{#each saleDiscountFacets.facets}}
                    {{#ifCond @index '>' 0}}
                        <div class="facetFilter-item-menu-title">{{this.name}}</div>
                        <ol class="facetFilter-item-menu">
                            {{#each this.values}}
                                <li class="facetFilter-item-list" data-facet-name="{{../name}}" data-facet-code="{{../code}}" data-value-name="{{this.name}}" data-value-originValue="{{this.originValue}}" data-value-count="{{this.count}}">
                                    <label>
                                        <label class="facetFilter-item-list-checkbox {{#if this.selected}}facetFilter-checkbox-checked{{/if}}"></label>
                                        <span class="facetFilter-item-list-option">{{this.name}} ({{this.count}})</span>
                                    </label>
                                </li>
                            {{/each}}
                        </ol>
                    {{/ifCond}}
                {{/each}}
            </div>
        </div>
    {{/if}}

    {{#ifCond currentDeviceType '!=' deviceTypes.desktop}}{{!--only for tablet and mobile--}}
        <div class="facetFilter-applyFilters"><button class="a-button applyFilters-button ">{{fetchByOrder rsbs.[text.applyFilters] 'Apply Filters'}}</button></div>
    {{/ifCond}}
</script>



