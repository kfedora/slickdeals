<script type="text/javascript">
    window.finance = window.finance || {
        updateFinanceOptions: function () {
            setTimeout(function () {
                if (location.href.indexOf('data-center') == -1 && location.href.indexOf('server') == -1) {
                    var siteRoot = $('meta[name="siteRoot"]').attr('content');
                    var url = siteRoot + '/landingpage/lenovo-financing-options/';
                    var _fo = $(".financeOptionsHolder");
                    var _a = $("a.financing-options");
                    if (_a.length == 0) {
                        _fo.each(function () {
                            var _prccGrid = $(this).parents("div.facetResults-g-h-item").find(".isBlock.xblodSize").text().replace(/[,\$\.]/g, "");
                            var _prccList = $(this).parents("div.facetResults-l-item ").find(".isBlock.xblodSize").text().replace(/[,\$\.]/g, "");
                            if ($(this).is('.grid-view')) {
                                $(this).prepend('<a class="financing-options" style="min-height:25px;" target="_blank" href="' + url + '">&nbsp;<klarna-placement data-id="bc4420fe-c00e-4d4c-b870-f0409e5fd089" data-inline="true" data-purchase_amount="' + _prccGrid + '" ></klarna-placement></a><a class="financing-options" target="_blank" href="' + url + '">Additional Financing Options</a>');
                            } else if ($(this).is('.list-view')) {
                                $(this).prepend('<a class="financing-options" style="min-height:25px;" target="_blank" href="' + url + '">&nbsp;<klarna-placement data-id="bc4420fe-c00e-4d4c-b870-f0409e5fd089" data-inline="true" data-purchase_amount="' + _prccList + '" ></klarna-placement></a><a class="financing-options" target="_blank" href="' + url + '">Additional Financing Options</a>');
                                $(this).addClass("finance-applied-rt");
                            }
                        });
                    }
                    window.kudt = window.kudt || [];
                    window.kudt.push({
                        eventName: 'refresh-placements'
                    });
                }
            }, 5000);
        }
    }
</script>
