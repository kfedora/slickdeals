<script id="gallery-template" type="text/x-handlebars-template">
    <div class="mediaGallery">
        <div class="subseries-gallery noscript galleria">
            {{#ifCond galleryType '==' 'image'}}
                {{#each galleryPhoto}}
                    <figure itemprop="image" itemtype="http://schema.org/ImageObject" itemscope="" class="image">
                        <meta content="{{this.imageAltText}}" itemprop="name" />
                        <meta content="{{this.imageAltText}}" itemprop="description" />
                        <a itemprop="contentUrl" href="{{this.image}}" imgName="{{this.title}}">
                            <img src="{{this.thumbnailImage}}" itemprop="thumbnail" data-title="{{this.thumbnailImageAltText}}" alt="{{this.thumbnailImageAltText}}" data-big="{{this.image}}">
                        </a>
                    </figure>
                {{/each}}
            {{/ifCond}}
            {{#ifCond galleryType '==' 'video'}}
                {{#each galleryVideo}}
                    <figure itemprop="video" itemtype="http://schema.org/VideoObject" itemscope="" class="image">
                        <meta content="{{this.title}}" itemprop="name"/>
                        {{#ifCond this.description '!=' null}}
                            <meta content="{{this.description}}" itemprop="description"/>
                        {{/ifCond}}
                        {{#ifCond videoType '==' 'Youtube'}}
                            <meta content="{{this.contentUrl}}" itemprop="embedURL"/>
                            <a itemprop="contentUrl" data-type="{{this.videoType}}" href="{{this.contentUrl}}">
                                <img class="iframe" src="{{this.thumbnailImage}}" title="{{this.thumbnailImageAltText}}" alt="{{this.thumbnailImageAltText}}">
                            </a>
                        {{/ifCond}}
                        {{#ifCond videoType '==' 'BrightCove'}}
                            {{#ifCond this.duration '!=' null}}
                                <meta content="{{this.duration}}" itemprop="duration"/>
                            {{/ifCond}}
                            <meta content="{{this.videoId}}" itemprop="embedURL"/>
                            <a itemprop="contentUrl" data-type="{{this.videoType}}" href="{{this.videoId}}">
                                <img src="{{this.thumbnailImage}}" title="{{this.thumbnailImageAltText}}" alt="{{this.thumbnailImageAltText}}">
                            </a>
                        {{/ifCond}}
                        {{#ifCond videoType '==' 'Hosted'}}
                            <meta content="{{this.embedUrl}}" itemprop="embedURL"/>
                            <a itemprop="contentUrl" data-type="{{this.videoType}}" href="{{this.embedUrl}}">
                                <img src="{{this.thumbnailImage}}" title="{{this.thumbnailImageAltText}}" alt="{{this.thumbnailImageAltText}}">
                            </a>
                        {{/ifCond}}
                    </figure>
                {{/each}}
            {{/ifCond}}
            {{#ifCond galleryType '==' '360'}}
                <a href="{{gallery360View}}"><span class="iframe">360</span></a>
            {{/ifCond}}
        </div>
    </div>
</script>



