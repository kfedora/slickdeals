<div v-if="item.Stack.Pricing.DellPrice.Value != null" class="alienware-override dell-pricing-section">
    <div v-if="item.Stack.Pricing.ShowMarketPrice || item.Stack.Pricing.ShowSavings" class="dell-pricing-total-savings-section">
            <p v-if="item.Stack.Pricing.ShowMarketPrice" class="top-padding-5 bottom-offset-0 vertical-overflow" v-bind:data-testId="testidprefix+'marketprice'" data-testId="systems_stack_marketprice">
                <a data-toggle="tooltip"
                   data-placement="auto left"
                   title=""
                   v-bind:title="(isCtry('us') && isSeg('bsd') && item.Stack.Pricing.MarketValueLegalText && item.Stack.Pricing.MarketValueLegalText.Value != '')?item.Stack.Pricing.MarketValueLegalText.Value:''"
                   onmouseenter="if ($(window)[0].Dell.Metrics.sc.country === 'us' && $(window)[0].Dell.Metrics.sc.segment !== 'bsd') { $(this).tooltip('show') }">
                    <small v-text="item.Stack.Pricing.MarketValue.Label">List Price</small>
