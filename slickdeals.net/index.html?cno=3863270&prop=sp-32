                                            <div class="row">
                                                <pspricing v-for="(item, index) in row" v-bind:item="item" v-bind:index="index" v-bind:rowindex="rowindex" v-bind:testidprefix="item.Stack.Type !== 0 ? 'snp_stack_' : 'systems_stack_'">
