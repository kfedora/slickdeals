                               v-bind:data-idx="getIndex(index, rowindex)"
                               v-if="(item.Options && item.Options.Settings.ShowAddToCart && item.Options.Settings.ShowCtas && !item.Stack.FastDelivery.EligibleForFastDelivery && !item.Stack.Links.DisableAddToCart && !item.Stack.ProductSoldOut)"
