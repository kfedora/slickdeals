        <script type="text/javascript" language="JavaScript">

           // For Tracking
            function trackTabInfo(tabElementID) {
                var clickedTabInfo = new Object();
                clickedTabInfo.assetId = tabElementID;
                clickedTabInfo.type = 'TabbedContent';
                clickedTabInfo.position = 1;
                trackClickedPromos(clickedTabInfo);
            }

            function pop_win(url) {
                window.open(url,"pop_win","height=500,width=700,location=yes,menubar=yes,scrollbars=yes,toolbar=yes,resizable=yes");
            }


            //Working of the tabs
            dojo.addOnLoad(function(){
            		 var contentDivs=dojo.query("#contentContainer > div");
                     dojo.connect(dojo.byId("tabsList"),"click",function(event){
                     	var _target=event.target;
                     	if(dojo.hasClass(_target,"tabsListItem") && !dojo.hasClass(_target,"active")){

                     		dojo.query("li.active").removeClass("active");
                     		dojo.addClass(_target,"active");
                     		var targetId=_target.id;
                     		var contentId=targetId.replace("tab","content");
                     		contentDivs.addClass("hiddenContent");
                     		dojo.removeClass(dojo.byId(contentId),"hiddenContent");
                     	}
                     });

            });

        </script>











