<script type="text/javascript">
    var currentUrl = "https://www.shutterfly.com/photo-gifts/gift-certificates";
    var calendarSizeSpecified = "";
    (function ($) {
        if (currentUrl.indexOf('/wall-calendars') < 0) {
            $('#pipContainer').css('display', 'block');
            $('.pageTitleContainer').css('display', 'block');
        } else {
            $('.calendar-pip-container').css('display', 'block');
        }

        if (calendarSizeSpecified.toLowerCase() == "8x11") {
            $("input[type='radio'][name='calendarSize'][value='8x11']").prop('checked', true);
        }
    })(jQuery);
</script>
