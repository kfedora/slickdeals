<script>
		// Hide bottom mobile adhesion when any input is focused so that it is not pushed up by native keyboards
		(function() {
			if (!vsIsDesktop) {
				$(document)
					.on("focus", "textarea, input, .fr-wrapper", function() {
						$(".california-sticky-footer-ad-container").hide();
					})
					.on("blur", "textarea, input, .fr-wrapper", function() {
						$(".california-sticky-footer-ad-container").show();
					});
			}
		})();
	</script>


<script>
	var anchor = window.location.hash.match(/^#post(\d+)$/)
	if (anchor && anchor.length === 2) {
		var redirectAnchor = 'post-' + anchor[1];
		// if we have an anchor on the page, simply jump to the anchor
		if (document.getElementById(redirectAnchor) != null) {
			window.location.hash = redirectAnchor;
		} else {
			window.location = "/posts/" + anchor[1];
		}
	}
</script>

</body>
