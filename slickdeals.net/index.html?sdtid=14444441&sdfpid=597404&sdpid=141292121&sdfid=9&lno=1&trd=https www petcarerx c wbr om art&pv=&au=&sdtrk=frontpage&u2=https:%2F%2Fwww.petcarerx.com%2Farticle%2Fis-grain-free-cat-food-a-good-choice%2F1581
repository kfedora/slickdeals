		<script type="text/javascript">
			$(function() {

				function doLogin(username, password, redirUrl) {
					var form = document.createElement("form");
					var element1 = document.createElement("input");
					var element2 = document.createElement("input");
					var element3 = document.createElement("input");
					form.method = "POST";
					form.action = "/login/login_frame.aspx";
					element1.value = username;
					element1.name = "tbUsername";
					form.appendChild(element1);
					element2.value = password;
					element2.name = "tbPassword";
					form.appendChild(element2);
					element3.value = redirUrl || "";
					element3.name = "tbRedirUrl";
					form.appendChild(element3);				
					document.body.appendChild(form);
					form.submit();
				}

				$('#tbPassword').on('keydown', function(e) {
					e = e.which || e.keyChar;
					if (e == 13) {
						$("#global_btnSignIn").click();
					}
				});

				$('#global_btnSignIn').on('click', function(e) {
					e.preventDefault();
					$("#tbUsername,#tbPassword").removeClass('error');
					if (!$('#tbPassword').val().length) {
						$('#tbPassword').addClass('error');
					} else if (!$('#tbUsername').val().length) {
						$('#tbUsername').addClass('error');
					} else {
						doLogin($('#tbUsername').val(), $('#tbPassword').val(), $(this).data('redir-url'));
					}
					return false;
				});

				$('#globalnav_login_contain a.forgot').click(function(e) {
					$('#forgot_screen_1').show();
				});


				$('#globalnav_login_contain .ToolTip button.close').click(function(e) {
					$(this).parents('.ToolTip').hide();
				});

				$("#global_forgot_pw_submit").on("click", function(ev) {
					var self = this;
					ev.preventDefault();
					$.ajax({
						url: "/ajax/PasswordReset.ashx",
						data: {
							Logon: $("#global_forgot_pw_uname").val()
						},
						dataType: "json",
						type: "POST",
						success: function(data) {
							$(".input .ToolTip.fill").hide();
							if (data.success) {
								$(self).closest(".input").find(".ToolTip.forgot_result")
									.removeClass("success error").addClass("success").find(".Heading4")
									.html("THANK YOU! An email was sent to <strong>" + data.email + "</strong>. It includes information on changing and confirming your new password.").end().show();
								$("#global_forgot_pw_uname").val('');
							} else {
								$(self).closest(".input").find(".ToolTip.forgot_result")
									.removeClass("success error").addClass("error").find(".Heading4")
									.html("The username/email you have entered could not be found. Please fix the name and try again or call us for support: 1-800-844-1427").end().show();
								$("#global_forgot_pw_uname").val('');
							}
						}
					});
					return false;
				});
			});
		</script>

	</div>
</div>
                           </div>                            
                             <p><span class="Heading">or</span> <a href="/login/?fb=1" class="Button primary medium facebook" id="ut_nav_fb_login_btn">Sign in with Facebook</a></p>                        

						</div>
