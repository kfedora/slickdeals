                                <script>
                                    $(function () {
                                        $('#tbEmailSignup').on('click', doSignup);
                                        function doSignup() {
                                            var t = $('input[name="header_emailsignup"]').removeClass('error');
                                            if (PRX_Utils.testEmail(t.val())) {
                                                $.getJSON('/ajax/listsignup.ashx', { email: t.val() }, function (data) {
                                                    if (data.success) {
                                                        t.attr('disabled', 'disabled');                                                        
                                                    }
                                                    else {
                                                        PRX_Utils.doRedAlert('Whoops', 'There was an error, please try again later.');
                                                    }
                                                });
                                                $('#tbEmailSignup').text('Thanks!').toggleClass('primary secondary').off('click', doSignup).css('pointer-events', 'none');
                                            }
                                            else {
                                                t.addClass('error');
                                            }
                                            return false;
                                        }
                                    });
                                </script>
