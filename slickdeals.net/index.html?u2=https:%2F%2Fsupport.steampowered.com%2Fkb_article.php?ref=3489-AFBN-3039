		<script>
			
			// language select
			$(document).ready(function(){
				$("#language_dropdown_button").on("click", function(){
					$("#language_dropdown").fadeIn();
				});
				$(document).on("click", function(event){ 
					if( $(event.target).attr("id") != "language_dropdown_button_span" ){
						$("#language_dropdown").fadeOut(); 
					}				
				});
			});
			// set lanugage link url
			$("#language_dropdown a").each(function(){
				var url = window.location.href;
				if( url.indexOf("?") === -1 ){
					url = url + "?l=" + $(this).attr("language");
				}
				else{
					if( url.indexOf("&l=") !== -1  ){
						var langVar = url.indexOf("&l=");
						url = url.substring(0, langVar) + "&l=" + $(this).attr("language");
					}
					else if( url.indexOf("?l=") !== -1 ){
						var langVar = url.indexOf("?l=");
						url = url.substring(0, langVar) + "?l=" + $(this).attr("language");
					}
					else{
						url = url + "&l=" + $(this).attr("language");
					}	
				}
				$(this).attr("href", url);
			});
			
		</script>
