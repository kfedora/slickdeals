  miniPop: ['sears-us', 'kmart-us'].indexOf(shc.chronStoreName) >= 0,
  addToCart: ['product', 'shoppingcart', 'browse-page'].indexOf($('body').attr('id')) >=0 || /managemyaccount|automotive\-tires\-wheels\-tires/.test(location.href),
  pheroType: shc.storeId === '10151' ? 'TELLURIDE' : 'ASAP',
  autofillCategoryOff: true,
  leasing:true,
  OCPviaHTTPS: true,
  kioskPayNow: false,
  searchHistory: true
});
