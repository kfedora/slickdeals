<script>
  <!-- patch for translated search text -->
  var sText = document.getElementById('search-text');
  var sElem = document.getElementById('query');
  sText.addEventListener("click", function(){
  	sElem.focus();
  });
  sElem.addEventListener("keydown", function(){
  		sText.style.display = 'none';
  });
  sElem.addEventListener("blur", sElemEval);
  
  function sElemEval() {
  	var sInput = sElem.value;
  	if(sInput.length == 0){
  		sText.style.display = '';
  	} else {
  		sText.style.display = 'none';
  	}
  }
  document.ready(sElemEval());
