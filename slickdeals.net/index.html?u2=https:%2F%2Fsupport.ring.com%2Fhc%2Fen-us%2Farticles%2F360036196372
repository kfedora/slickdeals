  	"Page Type": "Article"
	});
}
  
if (window.location.href.indexOf("sections") > -1) {
   mixpanel.track("Help Center - Section Viewed", {
  	"Section Name":  document.title,
  	"Language": document.documentElement.lang,
  	"Page Type": "Section"
	});
}
  
if (window.location.href.indexOf("categories") > -1) {
   mixpanel.track("Help Center - Category Viewed", {
  	"Category Name":  document.title,
  	"Language": document.documentElement.lang,
  	"Page Type": "Category"
	});
}
    
if (window.location.href.indexOf("sections") == -1 && window.location.href.indexOf("articles") == -1 && window.location.href.indexOf("categories") == -1) {
   mixpanel.track("Help Center - Page Viewed", {
  	"Page Name":  document.title,
  	"Language": document.documentElement.lang,
  	"Page Type": "Home/Search"
	});
}
  
  </script>
  
