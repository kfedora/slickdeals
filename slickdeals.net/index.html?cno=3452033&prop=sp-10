                        <input type="text" name="postalCode" id="postalCode" class="form-cell form-textbox form-textbox-text" {{#maxSize}} maxlength="{{maxSize}}" {{/maxSize}} placeholder="{{label}}" data-ase-placeholder="shim" />
                        <div class="form-icons-wrapper form-icons-focusable">
                            <div data-render="input-reset" data-target="#postalCode" data-clear-text="Reset" class="form-icons form-icons-small"></div>
                        </div>
                        <div class="ase-validator-message form-message-wrapper" id="postalCode-error"></div>
                        <span class="as-pincode-verifybtn">
                            {{#buttonText}}
                            <button class='button merchandising' type="submit">{{buttonText}}</button>
                            {{/buttonText}}
                        </span>
                    </validator>
                {{/pinCode}}
                </form>
            </div>
        </div>
