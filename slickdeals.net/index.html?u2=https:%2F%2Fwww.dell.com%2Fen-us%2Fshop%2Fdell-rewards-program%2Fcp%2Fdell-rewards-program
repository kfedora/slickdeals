        <script type="application/x-suppress" data-src="https://afcs.dellcdn.com/csb/masthead/bundles/1.0.1.574/js/countrylistscripts.min.js" defer></script>



<div id="mfe-site-wide-banner" style="color:#333333"></div>

<script type="application/x-suppress" defer>
    (function($,window) {
        var $messagebar = $("#mfe-site-wide-banner");

        function getMessages() {
            var domain = window.location.protocol + "//" + window.location.host,
                pageMetaData = Dell.Metrics.PageMetadata,
                c = pageMetaData.getOrCreate("COUNTRY").Content.toLowerCase(),
                l = pageMetaData.getOrCreate("LANGUAGE").Content.toLowerCase(),
                s = pageMetaData.getOrCreate("SEGMENT").Content.toLowerCase(),
                cs = pageMetaData.getOrCreate("CUSTOMERSET").Content.toLowerCase(),
                app = "Shop";

            if (domain.indexOf("www-wip") < 0) {
                domain = "https://www.dell.com";
            }
            var url = domain + "/csbapi/masthead/messagebar/" + c + "/" + l + "/" + s + "/" + cs + "/" + app;

            $.get(url)
                .done(function(html) {
                    if (html) {
                        $messagebar.html(html);

                    } else {
                        $messagebar.hide();
                    }
                })
                .fail(function(error) {
                    $messagebar.hide();
                    console.log(error);
                });
        }

        getMessages();
    })(jQuery,window);



</script>
</div>

            






