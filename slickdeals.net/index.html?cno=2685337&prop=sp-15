            var moduleListAllArray = marketingDigitalData.page.modules.moduleListAll;
            
            if (!isModal) {
                marketingDigitalData.page.modules.moduleList.push(moduleData);
                marketingDigitalData.page.modules.moduleListAll.push(moduleData);
            }
            else {
                
                function findObjectByKey(array, key, value) {
                    for (var i = 0; i < array.length; i++) {
                        if (array[i][key] === value) {
                            return array[i];
                        }
                    }
                    return null;
                }
            
                var objectId = findObjectByKey(moduleListAllArray, 'ID', moduleData.ID);
                
                if(!objectId) {
                    marketingDigitalData.page.modules.moduleCount--;
                    marketingDigitalData.page.modules.moduleListAll.push(moduleData); 
                } 
            }
            
