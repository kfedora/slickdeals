            marketingDigitalData.page.modules.moduleCount++;
            
            var title = String("You’ll love our network. Try it on your phone before switching.").replace(/&nbsp;/g, ' ').replace(/<(?:.|[\s\S])*?>/gm, '').replace(/ {2}/gm, ' ');

            var moduleData = { 
                "ID": "1a26a6601b913ee2c089c4d5cf5d480b9fe5732287690b4803dab845c1046fa6",
                "parentComponentType": "Hero Content",
                "placementID": "index " + marketingDigitalData.page.modules.moduleCount,
                "titleText": title,
                "actionableElements": JSON.parse("[{\x22ID\x22:\x221a26a6601b913ee2c089c4d5cf5d480b9fe5732287690b4803dab845c1046fa6\u002Dcovers\x22,\x22type\x22:\x22Link\x22,\x22text\x22:\x22covers 99% of Americans\x22,\x22targetUrl\x22:\x22#\x22,\x22eventType\x22:\x22click\x22}]")
