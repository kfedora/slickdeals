        <script type="text/javascript">
            
            marketingDigitalData.page.modules.moduleCount++;
            
            var title = String("Start your free trial now.").replace(/&nbsp;/g, ' ').replace(/<(?:.|[\s\S])*?>/gm, '').replace(/ {2}/gm, ' ');

            var moduleData = { 
                "ID": "a1635396ab1b2b7ccef39d935ef3e620d876e24ea2773341ec87d237a8fcc971",
                "parentComponentType": "Hero Content",
                "placementID": "index " + marketingDigitalData.page.modules.moduleCount,
                "titleText": title,
                "actionableElements": JSON.parse("[]")
            };
            
            var isModal = document.getElementById("a1635396ab1b2b7ccef39d935ef3e620d876e24ea2773341ec87d237a8fcc971").classList.contains('display-dialog');
            var moduleListAllArray = marketingDigitalData.page.modules.moduleListAll;
            
            if (!isModal) {
                marketingDigitalData.page.modules.moduleList.push(moduleData);
                marketingDigitalData.page.modules.moduleListAll.push(moduleData);
            }
            else {
                
                function findObjectByKey(array, key, value) {
                    for (var i = 0; i < array.length; i++) {
                        if (array[i][key] === value) {
                            return array[i];
                        }
                    }
                    return null;
                }
            
                var objectId = findObjectByKey(moduleListAllArray, 'ID', moduleData.ID);
                
                if(!objectId) {
                    marketingDigitalData.page.modules.moduleCount--;
                    marketingDigitalData.page.modules.moduleListAll.push(moduleData); 
                } 
            }
            
