<script>
    if( typeof(marketingDigitalData) == "undefined" ) {
        var marketingDigitalData = {"page":{"pageInfo":{"pageID":"/content/t-mobile/consumer/offers/free-trial","templateName":"marketing-page-template","pageName":"Lander","pageTitle":"Free 30-Day Trial with Test Drive® Hotspot Device | T-Mobile","version":"","pageURL":"","language":"en_US","variant":"","currencyCode":"USD","TFN":{}},"category":{"primaryCategory":"Network Trial","subCategory":""},"modules":{"moduleCount":0,"moduleList":[],"moduleListAll":[]}}};
    }
    marketingDigitalData.page.pageInfo.pageURL = window.location.href;
    marketingDigitalData.invoca = marketingDigitalData.invoca || {};
    marketingDigitalData.invoca.fireTag = "T";
    marketingDigitalData.invoca.numberSelector = ".tmo_tfn_number";
    
    if( typeof(marketingTMOPersonalize) == "undefined" ) {
        var marketingTMOPersonalize = {};
