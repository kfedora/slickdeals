          $('#primary').find("div.promotional-message:contains('20% OFF WITH CODE: JUST4U')").hide();


          $(document).ajaxComplete(function() {
              $('#primary').find("div.promotional-message:contains('Free U.S. Shipping & Returns')").hide();
              $('#primary').find("div.promotional-message:contains('Free Shipping & Returns')").hide();
              $('#primary').find("div.promotional-message:contains('Free Shipping On Swim')").hide();
