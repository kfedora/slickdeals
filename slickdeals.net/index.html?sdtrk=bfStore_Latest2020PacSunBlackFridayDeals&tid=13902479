<script>
$(function() {
  $('.pptou-close').click(function(e) {
      e.preventDefault();
      $.cookie('ccpa', '1', { expires: null, path: '/' });
      $("#pptou-sticky").fadeOut();
  });

  if($.cookie('ccpa') != '1' ){
      $("#pptou-sticky").show();
  } else {
      $("#pptou-sticky").hide();
  }

  if(window.utag_data && utag_data.page_type && utag_data.page_type === 'checkout'){
      $('#pptou-sticky').hide();
  }
});
</script>


