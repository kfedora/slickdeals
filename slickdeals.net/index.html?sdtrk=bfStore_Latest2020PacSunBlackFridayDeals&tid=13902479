  var psObj = {
    windowLoaded: false,
    lightBox: function(options) {
      var o = $.extend({
          boxContent: '',
          boxWidth: 600,
          boxHeight: 450,
          boxTracking: '',
          boxClass: '',
          boxOnFinish: '',
          boxOnClose: ''
        }, options),
        windowHeight = $(window).height() - 115;

      if (o.boxClass !== 'psLightbox') o.boxClass += ' psLightbox';

      if (o.boxHeight === 'auto') {
        var $hiddenDiv = $('<div />')
            .attr('class', 'visuallyhidden')
            .html('<div class="' + o.boxClass + '">' + o.boxContent + '</div>')
            .appendTo(psObj.body),
          $div = $hiddenDiv.children('div:first');

        divWidth = o.boxWidth - 4,
          divWidth = ($div.css('padding-left')) ? divWidth - parseInt($div.css('padding-left')) : divWidth,
          divWidth = ($div.css('padding-right')) ? divWidth - parseInt($div.css('padding-right')) : divWidth;

        $div.css('width', divWidth + 'px');

        o.boxHeight = $div.outerHeight();

        $hiddenDiv.remove();
      }

      if (windowHeight < o.boxHeight) o.boxHeight = windowHeight;

      if (psObj.windowLoaded) {
        psObj.lightBoxOpen(o);
      } else {
        $(window).load(function() {
          psObj.lightBoxOpen(o);
        });
      }
    },
    lightBoxOpen: function(o) {

      app.lightbox({
        content: o.boxContent,
        width: o.boxWidth,
        height: o.boxHeight,
        customClass: o.boxClass,
        onFinish : function(){
          if (o.boxTracking !== '') app.linkTrack(o.boxTracking, 'prop21');
          if (o.boxOnFinish !== '') o.boxOnFinish();
        },
        onClose: function(){
          if (o.boxOnClose !== '') o.boxOnClose();
        }
      });

    },
    lightBoxVideo: function(options) {

      var o = $.extend({
        boxWidth: 604,
        boxHeight: 367,
        boxClass: 'psLightboxNoPad'
      }, options);

      psObj.lightBox(o);

    },
    lightBoxVideoFull: function(options) {

      var o = $.extend({
        boxWidth: 1080,
        boxHeight: 720,
        boxClass: 'psLightboxNoPad'
      }, options);

      psObj.lightBox(o);

    },
    lightBoxYoutube: function(videoID, tracking) {

      var o = {
        boxContent: '<div style="overflow:hidden;height:367px;"><object width="604" height="367"><param name="movie" value="https://www.youtube.com/v/' + videoID + '&amp;hl=en_US&amp;fs=1&autoplay=1&amp;vq=hd1080"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="https://www.youtube.com/v/' + videoID + '&amp;hl=en_US&amp;fs=1&autoplay=1&amp;vq=hd1080" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="604" height="367"></embed></object></div>',
        boxTracking: (tracking === '') ? '' : 'YouTube:' + tracking,
        boxOnClose: function(){
          $('#dialog-container').empty();
        }
      };

      psObj.lightBoxVideo(o);

    },
    lightBoxYoutubeFull: function(videoID, tracking) {

      var o = {
        boxContent: '<div style="overflow:hidden;height:720px;"><object width="1080" height="720"><param name="movie" value="https://www.youtube.com/v/' + videoID + '&amp;hl=en_US&amp;fs=1&autoplay=1&amp;vq=hd1080"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="https://www.youtube.com/v/' + videoID + '&amp;hl=en_US&amp;fs=1&autoplay=1&amp;vq=hd1080" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="1080" height="720"></embed></object></div>',
