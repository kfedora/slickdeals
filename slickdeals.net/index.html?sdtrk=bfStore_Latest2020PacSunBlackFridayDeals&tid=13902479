    // hides emp fog items form search pages and search suggestions
    
    (function fogHideSearch(){

        var foglist = function() {
                           $( ".product-name:contains('Fear Of God - FOG')" ).parent().parent().hide(); 
                      };
        $(document).ajaxComplete(function() {
            $( "#suggestions .suggestion:contains('Fear Of God - FOG')" ).hide();
        });

        if(window.utag_data && utag_data.page_type && utag_data.page_type === 'search'){

            foglist();

            $(document).ajaxComplete(function() {
                foglist();
            });

        }

        else setTimeout(fogHideSearch,1000);

    })();

    


