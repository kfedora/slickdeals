  //responsive image maps
  (function jqInit(){

    if(window.jQuery){



      ;(function($) {
        $.fn.rwdImageMaps = function() {
          var $img = this;

          var rwdImageMap = function() {
            $img.each(function() {
              if (typeof($(this).attr('usemap')) == 'undefined')
                return;

              var that = this,
                $that = $(that);

              // Since WebKit doesn't know the height until after the image has loaded, perform everything in an onload copy
              $('<img />').load(function() {
                var attrW = 'width',
                  attrH = 'height',
                  w = $that.attr(attrW),
                  h = $that.attr(attrH);

                if (!w || !h) {
                  var temp = new Image();
                  temp.src = $that.attr('src');
                  if (!w)
                    w = temp.width;
                  if (!h)
                    h = temp.height;
                }

                if($that.width() > 0 && $that.height() > 0){

                  var wPercent = $that.width()/100,
                    hPercent = $that.height()/100,
                    map = $that.attr('usemap').replace('#', ''),
                    c = 'coords';

                  $('map[name="' + map + '"]').find('area').each(function() {
                    var $this = $(this);
                    if (!$this.data(c))
                      $this.data(c, $this.attr(c));

                    var coords = $this.data(c).split(','),
                      coordsPercent = new Array(coords.length);

                    for (var i = 0; i < coordsPercent.length; ++i) {
                      if (i % 2 === 0)
                        coordsPercent[i] = parseInt(((coords[i]/w)*100)*wPercent);
                      else
                        coordsPercent[i] = parseInt(((coords[i]/h)*100)*hPercent);
                    }
                    $this.attr(c, coordsPercent.toString());
                  });

                }

              }).attr('src', $that.attr('src'));
            });
          };
          $(window).resize(rwdImageMap).trigger('resize');

          return this;
        };
      })($);

      $('#wrapper').find('img[usemap]').rwdImageMaps();

    }else setTimeout(jqInit,500);

  })();


  // Dynamic Yield Script
  (function setupData(){
    if(window.utag_data){

      var u = {};
      u.data = {
        "page_type" : "",
        "page_data" : []
      };

      window.DY = window.DY || {};
      switch(utag_data.page_type){
        case 'homepage':
          u.data.page_type = 'HOMEPAGE';
          break;
        case 'category home': case 'category': case 'sub-category':
        u.data.page_type = 'CATEGORY';
        if(utag_data.page_category_name) u.data.page_data.push(utag_data.page_category_name);
        if(utag_data.page_subcategory_name) u.data.page_data.push(utag_data.page_subcategory_name);
        if(utag_data.page_tercategory_name) u.data.page_data.push(utag_data.page_tercategory_name);
        if(utag_data.page_fourthcategory_name) u.data.page_data.push(utag_data.page_fourthcategory_name);
        break;
        case 'product':
          u.data.page_type = 'PRODUCT';
          u.data.page_data = (window.variant) ? [variant.id] : utag_data.default_variant_sku;
          break;
        default:
          if(utag_data.page_name === 'basket'){
            u.data.page_type = 'CART';
            u.data.page_data = utag_data.product_sku;
          } else {
            u.data.page_type = 'OTHER';
          }
          break;
      }
      if(u.data.page_type) window.DY.recommendationContext = {type:u.data.page_type};
      if(u.data.page_data.length) window.DY.recommendationContext.data = u.data.page_data;

      if(utag_data.page_name === "confirmation"){

        (function sendPurchaseEvent(){
          if(window.DY && window.DY.API){
            var value = utag_data.order_subTotal,
              currency = utag_data.int_currency || utag_data.site_currency,
              length = utag_data.product_sku.length,
              cart = [],
              i = count = 0,
              pickup = false,
              isLottieMoss = false;

            for(i; i<length; i++){
              cart.push({
                productId: utag_data.product_sku[i],
                quantity: (parseFloat(utag_data.product_quantity[i]) || 1),
                itemPrice: utag_data.product_selling_price[i]/utag_data.product_quantity[i],
                division: utag_data.product_category[i],
                subdept: utag_data.product_classification[i],
                brand: utag_data.product_brand[i],
                productName : utag_data.product_name[i]
              });
              if(utag_data.product_pickticket[i] == "PCK"){
                count += parseInt(utag_data.product_quantity[i]); //add quantity orders of Pick Up in Store
                pickup = true;
              }
              if(utag_data.product_brand[i] === 'Lottie Moss'){
                isLottieMoss = true;
              }
            }

            window.DY.API('event', {
              name: 'Purchase',
              properties: {
                dyType: 'purchase-v1', // Identifies this event as a purchase
                value: value, // Total cart value in actual payment currency, as float (dollars dot cents)
                currency: currency, // Optional non-default currency used
                cart: cart,
                bopisBoolean: pickup,
                bopisCount: count
              }
            });

            if(isLottieMoss){
              window.DY.API('event', {
                name: 'Purchase - Lottie Moss',
                properties: {
                  dyType: 'purchase-lottie-moss-v1', // Identifies this event as a purchase
                  value: value, // Total cart value in actual payment currency, as float (dollars dot cents)
                  currency: currency, // Optional non-default currency used
                  cart: cart,
                  bopisBoolean: pickup,
                  bopisCount: count
                }
              });
            }
          }else setTimeout(sendPurchaseEvent,500);
        })();

      }

    }else setTimeout(setupData,500);
  })();
</script>
