    //remove coachella from url 
    (function removeCochellaURL(){
        if(document.location.href.indexOf('coachella') != -1 || document.location.href.indexOf('Coachella') != -1 ){
          var uri = window.location.toString();
            if (uri.indexOf("?") > 0) {
                var clean_uri = uri.substring(0, uri.indexOf("?"));
                window.history.replaceState({}, document.title, clean_uri);
            }
        }
    })();

     
