      window.BSL = {
        errs: [],
        rejects: [],
        onError: function(e) {
          if (window.BSL && window.BSL.errs) {
            window.BSL.errs.push(e);
          } else {
            console.error(e)
          };
        },
        onReject: function(e) {
          if (window.BSL && window.BSL.rejects) {
            window.BSL.rejects.push(e);
          } else {
            console.error(e)
          };
        }
      };
      window.addEventListener('error', function() {
        try {
          BSL.onError
        } catch (e) {}
      });
      window.addEventListener('unhandledrejection', function() {
        try {
          BSL.onReject
        } catch (e) {}
      });
