 <option value="replies">Replies</option>
 <option value="views">Views</option>
  </select>
 <select name="order" style="vertical-align: middle; font-size: 12px;">
 <option value="desc">Desc</option>
 <option value="asc">Asc</option>
 </select>
 </div>
 </fieldset>
 <br />
 </td>
 </tr>
  </table>
 </form>
