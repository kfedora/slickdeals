  <script>
  var queueFunction = {
    funcs: [],
    queue: function (key, callback)
    {
      queueFunction.funcs[key] = queueFunction.funcs[key] || [];
      queueFunction.funcs[key].push(callback);
    },
    execute: function (key)
    {
      if (typeof queueFunction.funcs[key] != "undefined" && queueFunction.funcs[key].length)
      {
        for (var i = 0; i < queueFunction.funcs[key].length; i++)
        {
          queueFunction.funcs[key][i].call(window);
        }
        queueFunction.funcs[key] = [];
      }
    }
  };

  vbmenu_register = function (controlid, noimage, datefield)
  {
    queueFunction.queue('vbmenu_register', function ()
    {
      vbmenu_register(controlid, noimage, datefield);
    });
  };
</script>
              
