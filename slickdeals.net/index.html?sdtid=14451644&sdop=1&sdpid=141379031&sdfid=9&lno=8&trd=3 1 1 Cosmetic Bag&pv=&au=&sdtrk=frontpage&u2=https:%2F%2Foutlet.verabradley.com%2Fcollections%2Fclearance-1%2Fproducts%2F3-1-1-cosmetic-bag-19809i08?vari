<script>
    window.narvar('eddCalculator', {});

    // narvar js overrides some consolem methods, luckily it adds a _restore
    // sub-method to each function it mangles -- we want to restore all asap

    (function restoreConsole (todo) {
        const _todo = todo.map((method) => {
            try {
                console[method]._restore();
            } catch (err) {
                return method;
            }
        }).filter(Boolean);

        if (_todo.length) setTimeout(restoreConsole, 50, _todo);
    })(["log", "debug", "info", "warn", "error"]);
</script>
