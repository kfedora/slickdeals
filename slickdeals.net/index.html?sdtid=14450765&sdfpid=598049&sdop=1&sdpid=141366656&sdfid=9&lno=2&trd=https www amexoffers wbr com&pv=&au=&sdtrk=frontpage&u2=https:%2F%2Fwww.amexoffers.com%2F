<script type="text/javascript">
	// slideshow video automation
	
	function pauseAllVideos(){
		$('#video_slide video').each(function(jqo){
			try { 
				jqo[0].pause();
				jqo[0].load();
				console.log("Pausing this video!");
			} catch (e) { console.log("eh, no vid on this slide");}
		});
	}
	
	function playVideoInJQObject(jqo){
		try {
			var vidin = $('video', jqo)[0];
			vidin.play();
		} catch (e) { 
			console.log("No vid this slide...");
		}
	}
	
	$(function(){
		$('#video_slide video')[0].play();

		$('#video_slide').on('cycle-after', function(e, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag){
			console.log("cycle after, playing video!");
			// pauseAllVideos();
			playVideoInJQObject(incomingSlideEl);
		});

		$("#video_slide").on('cycle-before', function(e, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag){
			console.log("cycle before, pausing ALL video!");
			pauseAllVideos();
			playVideoInJQObject(incomingSlideEl);
	    });


	});

</script> 
