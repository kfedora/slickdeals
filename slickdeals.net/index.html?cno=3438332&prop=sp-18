      (function() {
        var attempt = 0;
        var pageViewCalled = false;
        function InjectHook() {
          if (window.__satelliteLoaded) {
            var originalTrack = window._satellite.track;
            window._satellite.track = function() {
              var args = [].slice.apply(arguments);
              if (args[0] === 'delayed_page_load') {
                pageViewCalled = true;
              }
              window._satellite.pageBottom();
              return originalTrack.apply(window._satellite, args);
            }
            window.addEventListener("load",
              function(event) {
                setTimeout(function() {
                  if (!pageViewCalled) {
                    originalTrack('delayed_page_load');
                  }
                }, 5000);
              }
            );
          } else {
            attempt++;
            if (attempt < 40) {
              setTimeout(InjectHook, 50);
            } else {
              console.warn('failed to hook satellite');
            }
          }
        }
        InjectHook();
      })();
