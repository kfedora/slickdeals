    <script>
        $(function(){

            chatAvailability("load");

            $(".chat_icon_link").click(function(){
                chatAvailability("click");
            });

            function chatAvailability(checkType){

                //alert("Chat Availability Function Start");

                // Variables required for the chat availability check
                var appId = '1b1233d856ae4b7fbb279b44da9bc6f0';
                var uuid = createUUID();
                var apiURL = 'https://bjscc.brightpattern.com//clientweb/api/v1/availability?tenantUrl=bjscc.brightpattern.com';

                // HTML for chat icons which will be inserted via Javascript
                var unavailChat = "<a class='invisible chat_icon_link' href='javascript:void(0)'><img id='chatIcon' src='https://827l0h99.media.zestyio.com/chat-unavail.png'><div class='contact-list-description'>Chat - Closed</div></a>";
                var availChat = "<a class='chat_icon_link' href='javascript:void(0)'><img id='chatIcon' src='https://827l0h99.media.zestyio.com/icon-chat2.svg'><div class='contact-list-description'>Chat</div></a>";

                // Bright Pattern API call
                $.ajax({

                    url:apiURL,
                    async:false,
                    beforeSend: function (xhr){

                        xhr.setRequestHeader("Authorization", "MOBILE-API-140-327-PLAIN appId=" + appId + ", clientId=" + uuid)

                    },
                    success: function(data){

                        // Check returned data
                        if(data.chat == 'available'){

                            // Clear current chat contact div
                            $("#contactChat").empty();

                            // Add available chat HTML element to chat contact div
                            $("#contactChat").append(availChat);
							$("#footer-shop-icon").html(availChat);
							$("#footer-shop-icon").prop("onclick", null).off("click");

                            // On click load Bright Pattern initialization script
                            if(checkType == "click"){

                                $.getScript("https://bjscc.brightpattern.com/clientweb/chat-client-v4/js/init.js",function(){

                                    // Wait for the chat widget to load
                                    setTimeout(function(){

                                        // Fire click event on chat widget
                                        $("#sp-chat-widget").trigger('click');

                                    },3000);

                                });

                            }

                        }else{

                            // Clear current chat contact div
                            $("#contactChat").empty();

                            // Add unavailable chat HTML element to chat contact div
                            $("#contactChat").append(unavailChat);

                        }
                    }

                });

            }

            /**
             * function createUUID
             *
             * Creates a UUID for use in Bright Pattern API calls
             *
             * @returns {string}
             */

            function createUUID(){

                var dt = new Date().getTime();
                var uuid;

                uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                    var r = (dt + Math.random() * 16) % 16 | 0;
                    dt = Math.floor(dt / 16);
                    return (c == 'x' ? r : (r & 0x3 | 0x8)).toString(16);
                });

                return uuid;

            }

        });

    </script>
