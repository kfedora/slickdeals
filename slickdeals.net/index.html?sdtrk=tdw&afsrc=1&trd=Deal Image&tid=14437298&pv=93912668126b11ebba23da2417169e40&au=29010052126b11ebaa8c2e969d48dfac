		<script type="text/javascript">
			jQuery(function($) {
				var socialPraise = {
					maxQuotes: $('.social-quotes').find('.quote').length, quoteIndex: 0,delay: 5000,

					init: function() {
						socialPraise.randomizeQuotes();
						socialPraise.start();
					},
					start: function() {

						// Show the first quote immediatelly
						socialPraise.nextQuote();

						// Show other quotes every x seconds
						socialPraise.loop( socialPraise.delay );

					},
					randomizeQuotes: function() {
						var parent = $('.social-quotes'),
							divs   = parent.children();

						while ( divs.length ) {
							parent.append( divs.splice( Math.floor( Math.random() * divs.length ), 1 )[0] );
						}

						$('.social-feed').html( $('.social-quotes').html() );
						$('.social-feed').find('.quote').hide();
					},
					// Display the next quote
					nextQuote: function() {
						$('.active-quote').removeClass('active-quote').hide();
						socialPraise.next();
					},
					next: function() {

						if ( socialPraise.quoteIndex >= socialPraise.maxQuotes ) {
							socialPraise.quoteIndex = 0;
						}

						var quotes = $('.social-feed').find('.quote');
						quotes.eq(socialPraise.quoteIndex).addClass('active-quote').show();

						socialPraise.quoteIndex++;
					},
					// Run the automated social praise
					loop: function( interval ) {

						socialPraise.interval = setInterval( function() {
							socialPraise.nextQuote();
						}, interval );
					}
				}
				socialPraise.init();
			})
		</script>

	{{/if}}

