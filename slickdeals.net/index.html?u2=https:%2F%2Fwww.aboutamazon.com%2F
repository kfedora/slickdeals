        let parentClass = '.vex'
        let grecaptchaWidgetId = ''
        let grecaptchaSitekey = ''

        function interstitialOnloadCallback () {
            grecaptchaSitekey = document.querySelector('.ofd-button').dataset.sitekey
            if (grecaptchaSitekey) {
                // Here we need to specifically use the .vex copy of the .ofd-button in order to listen on the click
                let grecaptchaElement = document.querySelector(parentClass + ' .ofd-button')
                if (grecaptchaElement) {
                    grecaptchaWidgetId = grecaptcha.render(grecaptchaElement, {
                        'sitekey': grecaptchaSitekey,
                        'callback' : interstitialRecaptchaCallback,
                        'expiredCallback': interstitialExpiredCallback,
                        'size' : 'invisible'
                    })
                }
            }
        }

        function interstitialRecaptchaCallback (token) {
            let ofdUtils = new window.ofdUtils(grecaptcha, grecaptchaWidgetId, token, parentClass)
            if (ofdUtils) {
                ofdUtils.recaptchaCallback()
            }
        }

        // Automatically resets recaptcha after expiration, set as callback function for data-expired-callback attribute
        function interstitialExpiredCallback () {
            if (grecaptcha) {
                grecaptcha.reset()
            }
        }
    </script>
