(window.AmazonUIPageJS ? AmazonUIPageJS : P).when('A').execute(function(A){
  if(A.preload){
    A.preload('https://images-na.ssl-images-amazon.com/images/I/41FXFq6e9sL._RC|71VUoYh9w1L.js,61qkDhcEyZL.js,41W9ohA0e+L.js,11vrNkbdcvL.js,21qaguVEGfL.js,41pmMQqQVTL.js,51OfJS9QO2L.js,313jWehHlpL.js_.js?AUIClients/NavDesktopMetaAsset&ZUIyZrxo#desktop');
