<script id="mini_cart.html" type="text/ng-template"><div id="mini-cart" class="mini-cart-toggle" ng-mouseover="hover($event)" ng-mouseleave="dismiss($event)" aria-label="Shopping cart, contains {{getItemCount()}} items, subtotal is {{ getTotal() | currency }}" tabindex="0" role="link" aria-haspopup="true" aria-controls="mini-cart-list" aria-expanded="false">

