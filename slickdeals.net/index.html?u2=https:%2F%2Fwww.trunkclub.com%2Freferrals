<script type="text/javascript">

  $('[data-scrolled-into-view]').on('inview', function(event, isInView) {
    if (isInView) {
      $(this).attr('data-scrolled-into-view', 'true');
      window.dispatchEvent(new Event('resize'));
    } else {
      // element has gone out of viewport
      // $(this).attr('data-scrolled-into-view', '');
    }
  });
  let clubhouseCookie = function(e){
    let clubCookie = document.cookie.split(';').filter((item) => item.includes('user_location='))
    if (clubCookie.length) {
      let clubhouse = "";
      switch (clubCookie[0].split('=')[1]){
        case '1120':
          clubhouse= "boston";
          break;
        case '1600':
          clubhouse= "chicago";
          break;
        case '1920':
          clubhouse= "dallas";
          break;
        case '8840':
          clubhouse= "washington-dc";
          break;
        case '4480':
          clubhouse= "los-angeles";
          break;
        case '5600':
          clubhouse= "new-york";
          break;
      }
      let date = new Date();
      date.setTime(date.getTime()+(1*24*60*60*1000));
      document.cookie = "clubhouse_location="+ clubhouse +"; expires="+ date.toGMTString() +"; path=/";
    }
  }
  window.addEventListener("DOMContentLoaded", function() {
    clubhouseCookie();
 }, false);

 var permanentCookieOptions = { };
 var temporaryCookieOptions = { };
 var shortCookieOptions     = { };
 permanentCookieOptions = { path: "/", expires: 7300 };
 temporaryCookieOptions = { path: "/" };
 shortCookieOptions     = { path: "/", expires: 30 };

 let queryString = window.location.search;
 let urlParams = new URLSearchParams(queryString);

 var convertParamToCookie = function(paramName, cookieName, options) {
   if(urlParams.get(paramName)) {
     let date = new Date();
     date.setTime(date.getTime()+(options.expires*24*60*60*1000));
     document.cookie = `${cookieName}=${urlParams.get(paramName)}; expires=${date.toGMTString()};`;
   }
 }

 var convertReferralParamsToCookies = function() {
   if(urlParams.get('referral_code')) {
     if (urlParams.get('referral_code') === 'trunkclubforbusiness') {
       document.cookie = "referral_code="+ urlParams.get('referral_code') +"; path=/";
       document.cookie = "raw_referral_type=Trunk Club Employee; path=/";
       document.cookie = "raw_referral_detail="+ urlParams.get('referral_code') +"; path=/";
     } else {
       document.cookie = "referral_code="+ urlParams.get('referral_code') +"; path=/";
       document.cookie = "raw_referral_type=Promotion; path=/";
       document.cookie = "raw_referral_detail="+ urlParams.get('referral_code') +"; path=/";
     }
   }
 }

 convertReferralParamsToCookies();

 convertParamToCookie("ex",             "ex",             temporaryCookieOptions);
 convertParamToCookie("var",            "var",            temporaryCookieOptions);
 convertParamToCookie("utm_name",       "utm_campaign",   permanentCookieOptions);
 convertParamToCookie("utm_campaign",   "utm_campaign",   permanentCookieOptions);
 convertParamToCookie("v",              "variant",        permanentCookieOptions);
 convertParamToCookie("c",              "url_campaign_id",shortCookieOptions);
 convertParamToCookie("c",              "cid",            shortCookieOptions);
 convertParamToCookie("ranSiteID",      "site_id",        shortCookieOptions);
 let refDate = new Date();
 refDate.setTime(refDate.getTime()+(shortCookieOptions.expires*24*60*60*1000));
 document.cookie = "ref="+ window.location.href +"; path=/; expires="+ refDate.toGMTString() + ";";
 utmVariables = {};
 utmVariables.source  = urlParams.get('utm_source');
 utmVariables.medium  = urlParams.get('utm_medium');
 utmVariables.term    = urlParams.get('utm_term');
 utmVariables.content = urlParams.get('utm_content');
 document.cookie = "utm_variables="+ JSON.stringify(utmVariables)+"; path=/";

  // Segment

 $(document).ready(function(){

  $('.js-ccpa-confirm').on('click', function(){
    localStorage.setItem("ccpa", "Do Not Sell");
  });

  var localStorageTest = localStorage.getItem("ccpa");

  if (localStorageTest == null) {
    window.analytics=window.analytics||[],window.analytics.methods=["identify","group","track","page","pageview","alias","ready","on","once","off","trackLink","trackForm","trackClick","trackSubmit"],window.analytics.factory=function(t){return function(){var a=Array.prototype.slice.call(arguments);return a.unshift(t),window.analytics.push(a),window.analytics}};for(var i=0;i<window.analytics.methods.length;i++){var key=window.analytics.methods[i];window.analytics[key]=window.analytics.factory(key)}window.analytics.load=function(t){if(!document.getElementById("analytics-js")){var a=document.createElement("script");a.type="text/javascript",a.id="analytics-js",a.async=!0,a.src=("https:"===document.location.protocol?"https://":"http://")+"cdn.segment.io/analytics.js/v1/"+t+"/analytics.min.js";var n=document.getElementsByTagName("script")[0];n.parentNode.insertBefore(a,n)}},window.analytics.SNIPPET_VERSION="2.0.9",
          window.analytics.load('kg4p3yyapg');
        window.analytics.page();
    analytics.ready(function() {
      if(window.ga) {
        ga(function(tracker) {
          $.cookie("visitor_guid", tracker.get('clientId'), permanentCookieOptions)
        });
        ga('set', 'userID', );
      }
      // Basic global GA event tracking through Segment via data attributes
      $('body').on('click', '.trackEvent', function(e) {
        var t = $(this);
        analytics.track(t.data('ga-action'), {
          category: t.data('ga-category'),
          label: t.data('ga-label')
        });
      });
    });

  } // end of localStorage

  $('.js-accordion').each(function(){
    $(this).find('.js-accordionTrigger').on('click', function(e){
      e.preventDefault();
      $(this).next().slideToggle();
      return false;
    });
  });

   });
</script>

<!-- Trustev -->

<script type="text/javascript">
  (function() {
    if( !/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {

      // Manually creating the JS scripts. Only rendering on mobile. Appending the scripts after #core-js

      var pureCloudWebChat = document.createElement('script');
      var pureCloudCore = document.createElement('script');
      var core = document.getElementById('core-js');

      pureCloudWebChat.type = 'text/javascript';
      pureCloudWebChat.async = true;
      pureCloudWebChat.src = 'https://apps.mypurecloud.com/webchat/jsapi-v1.js';
      pureCloudWebChat.setAttribute('id', 'purecloud-webchat-js');
      pureCloudWebChat.setAttribute('region', 'us-east-1');
      pureCloudWebChat.setAttribute('org-guid', 'f92ce956-ce2d-4f98-a30d-e70ec431c2c4');
      pureCloudWebChat.setAttribute('deployment-key', '6595dd0f-7df2-437e-b4ba-3ec09fa996c2');

      pureCloudCore.type = 'text/javascript';
      pureCloudCore.async = true;
      pureCloudCore.src = 'https://apps.mypurecloud.com/widgets/9.0/cxbus.min.js';
      pureCloudCore.setAttribute('onload', "javascript:CXBus.configure({debug:false,pluginsPath:'https://apps.mypurecloud.com/widgets/9.0/plugins/'}); CXBus.loadPlugin('widgets-core');");

      core.parentNode.insertBefore(pureCloudWebChat, core.nextSibling);

      core.parentNode.insertBefore(pureCloudCore, core.nextSibling);

    }
  })();
</script>

  <script>
  (function() {
    $('a[href*="#"]:not([href="#"])').click(function(e) {
      if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
        var target = $(this.hash);
        target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
        if (target.length) {
          e.preventDefault();

          let defaultPos = 60;
          if ($('body').hasClass('faq')) {
            if ($('#faq-nav-wrapper').hasClass('is-fixed')) {
              defaultPos = 130;
            } else {
              defaultPos = 142;
              
              if ($(this).attr('href') == '#the-basics') {
                defaultPos = 130;
              }
            }
          }

          $('html, body').animate({
            scrollTop: target.offset().top - defaultPos
          }, 500);
        }
      }
    });
  })();
</script>


    <script src="/assets/js/templates/PageBuilder.js"></script>

  


