<script id="unifiedPriceTpl" type="text/x-handlebars-template">
    {{var 'sellPrice' myPriceItem.sellPrice}}
    {{var 'teaserPrice' myPriceItem.teaserPrice}}
    {{var 'nonLoyaltyPrice' myPriceItem.nonLoyaltyPrice}}
    {{var 'sellPriceSavingsAmount' myPriceItem.formattedSellPriceSavingsAmount}}

    {{#xif 'myPriceItem.priceAfterRebates > 0 && !priceExcludingRebates'}}
        {{var 'sellPrice' myPriceItem.priceAfterRebates}}
        {{var 'teaserPrice' myPriceItem.teaserPriceAfterRebates}}
        {{var 'nonLoyaltyPrice' myPriceItem.nonLoyaltyPriceAfterRebates}}
        {{var 'sellPriceSavingsAmount' myPriceItem.formattedSellPriceSavingsAmountAfterRebates}}
    {{/xif}}

    {{#xif '!myPriceItem.mapPriceApplicable || !showMapPrice'}}

        {{#xif 'showLoginJoin && myPriceItem.showTeaserPrice'}}
            {{var 'showLoginJoinLink' true}}
        {{/xif}}

            
            <div class="unified_price_block {{fontClass}} {{#xif 'myPriceItem.teaserPrice > 0'}}unified_price_rmp{{/xif}} {{#if alignPricing}}unified_pricing_align{{/if}}">
                <div class="unified_price_table">
                        
                    {{#xif 'myPriceItem.showRetailPrice'}}
                        <div class="unified_price_row unified_reg_price {{#if myPriceItem.clearanceSku}} rr_product_clearance_price{{/if}}">
                            <span class="price_column right"><span class="strike">&#36;{{toFixed myPriceItem.retailPrice}} </span></span>
                            <span class="price_column"> <span class="normal priceColumnSmarter">{{toLowerCase myPriceItem.uom}} (Reg)</span></span>
                        </div>
                    {{/xif}}

                    
                    {{#if myPriceItem.showNonLoyaltyPrice}}
                        <div class="unified_price_row unified_non_member_price unified_reg_price">
                            <span class="price_column right"><span class="strike">&#36;{{toFixed nonLoyaltyPrice}}</span></span>
                            <span class="price_column">
                                {{#xif 'myPriceItem.totalRebatesAmt > 0 || myPriceItem.nonLoyaltySavingsAmount > 0'}}
                                    Sale
                                    {{#xif 'showSavingsAmount && myPriceItem.formattedNonLoyaltySavingsAmount > 0'}}
                                        <span class="savings_detail">(Save &#36;{{checkPrice myPriceItem.formattedNonLoyaltySavingsAmount}})</span>
                                    {{/xif}}
                                {{else}}
                                    <span class="normal priceColumnSmarter">{{toLowerCase myPriceItem.uom}} (Reg)</span>
                                {{/xif}}
                            </span>
                        </div>
                    {{/if}}
                        
                    <div class="unified_price_row unified_sale_price red_price">
                        {{#xif 'sellPrice == 0'}}
                            {{var 'sellPrice' '0.00'}}
                        {{/xif}}
                        <span class="price_column right">&#36;{{toFixed sellPrice}}</span>
                        <span class="price_column red_price">
                            {{#xif 'myPriceItem.rewardMemberSku && !myPriceItem.showTeaserPrice'}}
                                Member
                                {{#xif 'showSavingsAmount && sellPriceSavingsAmount > 0'}}
                                    <span class="savings_detail">(Save &#36;{{checkPrice sellPriceSavingsAmount}})</span>
                                {{/xif}}
                            {{else}}
                                {{#if myPriceItem.clearanceSku}}
                                    <span  class="db" >Clearance</span>
                                {{else}}
