<script type="text/javascript">
    $(document).ready(function () {
        var qrystr = GetParameterValues('popup');
        function GetParameterValues(param) {
            var url = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for (var i = 0; i < url.length; i++) {
                var urlparam = url[i].split('=');
                if (urlparam[0] == param) {
                    return urlparam[1];
                }
            }
        }

        if (qrystr == "1") {
            setInterval(function () {
                if (!$(".TTwriteReview a").hasClass("already-checked")) {
                    $(".TTwriteReview a").trigger('click');
                    $(".TTwriteReview a").addClass("already-checked");
                }
            }, 10);

        }
    });

</script>

<script type="text/javascript">
    var turnToConfig = {
        siteKey: "Fbh1j7KwMmRh9Cvsite",
        setupType: "staticEmbed",
        reviewsSetupType: "staticEmbed",
        skipCssLoad: true,
        reviewsTeaserFunc: reviewsTeaser,
        iTeaserFunc: qaTeaser,
        reviewsTruncateLineLength: 100,
        chatter: {
            minimumCommentCount: 1
        },
        registration: {
            localGetLoginStatusFunction: localGetLoginStatusFunction,
            localRegistrationUrl: '/TurnToSSO/Index',
            localGetUserInfoFunction: localGetUserInfoFunction,
            localLogoutFunction: localLogoutFunction
        }
    };
    (function() {
        var tt = document.createElement('script'); tt.type = 'text/javascript'; tt.async = true;
        tt.src = document.location.protocol + "//static.www.turnto.com/traServer4_3/trajs/" + turnToConfig.siteKey + "/tra.js";
        $(document.body).append(tt);
    })();
    function qaTeaser (){
        if(typeof(TurnToItemData) == 'undefined' || typeof(TurnToItemData.counts) == 'undefined') {
            return '';
        } else {
            var answerCount = TurnToItemData.counts.a;
            var questionCount = TurnToItemData.counts.q;
            var teaserContainer = TurnTojQuery(".TurnToItemTeaser");
            var teaserHtml = ' | ';
            if(questionCount == 0) {
                teaserHtml = teaserHtml + '<a id="askQuestion" title="Ask a Question" href="#questionstab">Ask a Question</a>';
            } else {
                teaserHtml = teaserHtml + '<a id="readQuestions" title="Questions & Answers" href="#QuestionsandAnswers">' + questionCount + ' Question' + (questionCount == 1 ? '' : 's') + ', ' + answerCount + ' Answer' + (answerCount == 1 ? '' : 's') + '</a>';
            }
            var reviewCount = TurnToItemData.counts.r;
            //if( reviewCount == 0 && questionCount == 0 ) {
            //    teaserHtml += " [ no review or questions]";
            //}
            // Fill container with Q&A content
            teaserContainer.html(teaserHtml);
            // Add the appropriate listeners to the links

            if ($(window).width() > 1200) {
                teaserContainer.find('#readQuestions').click(function (e) {
                    toggleTab('qaTab');
                    infoH = $('.info-row').outerHeight();
                    $("html, body").animate({ scrollTop: $('#QuestionsandAnswers').offset().top - infoH }, 500);
                });
            } else {
                teaserContainer.find('#readQuestions').click(function (e) {
                    $('html, body').animate({
                        scrollTop: $("#qnaTab").offset().top
                    }, 500);
                    $('#qnaTab').trigger('click');
                });
            }

            if ($(window).width() > 1200) {
                teaserContainer.find('#askQuestion').click(function (e) {
                    toggleTab('qaTab');
                    infoH = $('.info-row').outerHeight();
                    $("html, body").animate({ scrollTop: $('#QuestionsandAnswers').offset().top - infoH }, 500);
                });
            }
            else {
                teaserContainer.find('#askQuestion').click(function (e) {
                    $('html, body').animate({
                        scrollTop: $("#qnaTab").offset().top
                    }, 500);
                    $('#qnaTab').trigger('click');
                });
            }
        }
    }
    function reviewsTeaser() {
        if(typeof(TurnToItemData) == 'undefined' || typeof(TurnToItemData.counts) == 'undefined') {
            return '';
            teaserContainer.find('#readReviews').click(function (e) {
                toggleTab('reviewsTab');
                infoH = $('.info-row').outerHeight();
                $("html, body").animate({ scrollTop: $('#reviews').offset().top - infoH }, 500);
            });
        } else {
            var reviewCount = TurnToItemData.counts.r;
            var teaserContainer = TurnTojQuery(".TurnToReviewsTeaser");
            var teaserHtml = '';
            var isUserLogin = "False";
            // round the average rating to the nearest tenth
            var rating = Math.round((TurnToItemData.counts.ar + 0.25) * 100.0) / 100.0;
            rating = rating.toString();
            var decimal = parseInt(rating.substring(2, 3))
            rating = rating.substring(0, 1) + "-" + (decimal >= 5 ? '5' : '0')
            if(reviewCount == 0) {
                teaserHtml = '<div class="TT2center TTratingBox TTrating-0-0"></div> ';
                teaserHtml += '<a id="writeReview" title="Review" href="javascript:void(0)">Write a Review</a>';
            } else {
                teaserHtml = '<div class="TTratingBox TTrating-' + rating + '"></div><a id="readReviews" title="Reviews" href="javascript:void(0)">' + reviewCount + ' Review' + (reviewCount == 1 ? '' : 's') + '</a>';
            }
            teaserContainer.html(teaserHtml);
            // Listener on 'read reviews' link
            if ($(window).width() > 1200) {
                teaserContainer.find('#readReviews').click(function (e) {
                    toggleTab('reviewsTab');
                    infoH = $('.info-row').outerHeight();
                    $("html, body").animate({ scrollTop: $('#reviews').offset().top - infoH }, 500);
                });
            } else {
                teaserContainer.find('#readReviews').click(function (e) {
                    $('html, body').animate({
                        scrollTop: $("#revTab").offset().top
                    }, 500);
                    $('#revTab').trigger('click');
                });
            }

            // Listener on 'write review' link
            teaserContainer.find('#writeReview').click(function(e) {
                TurnTo.writeReview();
            });
        }
    }
    function toggleTab(tab)
    {
        $('.infotabs.responsive a[href="#' + tab + '"]').tab('show');
    }
    function toggleTab1(tab)
    {
        $('.infotabs a[href="#' + tab + '"]').show();
    }
    function localGetLoginStatusFunction(callbackFn) {

        getTurnToSSOInfo(callbackFn);
    }
    function localGetUserInfoFunction(callbackFn) {

        getTurnToSSOInfo(callbackFn);
    }
    function localLogoutFunction(callbackFn) {

        $.ajax({
            type: "POST",
            url: "/product/logoutTurnToSSO",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                callbackFn({ user_auth_token: null });
            },
            error: function () {
                location.href = '/StaticContent/generalerror';
            }
        });
    }

    function getTurnToSSOInfo(callbackFn) {
        var isLogin = false, turnto_token = "", turnto_first_name = "", turnto_last_name = "", turnto_email_address = "", turnto_signature = "";

        $.ajax({
            type: "POST",
            url: "/product/getTurnToSSOinfo",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data != null) {
                    isLogin = data.IsLogin;

                    turnto_token = data.turnto_token;
                    turnto_first_name = data.turnto_first_name;
                    turnto_last_name = data.turnto_last_name;
                    turnto_email_address = data.turnto_email_address;
                    turnto_signature = data.turnto_signature;

                    if (isLogin) {

                        var obj = {};
                        obj.user_auth_token = turnto_token;
                        obj.first_name = turnto_first_name;
                        obj.last_name = turnto_last_name;
                        obj.email = turnto_email_address;
                        obj.email_confirmed = true;


                        callbackFn(obj, turnto_signature);
                    }
                    else {
                        callbackFn({ user_auth_token: null });
                    }
                }
            },
            error: function () {
                location.href = '/StaticContent/generalerror';
            }
        });
    }


</script>
