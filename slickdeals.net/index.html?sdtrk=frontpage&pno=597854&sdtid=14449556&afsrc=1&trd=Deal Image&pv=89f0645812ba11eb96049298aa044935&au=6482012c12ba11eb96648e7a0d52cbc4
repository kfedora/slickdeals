<script>
    $(window).on("scroll", function (e) {
        var qnaTab = $('#accordion .extra-added a:contains("Questions")'),
            revTab = $('#accordion .extra-added a:contains("Reviews")'),
            winWidth = $(window).width();
        $(qnaTab).attr('id', 'qnaTab');
        $(revTab).attr('id', 'revTab');
        var width = window.innerWidth;
        var isIE = false || !!document.documentMode;
        var isEdge = !isIE && !!window.StyleMedia;
        if (isIE == true || isEdge == true) {
            if (width > 1023) {
                if ($(window).scrollTop() > 700) {
                    $('.infotabs').addClass('fixed-tab');
                }
                else if ($(window).scrollTop() < 700) {
                    $('.infotabs').removeClass('fixed-tab');
                }
            }
            if (width <= 1023 && width >= 900) {
                if ($(window).scrollTop() > 900) {
                    $('.infotabs').addClass('fixed-tab');
                }
                else if ($(window).scrollTop() < 900) {
                    $('.infotabs').removeClass('fixed-tab');

                }

            }
            if (width <= 900 && width >= 300) {
                if ($(window).scrollTop() > 1000) {
                    $('.infotabs').addClass('fixed-tab');
                }
                else if ($(window).scrollTop() < 1000) {
                    $('.infotabs').removeClass('fixed-tab');

                }

            }

        }

        else {
            if (width > 1023) {
                if ($(window).scrollTop() > 1100) {
                    $('.infotabs').addClass('fixed-tab');
                }
                else if ($(window).scrollTop() < 1100) {
                    $('.infotabs').removeClass('fixed-tab');
                }
            }
            if (width <= 1023 && width >= 900) {
                if ($(window).scrollTop() > 1100) {
                    $('.infotabs').addClass('fixed-tab');
                }
                else if ($(window).scrollTop() < 1100) {
                    $('.infotabs').removeClass('fixed-tab');

                }

            }
            if (width <= 900 && width >= 300) {
                if ($(window).scrollTop() > 1500) {
                    $('.infotabs').addClass('fixed-tab');
                }
                else if ($(window).scrollTop() < 1500) {
                    $('.infotabs').removeClass('fixed-tab');

                }

            }
        }
    });
</script>

<script>
    $(window).on("orientationchange", function () {
        if (window.orientation == 90) {
            $('.infotabs').removeClass('fixed-tab');
        }
    });
</script>
<script src="/assets/js/swiper.min.js"></script>

<script type="text/javascript">

    $(document).ready(function () {
        $(document).on("click", "#readReviews", function () {
            $('.tab-pane').removeClass("active");
            $(".infotabs li").removeClass("active");
            $("#reviewslist").addClass("active");
            $('#reviewsTab').addClass("active");
            infoH = $('.info-row').outerHeight();
            $("html, body").animate({ scrollTop: $('#reviews').offset().top - infoH }, 500);

        });
        $(document).on("click", "#readQuestions", function () {
            $('.tab-pane').removeClass("active");
            $(".infotabs li").removeClass("active");
            $("#questionslist").addClass("active");
            $('#qaTab').addClass("active");
            infoH = $('.info-row').outerHeight();
            $("html, body").animate({ scrollTop: $('#QuestionsandAnswers').offset().top - infoH }, 500);

        });
    });
    $(document).ready(function () {
        if ($(window).width() < 769) {
            $("#collapseTwo").hide();
            $("#collapseOne").hide();
            $(".cart-sec-3 .panel-title a").addClass("collapsed");
        }
    });
</script>
