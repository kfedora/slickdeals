var xx2 = 0;
if (content2.style.display === "none") {
content2.style.display = "block";
$(e.target).addClass("up-cross-open");
$(e.target).removeClass("down-cross-open");
} else {
content2.style.display = "none";
$(e.target).addClass("down-cross-open");
$(e.target).removeClass("up-cross-open");
}
});
