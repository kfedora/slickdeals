<script>
window.onscroll = function() {myFunctionPlpMobileSticky()};
var navbar = document.getElementById("navbar-plp-mobile");
var navy = document.getElementById("navy");
var sticky = navbar.offsetTop;
function myFunctionPlpMobileSticky() {
if (window.pageYOffset >= sticky && $(document).scrollTop() > $(".products-counts-sorting").offset().top) {
navbar.classList.add("sticky-mobile")
} else {
navbar.classList.remove("sticky-mobile");
}
}
if ( $(window).scrollTop() > $(".products-counts-sorting").offset().top ){
navbar.classList.add("sticky-mobile");
}
</script>
