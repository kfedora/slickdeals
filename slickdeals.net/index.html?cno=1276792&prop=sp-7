    <script>
$(function () {
    if (Foundation.MediaQuery.current === 'small') {
        $('#splash_content').insertAfter('#horizon_tech_vid').show();
    } else {
        $('#splash_content').show();
    }
});
$(window).on('changed.zf.mediaquery', function (event, newSize, oldSize) {
    if (oldSize === 'small') {
        $('.home-splash-txt').css('font-size', 'inherit');
      $('.vid-loop-container--horizon-tech .content-wrap').html($('#splash_content'));
    } else if (newSize === 'small') {
      $('#splash_content').insertAfter('#horizon_tech_vid')
    }
})
$(document).on('open.zf.reveal', '#teaser-vid', function (e) {
    e.preventDefault()
    var vid = '<iframe src="https://www.youtube.com/embed/mR-gEtLnyLQ?autoplay=1&autohide=1&rel=0&showinfo=0&modestbranding=1&enablejsapi=1" frameborder="0" allowfullscreen></iframe>'
    // var vid = '<iframe src="https://www.youtube.com/embed/zy-8riroQO0?autoplay=1&autohide=1&rel=0&showinfo=0&modestbranding=1&enablejsapi=1" frameborder="0" allowfullscreen></iframe>'
    var $vid = $(vid);
    $(this).find('.flex-video').html('');
    $(this).find('.flex-video').append($vid);
})
$(document).on('closed.zf.reveal', '#teaser-vid', function (e) {
    e.preventDefault()
    $(this).find('.flex-video').remove('iframe');
    $(this).find('.flex-video').html('');
})
</script>
