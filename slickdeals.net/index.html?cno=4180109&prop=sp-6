<script type="text/javascript">
    //check for AUI
    if (window.P && window.P.AUI_BUILD_DATE) {
        P.when('jQuery', 'ready').execute(function($) {
$("html").addClass("js");
  function hideAndShow(toShow, toHide){
    $(toHide).hide();
    $(toShow).show();
  }
  
  $(function() { 
  $('.lessLink').click(function() {
    var baseId = "#" + $(this).attr('link-id');
    hideAndShow(baseId + "_link_more", baseId + "_link_less");
    hideAndShow(baseId + "_less", baseId + "_more");
    return false;
  });

  $('.moreLink').click(function() {
    var baseId = "#" + $(this).attr('link-id');
    hideAndShow(baseId + "_link_less", baseId + "_link_more");
    hideAndShow(baseId + "_more", baseId + "_less");
    return false;
  });
  });
        });
    } else if (window.amznJQ && window.amznJQ.available) {
        // fallback to amznJQ
