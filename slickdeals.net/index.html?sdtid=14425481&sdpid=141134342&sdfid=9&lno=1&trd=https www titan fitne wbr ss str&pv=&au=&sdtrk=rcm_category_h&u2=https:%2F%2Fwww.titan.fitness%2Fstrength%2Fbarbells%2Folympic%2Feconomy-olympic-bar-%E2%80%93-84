<script>
function getCookie(name) {
    var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
    return v ? v[2] : null;
}
function setCookie(name, value, days) {
    var d = new Date;
    d.setTime(d.getTime() + 24*60*60*1000*days);
    document.cookie = name + "=" + value + ";path=/;expires=" + d.toGMTString();
}
if(getCookie("emailpop") >= 1){
    $('#email-popup').modal('hide');
} else {
    setTimeout(function() { 
        $('#email-popup').on('hide.bs.modal', function () {
            let cookieValue = 1;
            if(getCookie("emailpop")){
                cookieValue = parseInt(getCookie("emailpop")) + 1;
            }
            setCookie("emailpop", cookieValue,30);
        }).modal('show');
    }, 4500);
}
</script>

<!--Check Remember Me box on interaction with login-->
<script>
document.getElementsByName("loginEmail")[0].onchange = function() {checkRemember()};
document.getElementsByName("loginPassword")[0].onchange = function() {checkRemember()};
function checkRemember() {
  document.getElementById("rememberMe").checked = true;;
}
</script>
