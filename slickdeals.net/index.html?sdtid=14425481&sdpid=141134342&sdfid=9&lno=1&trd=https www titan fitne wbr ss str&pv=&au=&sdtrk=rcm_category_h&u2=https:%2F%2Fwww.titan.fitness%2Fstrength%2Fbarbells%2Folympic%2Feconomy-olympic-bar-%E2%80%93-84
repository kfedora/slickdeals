<script type="text/javascript">
	(function() {
    	var emailFormButton = document.getElementsByClassName('newsletter-submit');
    	var emailInput = document.getElementById('dwfrm_mcsubscribe_email');

    	if (emailFormButton.length) {
    		emailFormButton[0].addEventListener('click', function() {
    			window.analytics.identify(null, {
    	   			email: emailInput.value,
                    source: 'email-input',
    	  		}, {wcapiVersion: '2.0'});
    		});
    	}
    })();
</script> 








	<script type="text/javascript"> 
		(function() { 
		var productLineItems = document.getElementsByClassName('product-line-item');
		if(productLineItems.length) {
	    	
			//only fire on Cart page, not order summary
			var page = document.getElementsByClassName('page');
			if(page.length && page[0].dataset.action) {
				var wcProducts=[];
				for (let w = 0; w < productLineItems.length; ++w) {
					wcProducts.push({
						id: productLineItems[w].getElementsByClassName('quantity')[0].dataset.pid, 
			            vsku: productLineItems[w].getElementsByClassName('quantity')[0].dataset.pid, 
			            psku: productLineItems[w].getElementsByClassName('quantity')[0].dataset.pid, 
			            name: productLineItems[w].getElementsByClassName('line-item-name')[0].innerText, 
			            price: productLineItems[w].getElementsByClassName('price')[0].innerText, 
			            quantity: productLineItems[w].getElementsByClassName('quantity')[0].value, 
			            storeid: "TitanFitness"
					});
				}
		    	analytics.track('Viewed Cart', { 
		        cartLink: "https://www.titan.fitness/cart",
		        products: wcProducts }, {wcapiVersion: '2.0'});	
			}
		}
	    })(); 
	</script> 









      









