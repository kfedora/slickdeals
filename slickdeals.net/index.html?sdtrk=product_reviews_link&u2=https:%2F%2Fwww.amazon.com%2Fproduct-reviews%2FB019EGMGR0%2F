clearTimeout(this.timeout),this.logExceptionToCSM(s)}},HveCountdownTimer.prototype.createPaintWidgetTimeout=function(t,e){var i=this;this.timeout=window.setTimeout((function(){i.paintWidget(t)}),this.calculatePaintDelay(e,this.timeUnitProperties[this.endUnit].unitInMilliseconds))},HveCountdownTimer.prototype.setImageHrefProperties=function(t,e){e?(t.setAttribute("onclick","location.href='"+e+"'"),t.style.cursor="pointer"):(t.onclick=null,t.style.cursor="auto")},HveCountdownTimer.prototype.calculateTimeRemaining=function(t){var e=this.timeUnits.slice(this.timeUnits.indexOf(this.startUnit),this.timeUnits.indexOf(this.endUnit)+1);t<=0&&(t=0);for(var i=t,n={},o={},r="",s={},a=0;a<e.length;a++){r=e[a];var u=(s=this.timeUnitProperties[r]).unitInMilliseconds,d=Math.floor(i/u);(i-=d*u)>0&&r===this.endUnit&&(d+=1),n[r]=d}for(var m=e.length-1;m>0;m--){r=e[m],s=this.timeUnitProperties[r];var g=n[r],l=e[m-1],h=this.timeUnitProperties[l];g*s.unitInMilliseconds===h.unitInMilliseconds&&(n[r]=0,n[l]+=1)}for(var c=0;c<e.length;c++)r=e[c],s=this.timeUnitProperties[r],o[r]=s.formatFunction(n[r]);return o},HveCountdownTimer.prototype.calculatePaintDelay=function(t,e){return t%e==0?e:t%e},HveCountdownTimer.prototype.validateConstructWidgetRequest=function(t){for(var e=[],i=0;i<this.requiredConstructWidgetArguments.length;i++){var n=this.requiredConstructWidgetArguments[i];t[n]||e.push(n.toString()+" was not provided")}for(var o=0;o<this.numericWidgetArguments.length;o++){var r=this.numericWidgetArguments[o],s=t[r];if(s)this.isInt(s)||e.push(r.toString()+" is not numeric")}if(t.EndDate){var a=Date.parse(t.EndDate);isNaN(a)&&e.push("Invalid End Date: "+t.EndDate)}var u=this.timeUnits.indexOf(t.StartTimeUnit),d=this.timeUnits.indexOf(t.EndTimeUnit);return-1===u&&e.push("Invalid start time unit: "+t.StartTimeUnit),-1===d&&e.push("Invalid end time unit: "+t.EndTimeUnit),u>-1&&d>-1&&u>d&&e.push("The endTimeUnit is larger than the startTimeUnit"),e},HveCountdownTimer.prototype.handleConstructWidgetArgsError=function(t,e,i){this.logErrorArrayToCSM(e,"FATAL",i),this.constructFallback(t)},HveCountdownTimer.prototype.logErrorArrayToCSM=function(t,e,i){if(window.ueLogError)try{for(var n=0;n<t.length;n++){var o={message:t[n]};this.logExceptionToCSM(o,e,i)}}catch(r){}},HveCountdownTimer.prototype.logExceptionToCSM=function(t,e,i){if(window.ueLogError){var n={logLevel:e,attribution:"HighVelocityEvents - CountdownTimer - "+i};try{window.ueLogError(t,n)}catch(o){}}},HveCountdownTimer.prototype.appendLeadingZeros=function(t,e){for(var i=""+t,n=i.length>e?i.length:e,o="";o.length<e;)o+="0";var r=o+t;return r.substr(r.length-n)},HveCountdownTimer.prototype.getPixelFormat=function(t){return t.toString()+"px"},HveCountdownTimer.prototype.isValidTimeUnit=function(t){return this.timeUnits.indexOf(t)>-1},HveCountdownTimer.prototype.isInt=function(t){var e;return!isNaN(t)&&(0|(e=parseFloat(t)))===e},HveCountdownTimer.prototype.isBoolean=function(t){return"boolean"==typeof t},HveCountdownTimer.prototype.getDate=function(){return new Date};try{module&&(module.exports=HveCountdownTimer)}catch(error){}</script>
