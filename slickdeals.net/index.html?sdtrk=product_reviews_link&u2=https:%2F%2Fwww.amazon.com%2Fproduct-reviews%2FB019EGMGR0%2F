  <script type="text/javascript">function HveCountdownTimer(t){this.requiredConstructWidgetArguments=["EndDate","WidgetContainerId","BackgroundImageWidth","BackgroundImageHeight","TimeFontColor","TimeFontSize","TimeFontFamily","TimeFontWeight","TimeFontStyle","StartTimeUnit","EndTimeUnit"],this.numericWidgetArguments=["BackgroundImageWidth","BackgroundImageHeight","TimeFontWeight"];var e=this;this.timeUnits=["days","hours","minutes","seconds"],this.timeUnitProperties={days:{name:"days",unitInMilliseconds:864e5,formatFunction:function(t){return e.appendLeadingZeros(t,1)}},hours:{name:"hours",unitInMilliseconds:36e5,formatFunction:function(t){return e.appendLeadingZeros(t,2)}},minutes:{name:"minutes",unitInMilliseconds:6e4,formatFunction:function(t){return e.appendLeadingZeros(t,2)}},seconds:{name:"seconds",unitInMilliseconds:1e3,formatFunction:function(t){return e.appendLeadingZeros(t,2)}}};try{if(!t)throw"ConstructWidgetArgs is not defined in HveCountdownTimer constructor";var i=this.validateConstructWidgetRequest(t);if(i.length>0)return void this.handleConstructWidgetArgsError(t,i,"Construct Widget");if(this.widget=document.getElementById(t.WidgetContainerId),!this.widget)return void this.handleConstructWidgetArgsError(t,["Widget Container not found"],"Construct Widget");this.widget.style.height="100%",this.widget.style.width="100%",this.timeUnitDivs={},this.startUnit=t.StartTimeUnit,this.endUnit=t.EndTimeUnit,this.buildHtmlContent(t),this.paintWidget(new Date(t.EndDate).getTime())}catch(n){this.logExceptionToCSM(n,"FATAL","Construct Widget"),this.constructFallback(t)}}HveCountdownTimer.prototype.buildHtmlContent=function(t){var e=document.getElementById(t.WidgetContainerId+"-internal");this.setImageHrefProperties(e,t.ImageHref);for(var i=document.createDocumentFragment(),n=0;n<this.timeUnits.length;n++){var o=this.timeUnits[n],r=this.buildTimeUnitDiv(t[o+"XPosition"],t[o+"YPosition"],t.TimeFontColor,t.TimeFontSize,t.TimeFontWeight,t.TimeFontFamily,t.TimeFontStyle)