                    wcp_sbParent.insertBefore(wcp_ContainerSB, wcp_sbParent.children[0]);


                clearInterval(wpc_timer);
            };

        } catch (error) {
            setTimeout(function () {
                clearInterval(wpc_timer);
            }, 6000);
            console.dir(error);
