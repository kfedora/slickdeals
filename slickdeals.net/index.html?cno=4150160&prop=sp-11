        try {

            var wcp_sbParent = document.querySelector('.mainContent');
            
            if (wcp_sbParent) {

                    wcp_ContainerSB.innerHTML = '<div class="sbIdMe"><img src="//static.lenovo.com/na/small-business/benefits-page/smb-logo-pro-white.svg" alt="Lenovo Pro Logo"><p>Shopping for work? LenovoPRO has you covered with savings for Veteran-owned Small Businesses.</p><a href="' + siteRoot + '/business/benefits?IPromoID=LEN782388">Learn More</a></div>';

