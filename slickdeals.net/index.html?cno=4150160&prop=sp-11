<script type="text/javascript" SPK="8833549763644">var acc = document.getElementsByClassName("accordion");
var i;

for (i = 0; i < acc.length; i++) {
    acc[i].onclick = function(){
        /* Toggle between adding and removing the "active" class,
        to highlight the button that controls the panel */
         this.classList.toggle("active");

        /* Toggle between hiding and showing the active panel */
        var panel = this.nextElementSibling;
        if (panel.style.display === "block") {
            panel.style.display = "none";
        } else {
            panel.style.display = "block";
        }
    }
}</script>
<script type="text/javascript" SPK="8833549829180">var studentsBackToSchool = {

	init: function() {

		console.log('tessss');

		var getUrl = window.location.href;
		var getUrlHash = getUrl.split('#').pop();
	
		if(getUrlHash.indexOf('quiz') == 0 ) {
			 $('html, body').animate({
		        scrollTop: $(".student-about-you").offset().top
		    }, 2000);
		} 
	}
}

$(window).load(function() {
	setTimeout(studentsBackToSchool.init, 3000);
});

</script>
