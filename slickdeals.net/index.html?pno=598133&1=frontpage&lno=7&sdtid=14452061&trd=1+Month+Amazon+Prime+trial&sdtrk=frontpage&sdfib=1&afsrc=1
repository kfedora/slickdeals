<script>
  P.when("A", "a-carousel-framework").execute(function (A, framework) {
    framework.onInit("music-scroller", function (carousel) {
      var musicRotateInterval = A.interval(function () {
        carousel.gotoNextPage();
      }, 2500);
      A.$("#music-scroller .a-carousel-row-inner").click(function (data) {
        clearInterval(musicRotateInterval);
      });
    });
    framework.onInit("video-scroller", function (carousel) {
      var videoRotateInterval = A.interval(function () {
        carousel.gotoNextPage();
      }, 2500);
      A.$("#video-scroller .a-carousel-row-inner").click(function (data) {
        clearInterval(videoRotateInterval);
      });
    });
  });
