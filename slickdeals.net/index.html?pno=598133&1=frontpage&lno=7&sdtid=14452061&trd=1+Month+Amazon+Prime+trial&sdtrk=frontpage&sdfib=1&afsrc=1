<script type="text/javascript">
    P.when("A", "a-carousel-framework", "ready").execute(function(A, framework) {
        
        var tabs;
        var tabArray = [];
        var panels;
        
        var lastFocusedTab = 0; //Default to first tab
        
        // For easy reference
        var keys = {
            end: 35,
            home: 36,
            left: 37,
            up: 38,
            right: 39,
            down: 40
        };
        
        var direction = {
            37: -1,
            38: -1,
            39: 1,
            40: 1
        };
        
        var checkTabFocus = function(target) {
            focused = document.activeElement;
        
            if (target === focused) {
                selectTab(target, false);
            };
          };
        
        var focusEventHandler = function (event) {
            var target = event.target;
        
            setTimeout(checkTabFocus, 300, target);
        };
        
        var generateArrays = function () {
            tabs = document.querySelectorAll('#prime-cardcarousel-container [role="tab"]');
            panels = document.querySelectorAll('#prime-cardcarousel-container [role="tabpanel"]');
            
            for (i = 0; i < tabs.length; ++i) {
                tabArray.push(tabs[i]);
                tabs[i].index = i;
            };
        };
        
        var focusFirstTab = function () {
            tabs[0].focus();
        };
        
        var focusLastTab = function () {
            tabs[tabs.length - 1].focus();
        };
        
        var keydownEventListener = function (event) {
            var key = event.keyCode;
            
            switch (key) {
              case keys.end:
                event.preventDefault();
                selectTab(tabs[tabs.length - 1]);
                break;
              case keys.home:
                event.preventDefault();
                selectTab(tabs[0]);
                break;
            };
        };
        
        var keyupEventListener = function (event) {
            var key = event.keyCode;
            
            switch (key) {
              case keys.left:
              case keys.right:
                switchTabOnArrowPress(event);
                break;
            };
        };
        
        var switchTabOnArrowPress = function (event) {
            var pressed = event.keyCode;
        
            for (x = 0; x < tabs.length; x++) {
              tabs[x].addEventListener('focus', focusEventHandler);
            };
        
            if (direction[pressed]) {
              var target = event.target;
              if (target.index !== undefined) {
                if (tabs[target.index + direction[pressed]]) {
                  tabs[target.index + direction[pressed]].focus();
                }
                else if (pressed === keys.left || pressed === keys.up) {
                  focusLastTab();
                }
                else if (pressed === keys.right || pressed == keys.down) {
                  focusFirstTab();
                };
              };
            };
        };
        
        var generatedArrays = generateArrays();
    
        var selectTab = function(tab, setFocus) {
            setFocus = setFocus || true;
            
            deactivateOtherTabs();
            
            A.$(tab).addClass("carousel-button-selected").parents(".card-category").addClass('card-selected');
        
            tab.setAttribute('tabindex', '0');
        
            tab.setAttribute('aria-selected', 'true');
        
            var controls = tab.getAttribute('aria-controls');
        
            document.getElementById(controls).setAttribute('aria-hidden', 'false');
            document.getElementById(controls).setAttribute('tabindex', '0');
            
            var controls = "#" + controls;
            A.$(controls).find(".card-cta a").attr('tabindex', '0');
            
            if (setFocus) {
                tab.focus();
            };
            
            lastFocusedTab = tabArray.indexOf(tab);
        };
        
        var deactivateOtherTabs = function () {
            for (t = 0; t < tabs.length; t++) {
              tabs[t].setAttribute('tabindex', '-1');
              tabs[t].setAttribute('aria-selected', 'false');
              A.$(tabs[t]).removeClass("carousel-button-selected").parents(".card-category").removeClass('card-selected');
              tabs[t].removeEventListener('focus', focusEventHandler);
            };
        
            for (p = 0; p < panels.length; p++) {
              panels[p].setAttribute('aria-hidden', 'true');
              panels[p].setAttribute('tabindex', '-1');
              A.$(panels[p]).find(".card-cta a").attr('tabindex', '-1');
            };
        };
        
        var selectTabByIndex = function(index) {
            selectTab(tabs[index], false);
        };
        
        var getLastFocusedTabIndex = function () {
            return lastFocusedTab;
        };
        
        var getTablistLength = function () {
            return tabs.length;
        };
        
        //Expand cards when clicked
        var cards = A.$(".card-clickable");
            A.$(".card-clickable").click(function() {
                A.$(".prime-cardcarousel").addClass("carousel-active");
                cards.parent().removeClass("card-selected");
                A.$(this).parent().addClass("card-selected");
            });
        //Expand card containing the current carousel item and highlight toggle button corresponding to the current carousel item
        A.on("a:carousel:prime-main-carousel:change:pageNumber", function(data) {
            A.$(".card-content .carousel-page-button").removeClass("carousel-button-selected").parents(".card-category").removeClass("card-selected");
            A.$(".card-content .carousel-page-button").eq(data.newValue - 1).addClass("carousel-button-selected").parents(".card-category").addClass("card-selected");
        });
        framework.getCarouselByName("prime-main-carousel").setAttr("animation_speed", 9000);
        
        //Select the first item by default
        var defaultPageNumber = framework.getCarouselByName("prime-main-carousel").getAttr("pageNumber");
        A.$(".card-content .carousel-page-button").eq(defaultPageNumber - 1).addClass("carousel-button-selected");
        if(A.$(".prime-cardcarousel").hasClass("carousel-active")) {
            A.$(".card-content .carousel-page-button").eq(defaultPageNumber - 1).parents(".card-category").addClass("card-selected");
        }
        framework.onInit("prime-main-carousel", function(carousel) {
            carousel.gotoPage(1);
        
            //When a toggle button is clicked, go to the corresponding page
            A.$(".card-content .carousel-page-button").click(function() {
                var pageNumber = A.$(this).data("page");
                carousel.gotoPage(pageNumber);
            });
            A.$(".card-content .carousel-page-button").focus(function(e) {
                cards.parent().removeClass("card-selected");
                A.$(this).parents(".card-category").addClass("card-selected");
                var pageNumber = A.$(this).data("page");
                carousel.gotoPage(pageNumber);
            });
            
            A.$(".card-clickable").click(function(){
                var firstInCategory = A.$(this).parent().find(".card-content .carousel-page-button").eq(0).data("page");
                carousel.gotoPage(firstInCategory);
            });
            
            A.$("#prime-benefit-cards .carousel-page-button").keydown(keydownEventListener);
            A.$("#prime-benefit-cards .carousel-page-button").keyup(keyupEventListener);
        });
        framework.createAll();
        framework.initializeAll(); 
    });
</script><style type="text/css">
