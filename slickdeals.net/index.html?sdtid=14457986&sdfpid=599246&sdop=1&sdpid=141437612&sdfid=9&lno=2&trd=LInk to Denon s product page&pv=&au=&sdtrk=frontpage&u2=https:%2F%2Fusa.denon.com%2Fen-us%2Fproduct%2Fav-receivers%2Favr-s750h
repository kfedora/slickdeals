<script>

    $(".productClick").click(function () {
        var productId = $(this).closest(".promotedProduct").attr('data-pid');
        var price = $(this).parent().prev('.price-row').find('.price').text().replace(' RRP', '');
        var productName = $(this).closest(".promotedProduct").attr('data-varid');
        var CatName = $(this).closest(".promotedProduct").attr('data-catId');

        //alert(productId + ',' + price + ',' + productName);

        function addKeyValueInObj(key, value, object) {
            object[key] = value;
            return object;
        }

        var trackingObj = {};
        var trackingProducts = [];
        var trackingActionField = {};
        var trackingEcommerce = {};
        var trackingPurchase = {};
        var shippingInfo = "";
        var billInfo = "";
        var product = "";


        //*********************Tracking Add Order ID******************

        addKeyValueInObj('list', CatName, trackingActionField);

        var trackingProduct = {};

        //*********************Tracking Add Product Name******************
        addKeyValueInObj('name', productName, trackingProduct);
        ////*********************Tracking Add Product Id******************
        addKeyValueInObj('id', productId, trackingProduct);
        ////*********************Tracking Price Product Id******************
        addKeyValueInObj('price', price, trackingProduct);
        ////*********************Tracking brand Product Id******************
        addKeyValueInObj('brand', 'Denon', trackingProduct);
        ////*********************Tracking category Product Id******************
        addKeyValueInObj('category', CatName, trackingProduct);

        trackingProducts.push(trackingProduct);

        addKeyValueInObj('currencyCode', 'USD', trackingEcommerce);
        //*********************Tracking ActionField Product Id******************
        addKeyValueInObj('actionField', trackingActionField, trackingPurchase);
        //*********************Tracking Products Product Id******************
        addKeyValueInObj('products', trackingProducts, trackingPurchase);
        //*********************Tracking Products Product Id******************
        addKeyValueInObj('click', trackingPurchase, trackingEcommerce);
        //*********************Tracking purchase Product Id******************
        addKeyValueInObj('ecommerce', trackingEcommerce, trackingObj);
        //*********************Tracking event Product Id******************
        addKeyValueInObj('event', 'productClick', trackingObj);
        console.log("trackingObj>>>>>", trackingObj);
        window.dataLayer.push(trackingObj);
    });

$(document).ready(function () {
        $('.promotedProduct .add-to-cart-qty').hide();
        $('.promotedProduct').each(function () {
            var pID = $(this).attr("data-pId");
            var varId = $(this).attr("data-varId");

            $(this).find('#addtocart_productid').val(pID);
            $(this).find('#addtocart_variantid').val(varId);
            

            var st = $(this).find('.variant').attr('data-stockinfo');
            if (st == "Out-of-Stock") {
                $(this).find('.cls-dispBuy').show();
                //$(this).find('.cls-dispOutofstock').show();
            } else {
                $(this).find('.cls-dispBuy').show();

            }
        })


    })

    function OnVariantChange(element, variantid) {

       
        var PriceLabel = $('#PriceLabel').val();
        var catalogId = $(element).data('catalogid');
        var colorname = $(element).data('colorname');
        var obj = {
            catalogProdId: catalogId,
            variantId: variantid
        }

        jQuery.ajax({
            url: '/api/cxa/SoundUnitedSharedProductDetail/GetVariantStock',
            type: "POST",
            data: obj,
            dataType: "json",
            success: function (data) {

                console.log(data);

                if (data != null) {

                    var promotedProduct = $(element).closest('.promotedProduct');
                     if (promotedProduct.length) {

                         $(promotedProduct).find("#addtocart_variantid").val(variantid);

                         var list = $(promotedProduct).find(".variant");
                         var vcolor = $(promotedProduct).find(".color-name");
                         list.each(function (idx, el) {

                             var idAttr = $(el).attr('id');

                             if (idAttr.replace("varitem_", "") == variantid) {
                                 $(el).addClass("active");
                                 $(vcolor).text(colorname);

                             } else {
                                 $(el).removeClass("active");
                             }
                         });

                         var addToCartButton = promotedProduct.find('.add-to-cart-btn');
                         var statusMessage = promotedProduct.find('.stock-alert');
                         var companionstockMessage = promotedProduct.find('#companionstockMessage').val();
                         var priceRow = promotedProduct.find('.price-row');

                         var prodImg = promotedProduct.find(".img-box img");

                         var prodLink = $(promotedProduct).find(".productLink").attr('value');
                         var firstVarID = $(promotedProduct).attr("data-varId");
                         var prodLinkVarId = "";
                         if (prodLink != undefined && prodLink != '') {
                             if (firstVarID == variantid) {
                                 prodLinkVarId = prodLink;
                             }
                             else {
                                 prodLinkVarId = prodLink + '?varId=' + variantid + '';
                             }
                             $(promotedProduct).find(".prod-box .titleLink").attr('href', prodLinkVarId);
                             $(promotedProduct).find(".prod-box .imgLink").attr('href', prodLinkVarId);
                             $(promotedProduct).find(".prod-box .productClick").attr('href', prodLinkVarId);
                         }
                         if (data.ProductImage != null) {
                             if (data.ProductImage.ImageUrl != null) {

                                 $(prodImg).attr("src", data.ProductImage.ImageUrl + '?mw=500');
                             }
                             if (data.ProductImage.Alt != null) {

                                 $(prodImg).attr("alt", data.ProductImage.Alt);
                             }
                         }
                         if (data.StockModel != null) {
                             if (data.StockModel.StockInformationList.length > 0) {
                                 var elem = data.StockModel.StockInformationList[0];
                                
                                 if (elem.Status == "In-Stock") {
                                     //$(addToCartButton).removeAttr("disabled");
                                     //$(addToCartButton).text("Shop Now");
                                     $(promotedProduct).find('.cls-dispBuy').show();
                                     if (companionstockMessage.indexOf("¶count¶") != -1) {
                                         $(statusMessage).text("Only " + elem.Count + " left")
                                     }
                                 }
                                 else {
                                    // $(addToCartButton).prop("disabled", true);
                                     //$(addToCartButton).text("OutOfStock");
                                     $(promotedProduct).find('.cls-dispBuy').show();
                                 }
                             }
                             else {
                                 //$(addToCartButton).prop("disabled", true);
                                 $(promotedProduct).find('.cls-dispBuy').show();
                                 //$(addToCartButton).text("OutOfStock");
                             }
                         }

                         if (data.ProductVariantModel != null) {
                             var priceHtml = "";
                             if (data.ProductVariantModel.ListPrice != null) {
                                 if (data.ProductVariantModel.IsOnSale) {

                                     priceHtml = "<strong><del>$" + data.ProductVariantModel.ListPrice.toFixed(2) + "</del><span class='cur'>" + PriceLabel + "</span></strong>";
                                     priceHtml += "<strong class=''> $" + data.ProductVariantModel.AdjustedPrice.toFixed(2) + "</strong>";

                                 } else {
                                     if (data.ProductVariantModel.ListPrice != null) {
                                         priceHtml = "<strong class=''>$" + data.ProductVariantModel.ListPrice.toFixed(2) + "<span class='cur'>" + PriceLabel + "</span></strong>";
                                     }
                                     else {
                                         priceHtml = "<strong class=''></strong>";
                                     }
                                 }
                                 $(priceRow).html(priceHtml);
                             }
                         }
                     }
                 }
            },
            error: function (jqXHR) {
                if (jqXHR.status == 500) {
                    console.log('Internal error: ' + jqXHR.responseText);
                } else {
                    console.log('Unexpected error.');
                }
            }
        });
    }

</script>
