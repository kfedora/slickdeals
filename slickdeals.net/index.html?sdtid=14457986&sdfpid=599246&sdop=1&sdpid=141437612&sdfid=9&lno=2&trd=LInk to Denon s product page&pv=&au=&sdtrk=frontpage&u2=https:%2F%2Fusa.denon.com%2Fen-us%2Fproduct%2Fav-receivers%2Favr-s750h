    function GetVariantStockInfo(el) {
        
         var PriceLabel = $('#PriceLabel').val();
        var catalogId = jQuery("#catalogId").val();
        var variantid = jQuery(el).data("varid");
         
        
         $('.header-buy-box').find('#addtocart_variantid').val(variantid);
         $('.main-topsection .actions-block').find('#addtocart_variantid').val(variantid);
        
         jQuery("#firstVarId").attr('value', variantid);
        var obj = {
            catalogProdId: catalogId,
            variantId: variantid
        }

        jQuery.ajax({
            url: '/api/cxa/SoundUnitedSharedProductDetail/GetVariantStock',
            type: "POST",
            data: obj,
            dataType: "json",
            success: function (data) {

                console.log(data);
                

                if (data != null) {
                    if ($('.product-image-slider ').length) {
                        $('.product-image-slider').flickity('destroy');



                    }

                    jQuery(".main-topsection .var-color").each(function () {

                        if (jQuery(this).data("varid") == variantid) {
                            jQuery(this).addClass("active");
                            jQuery(".main-topsection .color-name").text(jQuery(this).data("varcolor"));
                            

                        } else {
                            jQuery(this).removeClass("active");
                        }
                    });



                    var addToCartButton = jQuery(".main-topsection .add-to-cart-btn");
                    var priceRow = jQuery(".main-topsection .price-row");
                    var headerpriceRow = jQuery(".header-buy-box .price-row");
                    var imageSlider = $('.product-main-img .product-image-slider');
                    var hdnProdQuan = $('#hdnIsProductQuantity').val();
                    var videoId = $('#videoId').val();
                    if (data.ProductImage != null) {

                        imageSlider.html('');
                        var prodImg = "";
                        var count = 0;
                        if (data.ProductImage.Images != null) {

                            for (var i = 1; i < data.ProductImage.Images.length; i++) {

                                if (count == 1 && videoId != '' && videoId != undefined) {
                                   
                                    $(imageSlider).append('<div class="item">'+
                                                   ' <figure>'+
                                                   ' <div class="video-box">'+
                                                   '<script src="https://fast.wistia.com/embed/medias/8p4eythunc.jsonp" async><\/script>'+
                                                   ' <script src="https://fast.wistia.com/assets/external/E-v1.js" async><\/script>' +
                                                   ' <div class="wistia_responsive_padding" style="padding:56.25% 0 0 0;position:relative;">'+
                                                   ' <div class="wistia_responsive_wrapper" style="height:100%;left:0;position:absolute;top:0;width:100%;">' +
                                                   ' <div class="wistia_embed wistia_async_' + videoId + ' videoFoam=true" style="height:100%;position:relative;width:100%">' +
                                                   ' <div class="wistia_swatch" style="height:100%;left:0;opacity:0;overflow:hidden;position:absolute;top:0;transition:opacity 200ms;width:100%;">'+
                                                   ' <img src="https://fast.wistia.com/embed/medias/8p4eythunc/swatch" style="filter:blur(5px);height:100%;object-fit:contain;width:100%;" alt="" aria-hidden="true" onload="this.parentNode.style.opacity=1;" />'+
                                                   ' </div> </div></div></div></div></figure></div>');
                                }
                                else {
                                    $(imageSlider).append('<div class="item">' +
                                        ' <figure>' +
                                        ' <a href="' + data.ProductImage.Images[i] + '" class="zoom-icon" data-caption="' + data.ProductImage.Alt + '"> </a>' +
                                        ' <img src="' + data.ProductImage.Images[i] + '" alt="' + data.ProductImage.Alt + '">' +
                                        ' </figure></div>');
                                }

                                count++;
                            }
                            
                        }
                        else if (data.ProductImage.ImageUrl != null) {
                            $(imageSlider).append('<div class="item">' +
                                ' <figure>' +
                                ' <a href="' + data.ProductImage.ImageUrl + '" class="zoom-icon" data-caption="' + data.ProductImage.Alt + '"> </a>' +
                                ' <img src="' + data.ProductImage.ImageUrl + '" alt="' + data.ProductImage.Alt + '">' +
                                ' </figure></div>');
                        }
                    }
                    
                    if (data.StockModel != null) {
                        if (data.StockModel.StockInformationList.length > 0) {
                            var elem = data.StockModel.StockInformationList[0];

                            if (elem.Status == "In-Stock") {
                                $('.cls-pdpdispBuy').show();
                                $('.cls-pdpdispOutofstock').hide();
                                if (hdnProdQuan == '1') {
                                    $('.main-topsection .actions-block').find('.add-to-cart-qty').show();
                                }
                               
                            }
                            else {
                                $('.cls-pdpdispOutofstock').show();
                                $('.cls-pdpdispBuy').hide();
                                $('.main-topsection .actions-block').find('.add-to-cart-qty').hide();
                                
                               
                            }
                        }
                    }

                    if (data.ProductVariantModel != null) {
                        var priceHtml = "";
                       
                        if (data.ProductVariantModel.ListPrice != null) {
                            if (data.ProductVariantModel.AdjustedPrice < data.ProductVariantModel.ListPrice) {

                                priceHtml = "<strong><del>$" + data.ProductVariantModel.ListPrice.toFixed(2) + "</del><span class='cur'>" + PriceLabel + "</span></strong>";
                                priceHtml += "<strong class=''>$" + data.ProductVariantModel.AdjustedPrice.toFixed(2) + "</strong>";
                                $('.name-price .price').text("$" + data.ProductVariantModel.AdjustedPrice.toFixed(2));
                                

                            } else {
                                priceHtml = "<strong class=''>$" + data.ProductVariantModel.ListPrice.toFixed(2) + "<span class='cur'>" + PriceLabel + "</span></strong>";
                                
                            }
                            jQuery(priceRow).html(priceHtml);
                            jQuery(headerpriceRow).html(priceHtml);
                        }
                    }


                    setTimeout(function () {
                   

                        $('.product-image-slider').flickity({
                            prevNextButtons: true,
                            pageDots: true,
                            autoPlay: false,
                            wrapAround: false,
                            contain: true,
                            cellAlign: 'left',
                            // fade: true,
                            adaptiveHeight: false,
                            on: {
                                ready: function () {
                                    $('.product-image-slider .video-box').each(function () {
                                        var thisIndex = $(this).parents('.item').index();

                                        $('.product-image-slider  .flickity-page-dots .dot').eq(thisIndex).append("<span class='video-icon'></span>");
                                    });

                                },
                                change: function (index) {
                                    console.log('Slide changed to' + index);
                                }
                            },
                            arrowShape: {
                                x0: 0,
                                x1: 60,
                                y1: 50,
                                x2: 60,
                                y2: 40,
                                x3: 60
                            }

                        });
                        $('.zoom-icon').fancybox({
                            baseClass: "gallery-pop"


                        });


                    }, 10);



                }
            },
            error: function (jqXHR) {
                if (jqXHR.status == 500) {
                    console.log('Internal error: ' + jqXHR.responseText);
                } else {
                    console.log('Unexpected error.');
                }
            }
        });
    }



</script>
