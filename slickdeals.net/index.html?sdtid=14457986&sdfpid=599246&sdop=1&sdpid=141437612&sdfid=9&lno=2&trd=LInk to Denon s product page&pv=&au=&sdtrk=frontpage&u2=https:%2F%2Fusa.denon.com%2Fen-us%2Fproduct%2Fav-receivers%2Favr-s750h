<script>
 
    var searchTearm = "";
    
    $("#gblsearchsubmit").click(function () {

        searchTearm = $.trim($("#txtGblsearch").val()).toLowerCase();
        if (searchTearm.length > 2) {
            window.location = '/search-results?s=' + searchTearm;
        }
    })
 
    $("#txtGblsearch").keydown(function (event) {
        if (event.keyCode == 13) {
            searchTearm = $("#txtGblsearch").val().toLowerCase();
            if (searchTearm.length > 2) {
			 if($('#hidnLocale').val()!="")
				{
					var url= '/'+$('#hidnLocale').val()+'/search-results?s=' + searchTearm;
					url=url.toLowerCase();
				    window.location = url;
				}
				else
				{
                  window.location = '/search-results?s=' + searchTearm;
				}
                
            }
        }

    })

