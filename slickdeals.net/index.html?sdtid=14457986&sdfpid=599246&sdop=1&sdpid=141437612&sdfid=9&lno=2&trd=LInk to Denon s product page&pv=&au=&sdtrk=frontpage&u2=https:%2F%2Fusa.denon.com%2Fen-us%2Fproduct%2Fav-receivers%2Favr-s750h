<script type="text/javascript">

    $(document).ready(function () {
        $('.add-to-cart-qty').hide();
        var hdnProdQuan = $('#hdnIsProductQuantity').val();
       
        var catalog = $("#pdp-data #catalogId").val();
        var varitem = $("#pdp-data #firstVarId").val();
        var objonload = {
            catalogProdId: catalog,
            variantId: varitem
        }
        console.log(varitem);
        $('.header-buy-box').find('#addtocart_variantid').val(varitem);
        $('.main-topsection .actions-block').find('#addtocart_variantid').val(varitem);
        jQuery.ajax({
            url: '/api/cxa/SoundUnitedSharedProductDetail/GetVariantStock',
            type: "POST",
            data: objonload,
            dataType: "json",
            success: function (data) {

                console.log(data);

                if (data != null) {

                    if (data.StockModel != null) {
                        if (data.StockModel.StockInformationList.length > 0) {
                            var elem = data.StockModel.StockInformationList[0];

                            if (elem.Status == "In-Stock") {
                                $('.cls-pdpdispBuy').show();
                                if (hdnProdQuan == '1') {
                                    $('.main-topsection .actions-block').find('.add-to-cart-qty').show();
                                }
                            }
                            else {
                                $('.cls-pdpdispOutofstock').show();
                                $('.main-topsection .actions-block').find('.add-to-cart-qty').hide();
                            }
                        }
                    }
                }
            },

            error: function (jqXHR) {
                if (jqXHR.status == 500) {
                    console.log('Internal error: ' + jqXHR.responseText);
                } else {
                    console.log('Unexpected error.');
                }
            }
        })

    })


    //(function () {

    //    var addToCartButton = jQuery(".main-topsection .add-to-cart-btn");
