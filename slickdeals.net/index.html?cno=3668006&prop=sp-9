        var matchResult;

        if (location.hostname === 'web') {
            document.domain = location.hostname;
            return;
        }

        matchResult = location.hostname.match(/([^.]+)\.\w{2,5}(?:\.\w{2})?$/);
        document.domain = matchResult !== null ? matchResult[0] : null;
    })(document, location)
</script>
                </head>

