<script>
    (function (window) {
        /**
         * ScriptBlock: https://chrome.google.com/webstore/detail/scriptblock/hcdjknjpbnhdoabbngpmfekaecnpajba?hl=en
         * AdBlock: https://chrome.google.com/webstore/detail/adblock/gighmmpiobklfepjocnamgkkbiglidom
         * ublock: https://chrome.google.com/webstore/detail/ublock-origin/cjpalhdlnbpafiamejdnhcphjbkeiagm?hl=en
         * AdBlock Plus: https://chrome.google.com/webstore/detail/adblock-plus/cfhdojbkjhnklbpkdaibdccddilifddb
         * NoScript: https://addons.mozilla.org/en-US/firefox/addon/noscript/
         * AdBlock Plus: https://addons.mozilla.org/en-US/firefox/addon/adblock-plus/
         */
        var currWinLoad = window.onload, e = window.encodeURIComponent,
            loaded = 0,
            jsLoaded = (function () {
                loaded = 1;
            }),
            elements = document.getElementById('adblocker') || window.adblocker; // named access

        function isNodeList(nodes) {
            var stringRepr = Object.prototype.toString.call(nodes);

            return typeof nodes === 'object' &&
                    /^\[object (HTMLCollection|NodeList|Object)\]$/.test(stringRepr) &&
                    (typeof nodes.length === 'number') &&
                    (nodes.length === 0 || (typeof nodes[0] === "object" && nodes[0].nodeType > 0));
        }

        function map(elements, callback) {
            var prop;

            if (!isNodeList(elements)) {
                elements = [elements];
            }

            elements = elements.filter(function (v) { return !!v; });

            for (prop in elements) {
                callback.call(this, elements[prop]);
            }
        }

        window.onload = function () { // onload is indicating that everything was loaded
            var context = { 'error_type': 'blocked_javascript' };
            var isLessThanIE9 = $('html').is('.lt-ie9');

            if (isLessThanIE9) {
                return;
            }

            if (loaded && window.adblocker !== false) {
                window.log('WARNING', 'onload was called, but adblocker is not defined, probably client has blocked JavaScript', context);
                map(elements, function (el) {
                    if (el.style) {
                        el.style.display = 'block';
                    }
                });
            } else if (!loaded) {
                window.log('WARNING', 'onload was called, but jsLoaded callback was not called, probably client has blocked JavaScript', context);
                map(elements, function (el) {
                    if (el.style) {
                        el.style.display = 'block';
                    }
                });
            } else if (loaded && window.adblocker === false) {
                map(elements, function (el) {
                    if (el.parentNode) {
                        el.parentNode.removeChild(el);
                    }
                });
            }
            currWinLoad && currWinLoad.apply(this, arguments);
        };

        window.encodeURIComponent = function (msg) {
            var ret = e.call(this, msg);
            // if msg was provided, but encodeURIComponent return empty string, this could mean that function is mocked by script blocker
            return msg && !ret ? msg : ret;
        };
        window.jsLoaded = jsLoaded;
    })(window);
</script>                
