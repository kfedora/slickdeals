    (function (jQuery) {
        var oldParseJSON = jQuery.parseJSON;

        jQuery.parseJSON = function (json) {
            if (!json && json !== null) {
                return null;
            }
            return oldParseJSON.apply( this, arguments );
        };
    })(jQuery);
