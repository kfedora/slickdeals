    (function() {      
      function setup() {
        Array.prototype.slice.call(document.querySelectorAll("a[href^='#']")).filter((function (t) {
            return "#" !== t.getAttribute("href") && "#0" !== t.getAttribute("href")
        })).map((function (t) {
             t.removeEventListener("click", anchorHandle), t.addEventListener("click", anchorHandle)
        }))
      }

      function anchorHandle(t) {
        t.preventDefault();
        var e = document.querySelector(t.currentTarget.getAttribute("href")),
            r = document.querySelector("#Sticky_Banner"),
            n = r ? r.offsetHeight : 0;

            window.scroll({
                top: getOffset(e) - n - 20,
                left: 0,
                behavior: "smooth"
            }), manageFocus(e.parentElement)
      }

      function manageFocus(t) {
          t.focus({
              preventScroll: !0
          }), document.activeElement !== t && (t.setAttribute("tabindex", "0"), t.focus({
              preventScroll: !0
          }), t.addEventListener("blur", (function t(e) {
              e.target.removeAttribute("tabindex"), e.target.removeEventListener("blur", t)
          })))
      }

      function getOffset(t) {
        return t.getBoundingClientRect().top + window.pageYOffset
      }

      setup();
    })();</script></body></html>