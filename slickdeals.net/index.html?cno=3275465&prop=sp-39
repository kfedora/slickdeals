<script type="text/javascript">
(function(WlpInjectable) {
    WlpInjectable.getClientContext = function() {
        var reqMetadata = {};
                        reqMetadata['ref'] = 'node_page';
                        reqMetadata['clientId'] = 'primeAcquisition';
                        reqMetadata['ingressId'] = 'DEXPDlp15247183011';
                        reqMetadata['primeCampaignId'] = 'primeAcquisition_prime_exc_assc_Desktop';
                        reqMetadata['redirectURL'] = 'https://www.amazon.com/b?node=16205654011';
                        reqMetadata['uspBlob'] = '{ &quot;element&quot;: { &quot;type&quot;: &quot;custom&quot;, &quot;style&quot;: { &quot;text-align&quot;: &quot;center&quot;, &quot;padding-bottom&quot;: &quot;60px&quot;, &quot;margin&quot;: &quot;0 auto&quot;, &quot;max-width&quot;: &quot;1024px&quot; }, &quot;customContent&quot;: { &quot;layout&quot;: &quot;75:25&quot;, &quot;left&quot;: { &quot;type&quot;: &quot;text&quot;, &quot;content&quot;: &quot;Join Prime today to get access to great products and deals exclusively for Prime Members&quot;, &quot;style&quot;: { &quot;font-weight&quot;: &quot;bold&quot; } }, &quot;right&quot;: &quot;button&quot; } }, &quot;overrides&quot;: { &quot;headingText&quot;: &quot;Just for Prime&quot;, &quot;subHeadingText&quot;: &quot;Enjoy shopping benefits just for Prime.&quot; } }';
        var uspBlob = (function() {
            var blob = reqMetadata['uspBlob'];
            if (!blob) return;
            try {
                var txt = document.createElement('div');
                txt.innerHTML = blob;
                return JSON.parse(txt.innerHTML);
            } catch (e) {};
        })() || {};

        var context = {
            clientId: reqMetadata['clientId'] || "default",
            ingressId: reqMetadata['ingressId'] || "PrimeDefault",
            primeCampaignId: reqMetadata['primeCampaignId'] || "PrimeDefault",
            benefitOptimizationId: reqMetadata['benefitOptimizationId'] || "default",
            planOptimizationId: reqMetadata['planOptimizationId'] || "default",
            ref: reqMetadata['ref'] || "",
            _ref: reqMetadata['_ref'] || "default_cs",
            redirectURL: reqMetadata['redirectURL'] || "/gp/homepage.html",
            cancelRedirectURL: reqMetadata['cancelRedirectURL'] 
        };

        if (uspBlob.element) context.element = uspBlob.element;
        if (uspBlob.overrides) context.overrides = uspBlob.overrides;
        
        return context;
    };
})(window.WlpInjectable = window.WlpInjectable || {});

(function(OfferAwareButton) {
    OfferAwareButton.create = function(context) {
        var buttonText = document.createElement('div');
        buttonText.innerHTML = 'Join Prime' ;
            
        var spanInnerMost = document.createElement('SPAN');
        spanInnerMost.setAttribute('aria-hidden', 'true');
        spanInnerMost.setAttribute('class', 'a-button-text a-text-center');
        spanInnerMost.appendChild(buttonText);
        
        var spanInner = document.createElement('SPAN');
        spanInner.setAttribute('class', 'a-button-inner');
        spanInner.appendChild(spanInnerMost);
        
        var spanWrapper = document.createElement('SPAN');
        spanWrapper.setAttribute('id', 'prime-day-hero-button');
        spanWrapper.setAttribute('class', "prime-signup-ingress a-button a-spacing-mini a-spacing-top-mini a-button-primary");
        spanWrapper.setAttribute('data-client-id', context.clientId);
        spanWrapper.setAttribute('data-ingress-id', context.ingressId);
        spanWrapper.setAttribute('data-campaign-id', context.primeCampaignId);
        spanWrapper.setAttribute('data-disable-prefetch', 'true');
        spanWrapper.setAttribute('data-redirect-url', context.cancelRedirectURL);
        if(typeof(context.cancelRedirectURL) != "undefined"){
          spanWrapper.setAttribute('data-cancel-redirect-url', context.cancelRedirectURL);
        }
        spanWrapper.setAttribute('data-benefit-optimization-id', context.benefitOptimizationId);
        spanWrapper.setAttribute('data-plan-optimization-id', context.planOptimizationId);
        spanWrapper.setAttribute('data-success-redirect-url', context.redirectURL);
        spanWrapper.appendChild(spanInner);
    
        document.getElementById('offer-aware-btn-cstm').appendChild(spanWrapper);
        return;
    };
    
    OfferAwareButton.createWLPSignupForm = function(context) {
        context.wait = "1";
        var wlpForm = document.createElement("form");
        wlpForm.setAttribute("action","/gp/prime/pipeline/membersignup");
        wlpForm.setAttribute("method","POST");
        for(var key in context) {
            var inputElem = document.createElement("input");
            inputElem.setAttribute("type","hidden");
            inputElem.setAttribute("name",key);
            inputElem.setAttribute("value",context[key]);
            if(key == "redirectURL" || key == "cancelRedirectURL") {
                inputElem.setAttribute("value",btoa(context[key]));
            }
            wlpForm.appendChild(inputElem);
        }
        return wlpForm;
    };
})(window.OfferAwareButton = window.OfferAwareButton || {});

P.when('wlp-jQuery-lite').execute(function($) {
    var context = WlpInjectable.getClientContext();
    var ele = $('#offer-aware-btn');
    var isRight = true;
    var layout = '10:90'.trim().split(':');
    var custContent = context.element ?context.element.customContent:null;
    if(custContent)var isRight = (custContent.right || '') == 'button';
    if(custContent)var layout = (custContent.layout || '50:50').trim().split(':');
    
    var isMobile = false;
    
    var btn = $('<div/>');
    btn.attr('id', 'offer-aware-btn-cstm');
    
    if(isMobile) {
        btn.css('width', '100%');
    } else {
        btn.css('width', (isRight ? layout[1] : layout[0]) + '%');
        btn.css('float', (isRight ? 'right' : 'left'));
        btn.css((isRight ? 'padding-left' : 'padding-right'), '10px');
    }
    
    var contentStyle ='';
    var contentDiv = $('<div/>');
    if(custContent) {
    var contentObj = custContent[isRight ? 'left' : 'right'] || {};
    var type = contentObj.type || 'text';
    var content = contentObj.content || '';
    content = decodeURIComponent(escape(content));
    var contentStyle = contentObj.style;
    
    var contentDiv = $('<div/>');
    if(isMobile) {
        contentDiv.css('width', '100%');
    } else {
        contentDiv.css(isRight ? 'margin-top' : 'margin-top', '10px');
        contentDiv.css('width', (isRight ? layout[0] : layout[1]) + '%');
        contentDiv.css('float', (isRight ? 'left' : 'right'));
        contentDiv.css((isRight ? 'padding-right' : 'padding-left'), '10px');
    }
    
    if(contentStyle) contentDiv.css(contentStyle);
    
    switch(type) {
        case 'img':
            $('<img/>', {
                src: content
            }).appendTo(contentDiv);
            break;

        case 'text':
            contentDiv.html(content);
            contentDiv.css('font-size', 'medium');
            break;
    }
    
    if(!isMobile) {
        if(contentDiv.height() > btn.height()) contentDiv.css((isRight ? 'border-right' : 'border-left'), '2px solid #DDDDDD');
        else btn.css((isRight ? 'border-left' : 'border-right'), '2px solid #DDDDDD');
    }
}
    if(isRight) {
        ele.empty().append(contentDiv);
        ele.append(btn);
    } else {
        ele.empty().append(btn);
        ele.append(contentDiv);
    }
    
        OfferAwareButton.create(context);
});
</script>











 


