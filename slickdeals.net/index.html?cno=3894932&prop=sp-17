<script>
  var ajax = new XMLHttpRequest();
  ajax.open('GET', '/agwa-ugp-assets/symbols-92a6875e2b92d0886a24a3fa4a9f8ab1.svg', true);
  ajax.send();
  ajax.onload = function(e) {
    if (this.status === 404) {
       return;
    }
    var div = document.createElement('div');
    div.innerHTML = ajax.responseText;
    document.body.insertBefore(div, document.body.childNodes[0]);
  };
</script>


    <script type="text/javascript">
      (function(){
        try {
          (new Function('let a={b:"c"};let b={...a}'))();
          removeManifest('/legacy-agwa-ugp-assets/');
          loadScript('/agwa-ugp-assets/vendor-2a3236f58c7f789d01d983a9072a7b5a.js');
          loadScript('/agwa-ugp-assets/agwa-af9dd754223a5cf43a28a9ac840a20ba.js');
        } catch(err) {
          removeManifest('/agwa-ugp-assets/');
          loadScript('/legacy-agwa-ugp-assets/vendor-2a3236f58c7f789d01d983a9072a7b5a.js');
          loadScript('/legacy-agwa-ugp-assets/agwa-af9dd754223a5cf43a28a9ac840a20ba.js');
        }
        function loadScript(src){
          const s=document.createElement('script');
          s.src=src;
          s.async=false;
          document.body.appendChild(s);
        }
        function removeManifest(type){
          var manifests =Array.prototype.slice.call(document.querySelectorAll('meta[name="agwa/config/asset-manifest"]'));
          manifests.forEach(function(m) {
            if (m.content.indexOf(type) !== -1) {
              document.head.removeChild(m);
            }
          });
        }
      })();
    </script>

