			function getUrlVars() {
				var vars = {};
				var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
					vars[key] = value;
				});
				return vars;
			}
			function getUrlParam(parameter, defaultvalue){
				var urlparameter = defaultvalue;
				if(window.location.href.indexOf(parameter) > -1){
					urlparameter = getUrlVars()[parameter];
				}
				return urlparameter;
			}
			function sanitizeUuid(uuid) {
				var regex = new RegExp("^\\b[0-9a-f]{8}\\b-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-\\b[0-9a-f]{12}\\b$", "g");
				var match = regex.exec(uuid);
				return match != null ? uuid : "";
			}
            function sendEmail() {
                var subject= "I Was Blocked";
                var body = "\r\n\r\n";
					body += "DO NOT MODIFY BEYOND THIS POINT!";
					body += "\r\n";
					body += "----------------------------------------------------------------------";
					body += "\r\n";
					body += "Block ID: ";
					body += sanitizeUuid(getUrlParam('uuid', ''));
					body += "\r\n";
					body += "----------------------------------------------------------------------";
					body += "\r\n\r\n";
                var uri = "mailto:feedback@moosejaw.com?subject=";
					uri += encodeURIComponent(subject);
					uri += "&body=";
					uri += encodeURIComponent(body);
                window.open(uri);
            }
			function addBlockID() {
				var blockId = sanitizeUuid(getUrlParam('uuid', ''));
				document.getElementById("blockId1").innerHTML = "Block ID: " + blockId;
				document.getElementById("blockId2").innerHTML = "Block ID: " + blockId;
			}
        </script>		
