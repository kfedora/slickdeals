					if(false){
					xhttp.setRequestHeader('X-EBAY-VI-PREFETCH2', 'true');
					}else{
					xhttp.setRequestHeader('X-EBAY-VI-PREFETCH', 'true');
					}
 					xhttp.send('');
					}
