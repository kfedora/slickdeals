<script>
	(function($){
		(typeof(window.sf2016)=='undefined')&&(window.sf2016=$.getScript('https://img.hsni.com/images/prod/scripts/sf2016/sf2016css.js'));
		if(!!$('body').is('.mobile')){
			$(document).ready(function(){
				window._extcc = $('<div class="_sf2016-zc _sf2016-mc">'
						+'<div class="_controls"></div>'
						+'<div class="_items"></div>'
					+'</div>')
					.insertAfter('._sf2016-mc:first')
					.find('._items')
					.on('beforeChange._extcc', function(event, slick, 
							currentSlide, nextSlide){
						//workaround for slick clone data-lazy issue
						//if we are wrapping around to the first slide again
						if(nextSlide == 0){
							//get the img src of that slide
							$.each($(slick.$slides[nextSlide]).add($(slick.$slides[nextSlide]).nextUntil('[data-slick-index="'+slick.options.slidesToScroll+'"]')).find('[src]'),function(i,v){
								//get the src of the img
								var src = $(v).attr('src');
								//find the clone that has that img and set src, remove extra attrs
								$(slick.$slider)
									.find('.slick-cloned [data-lazy="'+src+'"]')
									.attr('src',src)
									.removeAttr('data-lazy data-srcset data-sizes');
							});
							//stop firing the beforechange event
							$(slick.$slider).off('beforeChange._extcc');
						}
					})
					.slick({
						slidesToShow:1,
						slidesToScroll:1,
						appendArrows:'._sf2016-zc ._controls',
						infinite: true
					})
					.slick('slickAdd',$('._sf2016-mc:first').children());
			});
		}
	})(jQuery);
</script></div>
