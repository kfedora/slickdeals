<script language="javascript">
    var currentURL = escape(window.location.href);

    function js_sort(forms, type) {
        var Domain="https://www.monoprice.com/category/cables/networking-patch-cables/cat6a-ethernet-cables";
        if (type == 1) {
            var URL = $('option:selected', forms).attr('data-url');

            location.href = Domain.replace(/&amp;/g, '&')+"?"+URL;
        }
        else if (type == 2) {
            var URL = $('option:selected', forms).attr('data-url');
            location.href = Domain.replace(/&amp;/g, '&')+"?"+URL;
        }

    }

    function js_selected() {
        if (54 <= 0) {
            document.getElementById("noresult").style.display = 'block';
            document.getElementById("existresult").style.display = 'none';
        } else {
            document.getElementById("noresult").style.display = 'none';
            document.getElementById("existresult").style.display = 'block';
        }
        $("#idPriceFrom").ForceNumericOnly();
        $("#idPriceTo").ForceNumericOnly();
    }
    js_selected();

    $(document).ready(function() {
        $(".clear_filter").click(function() {
            js_clearAllFacets();
        });
    });
    $(".hawknotify-me").click(function(){
        $("#hawk_qty").focus();
    });


    $(function() {
        var url = window.location.search;
        if (!url.match("sort")) {
            $('#id_itemsort').val("");
        }
    });


    function GetQtyValue(value,id) {
        var p_id = id;
        $('#addtoCartQty-' + p_id).attr("unbxdparam_qty",value);
    }

</script>

<script type="text/javascript">
    // items per page
    function getQueryVariable(variable) {
        var query = window.location.search.substring(1);
        var vars = query.split("&");
        for (var i = 0; i < vars.length; i++) {
            var pair = vars[i].split("=");
            if (pair[0] == variable) { return pair[1]; }
        }
        return (false);
    }

    var rowString = getQueryVariable("rows");
    var rowVal = parseInt(rowString);
    var sortString = getQueryVariable("sort");
    var menuDisStr = getQueryVariable("menuDisStr");

    $('.hawk-viewNumber option[value="' + rowVal + '"]').prop('selected', true);
    $('.sortList option[value="' + sortString + '"]').prop('selected', true);

    if (menuDisStr.length > 1) {
        //  $('.hawk-maintitle').html(menuDisStr);
    }

    $(function () {
        $(window).on('popstate', function () {
            $('#id_itemsort').val("");
        });
    });
</script>




