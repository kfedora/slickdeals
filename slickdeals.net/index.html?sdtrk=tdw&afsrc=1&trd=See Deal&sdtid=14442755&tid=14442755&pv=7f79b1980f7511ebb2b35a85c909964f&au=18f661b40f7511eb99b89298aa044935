    <script>
  var paramsFix = (function () {
    // Vars
    var searchParams = window.location.search;
    var creativeParam = 'CREATIVEID';
    var adGroupParam = 'ADGROUPID';
    var queryParams = {};

    // values you want to find and replace
    var creativeID = 'unknown';
    var adGroupID = 'unknown';

    var replaceHistory = function () {
      var newURL = window.location.href.replace(creativeParam, creativeID);
      newURL = newURL.replace(adGroupParam, adGroupID);
      window.history.replaceState('', '', newURL);
    };

    var getParams = function () {
      // setting var defaults
      var searchObjs = searchParams.replace('?', '').split('&');

      // getting vars object
      searchObjs.forEach(function (value) {
        var newArray = value.split('=');
        if (newArray.length > 1) {
          queryParams[newArray[0]] = newArray[1];
        } else {
          queryParams[newArray[0]] = '';
        }
      });
    };

    var setNewValues = function () {
      // set the values you want here. we're either going to get utm_adtype AND utm_adgid OR
      // mttnsub1 AND mttnsub3. So we need to check to see if we are getting the utm's first
      // if not fall back to mttnsub's. If mttnsub's don't exist fall all the way back to unknown.
      if (queryParams.utm_adtype && queryParams.utm_adgid) {
        creativeID = queryParams.utm_adtype;
        adGroupID = queryParams.utm_adgid;
      } else if (queryParams.mttnsub1 && queryParams.mttnsub3) {
        creativeID = queryParams.mttnsub1;
        adGroupID = queryParams.mttnsub3;
      }
    };

    var init = function () {
      // get us some query params in a nice little object
      getParams();
      // validate we have a cid string with expected str matches else stop
      // processing

      if (
        queryParams.hasOwnProperty('cid') &&
        !queryParams.cid.match(creativeParam) &&
        !queryParams.cid.match(adGroupParam)
      ) {
        return false;
      }

      // set the values we're looking for
      setNewValues();
      // replace the history with the updated URL
      replaceHistory();
    };

    // kick things off
    init();
  })();
</script>
  
    <script>
  var ApplePayAPI = (function() {
    var eligibleVersion = 7;
    var isEligible = false;
    var isWrongVersion = false;
    var isIOS = false;
    var applePaySessionExists = false;
    var supportsVersion = false;
    var canMakePayments = false;

    if (window.location.protocol.match(/https/)) {
      isEligible = Boolean(
        window.ApplePaySession &&
        window.ApplePaySession.supportsVersion(eligibleVersion) &&
        window.ApplePaySession.canMakePayments());

      isWrongVersion = Boolean(
        window.ApplePaySession &&
        !window.ApplePaySession.supportsVersion(eligibleVersion));

      isIOS = Boolean(window.ApplePaySession);

      if (typeof window.ApplePaySession !== 'undefined') {
        applePaySessionExists = true;

        supportsVersion = window.ApplePaySession.supportsVersion(eligibleVersion);

        canMakePayments = window.ApplePaySession.canMakePayments();
      }
    }

    return {
      isEligible: isEligible ? 'true' : 'false',
      isWrongVersion: isWrongVersion ? 'true' : 'false',
      isIOS: isIOS ? 'true' : 'false',
      applePaySessionExists: applePaySessionExists ? 'true' : 'false',
      supportsVersion: supportsVersion ? 'true' : 'false',
      canMakePayments: canMakePayments ? 'true' : 'false',
    }
  })();

</script>
  
    
