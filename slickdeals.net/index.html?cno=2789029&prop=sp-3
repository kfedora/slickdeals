                ckSet = true;
                setCookieFS('FSreal', FS_COOKIE_SELECT_USERS, fsDays);
            }
        } else if (ckVal === FS_COOKIE_SURVEY || ckVal === FS_COOKIE_TESTING) {
            ckSet = true;
        } else if (ckVal === FS_COOKIE_ALL_USERS) {
            if (chosenUser) {
                setCookieFS('FSreal', FS_COOKIE_SELECT_USERS, fsDays);
                ckSet = true;
            } else if (lsVal === null) {
                deleteCookieFS('FSreal');
            } else {
                ckSet = true;
