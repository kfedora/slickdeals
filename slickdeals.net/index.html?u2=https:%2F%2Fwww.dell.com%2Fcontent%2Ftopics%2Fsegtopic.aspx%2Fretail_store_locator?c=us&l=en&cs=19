    <script>
        var Dell = Dell || {};
        Dell.Footer = Dell.Footer || {};
        Dell.Footer.BirdSeed = Dell.Footer.BirdSeed || {};
        Dell.Footer.BirdSeed.setBirdSeed = (function (birdseedHtml) {
            var divFooter = document.getElementsByClassName('ft-birdseed-wrap');
            if (divFooter.length > 0 && birdseedHtml.length > 0) {
                divFooter[0].setAttribute('data-exists', 'True');
                var divBirdSeed = document.getElementsByClassName('birdseed');
                if (divBirdSeed.length > 0) {
                    divBirdSeed[0].innerHTML = birdseedHtml;
                }
            }
            else {

                divFooter[0].setAttribute('data-exists', 'False');
            }

        });
    </script>


