<script>
    function resetForm () {
        $('.formField input').removeClass('invalid');
    }

    function isFormValid () {
        var email = $('#email').val(),
            hasErrors = false;

        if (!window.isEmailValid(email)) {
            $('#email').addClass('invalid');
            hasErrors = true;
        }

        if (hasErrors) {
            window.putOmnitureJSPData({
                event: {
                    eventInfo: {
                        eventName: "form error"
                    },
                    eventDetails: {
                        formName: "prospect email popup",
                        formerror: "Invalid email address"
                    }
                }
            });
        }

        return !hasErrors;
    }

    function hideEmailPopup() {
        var $modal = $('.modal');
        $modal.modal('hide');
        sessionStorage["ProspectEmailPopupShown"] = 'yes';
    }

    function showErrorMsg() {

        $("#err-message").append('This email address is already in our system.');
        $('#submitButton').removeAttr("disabled");
    }


    function submitForm () {
        resetForm();
        if (isFormValid()) {
            $('#submitButton').attr('disabled', 'disabled');
            $.ajax({
                url: '/saveprospectemail/saveemail.sfly',
                type: 'POST',
                data: $('#emailPromoForm').serialize(),
                dataType: 'json',
                success: function(data) {
                    if (!data.success) {
                        showErrorMsg();
                    }
                    else {
                        hideEmailPopup();
                    }
                }
            });

            window.putOmnitureJSPData({
                event: {
                    eventInfo: {
                        eventName: "form popup:submit"
                    },
                    eventDetails: {
                        formName: "prospect email popup"
                    }
                }
            });
        }
    }

    function isWeddingTN (source) {
        if ("HOMEPAGE" === source)
            return false;

        var content = document.querySelector("meta[name='keywords']").getAttribute("content");
        if ( (content != null) && (content.indexOf("wedding") >= 0 )) {
            return true;
        }
        return false;
    }


    $(document).ready(function () {

        var $modal = $('.modal'),
            source = document.getElementById('occasionType').value;

        document.getElementById('fullPathname').value = window.location.href;

        $('.modal-close').click(function () {
            hideEmailPopup();
        });

        if ( (sessionStorage["ProspectEmailPopupShown"] != 'yes') && !isWeddingTN(source) ) {

            $modal.modal('show');

            window.putOmnitureJSPData({
                event: {
                    eventInfo: {
                        eventName: "form popup:load"
                    },
                    eventDetails: {
                        popUpName: "prospect email popup"
                    }
                }
            });
        }

        $('#submitButton').click(function (e) {
            e.preventDefault();
        });

    });
</script>

        
    

