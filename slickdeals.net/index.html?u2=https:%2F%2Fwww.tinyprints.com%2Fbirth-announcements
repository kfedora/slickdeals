            localStorage.removeItem('FSSurvey');
        }
        if (localStorage.getItem('FSSFLYEmail') !== null) {
            localStorage.removeItem('FSSFLYEmail');
        }
        deleteCookieFS('FSLTLimited');
    </script>

    
