<p>
<script>
window.addEventListener("load", function(event) {
    $.get( "/coupons/api/session", function(data) {
    $('<input />')
      .attr('type', 'hidden')
      .attr('name', "_csrf")
      .attr('value', data.csrfToken)
      .appendTo('#coupon-subscription-form');
});

$("#coupon-subscription-form").submit( function(event) {
  event.preventDefault();
  const formData = {
    _csrf: $("#coupon-subscription-form input[name='_csrf']").val(),
    email: $("#coupon-subscription-form input[name='email']").val(),
    subscriber_type: 'coupons_channel'
  };
  $.post( "/coupons/subscribe", formData, function(data) {
