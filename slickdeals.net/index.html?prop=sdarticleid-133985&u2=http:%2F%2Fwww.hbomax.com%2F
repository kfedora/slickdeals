document.addEventListener('DOMContentLoaded', function () {
  // FAQs
  var collapseButtons = document.querySelectorAll('.max-section-faq [data-toggle="collapse"]');
  var collapseDivs = document.querySelectorAll('.max-section-faq .collapse');

  collapseButtons.forEach(function (collapseButton) {
    collapseButton.addEventListener('click', function (e) {
      if (
        document
          .getElementById(collapseButton.getAttribute('data-target'))
          .classList.contains('show')
      ) {
        document
          .getElementById(collapseButton.getAttribute('data-target'))
          .classList.remove('show');
      } else {
        collapseDivs.forEach(function (collapseDiv) {
          collapseDiv.classList.remove('show');
        });
        document
          .getElementById(collapseButton.getAttribute('data-target'))
          .classList.add('show');
      }

      // FAQ Toggle Tracking 
      window.dataLayer.push({
        event: 'ctaClick',
        metadata: {
          linkText: e.target.innerText,
          elementType: "text",
          moduleName: "FAQ",
          bandName: "FAQ",
          eventAction: "navigational link click",
          eventCategory: "User Clicked"
        }
      });
    });
  });

  // FAQ Link Tracking
  var collapseDivLinks = document.querySelectorAll('.max-section-faq .collapse a');

  collapseDivLinks.forEach(function (collapseDivLink) {
    collapseDivLink.addEventListener('click', function (e) {
      window.dataLayer.push({
        event: 'ctaClick',
        metadata: {
          linkText: e.target.innerText,
          linkURL: e.target.href,
          elementType: "text",
          moduleName: "FAQ",
          bandName: "FAQ",
          eventAction: "navigational link click",
          eventCategory: "User Clicked"
        }
      });
    });
  });

  // Max Originals Title Buttons
  var originalsButtons = document.querySelectorAll('.max-section-originals .btn-link');

  originalsButtons.forEach(function (originalsButton) {
    originalsButton.addEventListener('click', function sendToDataLayer(e) {
      window.dataLayer.push({
        event: 'contentClick',
        metadata: {
          altText: e.target.alt || e.target.getElementsByTagName('img')[0].alt,
          linkURL: e.target.href || this.href,
          moduleName: "Image",
          bandName: "Max Originals",
          eventAction: "Content tile Clicked",
          eventCategory: "User Clicked"
        }
      });
    })
  });
});
