<script id="cookie-temp" type="text/template">
	<div id="cookies_header">
		<div class="container-1080">
			<div class="pure-g mg_fix_10">
				<div class="pure-u-1-1 pure-u-lg-3-4 align-middle">
					<div class="cookies_block">
						We use cookies to help us improve our webpage. Please read our <a href="company/legal/cookie_policy">Cookie Policy</a>.
					</div>
				</div>
				<div class="pure-u-1-1 pure-u-lg-1-4 align-middle">
					<div class="cookies_block">
						<button id="cookies_btn_ok">OK</button>
					</div>
				</div>
			</div>
		</div>
	</div>
</script>
<script>
var WEBSITE_MODE = 'production';
var IS_AWS = Boolean('1');


(function () {
	// cookies banner
	function insertGTM() {
		console.log('INSERT GTM');
		var check_exists = $('iframe[src="https://www.googletagmanager.com/ns.html?id=GTM-KMVRHB6"]').length > 0 ? true : false;
		if (check_exists === false && WEBSITE_MODE !== 'local' && IS_AWS === true) {
			try {
				$('#gtm_wrap noscript').prepend('<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KMVRHB6" height="0" width="0" style="display:none;visibility:hidden"></iframe>');
				$('head').append("<!-- Google Tag Manager -->" +
					"<script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':" +
					"new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0]," +
					"j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=" +
					"'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);" +
					"})(window,document,'script','dataLayer','GTM-KMVRHB6');<\/script>" +
					"<!-- End Google Tag Manager -->");
			} catch (err) {
				console.warn('inser gtm warning', err);
			}
		}
	}
	function setCookie(cName, cvalue, exdays) {
		var d = new Date();
		d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);
		var expiresTime = d.toUTCString();
		var expires = "expires=" + expiresTime;
		// 紀錄 "確定" 的 key
		document.cookie = cName + '=' + cvalue + ';' + expires + ';path=/';
	}
	function getCookie(cName) {
		var cArray = document.cookie.split(";");
		for (var i = 0, j = cArray.length; i < j; i++) {
			var c = cArray[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(cName) == 0) {
				return c.substring(cName.length + 1, c.length);
			}
		}
		return "";
	}
	function cookieMobileVsPc() {
		if (window.matchMedia('(min-width: 1024px)').matches) {
			// PC 版
			$('#root').attr('style', '');
		} else {
			if (getCookie('syno_confirm_v2_answer') === "true") {
				$('#root').attr('style', '');
			} else {
				$('#root').css('padding-bottom', $('#cookies_header').outerHeight());
			}
		}
	}
	var ifOKCookie = getCookie('syno_confirm_v2_answer');
	if (ifOKCookie === "true") {
		insertGTM();
	} else {
		$('body').prepend($('#cookie-temp').html());

		$('body').on('click', '#cookies_btn_ok', function (e) {
			e.preventDefault();
			setCookie('syno_confirm_v2_answer', 'true', 30);
			$('#root').attr('style', '');
			insertGTM();
			$('#cookies_header').hide();
		});
	}

	/// 手機跟桌機位置不同做檢查
	// init first time.
	cookieMobileVsPc();
	// resize check for header, footer
	function doHdFtCheck() {
		// Haven't resized in 100ms!
		cookieMobileVsPc();
	}
	var doResizeCheck;
	window.onresize = function () {
		clearTimeout(doResizeCheck);
		doResizeCheck = setTimeout(doHdFtCheck, 100);
	};
}())
</script>
</body>
</html>