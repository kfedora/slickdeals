<div id="lang-dialog-app">
    <lang-dialog></lang-dialog>
</div>

<script type="text/x-template" id="lang-dialog-template">
	<section class="lang-dialog" :class="{'lang-dialog--active': isLangDialogActive}">
		<div
			:class="langDialogWrapperClassAttr"
			@click.self="handleDialogClose"
		>
			<div class="lang-dialog__list-wrapper">
				<div class="lang-dialog__list">
					<h3 class="lang-dialog__list__title">{__langTitle__}</h3>
					<div class="lang-dialog__list__category-container">
						<div
							v-for="(cateItem, cateName) in langCateTable"
							:key="cateName"
							class="lang-dialog__list__category"
						>
							<h4 class="lang-dialog__list__category__name">{__langCateNameTable[cateName]__}</h4>
							<ul class="lang-dialog__list__category__ul">
								<li
									v-for="(item, index) in chinaPolicyFilter(cateItem)"
									:key="index"
									class="lang-dialog__list__category__li"
								>
									<a
										:href="setLangUrl(langList[item])"
										:title="item"
										:dataLang="langList[item]"
										rel="nofollow"
										class="lang-dialog__list__category__link"
									>
										{__item__}
									</a>
								</li>
							</ul>
						</div>
					</div>
				</div>
				<button class="lang-dialog__close-btn" name="close" aria-label="close" @click="handleDialogClose">
					<div class="lang-dialog__close-btn__close"></div>
				</button>
			</div>
		</div>
	</section>
</script>

<script>
const langTitle = "Please select your preferred language";
const langList = {"Global - English":"en-global","Africa - English":"en-af","Australia - English":"en-au","Canada - English":"en-ca","Danmark - Dansk":"da-dk","Deutschland - Deutsch":"de-de","Espa\u00f1a - Espa\u00f1ol":"es-es","France - Fran\u00e7ais":"fr-fr","India - English":"en-in","Indonesia - English":"en-id","Italia - Italiano":"it-it","Magyarorsz\u00e1g - Magyar":"hu-hu","Malaysia - English":"en-my","Middle East - English":"en-me","M\u00e9xico - Espa\u00f1ol":"es-mx","Nederland - Nederlands":"nl-nl","New Zealand - English":"en-nz","Philippines - English":"en-ph","Polska - Polski":"pl-pl","Singapore - English":"en-sg","Sverige - Svenska":"sv-se","T\u00fcrkiye - T\u00fcrk\u00e7e":"tr-tr","United Kingdom - English":"en-uk","United States - English":"en-us","Vietnam - English":"en-vn","\u010cesk\u00e1 Republika - \u010ce\u0161tina":"cs-cz","\u0420\u043e\u0441\u0441\u0438\u044f - \u0420\u0443\u0441\u0441\u043a\u0438\u0439":"ru-ru","\u0e44\u0e17\u0e22 - \u0e44\u0e17\u0e22":"th-th","\u4e2d\u56fd - \u7b80\u4f53\u4e2d\u6587":"zh-cn","\u53f0\u7063 - \u7e41\u9ad4\u4e2d\u6587":"zh-tw","\u65e5\u672c - \u65e5\u672c\u8a9e":"ja-jp","\u9999\u6e2f\u7279\u5225\u884c\u653f\u5340 - \u7e41\u9ad4\u4e2d\u6587":"zh-hk","\ub300\ud55c\ubbfc\uad6d - \ud55c\uad6d\uc5b4":"ko-kr"};
const langCateNameTable = {
	'Americas': "Americas",
	'Asia': "Asia Pacific",
	'Europe': "Europe",
	'Region': "Region",
};
const langDialog = {
	template: '#lang-dialog-template',
	delimiters: ['{__', '__}'],
	data: function() {
		return {
			isLangDialogActive: false,
			langTitle: langTitle,
			langList: langList,
			langCateTable: {
				'Americas': ['Canada - English', 'México - Español', 'United States - English'],
				'Asia': ['Australia - English', 'India - English', 'Indonesia - English', 'Malaysia - English', 'New Zealand - English', 'Philippines - English', 'Singapore - English', 'Vietnam - English', 'ไทย - ไทย', '中国 - 简体中文', '台灣 - 繁體中文', '日本 - 日本語', '香港特別行政區 - 繁體中文', '대한민국 - 한국어'],
				'Europe': ['Česká Republika - Čeština', 'Danmark - Dansk', 'Deutschland - Deutsch', 'España - Español', 'France - Français', 'Italia - Italiano', 'Magyarország - Magyar', 'Nederland - Nederlands', 'Россия - Русский', 'Sverige - Svenska', 'Türkiye - Türkçe', 'Polska - Polski', 'United Kingdom - English'],
				'Region': ['Africa - English', 'Global - English', 'Middle East - English']
			},
			langCateNameTable: langCateNameTable
		}
	},
	computed: {
		langDialogWrapperClassAttr: function() {
			return {
				'lang-dialog-wrapper': true,
				'lang-dialog-wrapper--active': this.isLangDialogActive
			}
		}
	},
	methods: {
		setLangUrl: function(langCode) {
			return window.location.href.replace('en-us', langCode);
		},
		handleDialogClose: function() {
			this.isLangDialogActive = false;
			this.handleBodyOverflow();
		},
		handleBodyOverflow: function() {
			$('body').css('overflow', this.isLangDialogActive ? 'hidden' : 'auto');
		},
		chinaPolicyFilter: function(arr) {
			const vm = this;
			if ('zh-cn' === 'en-us' || 'cn' == 'us')
			return arr.filter(function(e) { return 'zh-tw' !== vm.langList[e] && 'zh-hk'  !== vm.langList[e]; });
			return arr;
		}
	},
	created: function() {
		const vm = this;
		$("body").on("click", ".change_language", function() {
			vm.isLangDialogActive = true;
			vm.handleBodyOverflow();
		});
	}
};

const langDialogApp = new Vue({
	el: '#lang-dialog-app',
	components: {
		'lang-dialog': langDialog
	},
});
</script>		</footer>
	</div>
