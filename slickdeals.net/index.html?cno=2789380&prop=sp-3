		window.onload = function() {
			var lastFocus;
			function modalShow () {
				lastFocus = document.activeElement;
			}
			function modalClose () {
				lastFocus.focus();
			}

			jQuery(".modalOpener").on("click",function(e){
				modalShow();
				// account for possibility of multiple modals
				myClasses=jQuery(this).attr("class").split(' ');
				modalName="";
				jQuery.each(myClasses,function(index,value){
					if(value.indexOf('modalOpener_') == 0) {
						modalName=value.substr(value.indexOf("modalOpener_")+12);
					}
				});
				jQuery(".modalOpenerModal.modalOpener_"+modalName).clone().appendTo("body").addClass("show").find(".modal_closer").focus();
			});

			jQuery("body").on('click', ".modalOpenerModal,.modal_closer", function(e){
				jQuery("body>.modalOpenerModal").remove();
				modalClose();
			});
			jQuery("body").on("click", ".modalOpenerModal *", function(e){
				e.stopPropagation();
			});
			jQuery("body").on("click", ".modalOpener", function(e){
				e.preventDefault();
			});
		}
	</script><!------ END: D SPOT ---></div></div></div></div><div class="mz-cms-block" ><div class="mz-cms-content"><div class="mz-content-widget-wrap"><div class="mz-content-widget-inner-wrap"><!------ BRANDS WE LOVE ---><style>

