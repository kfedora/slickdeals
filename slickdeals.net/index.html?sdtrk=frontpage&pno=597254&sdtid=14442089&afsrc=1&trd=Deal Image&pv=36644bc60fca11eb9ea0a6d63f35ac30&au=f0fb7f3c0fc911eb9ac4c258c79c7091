                       <button
                            data-dismiss="modal"
                            class="btn btn-default js-warning-modal-action"
                            data-action="yes"
                            href=""
                        >Yes</button>
                        <button data-dismiss="modal" class="btn btn-default js-warning-modal-action" data-action="no">No</button>
