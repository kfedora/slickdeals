     $(document).on('click','.pdp-wrapper .pdp-reviews [data-tab="reviews"]', function(){
        if (!$('.pdp-wrapper .pdp-reviews [data-tab="reviews"] .accordion-content div').length) {
            $('.pdp-wrapper .pdp-reviews [data-tab="reviews"] .accordion-content').load("https://www.jbl.com/reviewsandquestions?pid=JBLCLUB950NCBLKAM&type=reviews");
        }
    });

    $(document).on('click','.pdp-wrapper .pdp-reviews [data-tab="qa"]', function(){
        if (!$('.pdp-wrapper .pdp-reviews [data-tab="qa"] .accordion-content div').length) {
            $('.pdp-wrapper .pdp-reviews [data-tab="qa"] .accordion-content').load("https://www.jbl.com/reviewsandquestions?pid=JBLCLUB950NCBLKAM&type=questions");
        }
    });
</script>
        
