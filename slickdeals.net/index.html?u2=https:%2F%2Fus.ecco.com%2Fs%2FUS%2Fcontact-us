	<script>
	var captchaOnloadCallback = function() {
	grecaptcha.render('captchaElement', {
		'sitekey' : '6LeThW0UAAAAAMANcZz6_wyqGEpr6SpxI8LoVK36',
		'theme': 'light',
		'hl': 'en',
		'callback': 'captchaValidated',
		'expired-callback': 'captchaExpired'
	});
	};

	var captchaValidated = function() {
	var response = grecaptcha.getResponse();
	if (!response) {
		$('.js-captcha-error').text(Resources.CAPTCHA_ERROR);
		$('.js-captcha-error').show();
		$('.js-captcha-error').removeClass('u-visually-hidden');
		$("#reCaptchaResponse").val('').trigger('change');
	} else {
		$("#reCaptchaResponse").val(response).trigger('change');
		$('.js-captcha-error').addClass('u-visually-hidden');
		$('.js-captcha-error').hide();
		$('.js-captcha-error-msg').hide();
	}
	};

	var captchaExpired = function() {
	$("#reCaptchaResponse").val('').trigger('change');
	};
</script>

<script src="https://www.google.com/recaptcha/api.js?onload=captchaOnloadCallback&amp;render=explicit" async defer></script>





