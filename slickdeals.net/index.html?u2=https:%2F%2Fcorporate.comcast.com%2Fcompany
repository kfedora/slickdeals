         <form class="search-autocomplete__form" data-search="/search?">
    <script>

        var ESC_MAP = {
            '&': '&amp;',
            '<': '&lt;',
            '>': '&gt;',
            '"': '&quot;',
            "'": '&#x27;',
            "/": '&#x2F;'
         };

        function escapeHTML(s, forAttribute) {
            return s.replace(forAttribute ? /[&<>"'/]/g : /[&<>"'/]/g, function(c) {
                return ESC_MAP[c];
            });
        }

        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            var cleanedStr = decodeURIComponent(results[2].replace(/\+/g, " "));
            cleanedStr =  escapeHTML(cleanedStr);
            return decodeURIComponent(cleanedStr);
        }

        var html = '';
        html += '<div ';
        html += 'class="search-autocomplete" ';

        
html += 'data-autocomplete-id="navigation-search" ';
        

html += 'data-autocomplete-url="/cf-search/suggestions?q=" ';
        

html += 'data-autocomplete-placeholder="Search" ';
        

html += 'data-autocomplete-results-on-page=""';
        
var q = getParameterByName('q');
        if(q) {
            html += 'data-value="' + q + '" ';
        }

        html += '>';
        html += '</div>';
        document.write(html);
    </script>
    <button class="search-autocomplete__submit" type="submit" id="submit">
        <i class="icon-arrow" aria-hidden="true"></i>
        <span class="search-autocomplete__submit-text">Search</span>
    </button>
    <div class="search-autocomplete__separator"></div>
</form>
