<section class="m-hero-item f-x-left f-y-center context-accessory theme-dark f-precise-click">
    <picture>
        <source srcset="https://compass-ssl.xbox.com/assets/35/01/350100a9-e959-4a08-b020-44b55590e1d9.jpg?n=04395294_Page-Hero-1084_1920x720.jpg" media="(min-width: 1084px)">
