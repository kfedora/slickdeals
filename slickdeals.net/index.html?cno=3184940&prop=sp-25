<script>
//hreflang 
$("link[hreflang]").remove();
var urlRegion = document.URL.toLowerCase().split("xbox.com/")[1].slice(0, 5);
var allregions = ["en-us", "en-au", "en-hk", "en-in", "en-nz", "en-sg", "ja-jp", "ko-kr", "zh-hk", "zh-tw", "ar-ae", "ar-sa", "cs-cz", "da-dk", "de-at", "de-ch", "de-de", "el-gr", "en-gb", "en-ie", "en-za", "fi-fi", "fr-be", "fr-ch", "fr-fr", "he-il", "hu-hu", "it-it", "nb-no", "nl-be", "nl-nl", "pl-pl", "pt-pt", "ru-ru", "sk-sk", "sv-se", "tr-tr", "en-ca", "fr-ca", "es-ar", "es-cl", "es-co", "es-es", "es-mx", "pt-br"];
var hlurl = $("meta[property='og:url']").attr("content")
if (hlurl === undefined) {
  hlurl = "https://www.xbox.com/" + urlRegion;
} else {
  hlurl = hlurl.toLowerCase();
}
for (var i = 0; i < allregions.length; i++) {
  var regionpre = allregions[i].split("-")[0];
  var regionpost = allregions[i].split("-")[1].toUpperCase();
  var regionfull = regionpre + "-" + regionpost
  var hllink = hlurl.replace(urlRegion, regionfull);
  $("head").prepend('<link rel="alternate" href="' + hllink + '" hreflang="' + regionfull + '" />');
}
</script>
<script type="text/javascript" src="/en-US/global-resources/templates/MWF/JS/xbox-MWF.js"></script>
   
        </div>
</div>


        </div>

        
        

<script src="https://assets.xbox.com/xbcservicewebwww-2005-04161-0-0-main-rolling/shell/js/xboxcontextualstoresdk.min.js"></script><link href="/bundles/ContextualStore?v=Moh36pevwtgEB79D1Bx8EOc-bVLcprYuloAI-JyiHNI1" rel="stylesheet"/>
        <script type="text/javascript">
            var loginLiveUrl = "https://account.xbox.com/Account/Signin?returnUrl=https%3a%2f%2fwww.xbox.com%3a443%2fen-US%2fxbox-game-pass%2fpc-games%3ficid%3ddeals-page_R2_CP4_XboxGamePassPC_012720%26ranMID%3d24542%26ranEAID%3dlw9MynSeamY%26ranSiteID%3dlw9MynSeamY-5oyNQdOOXM2qsOy8p5nh0w%26epi%3dlw9MynSeamY-5oyNQdOOXM2qsOy8p5nh0w%26irgwc%3d1%26OCID%3dAID2000142_aff_7593_1243925%26tduid%3d(ir_)(7593)(1243925)(lw9MynSeamY-5oyNQdOOXM2qsOy8p5nh0w)()%26irclickid%3d%26opencontextualstore%3dtrue%26contextualstoreparams%3d%7b0%7d";
            var locale = "en-US";
            var region = "US";
            var featureExpIds = {"Ids":[]};
            var storeUrl = "https://www.microsoft.com/store/productId/{0}?locale=en-US&ocid=xboxcompdp";
            var purchaseContentIframe;
            var contextualStoreColorTheme ="Dark";
            var product;
            var sku;
            var expId;

            if (xboxContextualStore) {

                function logContextualStoreData (data, isMobileVersion) {
                    if (typeof window.awa === 'object' && window.awa.isInitialized === true) {
                        if (data) {
                            var overrides = {
                                contextualStoreData: {
                                    message: data,
                                },
                                isMobile: isMobileVersion
                            };

                            awa.ct.captureContentUpdate(overrides);
                        }
                    }
                }

                function isMobile() {
                    var mobileExp = /android|avantgo|blackberry|blazer|compal|elaine|fennec|hiptop|ip(hone|od|ad)|iris|kindle|lge |maemo|midp|mmp|mobile|o2|opera mini|palm( os)?|plucker|pocket|pre\/|psp|smartphone|symbian|treo|up\.(browser|link)|vodafone|wap|windows ce; (iemobile|ppc)|xiino/i;
                    var uagent = navigator.userAgent.toLowerCase();

                    if (mobileExp.test(uagent)) {
                        return true;
                    }

                    return false;
                };

                // TBD : remove layer of interfaces as a clean up task
                xboxContextualStore.Open = function(productId, skuId) {
                    purchaseContentIframe = document.getElementById('purchaseIframe');

                    if (purchaseContentIframe) {
                        xboxContextualStore.openContextualStore(productId, skuId, isMobile(), locale, region, "", loginLiveUrl, contextualStoreColorTheme);
                    }
                    else {
                        xboxContextualStore.openContextualStore(productId, skuId, isMobile(),null,null,null,null,contextualStoreColorTheme);
                    }
                };

                xboxContextualStore.OpenWithExp = function(expId, productId, skuId) {
                    product = productId;
                    sku = skuId;
                    exp = expId;

                    if (exp && featureExpIds && featureExpIds.Ids && featureExpIds.Ids.length > 0) {
                        var expFeatureFound = false;

                        for (var i=0; i < featureExpIds.Ids.length; i++) {
                            if (featureExpIds.Ids[i].toLowerCase() === exp.toLowerCase()){
                                expFeatureFound = true;
                                logContextualStoreData('Contextual store open for experiment id : ' + exp.toLowerCase() + 'with product id : ' + productId, isMobile());
                                xboxContextualStore.Open(product, sku);
                                break;
                            }
                        }

                        if(expFeatureFound !== true) {
                            // Open store url
                            var catalogId = product;
                            if (sku != null && sku !== undefined && sku.length > 0) {
                                catalogId = catalogId + "/" + sku;
                            }

                            logContextualStoreData('Redirect to store pdp with product id : ' + productId, isMobile());
                            window.open(storeUrl.replace('{0}', catalogId), '_blank');
                        }
                    }
                    else {
                        xboxContextualStore.Open(product, sku);
                    }
                }

                xboxContextualStore.init = function() {
                    if (xboxContextualStore.initializeContextualStore(locale, region, "", loginLiveUrl)) {
                        var urlParams = new URLSearchParams(window.location.search);
                        if (urlParams) {
                            // Look for Contextual store flag in url and if present then open contextual store
                            var returnUrlCSFlag = urlParams.get('opencontextualstore');

                            if (returnUrlCSFlag==='true') {
                                var productId = urlParams.get('contextualstoreparams');
                                if (productId) {
                                    xboxContextualStore.Open(productId);
                                }
                            }
                        }
                    }
                }

                xboxContextualStore.init();
            }
        </script>
