<script>
	(function(){
		//Creating all variables needed, reaching the DOM only one time for each of them
		var headerEl= document.getElementById("header"),
			menuEl=document.getElementById("header-menu"),
			mainNavEl= document.getElementById("header-nav"),
			mainNavCloseEl= document.getElementById("header-nav-close"),
			pageName=document.location.pathname.split("/").pop().split(".")[0],
			selectedLink=document.getElementById(pageName),
			closeMenu=function(){mainNavEl.className=mainNavEl.className.replace("show","");document.body.removeEventListener("click",closeMenu);},
			stopProp=function(e){if(e.stopPropagation){e.stopPropagation();}else{e.cancelBubble=true;}},
            body = document.querySelectorAll("body")[0],
            logo = document.querySelectorAll("#header .logo img")[0],
            logoStates = {
              dark:  '/etc/designs/delltechnologies/assets/img/delltechnologies.png',
              light: '/etc/designs/delltechnologies/assets/img/delltechnologies-light.png'
            };

        var changeLogo = function(state){
            logo.src = logoStates[state];
        };

        var checklogo = function(){
            if (headerLightClass()){
                changeLogo("light");
            }else{
                changeLogo("dark");
            }
        };

        var headerLightClass = function(){
            var classes = body.className.split(" ");

            for (var i = classes.length; i > 0; i--) {
                if (classes[i] == "header-light") {
                    return true;
                }
            }
            return false;
        };
        checklogo();
		//Sticky method
        // Koa: Modified for Search Bar
		window.addEventListener('scroll', function(){
			if (window.pageYOffset){
				//headerEl.className="sticky";
                $j('#header').addClass('sticky'); // Koa: change
                changeLogo("dark");
            }else{
				//headerEl.className="";
                $j('#header').removeClass('sticky'); // Koa: change
                checklogo();
			}
		});



		//Menu methods
		if(menuEl != undefined) {
			menuEl.addEventListener('click', function(event){
				mainNavEl.className="show";
				var cookieBanner = $("#_evh-ric");
				if(cookieBanner.length && ($(cookieBanner).css("opacity") != 0 && $(cookieBanner).css("display") != "none") ) {
					mainNavEl.className= mainNavEl.className + " cookie-banner";
				}
				stopProp(event);
				document.body.addEventListener("click",closeMenu);
			});
		}

		if(mainNavCloseEl != undefined) {
			mainNavCloseEl.addEventListener('click', function(event){
				closeMenu();
				stopProp(event);
			});
		}


		if(mainNavEl != undefined) {
			mainNavEl.addEventListener('click', function(event){
				stopProp(event);
			});
		}

		//Selecting the current page on the main Nav
		if(selectedLink){
			selectedLink.className="current-page";
		}

	})();
    
//    $(document).ready(function () {
