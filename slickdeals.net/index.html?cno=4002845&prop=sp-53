												<script type="text/javascript">
												   function changeResultsBottom() {										 
													 var selectedResultbottom = document.getElementById("selectResultbottom");
													 var selectedValuebottom = selectedResultbottom.options[selectedResultbottom.selectedIndex].value;	
													 var urlbottom= window.location.href;
													 
													 var urlpageSize= urlbottom.includes('pageSize=');
													 var pageSizevalbottom;
													 if(urlpageSize==true){										 
														 pageSizevalbottom='pageSize='+selectedValuebottom;											 
														 var res = urlbottom.indexOf("pageSize"); 
														 var resVal;																
														 if(12 < 10){
															 resVal = urlbottom.substr(res,10); 
														 }else if(12 < 100){
															 resVal = urlbottom.substr(res,11); 
														 }else{
														     resVal = urlbottom.substr(res,12); 
														 }											 
														 urlbottom = urlbottom.replace(resVal, pageSizevalbottom);
													 }else{																 
														 if (urlbottom.indexOf('?') > -1){
														   pageSizevalbottom='&pageSize='+selectedValuebottom;
														   urlbottom += pageSizevalbottom;
														 }else{
														   pageSizevalbottom='?pageSize='+selectedValuebottom;
														   urlbottom += pageSizevalbottom;
														 }
													 }

													 window.location.href = urlbottom;	
												   }
												</script>													
												<ul class="pagination pagination-sm">
												  <input tabindex="0" aria-label="Pagination" type="hidden" name="pageSizeVal" id="pageSizeVal"/>
													
