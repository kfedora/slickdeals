<script>
	require(["jquery"], function($){
		$(document).ready(function(){			           		
			$("#select_phone_bundles_btn_nozip").click(function(){
			if(document.getElementById("pdpAjaxRefresh") ){ 
			var activePaymtOption="";
				if(jQuery('.js-paymentOption').hasClass("active")){
					activePaymtOption = jQuery('.js-paymentOption.active').data('value');
		    	}
				if(WCParamJS.storeId =='10154' && (activePaymtOption === "" || activePaymtOption == null || activePaymtOption == undefined)){
					var selectPaymentMessage = dojo.query('.select_payment_option');
					selectPaymentMessage.style("display", "block");
					return false;
				}
				
			}
			$('#zipcode_needed').modal({show:true, keyboard:false, backdrop:'static'});
			$('body').prepend("<div class='modal-background'></div>");	
			$('body').on('shown.bs.modal', '#zipcode_needed', function () {
				$('input:text:visible:first', this).focus();
				$('input:text:visible:first', this).select();
			})
			$('#zipcode_needed').on('show.bs.modal', function (e) {	
			
					$('body').prepend("<div class='modal-background'></div>");					
				});
			
			});			
			$('#zipcode_needed').on('hidden.bs.modal', function (e) {
			
 						$('.modal-background').remove();
				});
				
			$("#zipPopup_zc").keydown(function(event){
			    if(event.keyCode == 13){
			        $("#pdp_zipbutton").click();
			    }
			});	
			
			// Phone number enter key validation
            $("#phoneNumber").keydown(function(event){
                if(event.keyCode == 13){
                    $("#phoneNumber-button").click();
                }
            });    
			
			
			$("#zip_cancelbutton").click(function(){
				dojo.byId('zipcode_needed_error').innerHTML="";
				dojo.byId('zipPopup_zc').value="";
				console.debug('cancelling '+dojo.byId('zipPopup_zc').value);
			  	$('body').removeClass("modal-open");
 					$('.modal-background').remove();
			});	
			
			
			
			MessageHelper.setMessage("TFLOCATION_ZIPCODE_POPUPALERT", "The zip code entered is not correct. Please verify the number and enter the zip code again.");
			MessageHelper.setMessage("TFLOCATION_ZIPCODE_POPUPEMPTYALERT", "Please enter your zip code.");
			MessageHelper.setMessage("TFLOCATION_LOCATION_NOTCOVERED", "The zip code entered is not covered by any of our devices at this time. Please enter a different zip code.");
			MessageHelper.setMessage("TFLOCATION_LOCATION_NOTCOVERED_FORPROD", "The zip code entered is not covered by this specific device at this time. Please enter a different zip code or select a different device.");
			MessageHelper.setMessage("TF_ENTER_PHONE_NUMBER", "Please enter your phone number.");
			MessageHelper.setMessage("TF_ST_LTO_POPUP_INVALID_PHONE", "Please enter a valid Phone number.");
			MessageHelper.setMessage("TF_ST_LTO_MIN_NOT_ACTIVE_ERROR", "Phone number is not active. Enter an active phone number.");
			MessageHelper.setMessage("TF_ST_LTO_MIN_NOT_REGISTERED_ERROR", "Phone number not registered. Enter a valid number.");
			MessageHelper.setMessage("TF_ST_LTO_MIN_INVALID_BRAND_ERROR", "Phone number does not belong to SimpleMobile.");
			MessageHelper.setMessage("TF_ST_LTO_NOT_ENROLLED_LLID_BRAND_ERROR", "The value you entered is invalid. Please review the information and try again.");
			MessageHelper.setMessage("TF_ERROR_NOT_VALID_ESN_PHONE", "There are no devices in our system with the information you provided. Please review the information and try again.");
			MessageHelper.setMessage("TF_ERROR_ESN_IN_DB_NOT_SL_DEVICE", "The information provided corresponds to another brand. Please review the information and try again.");
			MessageHelper.setMessage("TF_ERROR_ESN_IN_DB_SL_DEVICE_NOT_VALID_LLID", "We are sorry. Our records indicate that this Lifeline ID is not enrolled with SafeLink.");
				
		});// document.ready		
        
		
	});// require jquery
</script>


