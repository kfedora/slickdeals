				SearchBasedNavigationDisplayJS.init('https://shop.simplemobile.com/shop/CategoryNavigationResultsView?pageSize=12&manufacturer=&searchType=&resultCatEntryType=&catalogId=12551&categoryId=23502&langId=-1&storeId=11251&sType=SimpleSearch&filterFacet=');
