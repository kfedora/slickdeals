<script>
$(function () {
    $('#footer_signup').on('submit', function (e) {
        e.preventDefault()
        var $container = $('#footer_signup_container')
        var formData = new FormData(this)
        $container.fadeTo(600, 0.01)
        $.ajax('/signup', {
            processData: false,
            contentType: false,
            data: formData,
            type: 'POST',
            success: function (res) {
                console.log('SIGNUP DONE YAY')
                $container.html(res).fadeTo(400, 1)
            },
            headers: { 'X-CSRF-Token' : $('meta[name=csrf-token]').attr('content') },
        })
        return false;
    })
})
</script>

<script>
$('[data-scroll]').on('click', function () {
    $($(this).data('scroll')).goTo();
});

$(document).on('open.zf.reveal', '#teaser-vid', function (e) {
    e.preventDefault()
    var vid = '<iframe src="https://www.youtube.com/embed/mR-gEtLnyLQ?autoplay=1&autohide=1&rel=0&showinfo=0&modestbranding=1&enablejsapi=1" frameborder="0" allowfullscreen></iframe>'
    var $vid = $(vid);
    $(this).find('.flex-video').html('');
    $(this).find('.flex-video').append($vid);
})
$(document).on('closed.zf.reveal', '#teaser-vid', function (e) {
    e.preventDefault()
    $(this).find('.flex-video').remove('iframe');
    $(this).find('.flex-video').html('');
})
</script>
</body>
</html>
