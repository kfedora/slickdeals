        #NextBestActionContainer.customer-affinity .appContainer > .title {
            font-size: 20px;
        }
    }

    #NextBestActionContainer.customer-affinity .buttonBlockAlt .btn {
        line-height: 28px;
        font-size: 13px;
        height: 30px;
    }

    .size-s #NextBestActionContainer.customer-affinity .c-trench {
        margin-left: -28px;
        margin-right: -28px;
    }

    #NextBestActionContainer.customer-affinity div.image {
        padding: 0;
    }

    #NextBestActionContainer .shop-customer-affinity .appContainer.small.affinity {
        padding: 16px 0;
