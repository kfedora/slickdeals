function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
  for(var i = 0; i <ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}

function selectScenario(InCartVariation, NbaVariation) {
  if (!NbaVariation || NbaVariation === 'next-best-action') {
    document.getElementById(".in-cart-feature-scenario").style.display = "none";
  } else {
      document.getElementById(".next-best-action-scenario").style.display = "none";
  }
}
    $( document ).ready(function() {
        $('[data-scenario-final="category-navigation"],[data-scenario="category-navigation"]').parent().addClass("open_html_cat_nav");
         if (window._metaLayer.device_deviceClass === 's'
            && $('#ABTestScenario').attr('data-scenario') === 'customer-has-product'
            && !$('#ABTestScenario').attr('data-scenario-final')) {
                $('.in-cart-feature-trench').css('display', 'block');
            }       
        // Try scrolling up 1 pixel
        // $(window).scrollTop($(window).scrollTop()+1);
        // var InCartVariation = getCookie('in-cart-feature');
        // var NbaVariation = getCookie('next-best-action')
        // selectScenario(InCartVariation, NbaVariation);
        
        const topRatedLink = $('#NextBestActionContainer .buttonBlockAlt a:nth-of-type(2)');
	    
        if (topRatedLink && topRatedLink.length === 1 ) {
            let link = topRatedLink.attr('href');
            
            if (link) {
                link = link.replace('qp=customerreviews_facet%3DCustomer%20Rating~Top-Rated', 'sp=customerrating%20numberofreviewssaas');
               
                topRatedLink.attr('href', link);
            }
        }
    });
</script></div>

