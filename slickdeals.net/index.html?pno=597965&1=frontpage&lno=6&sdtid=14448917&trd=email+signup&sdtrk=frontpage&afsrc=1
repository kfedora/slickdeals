<script>
	var windowsizecheck = 0;
	var cancelClick = false;
	var ie7 = (navigator.userAgent.toLowerCase().indexOf("msie 7.0") > -1);
	var ie8 = (navigator.userAgent.toLowerCase().indexOf("msie 8.0") > -1);
	require(["dojo/ready"], function(ready){
	
	ready(function (){
	  var nav = dojo.query('#navbar ul.nav li.dropdown');
	  for(var i=0; i<nav.length; i++){
	  	if(!ie8 && !ie7){
	  		nav[i].addEventListener('touchstart', function(e){cancelClick=true;});
	  	}
	 	dojo.connect(nav[i], "onclick", function (){
	 	windowsizecheck = window.innerWidth;
	    if (windowsizecheck >= 768 && cancelClick==false){
	     	window.location=this.children[0].href;
	 	}
	 	cancelClick=false;
	});
	 
	 dojo.connect(nav[i], "onmouseenter", function() {
	 	windowsizecheck = window.innerWidth;
	    if (windowsizecheck >= 768 && cancelClick==false){
	     	this.className = this.className+' open';
	   	}
	   });
	 dojo.connect(nav[i], "onmouseleave", function() {
	  	windowsizecheck = window.innerWidth;
	    if (windowsizecheck >= 768 && cancelClick==false){
	   		this.className = this.className.replace('open','');
	   	}
	   });
	 
	  }
	});
	});
</script>
