					     require(["jquery"], function($){
							$(document).ready(function(){
							   var langCookieVal = '-1';
								 if(langCookieVal == '-1'){
									langCookieVal = 'en';
								 }else{
									langCookieVal = 'es';
								 }
								 var localeKey = 'locale_lang';
								 var localeKeyVal = dojo.cookie(localeKey);
								 if(localeKeyVal != null){
							          if(localeKeyVal.indexOf(langCookieVal) != -1 || langCookieVal == null){
							                return;
							          }else{
							                 dojo.cookie(localeKey, langCookieVal, {path:'/'});     
							          }
							     }else{
									 dojo.cookie(localeKey, langCookieVal, {path:'/'}); 					
							     }
							});
					 	 });		
