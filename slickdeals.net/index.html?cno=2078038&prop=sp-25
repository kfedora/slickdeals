<script type="text/javascript">(function(f) {f(window.P._namespace("AmazonClubSuggestionWidget"));}(function(P) {
if( window.P ) {
P.when("A", "a-modal", "abcSuggestionWidgetUtils", "ready").execute(function (A, popover, abcSuggestionWidgetUtils) {
var $ = A.$;
var suggestionWidgetPageState = A.state("suggestionWidgetPageState");
$(document).delegate("#suggestToClubButton", "click", function(event){
event.preventDefault();
$("#suggest-to-club-popover-loading").removeClass("aok-hidden");
var trigger = $("#suggestToClubButton");
var dataInput = {
"subPageType": suggestionWidgetPageState.subPageType,
"asin": suggestionWidgetPageState.asin
};
$.ajax({
type: "GET",
cache: false,
url: "/amazonbookclubs/suggestForClubsModal",
data: dataInput,
timeout: 8000,
success: function(data) {
if (data && !data.hasError) {
var options = {
"content": $(data).html(),
"width": 428,
"header": suggestionWidgetPageState.header,
"height": 400,
"padding": "0"
};
var footerEnabled = $("#shouldShowFooter", data).val();
if (footerEnabled === 'true') {
options.footer = suggestionWidgetPageState.footer;
}
$("#suggest-to-club-popover-loading").addClass("aok-hidden");
var suggestionWidgetPopover = popover.create(trigger, options);
suggestionWidgetPopover.show();
} else {
abcSuggestionWidgetUtils.showModalErrorMessage();
}
},
error: function() {
abcSuggestionWidgetUtils.showModalErrorMessage();
}
});
abcSuggestionWidgetUtils.logBookClubMetric("SuggestToClubModalButtonClickRW");
});
$(document).delegate('#suggestion-widget-submit-button', "click", function(event) {
var divToClose = $(this).closest(".a-popover").find(".a-button-close");
abcSuggestionWidgetUtils.submitSuggestionsAndShowMessage(divToClose, "RW");
});
});
}
}));</script>

<script type="text/javascript">(function(f) {f(window.P._namespace("AmazonClubSuggestionWidget"));}(function(P) {
if (window.P) {
P.when('A', 'jQuery', 'ready').register("abcSuggestionWidgetUtils", function(A, $) {
//Method to unhide the given div for a period of 'timeout' milliseconds.
function showTimeoutMessage(divId, timeout) {
$("#" + divId).removeClass("aok-hidden");
setTimeout(function() {
$("#" + divId).toggleClass("aok-hidden", true);
}, timeout);
}
function showModalErrorMessage() {
logBookClubMetric("SuggestToClubsPopupLoadAjaxError");
$("#suggest-to-club-popover-loading").addClass("aok-hidden");
showTimeoutMessage("suggest-to-clubs-error-section", 3000)
}
function logBookClubMetric(customTag) {
if(window.ue && window.ue.tag) {
window.ue.tag(customTag, "bookClubMetric");
window.ue.count(customTag, 1);
}
}
function submitSuggestionsAndShowMessage(popoverCloseId, logMetricName) {
logBookClubMetric("SuggestToClubSuggestButtonClick" + logMetricName);
var suggestionWidgetPageState = A.state("suggestionWidgetPageState");
var checkboxCount = document.querySelectorAll("input[name=clubList]:checked").length;
if (checkboxCount > suggestionWidgetPageState.maxClubsLimit) {
var errorMessage = currentDetailPageStateData.clubSelectionLimitExceededText;
errorMessage = errorMessage.replace("#DESELECT", checkboxCount - suggestionWidgetPageState.maxClubsLimit);
//Display the error banner on top of modal, and scroll to it.
$("#suggestion-count-exceeded-error-banner").find(".a-alert-content").text(errorMessage);
$('#suggestion-count-exceeded-error-banner').removeClass('aok-hidden');
$(".a-popover-inner").scrollTop(0);
} else {
$('#suggestion-count-exceeded-error-banner').addClass('aok-hidden');
$('#suggest-to-club-submit-loading').removeClass('aok-hidden');
$('#suggestion-widget-submit-button').addClass('aok-hidden');
var checkboxValues = [];
A.each($("input[name=clubList]:checked"), function(value, key) {
var currentClubId = $(value).val();
checkboxValues.push(currentClubId);
});
var input = {
"bookAsin": suggestionWidgetPageState.asin,
"clubIdList": checkboxValues,
"subPageType": suggestionWidgetPageState.subPageType
};
$.ajax({
type: "POST",
url: "/amazonbookclubs/submitClubSuggestions",
data: input,
traditional: true,
timeout: 5000,
success: function(data) {
if(data && !data.hasError) {
//remove the loading div
$('#suggest-to-club-submit-loading').addClass('aok-hidden');
//restore submit button
$('#suggestion-widget-submit-button').removeClass('aok-hidden');
//Add success message to div
$("#suggest-for-clubs-success-message").html($(data).html());
//Remove the button div
$("#suggestToClubButtonSection").addClass("aok-hidden");
//Close the popup or secondary view
$(popoverCloseId).click();
}
},
error: function() {
$('#suggest-to-club-submit-loading').addClass('aok-hidden');
$('#suggestion-widget-submit-button').removeClass('aok-hidden');
logBookClubMetric("SuggestToClubsAjaxError" + logMetricName);
showTimeoutMessage("suggest-to-club-submit-error", 3000);
}
});
}
}
return {
logBookClubMetric: function(customTag) {
return logBookClubMetric(customTag);
},
showModalErrorMessage: function() {
return showModalErrorMessage();
},
showTimeoutMessage: function(divId, timeout) {
return showTimeoutMessage(divId, timeout);
},
submitSuggestionsAndShowMessage(popoverCloseId, logMetricName) {
return submitSuggestionsAndShowMessage(popoverCloseId, logMetricName);
}
};
});
}
}));</script>
