<script type="text/javascript">(function(f) {f(window.P._namespace("AudibleDetailPageFeatureTemplates"));}(function(P) {
/*
* Audible have a sticky buybox which floats when customer scrolles down. EWC (Every Where Cart) is populated after dpx is rendered and it shows Cart
* items are (if there's anything in cart at all!). Implementation of EWC contents is it's a div that's on the screen all times which causes audible sticky
* buy box hiding behind right cart nav.
* This JS serves listner to the given object from dom and provides a callback function that performs certain actions when queried object is available in dom.
* USAGE:
*
*  ready('#ewc-content', function(element) {
*      $('#sticky_buy_box').addClass('stickyBuyBoxReucedSize');
*  });
* In this example i'll listening for div with id 'ewc-content' in DOM and if/whn it's available perform the inner function.
*/
(function(win) {
'use strict';
var listeners = [],
doc = win.document,
MutationObserver = win.MutationObserver || win.WebKitMutationObserver,
observer;
function ready(selector, fn) {
// Store the selector and callback to be monitored
listeners.push({
selector: selector,
fn: fn
});
if (!observer) {
// Watch for changes in the document
observer = new MutationObserver(check);
observer.observe(doc.documentElement, {
attributes: true,
childList: true,
subtree: true
});
}
// Check if the element is currently in the DOM
check();
}
function check() {
// Check the DOM for elements matching a stored selector
for (var i = 0, len = listeners.length, listener, elements; i < len; i++) {
listener = listeners[i];
// Query for elements matching the specified selector
elements = doc.querySelectorAll(listener.selector);
for (var j = 0, jLen = elements.length, element; j < jLen; j++) {
element = elements[j];
// Make sure the callback isn't invoked with the
// same element more than once
if (!element.ready) {
element.ready = true;
// Invoke the callback with the element
listener.fn.call(element, element);
}
}
}
}
// Expose `ready`
win.ready = ready;
})(this);
}));</script>
<script type="text/javascript">(function(f) {f(window.P._namespace("AudibleDetailPageFeatureTemplates"));}(function(P) {
P.when('A').execute(function(A){
var $ = A.$;
// If there are items in the cart then the cart div will be shown on right side. Adjust the sticky buybox to the available width.
ready('#ewc-content', function(element) {
$("#stickyBuyBox").width($('#dp').width());
});
window.addEventListener("resize", function(element) {
$("#stickyBuyBox").width($('#dp').width());
});

var weblabTriggered = false;

function isAboveScreen(element, buffer) {
//Window
var windowTop = $(window).scrollTop();
//Element
var bottom = $(element).offset().top + $(element).height();
return  windowTop > bottom;
}
$(window).bind("scroll.stickybuybox",function(){

var isEnabledForDevice = true || window.matchMedia("(orientation: portrait)").matches

var buyBoxId = '#buybox';
if("") {
buyBoxId = '#aud_mbr_form';
}
if (isEnabledForDevice && isAboveScreen(buyBoxId,0)) {
if (!weblabTriggered) {
weblabTriggered = true;
A.post("/hz/audible/wl",{
params: {
audible_weblabs: "ADBL_HS_PDP_STICKY_BB_147041"
},
error: function(data){
weblabTriggered = false;
}
});
}

var showBuyBox = true
if (showBuyBox && $("#stickyBuyBox").is(':hidden')) {
$("#stickyBuyBox").slideDown(200);
}
}
else {
if ($("#stickyBuyBox").is(':visible')) {
$("#stickyBuyBox").slideUp(200);
}
}
});
});
}));</script>







