<script>
    
    const vzButton = document.getElementById('vzButton');
    const attButton = document.getElementById('attButton');
    
    const vzSteps = document.getElementById('vzSteps');
    const attSteps = document.getElementById('attSteps');
    
    const vzLinks = vzSteps.getElementsByTagName('a');
    const attLinks = attSteps.getElementsByTagName('a');
    
    for(i=0; i < vzLinks.length; i++){ vzLinks[i].classList = 'btn-primary btn-brand decorated-link'; };
    vzLinks[1].classList = "btn-brand btn-secondary decorated-link tmo_tfn_number";
    vzLinks[1].style.marginBottom = "0";
    vzLinks[1].style.padding = "0";
    
    for(j=0; j < attLinks.length; j++){ attLinks[j].classList = 'btn-primary btn-brand decorated-link'; };
    attLinks[3].classList = "btn-brand btn-secondary decorated-link tmo_tfn_number";
    attLinks[3].style.marginBottom = "0";
    attLinks[3].style.padding = "0";
    
    function updateURL(thePath) {
     	let newURL = window.location.protocol + "//" + window.location.host + window.location.pathname + thePath;
        window.history.pushState({path:newURL},'',newURL);   
    }
    
    function vzSwitch() {
        if(attButton.classList.contains('active')){ attButton.classList.remove('active'); };
        if(attSteps.classList.contains('active')){ attSteps.classList.remove('active'); };
        attSteps.style.opacity = '0';
        attButton.setAttribute('aria-pressed', 'false');

        vzButton.classList = 'active';
        vzButton.setAttribute('aria-pressed', 'true');
        vzSteps.classList = 'active';
        setTimeout(function(){ vzSteps.style.opacity = '1' }, 10);
           
        updateURL("?carrier=verizon");
	}

    function attSwitch() {
        if(vzButton.classList.contains('active')){ vzButton.classList.remove('active'); };
        if(vzSteps.classList.contains('active')){ vzSteps.classList.remove('active'); };
        vzSteps.style.opacity = '0';
        vzButton.setAttribute('aria-pressed', 'false');
        
        attButton.classList = 'active';
        attButton.setAttribute('aria-pressed', 'true');
        attSteps.classList = 'active';
        setTimeout(function(){ attSteps.style.opacity = '1' }, 10);
        
        updateURL("?carrier=att");
    }
    
    function forceOpen() {
		const myQuery = window.location.search;
        const myParams = new URLSearchParams(myQuery);
        const myCarrier = myParams.get('carrier');
        if( myCarrier == 'verizon' ) { vzSwitch() }
        else if ( myCarrier == 'att' ) { attSwitch() };
    }
    
    forceOpen();
	
</script>
