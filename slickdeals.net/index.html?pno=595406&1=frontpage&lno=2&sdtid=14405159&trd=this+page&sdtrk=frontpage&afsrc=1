    $(function () {
      var $header = $('#main-header-navigation');
      $header.on(
        'click',
        '.header-container-tab-with-dropdown .header-container-link',
        function (e) {
          if (
            $(e.currentTarget)
              .parents('.header-container-tab-with-dropdown')
              .hasClass('active')
          ) {
            $(e.currentTarget)
              .parents('.header-container-tab-with-dropdown')
              .removeClass('active');
          } else {
            $('.header-container-tab-with-dropdown').removeClass('active');
            $(e.currentTarget)
              .parents('.header-container-tab-with-dropdown')
              .addClass('active');
          }
        }
      );

      $(document).on('click', function (e) {
        if ($('.header-container-tab-with-dropdown').has(e.target).length === 0) {
          $('.header-container-tab-with-dropdown').removeClass('active');
        }
      });

      $header.on(
        'mouseenter',
        '.header-container-tab-dropdown-category-list-item-link,' +
        ' .header-container-tab-dropdown-list-title-link',
        function (e) {
          $(e.currentTarget)
            .parents('.header-container-tab-dropdown-list')
            .find(
              '.header-container-tab-dropdown-category-list-item-link,' +
              ' .header-container-tab-dropdown-list-title-link'
            )
            .removeClass('hover');
          $(e.currentTarget).addClass('hover');
        }
      );

      $header.on('click', '.js-taxon-section-link', function (e) {
        var href = $(event.target).parent().attr('href');
        var pathname = href.split('#')[0];
        var hash = href.replace(/^.*?#/, '');

        if (window.location.pathname.indexOf(pathname) !== -1) {
          var target = $('[data-id=#' + hash + ']');
          scrollToCategory(target);
        }
      });
    })
  </script>

