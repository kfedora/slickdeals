<script type="text/javascript" src="/content/dam/vzwcom/solutions-and-services/verizon-visa-card/js/chunk-vendors.ae4a471d.js"></script>
<script type="text/javascript" src="/content/dam/vzwcom/solutions-and-services/verizon-visa-card/js/app.6ddc737d.js">
</script>
<script>
  // get parameter value
  var urlParams;
  (window.onpopstate = function () {
    var match,
      pl = /\+/g, // Regex for replacing addition symbol with a space
      search = /([^&=]+)=?([^&]*)/g,
      decode = function (s) {
        return decodeURIComponent(s.replace(pl, " "));
      },
      query = window.location.search.substring(1);

    urlParams = {};
    while (match = search.exec(query))
      urlParams[decode(match[1])] = decode(match[2]);
  })();

  var lc = urlParams["lc"];
  var cjevent = urlParams["cjevent"];
  var vend_id = urlParams["vendorid"];

  //code to generate a random oid
  var cj_rand = Math.floor((Math.random() * 10000000000000) + 1);

  // timestamp
  function getTimeStamp() {
    var now = new Date();
    return ((now.getMonth() + 1) + '-' +
      (now.getDate()) + '-' +
      now.getFullYear() + "_" +
      now.getHours() + '-' +
      ((now.getMinutes() < 10) ?
        ("0" + now.getMinutes()) :
        (now.getMinutes())) + '-' +
      ((now.getSeconds() < 10) ?
        ("0" + now.getSeconds()) :
        (now.getSeconds())));
  }


  //OID with random then timestamp
  var cj_oid = cj_rand + "_" + getTimeStamp();
  

  var hrefVal = '/od/cust/auth/shop?gotourl=/od/cust/auth/creditCardApply';
  var getStarted = document.getElementById('get-started');
  
  if (lc != undefined && lc.length > 0) {
    setCookie("lc", lc);
	//hrefVal = hrefVal + "&lc=" + lc;
  }
  
  if (cjevent != undefined && cjevent.length > 0) {
    //hrefVal = hrefVal + "&cjevent=" + cjevent;
    setCookie("cjevent", cjevent);
  }
  
  getStarted.setAttribute("href", hrefVal);
  getStarted.addEventListener('click', function () {
	if (vend_id != undefined && vend_id.length > 0 && vend_id == "CJM") {
		//hrefVal = hrefVal + '&oid=' + cj_oid;
		setCookie("oid",cj_oid);
	}
	
	window.location.href = hrefVal;
  });
  
	function setCookie(cookiename, cookievalue) {
	   document.cookie = cookiename + "=" + cookievalue + ";" 
	}
</script></body>
</html>
