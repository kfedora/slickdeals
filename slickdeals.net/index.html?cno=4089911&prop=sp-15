<script>
$(function () {
  $('.at78-banner-btn').on('click', function () {
    $($(this).attr('href')).goTo(100);
  })
    // show
    var $carousel = $('.carousel').removeClass('is-hidden');
    // trigger redraw for transition
    $carousel[0].offsetHeight;
    // init Flickity
    $carousel.flickity({
            "prevNextButtons": false,
            "pageDots": false,
            "wrapAround": true,
            "autoPlay": true,
            "adaptiveHeight": true
        });
})
