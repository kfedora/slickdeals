    splunkEle.alt = "";
    document.body.appendChild(splunkEle);
    window.brandName = 'SFLY';
    
        window.brandName = 'TP';
    
    var loggingURL = "https://www.shutterfly.com/application/log.gif?";
</script>
    
        
        
            
                
