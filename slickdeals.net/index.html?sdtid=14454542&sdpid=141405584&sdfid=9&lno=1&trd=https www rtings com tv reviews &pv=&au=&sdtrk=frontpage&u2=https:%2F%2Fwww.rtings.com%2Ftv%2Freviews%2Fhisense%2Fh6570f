\u003e4.7\u003c/span\u003e\n        \u003ca class=\&quot;t-link-style\&quot; href=\&quot;/tv/tests/motion/image-flicker\&quot;\u003eFlicker-Free\u003c/a\u003e\n    \u003c/li\u003e\n    \u003cli\u003e\n      \u003cspan class=\&quot;scores_describe-weight\&quot;\u003e \u0026lt;1%\u003c/span\u003e\n        \u003cspan class=\&quot;e-score_box\&quot; data-score-group=\&quot;ok\&quot; data-size=\&quot;v-small\&quot;\u003e6.2\u003c/span\u003e\n        \u003ca class=\&quot;t-link-style\&quot; href=\&quot;/tv/tests/picture-quality/color-accuracy\&quot;\u003ePre Calibration\u003c/a\u003e\n    \u003c/li\u003e\n    \u003cli\u003e\n      \u003cspan class=\&quot;scores_describe-weight\&quot;\u003e \u0026lt;1%\u003c/span\u003e\n        \u003cspan class=\&quot;e-score_box\&quot; data-score-group=\&quot;good\&quot; data-size=\&quot;v-small\&quot;\u003e9.2\u003c/span\u003e\n        \u003ca class=\&quot;t-link-style\&quot; href=\&quot;/tv/tests/picture-quality/color-accuracy\&quot;\u003ePost Calibration\u003c/a\u003e\n    \u003c/li\u003e\n\u003c/ul\u003e\n\n      \u003c/div\u003e\n      \u003cdiv class=\&quot;distribution_chart is-hidden\&quot; data-id=\&quot;11113\&quot; data-type=\&quot;ratings\&quot;\u003e\n  \u003cdiv class=\&quot;distribution_chart-header\&quot;\u003e\n    \u003cdiv class=\&quot;distribution_chart-title\&quot;\u003eScore distribution:\u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\&quot;distribution_chart-body\&quot;\u003e\n    \u003cdiv class=\&quot;distribution_chart-mark\&quot;\u003e\u003c/div\u003e\n    \u003cdiv class=\&quot;bar_chart\&quot;\u003e\n\u003csvg viewBox=\&quot;0 0 100 100\&quot;\u003e\n  \u003cg class=\&quot;bars\&quot;\u003e\n    \u003cg class=\&quot;bar template\&quot;\u003e\n      \u003crect class=\&quot;background\&quot;\u003e\u003c/rect\u003e\n      \u003crect class=\&quot;fill\&quot;\u003e\u003c/rect\u003e\n    \u003c/g\u003e\n  \u003c/g\u003e\n\u003c/svg\u003e\n\u003c/div\u003e\n    \u003cdiv class=\&quot;distribution_chart-range