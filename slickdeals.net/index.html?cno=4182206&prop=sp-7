<script>
    $(function () {
        $parent = $('#dynamic-reckoning-container');
        var $list = $parent.find('.shirts');
        if ($list.data('count') <= 1) return;

        var viewportCount = 5;
        var $items = $list.children();
        var itemWidth = $items.outerWidth('true');
        var itemCount = $items.length;

        var animating = false;
        var currentIndex = Math.floor(Math.random() * itemCount);

        var updateCurrent = function () {
            var $current = $($items[currentIndex]).find('img');
            var fullsize = $current.data('fullsize');
            var rank = $current.data('rank');
            $('.rank .number').text(rank);
            $parent.find('.hero').attr('src', fullsize);
            $list.children().removeClass('active');
            $($list.children()[currentIndex]).addClass('active');
        };

        var viewportIndex = Math.floor(currentIndex / 5) * 5;
        $parent.find('.scroll.left').toggleClass('enabled', !(viewportIndex < viewportCount));
        $parent.find('.scroll.right').toggleClass('enabled', !(viewportIndex >= itemCount - viewportCount));
        $list.css('left', '-' + viewportIndex * itemWidth + 'px');
        updateCurrent();

        var ensureVisible = function () {
            animating = true;
            $list.animate(
                { left: "-" + viewportIndex * itemWidth + 'px' },
                300,
                function () {
                    $parent.find('.scroll.left').toggleClass('enabled', !(viewportIndex < viewportCount));
                    $parent.find('.scroll.right').toggleClass('enabled', !(viewportIndex >= itemCount - viewportCount));
                    updateCurrent();  // This is only here to fix a Chrome bug.
                    animating = false;
                }
            );
        };

        var scrollRight = function () {
            if (animating) return;
            if (viewportIndex >= itemCount - viewportCount) return;
            viewportIndex += viewportCount;
            ensureVisible();
        };

        var scrollLeft = function () {
            if (animating) return;
            if (viewportIndex < viewportCount) return;
            viewportIndex -= viewportCount;
            ensureVisible();
        };

        var showNext = function () {
            currentIndex = (currentIndex + 1) % itemCount;
            updateCurrent();
            viewportIndex = Math.floor(currentIndex / 5) * 5;
            ensureVisible();
        };

        $parent.find('.left').click(scrollLeft);
        $parent.find('.right').click(scrollRight);

        var hoverTimeoutID = undefined;
        $parent.find('.shirt').hover(function () {
            var self = this;
            hoverTimeoutID = clearTimeout(hoverTimeoutID);
            hoverTimeoutID = setTimeout(function () {
                currentIndex = $('#dynamic-reckoning-container .shirt').index(self);
                updateCurrent();
            }, 200);
        }, function () {
            hoverTimeoutID = clearTimeout(hoverTimeoutID);
        });

        var intervalID = setInterval(showNext, 7500);
        $parent.hover(function () {
            intervalID = clearInterval(intervalID);
        }, function () {
            intervalID = setInterval(showNext, 7500);
        });
    });
</script>

