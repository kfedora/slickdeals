		cq_params.suggestedSearchText = '__UNDEFINED__';
	cq_params.products = [{
	    id: '43000301139',
	    sku: '',
	    textRelevance: '0.23795597'
	}];
	cq_params.showProducts = 'true';
	cq_params.personalized = 'false';
