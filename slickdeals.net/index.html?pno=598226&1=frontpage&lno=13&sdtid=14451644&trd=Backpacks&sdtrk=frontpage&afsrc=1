        request.open('HEAD', '/apps/checkout/isInstalled', true);

        request.onload = function() {
            if (request.status >= 200 && request.status < 400) {
                // Success!
                cashier_installed_on_site = true;
            } else {
                cashier_installed_on_site = false;
            }
        };

        request.onerror = function(e) {
            cashier_installed_on_site = false;
        };

