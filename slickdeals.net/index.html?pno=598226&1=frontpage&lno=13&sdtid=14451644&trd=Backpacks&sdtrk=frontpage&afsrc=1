    ORW.assetsUrl = "//cdn.shopify.com/s/files/1/0334/6553/2460/t/28/assets/placeholder.txt?v=11160318154034397263";
    ORW.appUrl = ORW.assetsUrl.split('/placeholder.txt')[0];
    ORW.appUrlHash = ORW.assetsUrl.split('/placeholder.txt')[1];
    ORW.filePath = "//cdn.shopify.com/s/files/1/0334/6553/2460/files/placeholder_100x.png?12773";
    ORW.filePath = ORW.filePath.split('/placeholder_100x.png')[0];
    ORW.formatMoney = function(money){
        // Reads in money in $104.00 format, not 104
        if(ORW.hideTrailingZeros){
            return money.replace(/((\,00)|(\.00))$/g, '');
        }else{
            return money;
        }
    };
    ORW.quickshopIneligibleProductTypes = [];ORW.customer_logged_in = false;
    
ORW.okta_domain = 'https://verabradleycustomer.okta.com';
        ORW.okta_client_id = '0oah75raczUOIyLCp4x6';
        ORW.okta_authorization_issuer = 'https://verabradleycustomer.okta.com/oauth2/default';
        ORW.okta_app_endpoint = '/apps/okta-givex-integration';
        ORW.okta_success_redirect = '/apps/okta-givex-integration/okta/login';
        ORW.okta_help_link = 'https://outlet.verabradley.com/pages/help-center#/collection/1705/article/8031';
        ORW.customer_id = null;
        ORW.okta_i18n = {
            en: {
                'account.unlock.email.or.username.placeholder': "Email",
                'account.unlock.email.or.username.tooltip': "Email",
                'error.auth.lockedOut': "Your account has been locked for 5 minutes. After 5 minutes, you may try to log in again or reset your password.",
                'error.username.required': "Please enter your email",
                'errors.E0000119': "Your account has been locked for 5 minutes. After 5 minutes, you may try to log in again or reset your password.",
                'password.forgot.email.or.username.placeholder': "Email",
                'password.forgot.email.or.username.tooltip': "Email",
                'password.forgot.emailSent.desc': "Email has been sent to {0} with instructions on resetting your password.",
                'primaryauth.help': "Note: If this is your first time logging in to the new outlet.verabradley.com, you will be required to reset your password. Please enter your email address above and follow the prompts.",
