<script>

  var rewardPoints = 0;
  var pointsEarned = 0 + 0;
  var pointsToReward = rewardPoints - pointsEarned;
  var pointsPercentToReward = 0;
  if (pointsToReward < rewardPoints) {
    pointsPercentToReward = parseFloat( pointsEarned / rewardPoints * 100).toFixed(2)+"%";
  } else {

  }

  $(document).ready(function(){
    $('.bg-pointsbar').css('width', pointsPercentToReward);
    $('.bg-pointsbar').attr('aria-valuemax', rewardPoints);
    // $('.points_away').html().replace('%points%', pointsToReward);

    $(".dashboard_mobile_toggle").click(function(){
        $("#mobile_popover_content_wrapper").slideToggle().html();
    });
  });
</script>

