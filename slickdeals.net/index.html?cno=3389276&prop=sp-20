<script>
    $('#accordion').on('click', 'a', function(e) {
        if($(e.target).is('i.glyphicon')){
          target = $(e.target);
        } else {
          target = $(e.target).find("i.glyphicon");
        }

        $('#accordion').find("i.glyphicon").not(target).each(function(){
          $(this).removeClass("glyphicon-chevron-up").addClass('glyphicon-chevron-down');
        });
    });

    $('.referafriend-accordion .panel-title').click(function() {
        $(this).find("i.glyphicon").toggleClass("glyphicon-chevron-down glyphicon-chevron-up");
    });
</script>
