<script>
if (!(checkFnArgs =>
    // IE11 users will LOVE this concatenation
    // eslint-disable-next-line prefer-template
    document.getElementById('gen-nav-' + checkFnArgs[0]))(["footer"])) {
  (fragmentPath => {
  const error = new Error(
    // eslint-disable-next-line prefer-template
    'ESI fragment ' + fragmentPath + ' requested, but not found in client',
  );
  // eslint-disable-next-line no-undef
  if (window.newrelic) {
    console.warn(error);
    // eslint-disable-next-line no-undef
    newrelic.noticeError(error, {
      type: 'ESI_LOAD_ERROR',
      fragment: fragmentPath,
    });
  } else {
    throw error; // let another monitoring system catch the error.
  }
})('footer')
} else {
  (fragmentPath => {
  // eslint-disable-next-line no-undef
  if (window.newrelic) {
    // eslint-disable-next-line no-undef
    newrelic.addPageAction('ESI_LOAD', { fragment: fragmentPath });
  }
})('footer')
}
</script>



          
  <script>
  const analyticsPageName = 'nikecom>guest>register';
  const errorContainerClass = '.mex-error-container';
  const loadingContainerClass = '.mex-loading-container';
  const mexUniteContainerClass = '.mex-unite-container';
  function getAudienceSegmenterExperiments() {
    const cookie =
      document.cookie
        .trim()
        .split(';')
        .find(c => c.includes('feature_tests=')) || '';
  
    const [, value = ''] = cookie.trim().split('=');
    if (!value) {
      return [];
    }
  
    return value.trim().split(',');
  }
  function trackPage() {
    if (!segment || !segment.load || !segment.page) {
      return;
    }
    segment.load('GQUZh6TtNwtjdph5OcjLslvOYMUeBMcd');
    segment.page(analyticsPageName, {
      event: analyticsPageName,
      type: 'page',
      site: 'nikecom',
      location: {
        language: 'en'
      },
      eventType: 'changeView',
      country: 'us',
      interactionType: 'load',
      abTest: {
        experiments: getAudienceSegmenterExperiments(),
      }
    });
  }

  function trackImageError(event) {
    if (!segment || !segment.track) {
      return;
    }
    segment.track('error_guestError', {
      event: 'error_guestError',
      type: 'error',
      site: 'nikecom',
      location: {
        language: 'en'
      },
      eventType: 'error',
      country: 'us',
      properties: {
        page: analyticsPageName,
        message: 'MEX Error: Error loading image',
        imageSource: event && event.target ? event.target.src : event,
      }
    });
  }

  function trackError(e) {
    if (!segment || !segment.track) {
      return;
    }
    segment.track('error_guestError', {
      event: 'error_guestError',
      type: 'error',
      site: 'nikecom',
      location: {
        language: 'en'
      },
      eventType: 'error',
      country: 'us',
      properties: {
        page: analyticsPageName,
        message: e ? e.message : '',
        raw: e && e.error ? e.error : e
      }
    });
  }

  window.addEventListener('load', function() {
    trackPage();
    // If Unite didn't load, hide the loading spinner and show an error message
    if (!window.nike || !window.nike.unite) {
      trackError({message: 'MEX Error: Error loading Unite script'});
      const errorContainer = document.querySelector(errorContainerClass);
      const mexLoading = document.querySelector(loadingContainerClass);

      if (errorContainer && errorContainer.classList.contains('hidden')) {
        errorContainer.classList.remove('hidden');
      }

      if (mexLoading && !mexLoading.classList.contains('hidden')) {
        mexLoading.classList.add('hidden');
      }
    }
  });

  window.addEventListener('error', trackError);

  function getQueryStringValue(param) {
    let paramValue = null;
    if (typeof window !== 'undefined' && window.location) {
      const search = decodeURIComponent(window.location.search || '').substr(1);
      const keyValuePairs = search.split('&');
      keyValuePairs.forEach(function(pair) {
        const keyValueArray = pair.split('=');
        if (keyValueArray.length > 1 && keyValueArray[0] === param) {
          paramValue = keyValueArray[1];
        }
      });
    }
    return paramValue;
  }

  function isWhitelistedDomain(url) {
    const whiteListedDomains = [
      'https://www.nike.com',
      'https://store.nike.com',
      'https://secure-store.nike.com',
      'https://nike.com',
    ];

    let domainIsWhitelisted = false;
    const continueUrlDomainMatches = url.match(/^https:\/\/[A-Za-z.-]+\.com/);
    if (continueUrlDomainMatches !== null && continueUrlDomainMatches.length > 0) {
      const continueUrlDomain = continueUrlDomainMatches[0];
      whiteListedDomains.forEach(function(domain) {
        if (domain === continueUrlDomain) {
          domainIsWhitelisted = true;
        }
      });
    }
    return domainIsWhitelisted;
  }

  function handleSuccess() {
    localStorage.setItem('dcm_member_joined_success', 'true');
    const continueUrl = getQueryStringValue('continueUrl');
    const redirectUrl = continueUrl !== null && isWhitelistedDomain(continueUrl) ? continueUrl : 'https://www.nike.com/';
    window.location.assign(redirectUrl);
  }

  function handleError() {
    const mexUniteContainer = document.querySelector(mexUniteContainerClass)
    const errorContainer = document.querySelector(errorContainerClass);
    nike.unite.destroyAllViews();

    if (mexUniteContainer && !mexUniteContainer.classList.contains('hidden')) {
      mexUniteContainer.classList.add('hidden');
    }

    if (errorContainer && errorContainer.classList.contains('hidden')) {
      errorContainer.classList.remove('hidden');
    }
  }

  (function appendGuestOrderLookupLink() {
    const referrer = getQueryStringValue('referrer');
    if (referrer === 'orders') {
      const guestLookupContainer = document.getElementById('guest-order-lookup-link');
      if (guestLookupContainer !== null) {
        const anchorNode = document.createElement('a');
        anchorNode.href='https://www.nike.com/orders/details';
        anchorNode.className = 'ncss-btn-tertiary-dark underline';
        const anchorText = document.createTextNode(
          'Look Up Your Order as a Guest'
        );
        anchorNode.appendChild(anchorText);
        guestLookupContainer.appendChild(anchorNode);
      }
    }
  })();

  function onNikeDotcomNavReady(dotcomNav) {
    dotcomNav.mount({
      showPrivacyPolicy: 2000,
      useGeoPrivacy: true,
    });
  }

  window.addEventListener('nikeUniteInit', function() {
    const dotcomNavUniteContainer = document.querySelector('.hf-modal-container .unite-container');
    const dotcomNavUniteContainerV2 = document.querySelector('.pre-modal-view .unite-container');
    const mexUniteContainer = document.querySelector('.mex-unite-container');
    const errorContainer = document.querySelector(errorContainerClass);
    const mexLoading = document.querySelector(loadingContainerClass);

    if (dotcomNavUniteContainer) {
      dotcomNavUniteContainer.parentNode.removeChild(dotcomNavUniteContainer);
    }

    if (dotcomNavUniteContainerV2) {
      dotcomNavUniteContainerV2.parentNode.removeChild(dotcomNavUniteContainerV2);
    }

    if (errorContainer && !errorContainer.classList.contains('hidden')) {
      errorContainer.classList.add('hidden');
    }

    if (mexUniteContainer && mexUniteContainer.classList.contains('hidden')) {
      mexUniteContainer.classList.remove('hidden');
    }

    if (mexLoading && !mexLoading.classList.contains('hidden')) {
      mexLoading.classList.add('hidden');
    }

    nike.unite.destroyAllViews();

    switch ('register') {
      case 'register':
        nike.unite.views.join.render();
        break;
      case 'email-signup':
      case 'chicago-2020-signup':
        nike.unite.views.emailOnlyJoin.render();
        break;
      default:
        nike.unite.views.login.render();
    }

    nike.unite.on.login.success.addHandler(handleSuccess);
    nike.unite.on.join.success.addHandler(handleSuccess);
  });

  NikeDotcomNavReady(onNikeDotcomNavReady);

  </script>
  
