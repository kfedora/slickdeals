<script>
	// BEGIN BFG TLS WARNING BANNER SHIM JS
document.addEventListener('DOMContentLoaded', function () {
    var bfgTlsWarningBanner = document.getElementById('bfg-tls-warning-banner');

    if (bfgTlsWarningBanner) {
        bfgTlsWarningBanner.parentNode.removeChild(bfgTlsWarningBanner);
        // document.body.removeChild(bfgTlsWarningBanner);
    }

    var BROWSER = {
        name: platform.name.toLowerCase(), // chrome | firefox | msie | safari | opera
        version: platform.version
    };

    var TLS_MIN_BROWSER_VERSIONS = {
        chrome: 30,
        firefox: 27,
        msie: 11,
        'microsoft internet explorer': 11,
        ie: 11,
        safari: 7,
        opera: 17
    };

    // Build the banner if the banner should be shown
    if (BROWSER.version < TLS_MIN_BROWSER_VERSIONS[BROWSER.name]) {
        var content = document.createElement('p');
        content.innerText = 'Your browser is out-of-date. Please update your browser. If you do not update your browser, you may not be able to sign in, access your account info, or make purchases on our site.';
        var close = document.createElement('a');
        close.className = 'bfg-banner-close';
        close.addEventListener('click', function () {
            document.getElementById('bfg-tls-warning-banner-shim').style.display = 'none';
        });
        var container = document.createElement('div');
        container.className = 'bfg-banner-container';
        var banner = document.createElement('div');
        banner.id = 'bfg-tls-warning-banner-shim';

        container.appendChild(content);
        container.appendChild(close);
        banner.appendChild(container);
        document.body.insertBefore(banner, document.body.firstChild);
    }
});
//  END BFG TLS WARNING BANNER SHIM JS
</script>


    

</div>
