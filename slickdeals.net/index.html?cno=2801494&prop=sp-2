<script>
	// BEGIN CLASSIC SITE BANNER SHIM JS
document.addEventListener('DOMContentLoaded', function () {
    var classicSiteBanner = document.getElementById('banner');
    if (classicSiteBanner) {
        classicSiteBanner.parentNode.removeChild(classicSiteBanner);
    }

    if (BigFish.Storage.getSessionData('classic') !== 'disabled') {
        var banner = document.createElement('div');
        banner.id = 'bfg-classic-site-banner-shim';
        var container = document.createElement('div');
        container.className = 'bfg-banner-container';
        var content = document.createElement('p');
        content.innerHTML = 'Looking for our <a href="https://www.bigfishgames.com/pc-games/">classic site</a>? All the content you know and love is still available.';
        var close = document.createElement('a');
        close.className = 'bfg-banner-close';
        close.addEventListener('click', function () {
            document.getElementById('bfg-classic-site-banner-shim').style.display = 'none';
            BigFish.Storage.setSessionData('classic', 'disabled');
        });

        container.appendChild(content);
        container.appendChild(close);
        banner.appendChild(container);
        document.body.prepend(banner);
    }
});
//  END CLASSIC SITE BANNER SHIM JS
</script>


    

