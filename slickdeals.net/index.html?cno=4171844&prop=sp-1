			type: "hl.desktop.ui.pages.SearchResultsPage",
			params: {}
		};
		
	}
	(function($) {
		$(window).ready(function() {
			hl.app.init(_currentPage.type, $("#page"), _currentPage.params);
		});
	})(jQuery);

</script>

