    "inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title": "Check another zip code",
    "inhome-widget-gdo-terminal-screen-your-garage-might-be-eligibile-title": "Your garage door might be eligible.",
    "inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-noprime": "Key In-Garage Delivery is exclusively for Prime members. Join today to enjoy the benefits of Key by Amazon, along with free shipping on Amazon, unlimited streaming with Prime Music and much more.",
    "inhome-widget-gdo-terminal-screen-your-garage-might-be-eligibile-body": "You will need a little more information to decide on the right option.",
    "inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-1-nosgh": "You\'ll just need to purchase a $SmartGarageHub first.",
    "inhome-widget-gdo-amazon-key-is-available-title": "Key by Amazon In-Garage Delivery is available in select areas. $SignUpLink to check address book eligibility.",
    "inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-2-nosgh-download-the-app": "download the app",
    "inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-1-nosgh-sgh-link-url": "https://www.amazon.com/Smart-Garage-Opener-Chamberlain-MYQ-G0301/dp/B075H7Z5L8/",
    };
    var isPrime = 0;
    var isRecognized = "0";
    var sessionId = "000-0000000-0000000";

    P.when(
	'A', 'a-button', 'a-dropdown', 'a-modal', 'ready'
    ).execute('key-inhome-landing-page-widget', function (A, getButtonCollection, dropdown, modal) {
	var $ = A.$;

        var entityMap = {
          '&': '&amp;',
          '<': '&lt;',
          '>': '&gt;',
          '"': '&quot;',
          "'": '&#39;',
          '/': '&#x2F;',
          '`': '&#x60;',
          '=': '&#x3D;'
        };

        function escapeHtml (string) {
            return String(string).replace(/[&<>"'`=\/]/g, function (s) {
                return entityMap[s];
            });
        }

	// Common code
	function setZipCodeEligibilityState(success, zipCode) {
	    var isZipCodeInputPopulated = zipCode ? true : false;

	    var successDetailString;
	    if (isRecognized == false) {
		successDetailString = stringHash['inhome-widget-zip-success-detail-unrec'];
	    } else {
		if (isPrime == false) {
		    successDetailString = stringHash['inhome-widget-zip-success-detail-non-prime'];
		} else {
		    successDetailString = stringHash['inhome-widget-zip-success-detail-with-prime'];
		}
	    }
	    successDetailString = successDetailString.replace("In-home", "Key by Amazon Garage");

	    var failureDetailTitle = isZipCodeInputPopulated ?
			stringHash['inhome-widget-zip-failure-title'] : stringHash['inhome-widget-zip-missing'] ;

	    failureDetailTitle = failureDetailTitle.replace("In-home", "Key by Amazon Garage");
	    $('#inhome-zipEligibilityOutputSuccessDetail').text(escapeHtml(successDetailString.replace(/%eligibleZips/, zipCode)));
	    $('#inhome-zipEligibilityOutputFailureDetailTitle').text(escapeHtml(failureDetailTitle));

	    $('#inhome-zipEligibilityOutput-initial').hide();
	    if (zipCode) {
		$('#inhome-programUpdatesSignup-neutral').show();
	    } else {
		$('#inhome-programUpdatesSignup-neutral').hide();
	    }
	    $('#inhome-zipEligibilityOutput-success').toggle(success);
	    $('#inhome-zipEligibilityOutput-failure').toggle(!success);
	    $('#inhome-zipEligibilityOutputFailureDetailText').toggle(isZipCodeInputPopulated);
	}

	function setProgramUpdatesSignupSuccess() {
	    $('#inhome-programUpdatesSignup-neutral').hide();
	    $('#inhome-programUpdatesSignup-success').show();
	}

	function getEligibilityFormData() {
	    var checkProgramEligibilityInput = {};

	    var zipCode = document.getElementById('inhome-checkProgramEligibilityFormTextInput').value;
	    if (zipCode) {
		checkProgramEligibilityInput.zipCode = zipCode;
	    }
	    checkProgramEligibilityInput.sessionId = sessionId;

	    checkProgramEligibilityInput.accessPointType = 'RESIDENCE';
	    checkProgramEligibilityInput.deviceType = 'RESIDENCE';
	    checkProgramEligibilityInput.deviceType = 'GARAGE_DOOR';
	    checkProgramEligibilityInput.doorType = dropdown.getSelect('inhome-doorType').val();
	    checkProgramEligibilityInput.deviceVendor = dropdown.getSelect('inhome-brand').val() === 'other' ? 'OTHER' : 'CHAMBERLAIN';
	    checkProgramEligibilityInput.deviceMake = dropdown.getSelect('inhome-brand').val();
	    checkProgramEligibilityInput.hasWifiLogo = dropdown.getSelect('inhome-wifi-logo').val() === 'has-wifi-logo';
	    var hasSectionalDoorWithSmartEyes = dropdown.getSelect('inhome-doorType').val() === 'SECTIONAL_DOOR_WITH_PHOTO_EYE_SENSORS';
	    checkProgramEligibilityInput.isDeviceEligible = hasSectionalDoorWithSmartEyes && !(checkProgramEligibilityInput.deviceVendor == 'OTHER');
	    return checkProgramEligibilityInput;
	}

	var storedFormData;
	function setStoredFormData(formData) {
	    storedFormData = formData;
	}
	function getStoredFormData() {
	    return storedFormData;
	}

	function resetZipCode() {
	    storedFormData = undefined;
	    $('#inhome-checkProgramEligibilityFormTextInput').val('');
	    $('#inhome-zipEligibilityOutput-initial').show();
	    $('#inhome-zipEligibilityOutput-success').hide();
	    $('#inhome-zipEligibilityOutput-failure').hide();
	    $('#inhome-zipEligibilityOutputFailureDetailText').hide();
	}

	// horrible hack for getting the pattern attribute in an AUI text input
	$('*[data-pattern]').each(function (index, el) {
	    $(el).attr('pattern', $(el).attr('data-pattern'));
	});

	$('#inhome-checkProgramEligibilityForm').submit(function (event) {
	    event.preventDefault();

	    var formData = getEligibilityFormData();
	    setStoredFormData(formData);
	    if (dropdown.getSelect('inhome-doorType') && dropdown.getSelect('inhome-doorType').val() === 'none') {
		$('#inhome-checkProgramEligibilityFormOutput-failure-title').text(stringHash['inhome-widget-gdo-check-your-garage-door-error-no-door-type']);
		$('#inhome-checkProgramEligibilityFormOutput-failure').show();
		return;
	    }
	    $('#checkProgramEligibilityFormOutput-failure').hide();
	    if (dropdown.getSelect('inhome-brand') && dropdown.getSelect('inhome-brand').val() === 'none') {
		$('#inhome-checkProgramEligibilityFormOutput-failure-title').text(stringHash['inhome-widget-gdo-check-your-garage-door-error-no-brand']);
		$('#inhome-checkProgramEligibilityFormOutput-failure').show();
		return;
	    }
	    if (dropdown.getSelect('inhome-wifi-logo') && dropdown.getSelect('inhome-wifi-logo').val() === 'none') {
		$('#inhome-checkProgramEligibilityFormOutput-failure-title').text(stringHash['inhome-widget-gdo-check-your-garage-door-error-no-wifi-logo']);
		$('#inhome-checkProgramEligibilityFormOutput-failure').show();
		return;
	    }
	    if (!formData.zipCode) {
		setZipCodeEligibilityState(false, formData.zipCode);
		return;
	    }
	    $('#inhome-checkProgramEligibilityFormOutput-failure').hide();
	    var myHandle = A.ajax('/gp/product/features/key/checkProgramEligibility.html', {
		method: 'get',
		params: formData,
		success: function (data) {
		    var formData = getStoredFormData();
		    var isZipCodeInputPopulated = 'zipCode' in formData;
		    var isZipCodeEligible = data.zip === formData.zipCode;
		    setZipCodeEligibilityState(isZipCodeEligible, formData.zipCode);
                    var brandInfo = brandsModel.brands[formData.deviceMake];
                    var brandMightHaveWifiLogo = brandInfo && brandInfo['mightHaveWifiLogo'] === "true";
	            var isSGHRequired = !brandMightHaveWifiLogo || !formData.hasWifiLogo;
		    var zipCodeEscaped = escapeHtml(formData.zipCode);
		    var screenName = formData.isDeviceEligible ? 'eligibleDoor' : 'ineligibleDoor';
		    if (formData.isDeviceEligible) {
			screenName += '-';
			screenName += isSGHRequired ? 'sghRequired' : 'sghNotRequired';
		    }
		    screenName += '-';
		    screenName += isZipCodeEligible ? 'eligibleZip' : 'ineligibleZip';
		    if (formData.isDeviceEligible && isZipCodeEligible) {
			screenName += '-';
			screenName += isPrime ? 'Prime' : 'NonPrime';
		    }
		    showTerminalScreen(screenName, zipCodeEscaped);
		},
		error: function (data) {
		    console.log('error', arguments);
		},
	    });
	});

	$('#inhome-signUpForProgramUpdates').click(function (event) {
	    event.preventDefault();
	    var myHandle = A.ajax('/gp/product/features/inhome/registerForUpdates.html', {
		method: 'get',
		params: getStoredFormData(),
		success: setProgramUpdatesSignupSuccess,
		error: function (data) {
		    console.log('error', data);
		},
	    });
	});

	//
	// GDO
	//
        function setDropdownEnabledState(name, enabled) {
            var d = dropdown.getSelect(name);
            d.update({
                status: enabled ? 'normal' : 'disabled',
            });
        }
        function resetGarageType() {
            dropdown.getSelect('inhome-doorType').setValue('none');
            resetBrands();
        }

        var brandsModel = {
	"brands": {
		"Access Master": {
			"mightHaveWifiLogo": "true"
		},
		"AssureLink": {
			"mightHaveWifiLogo": "true"
		},
		"BuildMark": {
			"mightHaveWifiLogo": "false"
		},
		"Chamberlain": {
			"mightHaveWifiLogo": "true"
		},
		"Craftsman": {
			"mightHaveWifiLogo": "true"
		},
		"Do-It Best": {
			"mightHaveWifiLogo": "false"
		},
		"Garage Access": {
			"mightHaveWifiLogo": "false"
		},
		"LiftMaster": {
			"mightHaveWifiLogo": "true"
		},
		"Linear": {
			"mightHaveWifiLogo": "false"
		},
		"Master Mechanic": {
			"mightHaveWifiLogo": "false"
		},
		"Raynor": {
			"mightHaveWifiLogo": "true"
		},
		"True Value": {
			"mightHaveWifiLogo": "false"
		}
	},
	"displayOrder": ["Access Master", "AssureLink", "BuildMark", "Chamberlain", "Craftsman", "Do-It Best", "Garage Access", "LiftMaster", "Linear", "Master Mechanic", "Raynor", "True Value"]
};

	function resetBrands() {
            setDropdownEnabledState('inhome-brand', false);
            var brands = dropdown.getSelect('inhome-brand');
            brands.getOptions().remove();
            brands.addOption({
                value: 'none',
                text: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-brand'],
                selected: true,
		 aria: {
		    label: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-brand']
		 }
	    }, 0);
	    var brandsArray = brandsModel.displayOrder.map(function(brand, index) { return {value: brand, text: brand, aria: {label: brand} } });
	    brandsArray.push({id: "other", value: "other", text: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-brand-value-other'], aria: {label: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-brand-value-other']}  });
	    brands.addOptions(brandsArray, 1);
	    // Split into groups after adding since aui dropdown doesn't have a JS api for this
	    var optGroup0 = $('#inhome-brand').append('<optgroup id="inhome-opt-group-0"/>');
	    var optGroup1 = $('#inhome-brand').append('<optgroup id="inhome-opt-group-1"/>');
	    $('#inhome-brand').children('option[value!="other"]').appendTo($('#inhome-opt-group-0'));
	    $('#inhome-brand').children('option[value="other"]').appendTo($('#inhome-opt-group-1'));
	    brands.setValue('none');
            resetWifiLogo();
        }

	function resetWifiLogo() {
            setDropdownEnabledState('inhome-wifi-logo', false);
            var wifiLogo = dropdown.getSelect('inhome-wifi-logo');
            wifiLogo.getOptions().remove();
            wifiLogo.addOptions([
		{value: 'none', text: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-wifi-logo'], selected: true, aria: {label: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-wifi-logo']} },
		{value: 'has-wifi-logo', text: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-wifi-value-0'], selected: false, aria: {label: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-wifi-value-0']} },
		{value: 'no-wifi-logo', text: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-wifi-value-1'], selected: false, aria: {label: stringHash['inhome-widget-gdo-check-your-garage-door-dropdown-wifi-value-1']} },
	    ], 0);
	}

	resetBrands(); // since is done via JS

	A.on('a:dropdown:selected:inhome-doorType', function (data) {
            resetBrands();
            if (data.value === 'none') return;

            setDropdownEnabledState('inhome-brand', true);
        });
	A.on('a:dropdown:selected:inhome-brand', function (data) {
            resetWifiLogo();
            if (data.value === 'none') return;

            setDropdownEnabledState('inhome-wifi-logo', true);
	});

	var sghNotRequiredPrimeParagraphHTML1 = stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-but-body'].replace("$DownloadTheApp",
																    "<b>" + stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-download-the-app'] + "</b>");
	var sghNotRequiredEligibleParagraphHTML1 = stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body'].replace("$DownloadTheApp",
																    "<b>" + stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-download-the-app'] + "</b>");
	var sghRequiredCommonParagraphHTML1 = stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-1-nosgh'].replace("$SmartGarageHub",
																    "<a href='"+ stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-1-nosgh-sgh-link-url']  + "'>" + stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-1-nosgh-sgh-link'] + "</a>");
	var sghRequiredPrimeParagraphHTML2 = stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-2-nosgh'].replace("$DownloadTheApp",
																    "<b>" + stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-2-nosgh-download-the-app'] + "</b>");
	var terminalScreenParameters = {
	    'eligibleDoor-sghNotRequired-ineligibleZip': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-but-title'],
		    'type': 'normal'
		},
		'paragraph-1': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-but-body'],
		'ctaButton-title': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title'],
		'ctaAction': function() {
		    resetZipCode();
		    $('#inhome-checkProgramEligibilityForm').show();
		    $('#inhome-gdoTerminalScreenSection').hide();
		}
	    },
	    'eligibleDoor-sghNotRequired-eligibleZip-Prime': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-title'],
		    'type': 'success'
		},
		'paragraph-1': sghNotRequiredEligibleParagraphHTML1,
		'showDownloadButtons': true
	    },
	    'eligibleDoor-sghNotRequired-eligibleZip-NonPrime': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-title'],
		    'type': 'success'
		},
		'paragraph-1': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-noprime'],
		'ctaButton-title': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title-noprime'],
		'ctaAction': function() {
		    window.location = "/gp/prime/pipeline/membersignup";
		}
	    },
	    'eligibleDoor-sghRequired-ineligibleZip': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-zipcode-ineligible-title'],
		    'type': 'normal'
		},
		'paragraph-1': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-but-body'],
		'ctaButton-title': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title'],
		'ctaAction': function() {
		    resetZipCode();
		    $('#inhome-checkProgramEligibilityForm').show();
		    $('#inhome-gdoTerminalScreenSection').hide();
		}
	    },
	    'eligibleDoor-sghRequired-eligibleZip-Prime': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-title'],
		    'type': 'success'
		},
		'paragraph-1': sghRequiredCommonParagraphHTML1,
		'paragraph-2': sghRequiredPrimeParagraphHTML2,
		'showDownloadButtons': true
	    },
	    'eligibleDoor-sghRequired-eligibleZip-NonPrime': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-title'],
		    'type': 'success'
		},
		'paragraph-1': sghRequiredCommonParagraphHTML1,
		'paragraph-2': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-body-noprime'],
		'ctaButton-title': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title-noprime'],
		'ctaAction': function() {
		    window.location = "/gp/prime/pipeline/membersignup";
		}
	    },
	    'ineligibleDoor-ineligibleZip': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-might-be-eligibile-zipcode-ineligible-title'],
		    'type': 'normal'
		},
		'paragraph-1': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-but-body'],
		'ctaButton-title': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title'],
		'ctaAction': function() {
		    resetZipCode();
		    $('#inhome-checkProgramEligibilityForm').show();
		    $('#inhome-gdoTerminalScreenSection').hide();
		}
	    },
	    'ineligibleDoor-eligibleZip': {
		'title': {
		    'string': stringHash['inhome-widget-gdo-terminal-screen-your-garage-might-be-eligibile-title'],
		    'type': 'normal'
		},
		'paragraph-1': stringHash['inhome-widget-gdo-terminal-screen-your-garage-might-be-eligibile-body'],
		'paragraph-2': stringHash['inhome-widget-gdo-terminal-screen-your-garage-might-be-eligibile-phone'],
		'ctaButton-title': stringHash['inhome-widget-gdo-terminal-screen-your-garage-is-eligible-cta-button-title'],
		'ctaAction': function() {
		    resetZipCode();
		    $('#inhome-checkProgramEligibilityForm').show();
		    $('#inhome-gdoTerminalScreenSection').hide();
		}
	    },
	};

	function updateTextAndClass(id, text, addClassName, allClassNames) {
	    $(id).html(text);
	    $(id).removeClass(allClassNames.join(" ")).addClass(addClassName)
	}

	function updateAriaLabel(id, ariaLabel) {
	    $(id).attr("aria-label", ariaLabel)
	}

	function updateParagraph(paragraphString, paragraphElemId, zipCode) {
	    updateTextAndClass(paragraphElemId, paragraphString.replace("\$ZipCode", zipCode), "State", ["inhome-successState"])
	}

	function showTerminalScreen(screenType, zipCode) {
	    var screenParameters = terminalScreenParameters[screenType];
	    var title = screenParameters['title'];
	    if (title) {
		updateTextAndClass("#inhome-terminalScreenTitle", title.string, "inhome-" + title.type + "State", ["inhome-successState"]);
		updateAriaLabel("#inhome-terminalScreenTitle", title.string)
		$('#inhome-terminalScreenTitle').show();
	    } else {
		$('#inhome-terminalScreenTitle').hide();
	    }
	    var paragraph1 = screenParameters['paragraph-1'];
	    if (paragraph1) {
		updateParagraph(paragraph1, "#inhome-terminalScreenParagraph1", zipCode)
		$('#inhome-terminalScreenParagraph1').show();
	    } else {
		$('#inhome-terminalScreenParagraph1').hide();
	    }
	    var paragraph2 = screenParameters['paragraph-2'];
	    if (paragraph2) {
		updateParagraph(paragraph2, "#inhome-terminalScreenParagraph2", zipCode)
		$('#inhome-terminalScreenParagraph2').show();
	    } else {
		$('#inhome-terminalScreenParagraph2').hide();
	    }
	    var showDownloadButtons = screenParameters['showDownloadButtons'];
	    if (showDownloadButtons) {
		$('.inhome-app-store-download-group').show()
	    } else {
		$('.inhome-app-store-download-group').hide()
	    }
	    var terminalScreenCTAButton = screenParameters['ctaButton-title'];
	    var buttonCTACollection = getButtonCollection('#inhome-terminalScreenCTAButton');
	    if (terminalScreenCTAButton) {
		buttonCTACollection.text(escapeHtml(terminalScreenCTAButton))
		buttonCTACollection.show()
		currentCTAButtonAction = screenParameters['ctaAction'];
	    } else {
		buttonCTACollection.hide()
		currentCTAButtonAction = null;
	    }
	    $('#inhome-gdoTerminalScreenSection').show();
	    $('#inhome-checkProgramEligibilityForm').hide();
	}
   
        $('#inhome-terminalScreenCTAButton').click(function (event) {
            event.preventDefault();
	    if (currentCTAButtonAction) {
		currentCTAButtonAction();
	    }
	});

	$('#inhome-seeExamplesLink').click(function (event) {
	    event.preventDefault();
            var $trigger = $('#inhome-seeExamplesLink');
            var modalWidth = "600";
            var options = {
                "inlineContent": $("#a-popover-seeExamplesModal").html(),
                "width": modalWidth,
                "popoverLabel": stringHash['inhome-widget-gdo-check-your-garage-door-see-examples-modal-title'],
                "name": "seeExamplesModal",
                "hideHeader": true
            };
            modal.create($trigger, options);
	});

	
    });
</script>
    
