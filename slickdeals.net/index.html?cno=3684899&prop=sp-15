<script type="text/javascript">
$.validate({modules : 'logic,toggleDisabled', showErrorDialogs:false});
/* Trigger Bootstrap*/
ddoInit();
/* Custom  */

    $("#catalogOptIn").change(function(){
        if(document.getElementById('catalogOptIn').checked){
            $("#catalogInfo input").prop("disabled",false);
            $("#catalogInfo select").prop("disabled",false);
            $("#catalogInfo").fadeTo('slow',1);
        }
        else{
            $("#catalogInfo").fadeTo('slow',0.5,function(){
                $("#catalogInfo input").prop("disabled",true);
                $("#catalogInfo select").prop("disabled",true);
            });
            
        }
    });

</script>


