		<script type="text/javascript">
					(function(data){
					    if(data){
					      var helper = (function(config){
					              var csstmp ='<link rel="stylesheet" href="url"  crossorigin="anonymous">';
					              //var jstmp = "<script type=\"text/javascript\" src=\"url\"><\/script>";
					              //var stash = new Array();
					              return {
					                css:function(link){
					                  for(var i in link){
					                      var csslink = csstmp.replace('url',config['commonWebResourcePath']+'/css'+link[i]['href']);
					                      if(link[i]['crossorigin']){
					                          csslink = csslink.replace('anonymous',link[i]['crossorigin']);
					                      }
					                      csslink = csslink.replace('crossorigin="anonymous"','');
					                      if(link[i]['rel']){
					                          csslink = csslink.replace('stylesheet',link[i]['rel']);
					                      }        
