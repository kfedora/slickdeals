         role="dialog">
        
            <div class="field field--name-field-nir-person-headshot field--type-image field--label-hidden field__item">    <picture>
                  <source srcset="/system/files-encrypted/styles/nir_person_large_bio_pic/encrypt/nasdaq_kms/people/2019/08/20/13-27-04/Lisa%20K-%20Headshot.png?itok=a5yqCoPW 1x" media="screen and (min-width: 1px)" type="image/png"/>
