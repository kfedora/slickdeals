    <script>
  /**
  * The following uses user agent detection to ensure that
  * baseline polyfills that are only needed by IE11
  * are only loaded by IE11, synchronously. It will then load All.min.js
  * for both the evergreen browsers and IE11.
  */
  (function () {
    /**
      * Adds a script tag to the head of the document.
      *
      * @param url The url of the script to add
      * @param isNotAsync True If the script should NOT be loaded asynchronously.
      *   Dynamically created scripts are by default aync === true.
      */
    function addScript(url, isNotAsync) {
      var script = document.createElement('script');
      script.setAttribute('src', url);

      if (isNotAsync) {
        script.async = false
      } else {
        script.async = true
        script.setAttribute('async', true)
      }

      document.head.appendChild(script);
    }

    var ua = window.navigator.userAgent;
    var isTrident = ua.match('Trident/7.0');
    var isEdge = document.documentMode || /Edge/.test(ua);

    if (isTrident || isEdge) {
      addScript("https://cdn.corporate.walmart.com/resource/00000167-56f7-d8c5-af6f-f7f7d45c0000/styleguide/All.IE.min.677aecd886cb29dac9eb927caa2c5ce3.gz.js", true);
    }

    
      addScript("https://cdn.corporate.walmart.com/resource/00000167-56f7-d8c5-af6f-f7f7d45c0000/styleguide/All.min.8a6a162d9828c9cef3cfd1c543b8a2dc.gz.js", isTrident);
    
  })();
</script>

