<script>
var google_tag_paramsG = {};
window.addEventListener('load', function() {
if (window.location.pathname == "/") {
google_tag_paramsG = {
ecomm_pagetype: 'home'
};
} else if (window.digitalData.page.category.pageType == "Category") {
google_tag_paramsG = {
ecomm_pagetype: 'category'
};
} else if (window.digitalData.page.category.pageType == "Product Detail Page") {
google_tag_paramsG = {
ecomm_pagetype: 'product',
ecomm_prodid: window.digitalData.event.product[0].productInfo.productID,
ecomm_totalvalue: document.querySelector('span[itemprop="price"]').getAttribute('content')
}
} else if (window.digitalData.page.category.pageType == "Shopping Cart") {
var cartItems = [];
document.querySelectorAll('.mini-cart-product').forEach(function(item){
cartItems.push(item.getAttribute('data-pid'));
});
google_tag_paramsG = {
ecomm_pagetype: 'cart',
ecomm_prodid: cartItems,
ecomm_totalvalue: document.querySelector('.order-value').getAttribute('data-total')
};
} else {
google_tag_paramsG = {
ecomm_pagetype: 'other'
}
}
gtag('event', 'page_view', {
'send_to': 'AW-1001005254',
'ecomm_pagetype': google_tag_paramsG.ecomm_pagetype,
'ecomm_prodid': google_tag_paramsG.ecomm_prodid,
'ecomm_totalvalue': google_tag_paramsG.ecomm_totalvalue
});
});
</script>
