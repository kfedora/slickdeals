        <script>
        $(document).ready(function () {
            $('#BillingEditCountry').on('change', function () {
                if ($(this).val() == "CA") {
                    $('#BillingEditZipcode').attr("maxlength", 7);
                    $('#BillingEditZipcode').mask('AAA AAA');
                    $('#BillingEditZipcode').attr("data-mask", "AAA AAA");
                }
                else if ($(this).val() == "US") {
                    $('#BillingEditZipcode').attr("maxlength", 10);
                    $('#BillingEditZipcode').mask('00000-AAAA');
                    $('#BillingEditZipcode').attr("data-mask", "00000-AAAA");
                }
                else {
                    $('#BillingEditZipcode').attr("maxlength", 15);
                    var attr = $('#BillingEditZipcode').attr('data-mask');
                    if (typeof attr !== typeof undefined && attr !== false) {
                        $('#BillingEditZipcode').unmask();
                        $('#BillingEditZipcode').removeAttr("data-mask");
                    }
                }
            });

            $('#BillingCountry').on('change', function () {
                if ($(this).val() == "CA") {
                    $('#BillingZipcode').attr("maxlength", 7);
                    $('#BillingZipcode').mask('AAA AAA');
                    $('#BillingZipcode').attr("data-mask", "AAA AAA");
                }
                else if ($(this).val() == "US") {
                    $('#BillingZipcode').attr("maxlength", 10);
                    $('#BillingZipcode').mask('00000-AAAA');
                    $('#BillingZipcode').attr("data-mask", "00000-AAAA");
                }
                else {
                    $('#BillingZipcode').attr("maxlength", 15);
                    var attr = $('#Zipcode').attr('data-mask');
                    if (typeof attr !== typeof undefined && attr !== false) {
                        $('#BillingZipcode').unmask();
                        $('#BillingZipcode').removeAttr("data-mask");
                    }
                }
            });
        });




        $(function () {

            var url = window.location.pathname,
                urlRegExp = new RegExp(url.replace(/\/$/, '') + "$"); // create regexp to match current url pathname and remove trailing slash if present as it could collide with the link in navigation in case trailing slash wasn't present there
            // now grab every link from the navigation
            $("#myTab li").each(function () {
                // and test its normalized href against the url pathname regexp
                var a = $(this).find("a");
                if (urlRegExp.test(a.attr("href").replace(/\/$/, ''))) {
                    $(this).find("a").attr("role", "tab");
                    $(this).addClass('active');
                    $(this).prev().addClass("prev");
                    $(this).next().addClass("next");
                }
            });
        });
        if ($('#createreview').length > 0) {
            $('#createreview').validate({
                rules: {
                    'rating': {
                        required: true
                    },
                    'title': {
                        required: true
                    },
                    'description': {
                        required: true
                    }
                },
                messages: {
                    'rating': "This field is required",
                    'title': "This field is required",
                    'description': "This field is required"
                }
            });
        }
        if ($('#updateemailvalidate').length > 0) {
            $('#updateemailvalidate').validate();
        }
        if ($('#updatepasswordvalidate').length > 0) {
            $('#updatepasswordvalidate').validate({
                rules: {
                    'current_password': {
                        required: true,
                        minlength: 6
                    },
                    'password': {
                        required: true,
                        minlength: 6
                    },
                    'password2': {
                        required: true,
                        minlength: 6,
                        equalTo: "#passwordchec"
                    }
                }
            });
        }
        if ($('#updateusernamefrom').length > 0) {
            $('#updateusernamefrom').validate();
        }
        $(document).ready(function () {
            $("#addressform").submit(function () {
                $("#addaddress").show();
                $('#addressform').validate()
                if ($('#addressform').valid()) {
                    return true;
                }
                else {
                    return false;
                }
            })
            $('#addressform').validate({
                rules: {
                    'u_zip': {
                        required: true,
                        minlength: function () {
                            if ($('#addressform').find('[name="u_country"]').val() == "CA") {
                                $('#addressform').find('[name="u_zip"]').attr("placeholder", 900010).attr("maxlength", 7).attr("data-mask", "AAA AAA");
                                return 6;
                            }
                            else if ($('#addressform').find('[name="u_country"]').val() == "US") {
                                $('#addressform').find('[name="u_zip"]').attr("placeholder", 900010).attr("maxlength", 10).attr("data-mask", "00000-AAAA");
                                return 5;
                            }
                            else {
                                $('#addressform').find('[name="u_zip"]').attr("placeholder", 90001).attr("maxlength", 15).unmask();
                                return 0;
                            }
                        },
                        maxlength: function () {
                            if ($('#addressform').find('[name="u_country"]').val() == "CA") {
                                $('#addressform').find('[name="u_zip"]').attr("placeholder", 900010).attr("maxlength", 7).attr("data-mask", "AAA AAA");
                                return 7;
                            }
                            else if ($('#addressform').find('[name="u_country"]').val() == "US") {
                                $('#addressform').find('[name="u_zip"]').attr("placeholder", 90001).attr("maxlength", 10).attr("data-mask", "00000-AAAA");
                                return 10;
                            }
                            else {
                                $('#addressform').find('[name="u_zip"]').attr("placeholder", 90001).attr("maxlength", 15).unmask();
                                return 15;
                            }

                        }
                    },
                    u_phone: {
                        required: true,
                        minlength: 14,
                    },
                },
                messages: {
                    u_phone: {
                        required: "This field is required",
                        minlength: "Please enter 10 digits",
                    }
                }

            });
        });

        $('#newaddressform1').validate({
            rules: {
                'u_zip': {
                    required: true,
                    minlength: function () {
                        if ($('#newaddressform1').find('[name="u_country"]').val() == "CA") {
                            $('#newaddressform1').find('[name="u_zip"]').attr("placeholder", 900010).attr("maxlength", 7).attr("data-mask", "AAA AAA");
                            return 6;
                        }
                        else if ($('#newaddressform1').find('[name="u_country"]').val() == "US") {
                            $('#newaddressform1').find('[name="u_zip"]').attr("placeholder", 90001).attr("maxlength", 10).attr("data-mask", "00000-AAAA");
                            return 5;
                        }
                        else {
                            $('#newaddressform1').find('[name="u_zip"]').attr("placeholder", "").attr("maxlength", 15).unmask();
                            return 5;
                        }
                    },
                    maxlength: function () {
                        if ($('#newaddressform1').find('[name="u_country"]').val() == "CA") {
                            $('#newaddressform1').find('[name="u_zip"]').attr("placeholder", 900010).attr("maxlength", 7).attr("data-mask", "AAA AAA");
                            return 7;
                        }
                        else if ($('#newaddressform1').find('[name="u_country"]').val() == "US") {
                            $('#newaddressform1').find('[name="u_zip"]').attr("placeholder", 90001).attr("maxlength", 10).attr("data-mask", "00000-AAAA");
                            return 10;
                        }
                        else {
                            $('#newaddressform1').find('[name="u_zip"]').attr("placeholder", "").attr("maxlength", 15).unmask();
                            return 15;
                        }
                    }

                },
                u_phone: {
                    required: true,
                    minlength: 14,
                },
            },
            messages: {
                u_phone: {
                    required: "This field is required",
                    minlength: "Please enter 10 digits",
                }
            }
        });


        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode;
            var allowzero = false;
            if (parseInt($('#pagemove').val()) >= 1) {
                allowzero = true;
            }
            return (!(charCode > 31 && (charCode <= 48 || charCode > 57)) || (charCode == 48 && allowzero == true));
        }

        $(document).ready(function () {
            $('#addressform').find('[name="u_country"]').on('change', function () {
                if ($(this).val() == "CA") {
                    $('#addressform').find('[name="u_zip"]').attr("maxlength", 7);
                    $('#addressform').find('[name="u_zip"]').mask('AAA AAA');
                    $('#addressform').find('[name="u_zip"]').attr("data-mask", "AAA AAA");
                }
                else if ($(this).val() == "US") {
                    $('#addressform').find('[name="u_zip"]').removeAttr("maxlength");
                    $('#addressform').find('[name="u_zip"]').mask('00000-AAAA');
                    $('#addressform').find('[name="u_zip"]').attr("data-mask", "00000-AAAA");
                }
                else {
                    $('#addressform').find('[name="u_zip"]').attr("maxlength", 15);
                    var attr = $('#addressform').find('[name="u_zip"]').attr('data-mask');
                    if (typeof attr !== typeof undefined && attr !== false) {
                        $('#addressform').find('[name="u_zip"]').unmask();
                        $('#addressform').find('[name="u_zip"]').removeAttr("data-mask");
                    }
                }
            });
        });

        $(document).ready(function () {
            $('#newaddressform1').find('[name="u_country"]').on('change', function () {
                if ($(this).val() == "CA") {
                    $('#newaddressform1').find('[name="u_zip"]').attr("maxlength", 7).attr("placeholder", 900010);
                    $('#newaddressform1').find('[name="u_zip"]').mask('AAA AAA');
                    $('#newaddressform1').find('[name="u_zip"]').attr("data-mask", "AAA AAA");
                }
                else if ($(this).val() == "US") {
                    $('#newaddressform1').find('[name="u_zip"]').removeAttr("maxlength");
                    $('#newaddressform1').find('[name="u_zip"]').mask('00000-AAAA');
                    $('#newaddressform1').find('[name="u_zip"]').attr("data-mask", "00000-AAAA").attr("placeholder", 900010);
                }
                else {
                    $('#newaddressform1').find('[name="u_zip"]').attr("maxlength", 15);
                    var attr = $('#newaddressform1').find('[name="u_zip"]').attr('data-mask').attr("placeholder", 90001);
                    if (typeof attr !== typeof undefined && attr !== false) {
                        $('#newaddressform1').find('[name="u_zip"]').unmask();
                        $('#newaddressform1').find('[name="u_zip"]').removeAttr("data-mask");
                    }
                }
            });
        });

        $(document).ready(function () {
            $(".mp-modals").find("input").removeAttr("title");
            $(".zipchange").removeAttr("title");
            $(".mp-modals").find("select").removeAttr("title");
            $('#addressform').find('[name="u_zip"]').removeAttr("maxlength");
            $('#addressform').find('[name="u_zip"]').mask('00000-AAAA');
            $('#addressform').find('[name="u_zip"]').attr("data-mask", "00000-AAAA");
        });
        $(document).ready(function () {
            $(document).on("click", ".mp-cancel", function () {
                $("label.error").remove();
                $("input").removeClass("error");
                $("select").removeClass("error");
                $('#addressform').find('[name="u_zip"]').unmask();
                $('#addressform').find('[name="u_zip"]').mask('00000-AAAA');
                $('.zipcodemismatchbilllingupdate').hide();
                $('.zipcodemismatchbillling').hide();
                $('.zipcodemismatch').hide();

            });
        });
