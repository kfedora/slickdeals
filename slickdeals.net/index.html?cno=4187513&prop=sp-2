<script>
P.when('A','a-carousel-framework','ready').execute("carousel-page-state-cd48bee0-2679-44bb-88b8-e661bedc69e0-selector_gl_group_holiday_dash_pricing", function(A, framework){

    A.delay(function(){
      var data = {
          'html': A.$(".ped_carousel_cd48bee0-2679-44bb-88b8-e661bedc69e0_selector_gl_group_holiday_dash_pricing")[0].outerHTML.replace("a-carousel-initialized","a-carousel-static")
      };
      A.state("default-carousel-cd48bee0-2679-44bb-88b8-e661bedc69e0-selector_gl_group_holiday_dash_pricing", data);
    }, 500);
});

P.when("A", "a-carousel-framework", "jQuery").execute(function (A, framework, $) {
     A.on("a:tabs:cd48bee0-2679-44bb-88b8-e661bedc69e0_selector_gl_group_holiday_dash_pricing:select", function (data) {
      var CWSDiv = data.selectedTab.$tab.closest(".CarouselWithSelector");
      var selectorID = "selector_gl_group_holiday_dash_pricing";
      var spinner = CWSDiv.children(".a-spinner" ).first();
      var sel_error = CWSDiv.children(".ped_selector_error_message" ).first();
      var carousel = CWSDiv.children(".ped_carousel_cd48bee0-2679-44bb-88b8-e661bedc69e0_selector_gl_group_holiday_dash_pricing").first();
      var categoryID = data.selectedTab.tabName;
      var pageStateName = categoryID+"-"+ "cd48bee0-2679-44bb-88b8-e661bedc69e0"+ "-" + "selector_gl_group_holiday_dash_pricing";

      var numPlacements = "50";
      var pageType = "Events";
      var pageTypeID = "holidaydash";
      var slotName = "slot-16";
      var placementID = "cd48bee0-2679-44bb-88b8-e661bedc69e0";

      if (window.ue && window.ue.count){
         ue.count(categoryID, (ue.count(categoryID) || 0) + 1);
      }

      A.fadeOut(sel_error, 500, 'ease-out', function() {
         sel_error.css("display", "none");
      });

      if (A.state(pageStateName)) {
            A.fadeOut(carousel, 500, 'ease-out', function() {
              carousel.html(A.state(pageStateName)['html']);
               A.fadeIn(carousel, 500, 'ease-out', function() {
                 carousel.css("display", "block");
                 framework.createAll();
                 framework.initializeAll();
               });
            });
      } else {

           A.fadeOut(carousel, 500, 'ease-out', function() {
              carousel.css("display", "none");
              spinner.css("display", "block");
           });

           A.$.ajax({
               type : "get",
               contentType: "application/json; charset=utf-8",
               url: "/gp/prime/exclusive/selector",
               data: {
                 'categoryID': categoryID,
                 'selectorID': selectorID,
                 'numPlacements': numPlacements,
                 'pageType': pageType,
                 'pageTypeID': pageTypeID,
                 'slotName': slotName,
                 'placementID': placementID
               },
               success : function(response) {

                  A.fadeOut(carousel, 500, 'ease-out', function() {
                     spinner.css("display", "none");
                     carousel.html(response);
                     A.fadeIn(carousel, 500, 'ease-out', function() {
                       carousel.css("display", "block");
                     });
                     framework.createAll();
                  });

                  parser = new DOMParser();
                  dom = parser.parseFromString(response, "text/html");
                  var not_enough_page = dom.getElementsByClassName("ped_carousel_not_enough")[0];
                  if(not_enough_page == null){
                     var html = {
                      'html': response
                     };
                     A.state(pageStateName, html);
                  }
                },
               error: function(error) {

                  A.fadeIn(spinner, 500, 'ease-out', function() {
                     spinner.css("display", "none");
                     sel_error.css("display", "block");
                  });
               }
           });
      }
    });
});
</script>

