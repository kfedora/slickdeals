<script type="text/javascript">
	$(document).ready(function() {
//$("#subnav ul ul li").next().css('border-top', 'none');
// $("#subnav ul ul li").siblings(":last-child").css('border-bottom', '#FFF');

//Back to top button
$("#back-top").hide();
$(function () {
	$(window).scroll(function () {
		if ($(this).scrollTop() > 300 & !$('body').hasClass('mmenu-push-toleft')) {
			$('#back-top').fadeIn();
		} else {
			$('#back-top').fadeOut();
		}
	});
	$('#back-top a').click(function () {
		$('body,html').animate({
			scrollTop: 0
		}, 800);
		return false;
	});
});
// Responsive
$("#subnav h3").click(function(){
	$("#subnav h3").removeClass();
	$("#subnav h3").next().slideUp();
	if(!$(this).next().is(":visible"))
	{
		$(this).addClass('active');	
		$(this).next().slideDown();
	}
});

$("#subnav li").click(function(){
	window.location=$(this).find("a").attr("href"); 
	return false;
});

$("#tabs").tabs();
$(".ui-tabs").tabs();

});

</script>
