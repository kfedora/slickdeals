<script type="text/javascript">
(function ( pdc ){

	pdc.isLiveURL = function isLiveURL(hostString) {
		var prodList = pdc.liveURLs || [],
			host = ((hostString && typeof(hostString) === 'string') ? hostString : window.location.host),
			len = prodList.length, i;
		for (i = 0; i < len; i += 1) {
			if (prodList[i] === host) {
				return true;
			}
		}
		return false;
	};


	pdc.isUserConsentedCookie = function() {
		return !(PDC.Region.getRegion() == 'SCEE' && $.cookie('eucookiepreference') == 'rejected');
	}

	pdc.addProperty = function(obj, name, val){
			if (!obj && !name){
				return;
			}

			pdc[obj] = pdc[obj] || {};

			if(typeof val != "undefined" && val != null){
				if(!pdc[obj].hasOwnProperty(name) || !pdc[obj][name]){
					pdc[obj][name] = val;
				}
			}
	};

	function sortRatings(a,b) {
		a.ea=Number(a.ea);
		b.ea=Number(b.ea);
		a.i=Number(a.i);
		b.i=Number(b.i);
		return (a.ea != b.ea ? a.ea-b.ea : a.i-b.i);
	}

	pdc.addProperty('Header', 'PostLoginAjaxUrl', '');
	pdc.addProperty('Header', 'AgeGateThreshold', '13');
	pdc.addProperty('Header', 'DateFormat', 'dd.MM.yyyy');
	pdc.addProperty('Header', 'MobileSignInAndConnectUrl', '');
	pdc.addProperty('Header', 'MyPlaystationLoginUrl', '//my.playstation.com');
	pdc.addProperty('Header', 'MyPlaystationCookieDecryptionUrl', '//my.playstation.com/playstation/psn/decrypt');
	pdc.addProperty('GameTile', 'DefaultImage', '//psmedia.playstation.com/is/image/psmedia/GameTile_Default_02Jun14?$GameTile_Small$');
	pdc.addProperty('Rating','PRS',{"tags":[{"name":"12plus-legacy","ea":"12","i":1},{"name":"16plus-legacy","ea":"16","i":2},{"name":"6plus","ea":"6","i":3},{"name":"16plus","ea":"16","i":4},{"name":"12plus","ea":"12","i":5},{"name":"3plus","ea":"3","i":6},{"name":"18plus","ea":"18","i":7}]});
	pdc.addProperty('Rating','SRS',{"tags":[]});

	if (pdc.Rating.PRS.tags instanceof Array) pdc.Rating.PRS = pdc.Rating.PRS.tags.sort(sortRatings);
	if (pdc.Rating.SRS.tags instanceof Array) pdc.Rating.SRS = pdc.Rating.SRS.tags.sort(sortRatings);

})(PDC);
</script>

