	PDC.funcQueue.executeQueue();
	const releaseDate = $('.releasedate');

	if (releaseDate) releaseDate.releaseDate();

	$(document).ready(function () {
		checkCM168Label();
	});

	function checkCM168Label() {
		try {
			$(".CM168-label").each(function () {
				if ($(this).text().trim().length == 0) {
					$(this).hide();
				}
			});
		} catch (e) {
			//console.log(e);
		}

	}
</script>


<script>
	(function (d, id) {
		if (document.getElementsByClassName('spirablevideo').length > 0) {
			var js, h = d.getElementsByTagName('head')[0];
			if (d.getElementById(id)) {
				return;
			}
			js = d.createElement('script');
			js.id = id;
			js.onload = function () {
				bootstrap();
			};
			js.src = "https://psvm.co/static/js/ps-bootstrap.min.js ";
			h.appendChild(js);
		}
	}(document, 'ps-bootstrap'));
</script>




