<script type="text/javascript">
	(function () {
		var gdprAppliesGlobally = false;

		function addFrame() {
			if (!window.frames['__cmpLocator']) {
				if (document.body) {
					var body = document.body,
						iframe = document.createElement('iframe');
					iframe.style.cssText = 'display:none';
					iframe.name = '__cmpLocator';
					body.appendChild(iframe);
				} else {
					// In the case where this stub is located in the head,
					// this allows us to inject the iframe more quickly than
					// relying on DOMContentLoaded or other events.
					setTimeout(addFrame, 5);
				}
			}
		}

		addFrame();

		function stubCMP() {
			var args = arguments;
			// console.log('stubCMP', args);
			__cmp.a = __cmp.a || [];

			if (!args.length) return __cmp.a;
			else if (args[0] === 'ping') {
				args[2]({
					"gdprAppliesGlobally": gdprAppliesGlobally,
					"cmpLoaded": false
				}, true);
			}
			else {
				__cmp.a.push([].slice.apply(args));
			}
		}

		function cmpMsgHandler(event) {
			var msgIsString = typeof event.data === 'string';
			var json = event.data;
			if (typeof json !== 'object') {
				try { json = JSON.parse(json); }
				catch (err) { return; }
			}
			if (!json.__cmpCall) return;

			var call = json.__cmpCall;
			window.__cmp(call.command, call.parameter, function (retValue, success) {
				var returnMsg = {
					"__cmpReturn": {
						"returnValue": retValue,
						"success": success,
						"callId": call.callId
					}
				};

				event.source.postMessage(msgIsString ?
					JSON.stringify(returnMsg) : returnMsg, '*');
			});
		}

		if (typeof (__cmp) !== 'function') {
			window.__cmp = stubCMP;
			__cmp.msgHandler = cmpMsgHandler;

			if (window.addEventListener)
				window.addEventListener('message', cmpMsgHandler, false);
			else
				window.attachEvent('onmessage', cmpMsgHandler);
		}
	})();
</script>


<script type="text/javascript">
	(function (id) {
		function append(scriptid, url, async) {
			var d = document, sn = 'script', f = d.getElementsByTagName(sn)[0];
			if (!f) f = d.head;
			var s = d.createElement(sn);
			s.async = async;
			s.id = scriptid;
			s.src = url;
			s.charset = 'utf-8';
			f.parentNode.insertBefore(s, f);
		}

		function is2parttld(value) {
			var tldindicators = ['co', 'com', 'info', 'web', 'info', 'gov', 'edu', 'biz', 'net', 'org'];
			var countryindicators = ['uk', 'us', 'fr', 'es', 'de', 'at', 'au', 'ae', 'be', 'br', 'ca', 'ch', 'cn', 'co', 'cz', 'dk', 'eg', 'eu', 'fi', 'gb', 'gr', 'hk', 'hr', 'hu', 'ie', 'in', 'jp', 'mx', 'nl', 'no', 'nz', 'pl', 'ro', 'ru', 'se'];
			return (tldindicators.indexOf(value) !== -1 || countryindicators.indexOf(value) !== -1);
		}

		function getRootDomain() {
			var parts = window.location.hostname.split('.');
			if (parts.length === 2) rootDomain = parts[0];
			else if (parts.length > 2) {
				// see if the next to last value is a common tld
				var part = parts[parts.length - 2];
				if (is2parttld(part)) {
					rootDomain = parts[parts.length - 3]; // go back one more
				}
				else {
					rootDomain = part;
				}
			}

			return rootDomain;
		}

		window.evidon = {};
		window.evidon.id = id;
		window.evidon.test = false;  // set to true for non-production testing.
		//window.evidon.userid = '';

		var cdn = '//c.evidon.com/', rootDomain = getRootDomain(), noticecdn = cdn + 'sitenotice/';
		append('evidon-notice', noticecdn + 'evidon-sitenotice-tag.js', false);
		append('evidon-location', cdn + 'geo/country.js', true);
		append('evidon-themes', noticecdn + id + '/snthemes.js', true);
		if (rootDomain) append('evidon-settings', noticecdn + id + '/' + rootDomain + (window.evidon.test ? '/test' : '') + '/settings.js', true);

		const setCookie = (name, value, domain, days) => {
			var Days = 30;
			if (days) {
				Days = days;
			}
			var today = new Date();
			var updatedDate = today.getTime() + (Days * 24 * 60 * 60 * 1000);
			var expiry = new Date(updatedDate).toGMTString();
			var cookie = name + "=" + escape(value) + ";expires=" + expiry + ";path=/";
			if (domain) {
				cookie += ";domain=" + domain;
			}
			document.cookie = cookie;
		};

		const cookieProperties = {
			LABELS: {
				EU_COOKIE: 'eucookiepreference',
				SAT_TRACK: 'sat_track',
				PREFERENCE_ACCEPT: 'accept',
				PREFERENCE_REJECT: 'reject',
				DTM_TRUE: true,
				DTM_FALSE: false,
			},
			DOMAIN: (document.location.href.indexOf('.playstation.com') > 0) ? '.playstation.com' : null, //if domain is not playstation, set to null so works on localhost
			COOKIE_AGE: 3650
		};

		window.evidon.priorConsentCallback = function (categories, vendors, cookies) {
			// add the tags which need to wait for prior consent
			// here.  This should be all your advertising tags and
			// probably most of your social and tracking tags.

			setCookie(cookieProperties.LABELS.EU_COOKIE,
				cookieProperties.LABELS.PREFERENCE_ACCEPT,
				cookieProperties.DOMAIN,
				cookieProperties.COOKIE_AGE);

			setCookie(cookieProperties.LABELS.SAT_TRACK,
				cookieProperties.LABELS.DTM_TRUE,
				cookieProperties.DOMAIN,
				cookieProperties.COOKIE_AGE);
		};

		window.evidon.closeCallback = function () {
			// this is executed if the user closed a UI element without either Accepting (providing consent)
			// or Declining (declining to provide consent).
		};

		window.evidon.consentWithdrawnCallback = function () {
			// this is exeucted if the user withdraws consent and elects to
			// no longer allow technologies to run on the site.
			setCookie(cookieProperties.LABELS.EU_COOKIE,
				cookieProperties.LABELS.PREFERENCE_REJECT,
				cookieProperties.DOMAIN,
				cookieProperties.COOKIE_AGE);

			setCookie(cookieProperties.LABELS.SAT_TRACK,
				cookieProperties.LABELS.DTM_FALSE,
				cookieProperties.DOMAIN,
				cookieProperties.COOKIE_AGE);
		};

		window.evidon.consentDeclinedCallback = function () {
			// this is executed if the user explicitly declines giving consent by
			// using a Decline button
			setCookie(cookieProperties.LABELS.EU_COOKIE,
				cookieProperties.LABELS.PREFERENCE_REJECT,
				cookieProperties.DOMAIN,
				cookieProperties.COOKIE_AGE);

			setCookie(cookieProperties.LABELS.SAT_TRACK,
				cookieProperties.LABELS.DTM_FALSE,
				cookieProperties.DOMAIN,
				cookieProperties.COOKIE_AGE);
		}
	})(6063);
</script>
	













<script type="text/javascript">

    /*
     *  Set's 'sat_track' cookie before DTM analytics initialise
     */
    (function (){

        Date.prototype.addDays = function(days) {
            var date = new Date(this.valueOf());
            date.setDate(date.getDate() + days);
            return date;
        };

        // Set Adobe DTM 'sat_track' cookie to true if does not exist. track by default
        if(document.cookie.split("sat_track=").length < 2) {
            // If domain is not playstation, set to localhost so it works on localhost
            var domain = (document.location.href.indexOf('.playstation.com') > 0) ? ".playstation.com" : "localhost",
                date = new Date();

            document.cookie = "sat_track=true; expires=" + date.addDays(3650) + "; domain=" + domain + "; path=/";
        }

    })();

</script>





	
















	
	

	
	
