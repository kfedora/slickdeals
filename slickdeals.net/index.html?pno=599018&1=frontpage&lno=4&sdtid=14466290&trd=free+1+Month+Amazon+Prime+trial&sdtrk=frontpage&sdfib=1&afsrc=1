<script type="text/javascript">
    P.when('A', 'ready').register('sticky-nav', function(A){
        var $ = A.$;
        var isCardsExpanded = false;
        function adjustFixedPosition(element) {
            var navHeight = 154;
            var scrollDist = window.pageYOffset;
            if(scrollDist > navHeight) {
                element.css("position", "fixed");
            } else {
                element.css("position", "absolute");
            }
        }
        
        function isOnScreen(element) {
            var distFromTop = $(element).offset().top + $(element).height();
            var scrollDist = window.pageYOffset;
            return distFromTop > scrollDist;
        }

        var isButtonOnScreen = isOnScreen('#prime-header-CTA');
        var isCardsTableOpen = false;
        
        var scrollHandler = function() {
            if(!isCardsTableOpen) {
                if(isOnScreen('#prime-header-CTA') != isButtonOnScreen) {
                    A.slideToggle($('#sticky-nav-header-wrapper'), 500, 'ease-out', function(){});
                    isButtonOnScreen = !isButtonOnScreen;
                }
            }
            var stickyNavBorderWrapper = $('#sticky-nav-border-wrapper');
            adjustFixedPosition(stickyNavBorderWrapper);
        }
        
        A.on('scroll', scrollHandler);
        window.onload = window.onpageshow = function() {
            var isButtonOnScreen = isOnScreen('#prime-header-CTA');
            scrollHandler();
        };

        function toggleAlignment($element) {
            var logowidth = $element.css( "justify-content" );
            if(logowidth === "center") {
                $element.css( "justify-content", "space-between");
            } else {
                $element.css( "justify-content", "center");
            }
        }
        
        function toggleColor($element) {
            var backgroundColor = $element.css( "background-color" );
            if(backgroundColor === "rgb(35, 47, 62)" || backgroundColor === "") {
                $element.css( "background-color", "white" );
            } else {
                $element.css( "background-color", "#232f3e" );
            }
        }
        
        var stickyNavToggle = function() {
            var $stickyNavHeader = $("#sticky-nav-header");
            toggleColor($stickyNavHeader);
            A.slideToggle($('#hidden-content'), 500, 'ease-out', function(){});
            isCardsTableOpen = !isCardsTableOpen;
            A.fadeToggle($('.plan-expander-icon, .see-more-plans, #sticky-nav-CTA, .sticky-nav-prime-logo, #sticky-nav-prime-logo'), 0, 'ease-out', function() {
            });
            A.fadeToggle($('.plan-collapser-icon'), 0, 'ease-out', function() {
            });
            toggleAlignment($stickyNavHeader);
            if(isOnScreen('#prime-header-CTA') != isButtonOnScreen) {
                A.slideToggle($('#sticky-nav-header-wrapper'), 500, 'ease-out', function(){});
                isButtonOnScreen = !isButtonOnScreen;
            }
            if(isCardsTableOpen) {
                $("#sticky-nav-close-button").focus();
            }
        }
        
        var stickyNavOpenCloseAnywhere = function() {
            A.slideToggle($('#sticky-nav-header-wrapper'), 500, 'ease-out', function(){});
            isButtonOnScreen = !isButtonOnScreen;
            var $stickyNavHeader = $("#sticky-nav-header");
            toggleColor($stickyNavHeader);
            isCardsTableOpen = !isCardsTableOpen;
            A.slideToggle($('#hidden-content'), 500, 'ease-out', function(){});
            A.fadeToggle($('.plan-expander-icon, .see-more-plans, #sticky-nav-CTA, .sticky-nav-prime-logo, #sticky-nav-prime-logo'), 0, 'ease-out', function() {
            });
            A.fadeToggle($('.plan-collapser-icon'), 0, 'ease-out', function() {
            });
            toggleAlignment($stickyNavHeader);
            var stickyNavBorderWrapper = $('#sticky-nav-border-wrapper');
            adjustFixedPosition(stickyNavBorderWrapper);
            if(isCardsTableOpen) {
                $("#sticky-nav-close-button").focus();
            }
        }
        
        $('#expand-membership-plans').delegate("div", "click", stickyNavToggle);
        $('#see-more-plans-text').delegate("a", "click", stickyNavOpenCloseAnywhere);
        $('#see-more-plans-text').delegate("span", "click", stickyNavOpenCloseAnywhere);
        $('#see-more-plans-text').delegate(".click-to-expand-plans", "click", stickyNavOpenCloseAnywhere);
        $('#see-more-plans-text span').bind("keydown", function(e){
            var keycode = e.which; 
            if(keycode==13){
                stickyNavOpenCloseAnywhere();
            }
        })
        $('#see-more-plans-text .click-to-expand-plans').bind("keydown", function(e){
            var keycode = e.which; 
            if(keycode==13){
                stickyNavOpenCloseAnywhere();
            }
        })
        $('#plan-card-comparison .close-button').delegate('#sticky-nav-close-button', "click", stickyNavToggle);
        $('#sticky-nav-close-button').bind("keydown", function(e){
            var keycode = e.which; 
            if(keycode==13){
                stickyNavToggle();
            }
        })
    });
</script>




