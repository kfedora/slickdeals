<script>
    $(document).ready(function () {
        var trackingObj = sl.sc.tracking.getTrackingCookieData();
        sl.sc.tracking.trackNUEPBrowseStores(trackingObj);

        $('.action-mobile-store-select').click(function (e) {
            trackingObj = {
                sl_navorigin: $(this).attr("data-tracking-origin"),
                sl_navmethod: $(this).attr("data-tracking-method"),
                //sl_navorigin: 'nuep',
                //sl_navmethod: 'click',
                isSneakPeek: 'False'
            };

            sl.sc.tracking.setTrackingCookie(trackingObj);
            //sl.sc.tracking.trackEntry();
        });

        // clicking anywhere in the container will redirect to the landing page
        // allow the phone numbers to be clicked to trigger a phone call
        $('.storeLocTile p, .storeLocTile .action-tracking-alternative-ad').not('.storePhone').on('click', function (e) {
            e.preventDefault();

            window.location = $(e.currentTarget).closest('.storeLocTile').attr('data-store-url');
        });
    });
</script>                <div class="cb"></div>
            </div>
            <!--end of otherStoresWrap -->
    </div>
    <div class="cb"></div>
<script>
    $(document).ready(function () {        
        
            // Check to see if they want more stores
            $('#action-alternative-location').submit(function() {
                // tracking values for trackNUEPBrowseStores
                var data = {
                    sl_navorigin: $("#action-alternative-location").attr("data-tracking-origin"),
                    sl_navmethod:  $("#action-alternative-location").attr("data-tracking-method"),
                    isSneakPeek: 'False'
                }
                sl.sc.tracking.setTrackingCookie(data);
            });

            $(".action-mapcitystatezip-submit").on("mouseup touchend", function(e){
                $("#action-alternative-location").attr("data-tracking-method", "button")
            });
        
        var $inputBox, textValue;
        $("#NUEPStoreListInput").on('click keypress', function (e) {
            $inputBox = $(e.currentTarget);
            textValue = $inputBox.val().toLowerCase();
            
            if (textValue === 'enter your zip code or city, state') {
                $inputBox.val('');
            }
        }).on('blur', function(e) {
            $inputBox = $(e.currentTarget);
            textValue = $inputBox.val().toLowerCase();
            
            if (!textValue) {
                $inputBox.val('Enter your Zip Code or City, State');
            }
        });

        $(".action-mobile-store-select").click(function () {
            var obj = {
                'sl_navorigin': 'nuep',
                'sl_navmethod': 'click'
            };
            
            sl.sc.tracking.setTrackingCookie(obj);
            //sl.sc.tracking.trackEntry();
        });
        
        // Map & Driving Directions handler
        $(".action-trackExternalDirectionsNuepList").die().live("click",function(e){
            e.preventDefault();

            sl.sc.event.dispatch('trackMapWtEvent', {
                sl_storeid: "2596033",
                sl_storename: "Ag Supply Ace Hardware 716 13th Ave SW-Bldg A Quincy, WA 98848",
                sl_dma:"Quincy, WA",
                sl_postalcode: "98848",
                sl_navorigin:$(e.target).attr("data-tracking-origin"),
                sl_navmethod:$(e.target).attr("data-tracking-method")
            });
            window.open($(e.target).attr("href"), '_blank');
        });
    });
</script>
        <script type="text/javascript">
            $(function () {
                sl.sc.event.dispatch(sl.sc.application.events.NUEP_RENDERED);
            });
        </script>

                
                <div class="dispNone overlay-listingdetail-videoContainer modalContainer videoModalContainer">
</div>
            </div>
            

            
        </div>

        
