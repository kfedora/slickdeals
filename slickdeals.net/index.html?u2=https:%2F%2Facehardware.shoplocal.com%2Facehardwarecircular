        <script type="text/javascript">

        $(document).ready(function () {
            // registering lists (cookie)
            sl.sc.shoppingList.registerLists([
                { name: "Acesl", type: sl.sc.shoppingList.LIST_TYPE_SHOPPING_LIST }
            ]);

            // list counts
            sl.sc.shoppingList.registerCountElements("Acesl", ['.action-mainCount']);
            sl.sc.shoppingList.registerCountElements("Acealt", ['.action-alternateCount']);

            // init
            if (!sl.sc.shoppingList.deferInit) {
                sl.sc.shoppingList.init();
            }

        });
        </script>



    
    



<script src="/Content/Scripts/ShopLocal/v2.0.0/sl.sc.tracking.js?v=10_24_2020" type="text/javascript"></script>
