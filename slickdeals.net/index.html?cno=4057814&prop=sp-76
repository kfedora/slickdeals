        var dataLayer = [{
  "facebook_app_id": "215160975166467",
  "google_analytics_id": "UA-65179481-1",
  "coremetrics_client_id": "90028889",
  "coremetrics_client_url": "ww62.hsn.com",
  "page_type": "Grid",
