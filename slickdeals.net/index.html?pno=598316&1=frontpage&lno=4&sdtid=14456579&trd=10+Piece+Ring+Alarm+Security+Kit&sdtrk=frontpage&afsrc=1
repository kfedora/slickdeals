<script type="text/javascript">
  $(window).ready(function() {
    
    function onClick(techBlock, infoBlock) {
      techBlock.toggleClass('on');
      this.toggleClass('on');
      infoBlock.slideToggle();
    }

    $('.category-block').each(function (index) {
      $this = $(this);
      $techBlock = $this.closest('.tech-block');
      $infoBlock = $techBlock.find('.info-block').first();
      
      // Binding clicks
      $this.click(onClick.bind($this, $techBlock, $infoBlock));

      // First Block Expanded is not needed now
      // if (index == 0) {
      //  $this.click();
      // }

    });
  });
</script>

