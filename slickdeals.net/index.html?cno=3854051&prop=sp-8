<script>
$(function () {
    $('#footer_signup').on('submit', function (e) {
        e.preventDefault()
        var $container = $('#footer_signup_container')
        var formData = new FormData(this)
        $container.fadeTo(600, 0.01)
        $.ajax('/signup', {
            processData: false,
            contentType: false,
            data: formData,
            type: 'POST',
            success: function (res) {
                console.log('SIGNUP DONE YAY')
                $container.html(res).fadeTo(400, 1)
            },
            headers: { 'X-CSRF-Token' : $('meta[name=csrf-token]').attr('content') },
        })
        return false;
    })
})
</script>

