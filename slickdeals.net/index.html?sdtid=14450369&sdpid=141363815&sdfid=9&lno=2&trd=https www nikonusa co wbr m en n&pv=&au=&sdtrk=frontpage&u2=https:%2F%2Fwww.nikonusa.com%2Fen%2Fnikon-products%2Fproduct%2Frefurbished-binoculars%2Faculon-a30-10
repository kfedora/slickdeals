            var siteProtocol = 'http:',
                siteUrl = siteProtocol + '//www.nikonusa.com',
                contactPointTel = '+1-800-645-6687';
            
                
                    nikon.async.add(function() {
                        var el = document.createElement('script'),
                            logoUrl = '';
                        if ( nikon.utils && nikon.utils.asset && nikon.utils.asset.get_cdn ) {
                            logoUrl = siteProtocol + nikon.utils.asset.get_cdn('/static/images/logos/logo_nikon.gif');
                        }

