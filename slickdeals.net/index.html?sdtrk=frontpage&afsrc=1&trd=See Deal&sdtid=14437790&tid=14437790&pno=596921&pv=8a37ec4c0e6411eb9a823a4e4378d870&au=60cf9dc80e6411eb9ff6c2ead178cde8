<script>
    (function(){
        var $basket = $('#mini-cart'),
            retUrl = 'https://' + window.location.hostname + '/cart',
            brand = 'jbl';
        // Update all links that contain support.jbl.com 
        $(document).on('ready ajaxComplete', function(){
            var basketItems = 0;
            switch(brand){
                case 'ha': {
                    basketItems = $('.mini-cart-total span:first', $basket).text();
                }; break;
                default: basketItems = $basket.find('.product_quantity .products_number').text().replace(/\s/g, '');
            }       
            $('a[href*="support.jbl.com"]').each(function(){
                var href = $(this).attr('href');
                href = updateQueryString('basketContents', basketItems, href);
                href = updateQueryString('basketUrl', retUrl, href);
                $(this).attr('href', href);
            });
        });   
        function updateQueryString(key, value, url) {
            if (!url) url = window.location.href;
            var re = new RegExp("([?&])" + key + "=.*?(&|#|$)(.*)", "gi"),
                hash;
            if (re.test(url)) {
                if (typeof value !== 'undefined' && value !== null) {
                    return url.replace(re, '$1' + key + "=" + value + '$2$3');
                } 
                else {
                    hash = url.split('#');
                    url = hash[0].replace(re, '$1$3').replace(/(&|\?)$/, '');
                    if (typeof hash[1] !== 'undefined' && hash[1] !== null) {
                        url += '#' + hash[1];
                    }
                    return url;
                }
            }
            else {
                if (typeof value !== 'undefined' && value !== null) {
                    var separator = url.indexOf('?') !== -1 ? '&' : '?';
                    hash = url.split('#');
                    url = hash[0] + separator + key + '=' + value;
                    if (typeof hash[1] !== 'undefined' && hash[1] !== null) {
                        url += '#' + hash[1];
                    }
                    return url;
                }
                else {
                    return url;
                }
            }
        }
    })();
</script>
