<!-- This script truncates the text to 80 characters -->
<script>
   (function() {
   /**
    * This code snippet truncates the text to a maximum length of 80 characters.
    * The maximum length can be adjusted by editing the maxTextLength default or
    * setting a data-length attribute on a DOM Node.
    * 
    * Use a class class of "full-text" to avoid truncation.
    *
    * This should be added to the end of every template.
    *
    * @param {DOMNode} element - The element link as a native DOM element
    * @param {Number} maxTextLength
    */
   function truncateText(element, maxTextLength) {
     var truncatedEl= element.innerText.trim().substring(0, maxTextLength) + '...';
     $(element).html(truncatedEl)
   }

   var elements = $('.dwa-landing a');

   for (var i = 0; i < elements.length; i++) {
     var el = elements[i];
     var textLength = el.innerText.length;
     var maxTextLength = parseInt(el.getAttribute('data-length')) || 81;
     // check that the text being trucated is not required
     var isSnipped = /full-text/.test(el.className);
     if (textLength > maxTextLength && !isSnipped) {
       truncateText(el, maxTextLength);
     }
   }
 })();
</script>

<script>
  (function () {
    /**
     * This code snippet truncates the text to a maximum length of 80 characters.
     * The maximum length can be adjusted by editing the maxTextLength default or
     * setting a data-length attribute on a DOM Node.
     *
     * This should be added to the end of every template.
     *
     * @param {DOMNode} element - The element link as a native DOM element
     */
    function truncateText(element) {
      var maxTextLength = parseInt(element.getAttribute('data-length')) || 51;
      var truncatedEl = element.innerText.trim().substring(0, maxTextLength) + '...';
      $(element).html(truncatedEl)
    }

    var elements = $('.shortCustom a');

    for (var i = 0; i < elements.length; i++) {
      var el = elements[i];
      var textLength = el.innerText.length;
      // check that the text being trucated is not required
      var isSnipped = /full-text/.test(el.className);
      if (textLength > 50 && !isSnipped) {
        truncateText(elements[i]);
      }
    }
  })();
</script></div>
