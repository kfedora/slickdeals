    //PDF Tracking catch-all
    document.addEventListener("DOMContentLoaded", function(e) {
        var ls = document.getElementsByTagName("a");
        for(i = 0; i < ls.length; ++i){
            l = ls[i];
            if(!c(l) && l.href.substr(l.href.length - 4).toLowerCase() == ".pdf"){
                l.addEventListener("click", function(e){
                    // LEGACY GA
                    _gaq.push(['_trackEvent', 'Amtrak.com - PDF', 'Download PDF - Generic', e.target.href]);
                    // NEW GA
                    //ga('send', 'event', 'Amtrak.com - PDF', 'Download PDF - Generic', e.target.href);
                });
            }

            if(c(l)){
                (function () {
                    var ct;
                    var ev;
                    var lb;
                    if(l.hasAttribute("data-ga-category")){
                        ct = l.dataset.gaCategory;
                        var r = /\\u([\d\w]{4})/gi;
                        ct = ct.replace(r, function (match, grp) {
                            return String.fromCharCode(parseInt(grp, 16)); } );
                        ct = unescape(ct);
                    }
                    else {
                        ct = "Amtrak.com - Event";
                    }

                    if(l.hasAttribute("data-ga-event")) {
                        ev = l.dataset.gaEvent;
                        var r = /\\u([\d\w]{4})/gi;
                        ev = ev.replace(r, function (match, grp) {
                            return String.fromCharCode(parseInt(grp, 16)); } );
                        ev = unescape(ev);
                    }
                    else {
                        ev = "Link Click"; 
                    }

                    if(l.hasAttribute("data-ga-label")) {
                        lb = l.dataset.gaLabel;
                        var r = /\\u([\d\w]{4})/gi;
                        lb = lb.replace(r, function (match, grp) {
                            return String.fromCharCode(parseInt(grp, 16)); } );
                        lb = unescape(lb);
                    }
                    else {
                        lb = l.href;
                    }
                    l.addEventListener("click", function(e){
                        // LEGACY GA
                        _gaq.push(['_trackEvent', ct, ev, lb]);
                        // NEW GA
                        //ga('send', 'event', ct, ev, lb);
                    });
                }());
            }
        }
    });

    function c(l){
        if (l.hasAttribute("data-ga") || 
            l.hasAttribute("data-ga-category") || 
            l.hasAttribute("data-ga-event") ||
            l.hasAttribute("data-ga-label")){

            return true;
        }
        return false;
    }

