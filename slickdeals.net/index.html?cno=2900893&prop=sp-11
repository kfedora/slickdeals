<script>
    if( typeof(marketingDigitalData) == "undefined" ) {
        var marketingDigitalData = {"page":{"pageInfo":{"pageID":"/content/t-mobile/consumer/offers/free-cell-phone-with-plan","templateName":"marketing-page-template","pageName":"free-cell-phone-with-plan","pageTitle":"Get a Free Cell Phone When You Switch to a T-Mobile Plan or Add a Line","version":"","pageURL":"","language":"en_US","variant":"","currencyCode":"USD","TFN":{}},"category":{"subCategory":""},"modules":{"moduleCount":0,"moduleList":[],"moduleListAll":[]}}};
    }
    marketingDigitalData.page.pageInfo.pageURL = window.location.href;
    marketingDigitalData.invoca = marketingDigitalData.invoca || {};
    marketingDigitalData.invoca.fireTag = "T";
    marketingDigitalData.invoca.numberSelector = ".tmo_tfn_number";
    
    if( typeof(marketingTMOPersonalize) == "undefined" ) {
        var marketingTMOPersonalize = {};
