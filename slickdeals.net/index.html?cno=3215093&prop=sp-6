    var accordion = new Foundation.Accordion($("#footer-accordion"), {});
</script><script>
    $(".row-mainlinks .tile-content").off("keypress").on("keypress", function (e) {
        if (e.which == 13) {
            $(this).click();
        }
    });
</script><script>
    FL.cart.decrementQty = function (t, n, e) {
        var a = $("#quantity_form").find("input[name='" + n + "']"),
            i = parseInt(a.val());
        1 !== i ? (i-- , a.val(i), $("#cart_form").find("input[name='" + n + "']").val(i), t.find(".js-qty").text(i),
            FL
                .Omniture.add({
                    linkTrackVars: "events,products",
                    linkTrackEvents: "scRemove",
                    events: "scRemove",
                    products: ";" + e + ";1"
                }).sendMicro(this, "Shopping Cart: Checkout"), FL.form.submit("quantity_form",
                    "FL.cart.updateQuantityResponse")) : t.find(".js-icon-remove").click()
    }, FL.cart.incrementQty = function (t, n, e) {
        var a = parseInt(t.find(".js-max-qty").val()),
            i = $("#quantity_form").find("input[name='" + n + "']"),
            r = parseInt(i.val());
        if (r >= a) return $("#maxQuantityErrorModal").find(".contents").html("This item is limited to " + a +
            " per customer."), void $("#maxQuantityErrorModal").foundation("open");
        r++ , i.val(r), $("#cart_form").find("input[name='" + n + "']").val(r), t.find(".js-qty").text(r), FL
            .Omniture
            .add({
                linkTrackVars: "events,products",
                linkTrackEvents: "scAdd",
                events: "scAdd",
                products: ";" + e + ";1"
            }).sendMicro(this, "Shopping Cart: Checkout"), FL.form.submit("quantity_form",
                "FL.cart.updateQuantityResponse")
    };
</script><script>
    if (FL.loyalty2Enable == "true") {
        $(".loyalty2-on").show();
        $(".loyalty2-off").hide();
    }
</script>
