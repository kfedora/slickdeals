<script>
    // Fix PR issue
    FL.reviews.setEnvData = function(){
        // this will check the env and return the API, merchant_group_id and merchant_id according to the env
        var server = location.hostname,
        flMerchantGroupId = '80655',
        flMerchantId = '992365',
        jdMerchantGroupId = '80682',
        jdMerchantId = '992365',
        flApiKeyProd = '73d2488a-06a9-4b39-81b4-8d2011eded2a',
        flApiKeyDev = '95d24f04-3a86-4aff-b7d9-bf3e7ab936eb',
        jdApiKeyProd = '90864d91-f833-41a9-bae3-c27fa4c81aa5',
        jdApiKeyDev = 'e2dca8a2-e071-4a1b-8433-db494d23e6e2';

        if (server === 'www.finishline.com') {
            // Are we on FL prod?
            api_key = flApiKeyProd,
            merchant_group_id = "80739",
            merchant_id = "88748";
        } else if (server === 'www.jdsports.com') {
            // Are we on JD prod?
            api_key = jdApiKeyProd,
            merchant_group_id = "80820",
            merchant_id = "498704";
        } else {
            // Check to see if we're on JD Dev
            if(FL.global.isJDSport()){
                api_key = jdApiKeyDev,
                merchant_group_id = jdMerchantGroupId,
                merchant_id = jdMerchantId;
            } else {
                api_key = flApiKeyDev,
                merchant_group_id = flMerchantGroupId,
                merchant_id = flMerchantId;
            }
        }

        return {
            api_key: api_key,
            locale: "en_US",
            merchant_group_id: merchant_group_id,
            merchant_id: merchant_id
        }
    };
</script><script>
  // Fix footer mobile issue
 setTimeout(function () {
    $(".footer-main .accordion .accordion-item:not(.no-accordion) .header-label, .footer-main .accordion .accordion-item:not(.no-accordion) span").unwrap();
    $(".footer-main .accordion-item:not(.no-accordion) .vertical-menu-header").wrap( '<a class="accordion-title" href="javascript:void(0)"></a>' );
    var accordion = new Foundation.Accordion($("#footer-accordion"), {});
  }, 1000);
</script>
