<script type="text/stache" id="FilterBarTemplate">
    <store-provider>
        <grid-page>
            <filter-bar>
                <action ($inserted)="bootstrap('filterData', 'sectionData')"/>
                {{#filterBar}}
                    {{#if settings.disableStickyElement}}
                        {{> FilterBar}}
                    {{else}}
                        {{> FilterBarStickyElement}}
                    {{/if}}
                {{/filterBar}}
                <div class="rs-wuipselect-filter-stickyplaceholder {{#if options.applied}}rs-wuipselect-filter-isapplied{{/if}}">
            </filter-bar>
        </grid-page>
    </store-provider>
</script>

<script type="text/stache" id="FilterBar">
        <div class="rs-wuipselect-filter-stickycontainer">
            {{#if settings.useOverlay}}
                {{>FilterBarOverlayTemplate}}
            {{else}}
                {{#if settings.expandable}}
                    {{>FilterBarExpandableTemplate}}
                {{else}}
                    {{>FilterBarStaticTemplate}}
                {{/if}}
            {{/if}}
        </div>
</script>

<script type="text/stache" id="FilterBarStickyElement">
    <sticky class="rs-wuipselect-filter-stickycontainer" data-min-height="#FilterTarget" data-no-css>
        {{#if settings.useOverlay}}
            {{>FilterBarOverlayTemplate}}
        {{else}}
            {{#if settings.expandable}}
                {{>FilterBarExpandableTemplate}}
            {{else}}
                {{>FilterBarStaticTemplate}}
            {{/if}}
        {{/if}}
    </div>
    </sticky>
</script>

<script type="text/stache" id="FilterBarExpandableTemplate">
<div class="rs-wuipselect-filter {{#if expanded}}rs-wuipselect-filter-isexpanded{{/if}} {{#if options.applied}}rs-wuipselect-filter-isapplied{{/if}} {{#if scrolled}}rs-wuipselect-filter-isscrolled{{/if}}"
        ($containerClickInside)="toggleExpanded('toggle', null, %event)" ($containerClickOutside)="toggleExpanded(false)" ($containerFocusOutside)="toggleExpanded(false)" data-container-events
        ($Esc)="toggleExpanded(false, '#filter-toggle')" data-key-event="Esc"
        ($documentScroll)="scrollFilterAction('#FilterTarget', '#filter-toggle', '.rs-wuipselect-filter-stickycontainer')" data-document-scroll>
    <div class="as-l-container as-l-container-fluid rs-wuipselect-filter-container">
        <div class="row" data-autom="filterContainer">
            <div class="rs-wuipselect-filter-buttons">
                <button class="rs-wuipselect-filter-toggle" data-autom="filterButton" id="filter-toggle" type="button" ($click)="toggleExpanded('toggle', null, %event)" aria-expanded="{{expanded}}" data-tracking-label="Selected">
                    <span class="rs-wuipselect-filter-toggleicon" aria-hidden="true"></span>
                    <span class="rs-wuipselect-filter-toggletext">Filter</span>
                </button>
                {{#options}}
                    {{#if applied}}
                        <div class="rs-wuipselect-filter-reset">
                            <button type="button" class="as-buttonlink" ($click)="clearFilters()" data-ase-trigger="click to focus #filter-toggle">Reset</button>
                        </div>
                    {{/if}}
                {{/options}}
            </div>

            {{#options}}
            {{#each items}}
                <fieldset class="rs-wuipselect-filter-option {{#if hasSwatches}}rs-wuipselect-filter-option-swatches{{/if}} rs-wuipselect-filter-{{dimensions.0}}">
                    <legend class="visuallyhidden">{{{filterLabel}}}{{#if appliedValues.length}}({{appliedValues.length}}){{/if}}</legend>
                    <div class="rs-wuipselect-filter-optiontitle" data-tracking-label="{{dimensions.0}}" aria-hidden="true">
                        {{{filterLabel}}}
                        {{#if appliedValues.length}}
                            <span class="rs-wuipselect-filter-appliedcount">
                                ({{appliedValues.length}})
                            </span>
                        {{/if}}
                    </div>

                    <materializer class="rs-wuipselect-filter-selectedvalue as-util-truncate" {data-shown}="filterBar.collapsed">
                    {{#if appliedValues.length}}
                        {{#if hasSwatches}}
                            {{#each appliedValues}}
                                {{#compare @index '<' 3}}
                                {{#swatchImage}}
                                <img src="{{srcSet.src}}" alt="{{value}}" width="{{width}}" height="{{height}}" class="rs-wuipselect-filter-selectedswatch">
                                {{/swatchImage}}
                                {{/compare}}
                            {{/each}}

                            {{#compare appliedValues.length '>' 3}}
                            &hellip;
                            {{/compare}}
                        {{else}}
                            {{#is appliedValues.length 1}}
                                {{#appliedValues}}{{{value}}}{{/appliedValues}}
                            {{else}}
                                {{{appliedValues.length}}} 
                            {{/is}}
                        {{/if}}
                    {{/if}}
                    </materializer>

                    <materializer class="rs-wuipselect-filter-valuesmaterializer" {data-shown}="filterBar.expanded" data-hide-height="0" data-show-height="computed">
                        <div class="rs-wuipselect-filter-values {{#if hasSwatches}}rs-wuipselect-filter-values-swatches{{/if}}">
                            {{>FilterValuesTemplate}}
                        </div>
                    </materializer>
                </fieldset>
            {{/each}}
            {{/options}}
        </div>
    </div>
</div>
</script>

<script type="text/stache" id="FilterBarStaticTemplate">
<div class="rs-wuipselect-filter">
    <div class="as-l-container">
        <div class="rs-wuipselect-filtertabnav-container">
            {{#options}}
            {{#each items}}
                <fieldset class="rs-wuipselect-filtertabnav-option">
                    <legend class="visuallyhidden">{{{filterLabel}}}</legend>
                    <div class="rs-wuipselect-filtertabnav-title" aria-hidden="true">{{{filterLabel}}}</div>

                    <ul class="rs-wuipselect-filtertabnav as-tabnav-tablist">
                        {{#defaultSelectionText}}
                        <li class="as-tabnav-tablist-item {{#unless applied}}as-tabnav-tablist-active{{/unless}}">
                            <input class="rs-wuipselect-filter-input {{#unless applied}}checked{{/unless}}" id="filter_{{id}}_default" type="radio" name="filter_{{id}}" ($change)="selectFilter(id, null)" {{#unless applied}}checked{{/unless}}/>
                            <label class="rs-wuipselect-filter-value as-tabnav-tablist-bluelink" for="filter_{{id}}_default" id="filterButton_{{id}}_default">{{{.}}}</label>
                        </li>
                        {{/defaultSelectionText}}
                        {{#each values}}
                        <li class="as-tabnav-tablist-item {{#if checked}}as-tabnav-tablist-active{{/if}}">
                            <input class="rs-wuipselect-filter-input {{#if checked}}checked{{/if}} {{#if disabled}}disabled{{/if}}" id="filter_{{id}}_{{key}}" type="radio" name="filter_{{id}}" ($change)="selectFilter(id, key)" {{#if checked}}checked{{/if}} {{#if disabled}}disabled{{/if}}/>
                            <label class="rs-wuipselect-filter-value as-tabnav-tablist-bluelink" for="filter_{{id}}_{{key}}" id="filterButton_{{id}}_{{key}}">{{{value}}}</label>
                        </li>
                        {{/each}}
                    </ul>

                    <span class="as-tabnav-tablist-navline" style="width:{{selectedElement.width}}px;transform:translateX({{selectedElement.offset}}px)"></span>
                </fieldset>
            {{/each}}
            {{/options}}
        </div>
    </div>
</div>
</script>

<script type="text/stache" id="FilterBarOverlayTemplate">
<div data-render="overlay"
        data-uid="FilterOverlay"
        data-sheet="true"
		data-has-screen="false"
		class="as-overlay as-overlay-fullscreen as-overlay-withscrim rs-wuipselect-filteroverlay"
                id="FilterOverlay"
        data-clickaway="false"
>

	<computed-height data-property="min-height" class="as-overlay-screen">

	    <div class="as-overlay-content" >
			        <div id="FilterOverlayContentContainer"></div>

	    </div>

	    
	    <button class="as-overlay-close ase-overlay-close"
			    data-ase-overlay="FilterOverlay"
                data-ase-click="hide">
            <i class="icon icon-close"></i>

                <span class="a11y">Close</span>
	    </button>

    </computed-height>

</div>
<div class="rs-wuipselect-filter" ($documentScroll)="scrollFilterAction(null, null, '.rs-wuipselect-filter-stickycontainer')" data-document-scroll>
    <div class="as-l-container as-l-container-fluid">
        <button class="rs-wuipselect-filter-toggle" type="button" ($click)="openOverlay('#FilterOverlay')" id="filter-toggle">
            <span class="rs-wuipselect-filter-toggleicon" aria-hidden="true"></span>
            <span class="rs-wuipselect-filter-toggletext">Filter</span>
        </button>
        {{#filterBar}}
            {{#options}}
                {{#if applied}}
                    <div class="rs-wuipselect-filter-reset">
                        <button type="button" class="as-buttonlink" ($click)="clearFilters()" data-ase-trigger="click to focus #filter-toggle">Reset</button>
                    </di>
                {{/if}}
            {{/options}}
        {{/filterBar}}
        <div class="rs-wuipselect-filter-viewtoggle">
            <span class="rs-wuipselect-view-label">View:</span>
            <div class="rs-wuipselect-filter-viewtoggle-buttons">
                <button ($click)="changeView('.rs-wuipselect-apollo', 'column')" aria-pressed="true" aria-label="Column View" class="rs-wuipselect-viewtoggle-1-up"></button>
                <span class="rs-wuipselect-divider"></span>
                <button ($click)="changeView('.rs-wuipselect-apollo', 'grid')" aria-pressed="false" aria-label="Grid View" class="rs-wuipselect-viewtoggle-2-up"></button>
            </div>
        </div>
    </div>
</div>
</script>

<script type="text/stache" id="FilterOverlayContentTemplate">
<store-provider>
    <grid-page>
        <filter-bar>
            <div class="rs-wuipselect-filter-content">
                {{#filterBar}}
                {{#options}}
                {{#if applied}}
                <div class="rs-wuipselect-filter-reset">
                    <button type="button" class="as-buttonlink" ($click)="clearFilters()" data-ase-trigger="click to focus #FilterOverlayDoneButton">Reset</button>
                </div>
                {{/if}}

                <button id="FilterOverlayDoneButton" type="button" class="button button-block rs-wuipselect-filteroverlay-done" data-ase-overlay="FilterOverlay" data-ase-click="hide">Done</button>

                <ul class="as-accordion-list rs-wuipselect-filter-accordion">
                    {{#each items}}
                    <li class="as-accordion-item {{#if expanded}}as-accordion-item-isexpanded{{/if}}">
                        <fieldset class="rs-wuipselect-filter-option {{#if hasSwatches}}rs-wuipselect-filter-option-swatches{{/if}}">
                            <legend class="as-accordion-header">
                                <button class="as-accordion-button" ($click)="toggleOptionExpanded(id, 'toggle', dimensions.0)" aria-controls="FilterAccordion-{{id}}" aria-expanded="{{expanded}}">
                                    <span class="as-accordion-title rs-wuipselect-filter-optiontitle">{{{filterLabel}}}</span>
                                    <span class="icon icon-plus as-accordion-plusicon" aria-hidden="true"></span>
                                </button>
                            </legend>
                            <materializer id="FilterAccordion-{{id}}" data-show-height="computed" data-hide-height="0" {data-shown}="expanded">
                                <div class="as-accordion-content">
                                    <div class="rs-wuipselect-filter-values {{#if hasSwatches}}rs-wuipselect-filter-values-swatches{{/if}}" tabindex="-1">
                                        {{>FilterValuesTemplate}}
                                    </div>
                                </div>
                            </materializer>
                        </fieldset>
                    {{/each}}
                </ul>
                {{/options}}
                {{/filterBar}}
            </div>
        </filter-bar>
    </grid-page>
</store-provider>
</script>

<script type="text/stache" id="FilterValuesTemplate">
{{#if hasSwatches}}
    <div class="colornav">
        <ul class="colornav-items">
            {{#defaultSelectionText}}
            <li class="colornav-item">
                <input class="rs-wuipselect-filter-input rs-wuipselect-filter-input-swatch" id="filter_{{id}}_default" type="checkbox" name="filter_{{id}}" ($change)="toggleFilter(id, null)" {{#unless applied}}checked{{/unless}}>
                <label class="rs-wuipselect-filter-value rs-wuipselect-filter-value-swatch colornav-link {{#unless applied}}current{{/unless}}" for="filter_{{id}}_default">
                    <span class="colornav-swatch">
                        <span class="colornav-label">{{{.}}}</span>
                    </span>
                </label>
            </li>
            {{/defaultSelectionText}}
            {{#each values}}
            {{#unless hidden}}
            <li class="colornav-item">
                <input class="rs-wuipselect-filter-input rs-wuipselect-filter-input-swatch {{#if disabled}}disabled{{/if}}" id="filter_{{id}}_{{key}}" type="checkbox" name="filter_{{id}}" ($change)="toggleFilter(id, key)" {{#if checked}}checked{{/if}} {{#if disabled}}disabled{{/if}}>
                <label class="rs-wuipselect-filter-value rs-wuipselect-filter-value-swatch colornav-link {{#if checked}}current{{/if}}" for="filter_{{id}}_{{key}}">
                    <span class="colornav-swatch">
                        {{#swatchImage}}
                        <img src="{{srcSet.src}}" alt="" width="{{width}}" height="{{height}}" class="as-colornav-swatch-img">
                        {{/swatchImage}}
                        <span class="colornav-label">{{{value}}}</span>
                    </span>
                </label>
            </li>
            {{/unless}}
            {{/each}}
        </ul>
    </div>
{{else}}
    {{#defaultSelectionText}}
    <div>
        <input class="rs-wuipselect-filter-input {{#unless applied}}checked{{/unless}}" id="filter_{{id}}_default" type="checkbox" name="filter_{{id}}" ($change)="toggleFilter(id, null)" {{#unless applied}}checked{{/unless}}/>
        <label class="rs-wuipselect-filter-value rs-wuipselect-filter-value-pill" for="filter_{{id}}_default">{{{.}}}</label>
    </div>
    {{/defaultSelectionText}}
    {{#each values}}
    {{#unless hidden}}
    <div>
        <input class="rs-wuipselect-filter-input {{#if checked}}checked{{/if}} {{#if disabled}}disabled{{/if}}" id="filter_{{id}}_{{key}}" type="checkbox" name="filter_{{id}}" ($change)="toggleFilter(id, key)" {{#if checked}}checked{{/if}} {{#if disabled}}disabled{{/if}}/>
        <label class="rs-wuipselect-filter-value rs-wuipselect-filter-value-pill" for="filter_{{id}}_{{key}}">{{{value}}}</label>
    </div>
    {{/unless}}
    {{/each}}
{{/if}}
</script>


