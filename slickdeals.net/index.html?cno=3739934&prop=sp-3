	.ajaxified-cart-feedback {
		position: absolute;
	  display: block;
	  line-height: 36px;
	  font-size: 90%;
	  vertical-align: middle;
	}
	.ajaxified-cart-feedback.success { 
	  color: #1998d5;
	}
	.ajaxified-cart-feedback.error { 
	  color: #f16522; 
	} 
	.ajaxified-cart-feedback a {
	  border-bottom: 1px solid;
	}
</style>




