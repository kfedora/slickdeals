<script>
    function GetQtyValue(value) {

        $('#addtocartqty').attr("unbxdparam_qty", value);
        return false;
    }
    $("#qtyminus").on('click', function () {
        var qty = $('#add-to-cart-qty').val();
        $('#addtocartqty').attr("unbxdparam_qty", qty);
    });
    $("#qtyplus").on('click', function () {
        var qty = $('#add-to-cart-qty').val();
        $('#addtocartqty').attr("unbxdparam_qty", qty);
    });

    function blockEvent(event) {
        if (event.keyCode == 13)
            return false;
    }

</script>            </div>
