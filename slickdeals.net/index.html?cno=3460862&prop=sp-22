<script>
var sprintMigrationBanner = new Vue({
	el:       '#sprintMigrationBanner',
	data:     {
		dismissed: true,
		cookieName: 'SMBtest02'
	},
	methods:  {
		getCookie(){
		    var pattern = RegExp(this.cookieName + "=.[^;]*")
		    matched = document.cookie.match(pattern)
		    if(matched){
		        var cookie = matched[0].split('=')
		        return cookie[1]
		    }
		    return false
		},
		setCookie(){
			var cookie = this.cookieName+"=true;";
			document.cookie = cookie;
			this.dismissed = true;
		}
	},
	computed: {
		expirationDate: function() {
			var now = new Date();
			var oneYr = new Date();
			oneYr.setYear(now.getYear() + 1);
			return oneYr;
		}
	},
	mounted:  function() {
		this.dismissed = this.getCookie();
	}
});
</script>			
