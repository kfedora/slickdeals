function RebateInfo(mypage,w,h) {
var feat;
if(screen.width){
var winl = (screen.width-w)/2;
var wint = (screen.height-h)/2;
}else{winl = 0;wint =0;}
if (winl < 0) winl = 0;
if (wint < 0) wint = 0;

feat = "status=no,toolbar=no,menubar=no,location=no,scrollbars=yes,resizable=yes,width="+w+",height="+h+",top="+wint+",left="+winl+"";
win = window.open(mypage,"Rebates",feat);
win.window.focus();
}
