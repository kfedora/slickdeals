  /*jQuery(document).ready(function(){
    setTimeout(function() {
        fixHeight();
    }, 1);
  });*/
  
  jQuery(window).load(function(){
    fixHeight();
  });
  
  
  jQuery(window).resize(function(){
    fixHeight();
  });
  
  
  function fixHeight(){
      if(parseInt(jQuery(window).width()) > 768){
        jQuery('.billboard').each(function(){
            //var imgHeight = parseInt(jQuery(this).children('.billboardImg').children('img').height());
            var contentHeight = parseFloat(jQuery(this).children('.billboardHeadline').height());
            //console.log(jQuery(this).children('.billboardHeadline').height());
            
            contentHeight = contentHeight + 16; //account for shifted top and bottom images
            jQuery(this).children('.billboardImg').css('height', contentHeight + 'px');
            //jQuery(this).children('.billboardHeadline').css('height', '');
        });
      } else {
        jQuery('.billboardImg').css('height', '');
        //console.log(jQuery(this).children('.billboardImg').html());
        //jQuery(this).children('.billboardHeadline').css('height', '');
      }
  }
</script>
