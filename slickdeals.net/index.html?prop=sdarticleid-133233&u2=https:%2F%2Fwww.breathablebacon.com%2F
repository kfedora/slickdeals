<script type="text/javascript">
	//<![CDATA[
(function(w, d, $){
  var cookieKey = 'fpvSignedUp';
  
  function openFullpageViolator() {
    if(doShowViolator()){
      $('#violator-curtin').fadeIn('1000', function(){
        $('#violator-curtin').addClass('open');
		setStoreDate();
      });
    }
  }

  function closeFullpageViolator() {
    $('#violator-curtin').fadeOut('1000', function(){
      $('#violator-curtin').removeClass('open');
      //openHeaderViolator()
    });

  }

  function setStoreDate(){
    var date = new Date;
	date.setDate(date.getDate() + 7);
	d.cookie=cookieKey+"=false; expires="+date+"; path=/";
  }

  function insertSigedUp(){
    if(isLocalStorageAvailable()){
      w.localStorage.setItem(storageKey, dateObj.toDateString());
    } else {
      d.cookie = storageKey + '=' + dateObj.toDateString();
    }
  }

  function doShowViolator(){
    var cookieVal = getValuesFromCookies(cookieKey);
	return cookieVal == undefined;
  }

  function getValuesFromCookies(key){
    var elements = d.cookie.split(';');
    for(var i=0; i<elements.length; i++){
      if(elements[i].indexOf(key) > -1){
        return elements[i].split('=')[1];
      }
    }
  }
  
  function openHeaderViolator(){
    var cookieVal = getValuesFromCookies(cookieKey);
	if(cookieVal === 'false') {
	  $('.violator-closed').addClass('violator-open');
      $('.violator-closed').removeClass('violator-closed');
	  $('.email-violator').slideDown();
	}
  }
})()

//]]>
</script>
