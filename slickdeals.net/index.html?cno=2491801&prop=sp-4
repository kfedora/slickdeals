<script type="text/javascript">
var _pdp = {
settings:null,
init:function(_settings){
_pdp.settings = _settings || {};
try {
var selectedvariant = $(_pdp.settings.variantButtonClass +'.' + _pdp.settings.activeClass);
_pdp.settings.imageSet = selectedvariant.length > 0 ? $(selectedvariant).find(_pdp.settings.dataNode).data(_pdp.settings.dataSet).split(",")
: $(_pdp.settings.variantButtonClass).find(_pdp.settings.dataNode).data(_pdp.settings.dataSet).split(",");
} catch (e) {
$.each($(_pdp.settings.variantButtonClass),function(i,v){
if (i === 0) {
_pdp.settings.imageSet = $(v).find(_pdp.settings.dataNode).data(_pdp.settings.dataSet).split(",");
}
});
}
_pdp.swapAssetsTo(_pdp.settings.imageSet);
_pdp.bindEvents();
},
buildCarousel:function(){
$(_pdp.settings.carouselParent).slick({
slidesToShow: 4,
slidesToScroll: 1,
arrows: true,
infinite: true,
dots: true,
responsive: [
{
breakpoint: 1140,
settings: {
slidesToShow: 3,
dots: false
}
},
{
breakpoint: 769,
settings: {
centerMode: true,
centerPadding: '190px',
slidesToShow: 1,
dots: false
}
},
{
breakpoint: 600,
settings: {
centerMode: true,
slidesToShow: 1,
centerPadding: '80px',
dots: false
}
}
]
});
$(".product-display__carousel--container").on("click",function(){
$(".product-display__left__img").attr("src",$(this).find(".product-display__carousel--img").attr("src"));
})
},
swapAssetsTo:function(_imageArray){
_pdp.settings.imageOne = _imageArray[0];
$(_pdp.settings.mainImageClass).find("img").remove();
$(_pdp.settings.mainImageClass).append("<img alt=\"image "+i+"\" class=\"product-display__left__img\" src="+_pdp.settings.imageOne+"\" />");
if($(_pdp.settings.carouselParent).hasClass("slick-initialized")){
$(_pdp.settings.carouselParent).slick("unslick");
}
$(".product-display__carousel--container").remove();
$.each(_imageArray,function(i,v){
var carouselNode = $("<div />");
carouselNode.addClass("product-display__carousel--container");
carouselNode.append("<img alt='' class=\"product-display__carousel--img\" src="+v+"\" />");
$(_pdp.settings.carouselParent).append(carouselNode);
})
_pdp.buildCarousel();
},
bindEvents:function(){
$.each($(_pdp.settings.variantButtonClass),function(i,v){
$(v).on("click",function(){
$(_pdp.settings.variantButtonClass).removeClass(_pdp.settings.activeClass);
$(this).addClass(_pdp.settings.activeClass);
_pdp.swapAssetsTo($(this).find(_pdp.settings.dataNode).data("image-set").split(","));
var swatchData = JSON.parse( $(this).find( ".swatch-data" ).text() );
var params = {
PIDVAR : swatchData.productID,
format : "ajax"
};
var url1 = app.util.appendParamsToUrl(app.urls.homepageModuleCall, params);
// var url1 = ;
app.ajax.load({
url: url1,
callback : function (data) {
var dataContainer = $(document).find("#product-value-container");
