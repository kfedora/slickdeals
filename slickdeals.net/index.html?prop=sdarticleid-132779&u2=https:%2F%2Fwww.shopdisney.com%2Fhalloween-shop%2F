            <div class="refinement refinement--movieshow">

                
                

                

                

                

                
                    <header class="refinement__header">
                        <button 
                            title="Movie / Show"
                            class="collapsed"
                            data-toggle="collapse"
                            aria-controls="#refinement--movieshow"
                            data-target="#refinement--movieshow"
                            aria-controls="#refinement--movieshow"
                            data-linkid="list_page_content/filter/refinement category/movie / show"
                        >
                            Movie / Show
                            <span class="refinement__header_icon refinement__header_icon--plus" aria-hidden="true">&plus;</span> 
                            <span class="refinement__header_icon refinement__header_icon--minus" aria-hidden="true">&minus;</span>
                        </button>
                    </header>
                

            
            
                
                    

                    
                
                    

                    
                
                    

                    
                
                    

                    
                
            

